[
  [
    "\uff08\u9ed1\u6697\u3002\u4e00\u7247\u9ed1\u6697\u3002\uff09",
    "\uff08\u8ff7\u8499\u7684\u96fe\u6c14\u548c\u9ed1\u6697\u4e00\u8d77\u51fa\u73b0\u5728\u6211\u773c\u524d\u3002\u6211\u542c\u5230\u6709\u6c34\u6ef4\u843d\u4e0b\u2026\u2026\u4f9d\u636e\u6c34\u6ef4\u4ea7\u751f\u7684\u56de\u58f0\u5224\u65ad\uff0c\u8fd9\u91cc\u975e\u5e38\u7a7a\u65f7\u3002\uff09",
    "\uff08\u8fd9\u91cc\u2026\u2026\u5e94\u8be5\u662f\u2026\u2026\uff09",
    "\u4f60\u8981\u79bb\u5f00\uff1f",
    "\u6211\u60f3\u77e5\u9053\u539f\u56e0\u3002",
    "\u5d29\u6bc1\u7684\u79e9\u5e8f\u6b63\u5728\u547c\u5524\u590d\u4f4d\uff0c\u8fd9\u6b8b\u7834\u7684\u4e16\u754c\uff0c\u7ec8\u4e8e\u80fd\u591f\u7edf\u4e00\u3002|\u6211\u4eec\u773c\u524d\u7684\u4e00\u5207\uff0c\u5c06\u91cd\u7ec4\u4e3a\u65b0\u7684\u4e16\u754c\u3001\u65b0\u7684\u79e9\u5e8f\u2014\u2014\u800c\u6211\u4eec\uff0c\u5373\u5c06\u950b\u8292\u6bd5\u9732\u3002",
    "\u2026\u2026\u53ea\u662f\u4f60\uff0c\u4e0d\u662f\u6211\u4eec\uff0c\u4f60\u9700\u8981\u7684\u4e0d\u662f\u6211\uff0c\u7235\u58eb\u3002|\u5bf9\u4e8e\u4f60\uff0c\u6211\u53ea\u662f\u4e00\u9897\u68cb\u5b50\u3001\u4e00\u4e2a\u53ef\u4ee5\u88ab\u8f7b\u6613\u66ff\u4ee3\u7684\u5229\u7528\u5bf9\u8c61\uff0c\u6211\u4e0d\u4f1a\u518d\u542c\u4f60\u547d\u4ee4\u4e86\u3002",
    "\u6211\u53ea\u60f3\u77e5\u9053\u4f60\u7684\u6700\u7ec8\u9009\u62e9\u3002",
    "\u4fe1\u5949\u7edd\u5bf9\u771f\u7406\u7684\u523a\u732c\uff0c\u5c06\u4e16\u754c\u7f6e\u4e8e\u5177\u6709\u552f\u4e00\u6027\u7684\u6846\u67b6\u4e0b\uff0c\u7ad6\u8d77\u6d51\u8eab\u7684\u523a\uff0c\u6297\u62d2\u6240\u6709\u5f02\u8bae\uff1b|\u72e1\u733e\u591a\u53d8\u7684\u72d0\u72f8\uff0c\u86f0\u4f0f\u5728\u6d69\u8361\u7684\u65f6\u4ee3\u6d2a\u6d41\u4e4b\u4e2d\uff0c\u772f\u8d77\u773c\u775b\uff0c\u4f53\u5bdf\u51fa\u8db3\u4ee5\u98a0\u8986\u4e16\u754c\u7684\u4e0d\u548c\u8c10\u97f3\u3002",
    "\u2014\u2014\u4f60\u4ee5\u524d\u5bf9\u6211\u63d0\u51fa\u7684\u95ee\u9898\uff0c\u6211\u73b0\u5728\u5c31\u5728\u505a\u9009\u62e9\u3002",
    "\u4f60\u4e0d\u60f3\u6210\u4e3a\u523a\u732c\uff1f",
    "\u4e5f\u4e0d\u60f3\u6210\u4e3a\u72d0\u72f8\u3002",
    "\u4f60\u9009\u62e9\u9003\u907f\u3002",
    "\u4e0d\uff0c\u662f\u4e0d\u518d\u88ab\u4f60\u5229\u7528\u3002|\u6211\u7684\u6240\u6709\u9009\u62e9\uff0c\u6700\u7ec8\u90fd\u4f1a\u7528\u6765\u7ed9\u4f60\u3001\u7ed9\u50cf\u4f60\u8fd9\u6837\u7684\u6df7\u86cb\u57ab\u811a\u3002",
    "\u6211\u505a\u4e86\u4ec0\u4e48\u9009\u62e9\uff0c\u8fd9\u91cd\u8981\u5417\uff1f\u4e16\u754c\u6ca1\u6709\u53d8\u5f97\u66f4\u597d\uff0c\u4e5f\u6ca1\u6709\u53d8\u5f97\u66f4\u574f\uff0c\u5b83\u6ca1\u6709\u56e0\u4e3a\u6211\u7684\u9009\u62e9\u4ea7\u751f\u4efb\u4f55\u53d8\u5316\uff01",
    "\u4e4b\u524d\u7684\u4e8b\uff0c\u6211\u4e0d\u4f1a\u8fa9\u89e3\u3002\u4f46\u4f60\u7684\u9009\u62e9\uff0c\u5fc5\u987b\u8de8\u8fc7\u65e2\u5b9a\u4e8b\u5b9e\u3002|\u9003\u907f\u65e0\u6cd5\u5bfc\u5411\u4efb\u4f55\u7ed3\u679c\uff0c\u4e00\u5473\u540c\u60c5\u81ea\u5df1\u7684\u53ea\u80fd\u662f\u61e6\u592b\u3002",
    "\u9003\u907f\uff1f\u61e6\u592b\uff1f\u5475\uff0c\u6211\u90fd\u8ba4\u4e86\u3002|\u4e0d\u8981\u518d\u6765\u627e\u6211\uff0c\u6211\u4e5f\u4e0d\u4f1a\u518d\u56de\u6765\u3002\u6211\u5df2\u7ecf\u544a\u8bc9\u8fc7\u4f60\uff0c\u81ea\u6211\u6d41\u653e\uff0c\u662f\u6211\u6700\u7ec8\u7684\u9009\u62e9\u3002",
    "\u7235\u58eb\uff0c\u6c38\u522b\u3002",
    "\u4f60\u5fc5\u987b\u505a\u51fa\u6b63\u786e\u9009\u62e9\uff0c\u4f60\u7684\u547d\u8fd0\u5fc5\u987b\u56de\u5230\u6b63\u8f68\u3002|\u4eab\u53d7\u73b0\u5728\u77ed\u6682\u7684\u79bb\u522b\u5427\uff0c\u800c\u6211\u4eec\u2014\u2014\u5fc5\u5c06\u518d\u89c1\u3002",
    "\u6307\u6325\u5b98\u2026\u2026"
  ],
  [
    "\u2014\u2014\u75bc\u75bc\u75bc\u2026\u2026",
    "\uff08\u6307\u6325\u5ba4\u91cc\uff0c\u5fd9\u788c\u4e8e\u603b\u63a7\u53f0\u524d\u7684\u95ea\u7535\u5934\u4e5f\u4e0d\u56de\u3002\uff09",
    "\u9192\u4e86\uff0c\u6307\u6325\u5b98\uff1f|\u5496\u5561\u673a\u65c1\u8fb9\u7684\u684c\u5b50\u4e0a\u6709\u9192\u9152\u8336\u3002",
    "\u2026\u2026\u9192\u2026\u2026\u4ec0\u4e48\uff1f",
