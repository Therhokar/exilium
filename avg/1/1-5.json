[
  [
    "\uff08\u95ea\u7535\u5c0f\u961f\u6b7c\u706d\u4e86\u90e8\u5206\u8822\u8822\u6b32\u52a8\u7684\u74e6\u826f\u683c\u5e2e\u6210\u5458\uff0c\u65e0\u4eba\u673a\u8d81\u9699\u964d\u843d\u3002\uff09",
    "\uff08\u4e00\u65f6\u540e\u9000\u7684\u74e6\u826f\u683c\u5e2e\u6210\u5458\u770b\u5230\u65e0\u4eba\u673a\u5373\u5c06\u964d\u843d\uff0c\u7eb7\u7eb7\u518d\u6b21\u56f4\u4e0a\u6765\u3002\uff09",
    "\uff08\u9762\u5bf9\u6c79\u6d8c\u7684\u4eba\u7fa4\uff0c\u4e00\u67b6\u5373\u5c06\u964d\u843d\u7684\u65e0\u4eba\u673a\u4e0b\u90e8\u7531\u540a\u81c2\u540a\u51fa\u4e86\u5927\u578b\u81ea\u52a8\u70ae\u53f0\uff0c\u5728\u4e0b\u843d\u7684\u540c\u65f6\u5411\u5468\u56f4\u626b\u5c04\u3002\uff09",
    "\uff08\u906d\u5230\u610f\u6599\u4e4b\u5916\u7684\u706b\u529b\u538b\u5236\uff0c\u74e6\u826f\u683c\u5e2e\u6210\u5458\u5404\u81ea\u9003\u7a9c\uff0c\u5176\u4ed6\u8f7d\u6709\u7269\u8d44\u7684\u65e0\u4eba\u673a\u5728\u81ea\u52a8\u70ae\u53f0\u7684\u706b\u529b\u52a0\u6301\u4e0b\u987a\u5229\u843d\u5730\u3002\uff09",
    "\u56af\u56af~\u76ee\u6807\u8fbe\u6210\u3002\u7269\u8d44\u9f50\u5168\uff0c\u4f60\u4eec\u4e5f\u8be5\u56de\u53bb\u4e86\uff1f|\u54e6\u5bf9\uff0c\u53ef\u8981\u8bb0\u5f97\u5e2e\u6211\u548c\u4f60\u4eec\u6307\u6325\u5b98\u95ee\u597d\u3002",
    "\u2026\u2026\u6307\u6325\u5b98\u7559\u4e0b\u4e86\u4e00\u53e5\u8bdd\uff0c\u5e0c\u671b\u80fd\u591f\u8f6c\u8fbe\u7ed9\u60a8\u3002",
    "\u4ec0\u4e48\uff1f",
    "\u2026\u2026\u5e0c\u671b\u3002",
    "\u6307\u6325\u5b98\u8bf4\uff0c\u5e0c\u671b\u574d\u584c\u98ce\u66b4\u4e0d\u4f1a\u541e\u566c\u963f\u5df4\u8d21\u7684\u7075\u9b42\u3002",
    "\u2014\u2014\u54c8\u2014\u2014\u597d\u7684\uff0c\u4f5c\u4e3a\u8fd9\u53e5\u8bdd\u7684\u8c22\u793c\uff0c\u501f\u4f60\u4eec\u4e00\u8f86\u8f66\uff0c\u628a\u7269\u8d44\u5e26\u56de\u53bb\u3002",
    "\u591a\u8c22\u3002",
    "\u79df\u91d1\u8bb0\u8d26\uff0c\u627f\u8499\u60e0\u987e\u3002|\u5e0c\u671b\u4f60\u4eec\u6307\u6325\u5b98\u80fd\u591f\u5728\u98ce\u66b4\u7684\u795d\u798f\u4e0b\u4e00\u5207\u987a\u5229\u3002",
    "\u2026\u2026\u591a\u8c22\u3002",
    "\u514b\u7f57\u4e3d\u79d1\u2014\u2014\u662f\u8fd9\u4e2a\u540d\u5b57\u5427\uff1f",
    "\u554a\u3002",
    "\u201c\u5c0f\u961f\u4e2d\u7f3a\u5931\u7684\u90e8\u5206\u201d\u2026\u2026|\u6709\u4f60\u52a0\u5165\uff0c\u6307\u6325\u5b98\u5e94\u8be5\u4e5f\u80fd\u677e\u53e3\u6c14\u3002",
    "\u2026\u2026\u554a\u3001\u554a\uff1f",
    "\u514b\u7f57\u4e3d\u79d1\uff0c\u7eb3\u7f8e\u897f\u4e1d\uff0c\u6765\u628a\u7269\u8d44\u88c5\u597d\u3002",
    "\u6765\u3001\u6765\u4e86\uff0c\u961f\u957f\uff01",
    "\u7b49\u4f60\u4eec\u901a\u8baf\u8bbe\u5907\u4fee\u590d\uff0c\u7ed9\u6211\u53d1\u4e2a\u4fe1\u53f7\u3002"
  ]
