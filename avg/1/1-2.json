[
  [
    "\u5168\u8f66\u81ea\u68c0\u5f00\u59cb\uff0c\u9884\u8ba1\u5b8c\u6210\u65f6\u95f40812\u3002",
    "\uff08\u6211\u7ad9\u5728\u6307\u6325\u5ba4\u7684\u843d\u5730\u7a97\u524d\u3002\u6b64\u523b\uff0c\u7a97\u5916\u4e00\u7247\u5b89\u9759\u3002\uff09",
    "\u2026\u2026\u8fde\u7eed\u7684\u574d\u584c\u98ce\u66b4\u3001\u6c14\u8c61\u8bb0\u5f55\u9519\u8bef\u3001\u5730\u5f62\u56fe\u9519\u8bef\u3001\u4efb\u52a1\u4fe1\u606f\u6cc4\u9732\u2026\u2026\u592a\u5de7\u5408\u4e86\u3002",
    "\u2014\u2014\u6307\u6325\u5b98\u6000\u7591\u8fd9\u6b21\u7684\u88ad\u51fb\u6709\u9884\u8c0b\uff1f",
    "\u5fc5\u7136\u3002\u9644\u8fd1\u73af\u5883\u60c5\u62a5\u7684\u4f9b\u5e94\u5546\u662f\u8c01\uff1f",
    "\u8d2d\u4e70\u8bb0\u5f55\u4e2d\u7684\u4ea4\u6613\u65b9\u663e\u793a\u4e3a\u9644\u8fd1\u7684B.R.I.E.F.\u5206\u4f1a\uff0c\u6807\u7b7e\u4e3a\u901a\u7528\u60c5\u62a5\uff0c\u6ca1\u6709\u7279\u6b8a\u7684\u4f9b\u5e94\u5546\u3002",
    "\u4efb\u52a1\u7684\u59d4\u6258\u65b9\uff1f",
    "\u8ba2\u5355\u4ea4\u6258\u65f6\uff0c\u663e\u793a\u7684\u59d4\u6258\u65b9\u4e3a\u533f\u540d\uff0c\u7531B.R.I.E.F.\u603b\u4f1a\u7ecf\u8f6c\uff0c\u53ea\u6709\u63a5\u6536\u8ba2\u5355\u7684\u59d4\u6258\u4eba\u548c\u5176\u6240\u5c5e\u673a\u6784\u7684\u57fa\u7840\u4fe1\u606f\u3002",
    "\u8fd9\u4e2a\u4efb\u52a1\u916c\u91d1\u9ad8\u6602\uff0c\u6709\u591a\u4e2a\u8d4f\u91d1\u730e\u4eba\u5c0f\u961f\u4e89\u62a2\uff0c\u6700\u540e\u843d\u5165\u6211\u4eec\u624b\u4e2d\u3002",
    "\uff08\u95ea\u7535\u505c\u6b62\u4e86\u624b\u4e2d\u7684\u64cd\u4f5c\uff0c\u7565\u5fae\u4fa7\u8eab\uff0c\u626d\u5934\u770b\u6211\u3002\uff09",
    "\u2026\u2026\u76ee\u524d\uff0c\u7f51\u7edc\u65e0\u6cd5\u8fde\u63a5\uff0c\u8fdb\u4e00\u6b65\u7684\u4fe1\u606f\u65e0\u6cd5\u83b7\u5f97\u3002\u4f46\u5c31\u76ee\u524d\u6211\u4eec\u5df2\u77e5\u7684\u60c5\u51b5\u6765\u770b\uff0c\u9664\u53bb\u5f02\u5e38\u7684\u916c\u91d1\u989d\u5ea6\uff0c\u8fd9\u4e2a\u4efb\u52a1\u4e0e\u8fc7\u5f80\u5176\u4ed6\u4efb\u52a1\u6ca1\u6709\u4efb\u4f55\u533a\u522b\u3002",
    "\u2026\u2026",
    "\u5148\u6062\u590d\u901a\u8baf\uff0c\u8fde\u63a5\u7f51\u7edc\u8865\u5145\u7269\u8d44\u4fee\u590d\u57fa\u5730\u8f66\u3002|\u2026\u2026\u4e5f\u9700\u8981\u5c3d\u5feb\u8d2d\u7f6e\u7d20\u4f53\u91cd\u542f\u514b\u7f57\u4e3d\u79d1\uff0c\u7eb3\u7f8e\u897f\u4e1d\u5947\u602a\u7684\u76f4\u89c9\u4e5f\u8bb8\u80fd\u5e2e\u4e0a\u5fd9\u3002",
    "\u2026\u2026\u9ed1\u5f71\u5f98\u5f8a\u4e8e\u591c\u96fe\u2026\u2026\u5c16\u9510\u7684\u68f1\u89d2\u2026\u2026",
    "\u5443\u2026\u2026\u597d\u7684\uff1f",
    "\u2026\u2026",
    "\u6709\u7fa4\u4f53\u6027\u751f\u9ab8\u63a5\u8fd1\uff0c\u5a01\u80c1\u5ea6II~III\u3002",
    "\u5e72\u6389\u5b83\u4eec\u3002"
  ],
  [
    "\u81ea\u68c0\u5b8c\u6210\uff0c\u5168\u8f66\u53d7\u635f49%\u3002|\u7eb3\u7f8e\u897f\u4e1d\u6b63\u5728\u534f\u52a9\u7f8e\u73b2\u8fdb\u884c\u5e94\u6025\u5904\u7406\uff0c\u4f46\u96f6\u4ef6\u7f3a\u5931\u4e25\u91cd\uff0c\u4fee\u590d\u56f0\u96be\u3002",
    "\u6218\u672f\u4eba\u5f62\u76ee\u524d\u53ea\u6709\u6211\u548c\u7eb3\u7f8e\u897f\u4e1d\uff0c\u5982\u679c\u6709\u5a01\u80c1\u5ea6\u66f4\u9ad8\u7684\u751f\u9ab8\u51fa\u73b0\uff0c\u6216\u8005\u74e6\u826f\u683c\u5e2e\u8ffd\u4e0a\u6765\uff0c\u6307\u6325\u5b98\uff0c\u6211\u4eec\u53ef\u80fd\u96be\u4ee5\u5e94\u5bf9\u3002",
    "\u603b\u4e4b\uff0c\u5148\u628a\u57fa\u5730\u8f66\u5f00\u8d77\u6765\u3002",
    "\u76ee\u7684\u5730\u5750\u6807\uff1f",
    "\u867d\u7136\u60f3\u7ee7\u7eed\u5411ZJKK32E-U8PB4Q\u524d\u8fdb\uff0c\u4f46\u76ee\u524d\u7684\u72b6\u51b5\u4e5f\u5b9e\u5728\u662f\u2014\u2014",
    "\u55ef\u2026\u2026",
    "\u73b0\u5728\u7684\u60c5\u51b5\uff0c\u4e0e\u4ee5\u524d\u6709\u4e9b\u76f8\u4f3c\u3002",
    "\u2026\u2026\uff1f",
    "\u4ee5\u524d\uff0c\u6307\u6325\u5b98\u521a\u6210\u4e3a\u8d4f\u91d1\u730e\u4eba\uff0c\u5728\u6c61\u67d3\u533a\u4e2d\u5b8c\u6210\u59d4\u6258\u7684\u65f6\u5019\uff0c|\u8fd9\u79cd\u6beb\u65e0\u5934\u7eea\u7684\u60c5\u51b5\uff0c\u51fa\u73b0\u7684\u9891\u7387\u53ef\u662f\u76f8\u5f53\u9ad8\u3002",
    "\u4ee5\u524d\u2026\u2026\u54c8\u54c8\u3002",
    "\u90a3\u65f6\u5019\u7684\u6307\u6325\u5b98\uff0c\u548c\u73b0\u5728\u7684\u6307\u6325\u5b98\uff0c\u5dee\u522b\u76f8\u5f53\u5927\u3002",
    "\u90a3\u65f6\u7684\u4f60\uff0c\u548c\u73b0\u5728\u7684\u4f60\uff0c\u5dee\u522b\u4e5f\u76f8\u5f53\u5927\u2014\u2014|\u4e0d\uff0c\u4e0d\u5982\u8bf4\uff0c\u73b0\u5728\u7684\u4f60\uff0c\u624d\u66f4\u50cf\u201c\u4ee5\u524d\u7684\u4f60\u201d\u3002",
    "\u2026\u2026",
    "\u521a\u8fdb\u5165\u6c61\u67d3\u533a\u65f6\uff0c\u884c\u52a8\u6a21\u5f0f\u8fd8\u6ca1\u6709\u8f6c\u53d8\u8fc7\u6765\u2026\u2026\u7b49\u63a5\u8fde\u5403\u4e86\u4e8f\uff0c\u6211\u624d\u610f\u8bc6\u5230\u81ea\u5df1\u5df2\u7ecf\u4e0d\u662f\u4e4b\u524d\u7684\u201c\u6307\u6325\u5b98\u201d\u3002",
    "\u2026\u2026\u6307\u6325\u5b98\uff0c\u5c31\u53ea\u662f\u6307\u6325\u5b98\u3002",
    "\u54ea\u6015\u6211\u8fde\u6700\u57fa\u672c\u7684B.R.I.E.F.\u63a5\u6536\u7cfb\u7edf\u90fd\u7528\u4e0d\u597d\uff1f",
    "\uff08\u6211\u60f3\u5230\u4e86\u90a3\u6700\u8270\u96be\u7684\u51e0\u5e74\u3002\uff09",
    "\uff08\u8d77\u5148\uff0c\u53ea\u6709\u6211\u81ea\u5df1\u3002\u5f53\u65f6\uff0c\u8d4f\u91d1\u730e\u4eba\u8fd8\u4e0d\u662f\u4e2a\u6b63\u513f\u516b\u7ecf\u7684\u804c\u4e1a\uff0c\u6211\u5c31\u8fd9\u4e48\u6d51\u6d51\u5669\u5669\u5730\u505a\u70b9\u79c1\u4eba\u59d4\u6258\uff0c\u8fc7\u7740\u6709\u4eca\u5929\u6ca1\u660e\u5929\u7684\u65e5\u5b50\u3002\uff09",
    "\uff08\u7136\u540e\uff0c\u6211\u5148\u662f\u6361\u5230\u4e86\u95ea\u7535\uff0c\u540e\u6765\u53c8\u4e00\u8d77\u642d\u6551\u4e86\u7f8e\u73b2\u3002\uff09",
    "\uff08\u4e4b\u540e\u6ca1\u8fc7\u591a\u4e45\uff0cB.R.I.E.F.\u5efa\u7acb\uff0c\u201c\u8d4f\u91d1\u730e\u4eba\u201d\u7ec8\u4e8e\u6709\u4e86\u6b63\u89c4\u7684\u6d41\u7a0b\u3001\u6b63\u89c4\u7684\u6e20\u9053\uff0c\u5728\u90a3\u4e4b\u540e\uff0c\u827e\u83ab\u53f7\u624d\u6e10\u6e10\u5468\u8f6c\u8d77\u6765\u3002\uff09",
    "\uff08\u95ea\u7535\u89c1\u8bc1\u4e86\u6211\u7684\u8bb8\u591a\u4e0d\u582a\uff0c\u5374\u4e00\u76f4\u575a\u5b9a\u5730\u966a\u5728\u6211\u7684\u8eab\u8fb9\u3002\uff09",
    "\u53ea\u662f\u5206\u4e0d\u6e05\u6697\u5546\u7684\u79c1\u8bbe\u7f51\u7edc\u548c\u7f57\u8054\u7684\u516c\u7528\u7f51\u7edc\uff0c\u5c06\u8ba2\u5355\u4fe1\u606f\u53d1\u9001\u5230\u7f57\u8054\u653f\u5e9c\u90e8\u95e8\u90a3\u91cc\uff0c\u5e76\u4e14\u56e0\u4e3a\u8ba2\u5355\u4e2d\u51fa\u73b0\u7684\u8fdd\u7981\u54c1\u88ab\u5b9a\u4f4d\u901a\u7f09\u4e00\u4e2a\u6708\u800c\u5df2\u3002",
    "\u2026\u2026\u6c61\u67d3\u533a\u4fe1\u53f7\u592a\u5dee\uff0c\u6765\u56de\u5207\u6362\u7f51\u7edc\uff0c\u4e00\u65f6\u5931\u8bef\uff0c\u4e5f\u662f\u96be\u514d\u3002",
    "\u4e0d\u8fc7\uff0c\u73b0\u5728\u6709B.R.I.E.F.\u4ece\u4e2d\u534f\u8c03\uff0c\u6697\u5546\u7f51\u7edc\u4e0e\u7f57\u8054\u7f51\u7edc\u7684\u901a\u8baf\u4ee3\u7801\u4e5f\u505a\u4e86\u533a\u5206\uff0c\u53d1\u9519\u4fe1\u606f\u8fd9\u79cd\u4e8b\uff0c\u4ee5\u540e\u4e0d\u592a\u4f1a\u51fa\u2014\u2014",
    "\u6307\u6325\u5b98\uff0c\u7f57\u8054\u7f51\u7edc\u3002",
    "\u957f\u57ce\u5217\u8f66\u7684\u9a7b\u519b\u5728\u6c61\u67d3\u533a\u94fa\u8bbe\u7684\u4fe1\u53f7\u5854\uff0c\u201c\u4fe1\u6807\u201d\uff01",
    "\u65e0\u4eba\u673a\u98de\u56de\u65f6\u9014\u7ecf\u957f\u57ce\u5217\u8f66\uff0c\u4fe1\u6807\u7fa4\u5e94\u8be5\u5c31\u5728\u9644\u8fd1\u3002",
    "\u51fa\u53d1\uff0c\u76ee\u7684\u5730\u4fe1\u6807\u7fa4\uff01",
    "\u53bb\u201c\u501f\u201d\u4e2a\u8bbe\u5907\u628a\u7f51\u7edc\u548c\u901a\u8baf\u6062\u590d\uff0c\u4e4b\u540e\u5c31\u6709\u529e\u6cd5\u4e86\u3002"
  ]
