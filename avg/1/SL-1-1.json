[
  [
    "\u8fd9\u4e00\u5e26\u7684\u4fe1\u6807\u5854\u6709\u4e9b\u8001\u65e7\uff0c\u4ecd\u7136\u80fd\u591f\u4f7f\u7528\u7684\uff0c\u53ea\u6709\u8fd9\u4e24\u4e2a\u3002",
    "\u55ef\u2026\u2026\u8fd9\u4e2a\u578b\u53f7\u5df2\u7ecf\u592a\u843d\u540e\u4e86\uff0c\u548c\u6211\u4eec\u57fa\u5730\u8f66\u7684\u901a\u8baf\u6a21\u5757\u4e0d\u80fd\u5b8c\u5168\u517c\u5bb9\u3002",
    "\u8fd9\u9644\u8fd1\u7684\u51c0\u5316\u533a\u7f16\u53f7\u4ecd\u7136\u662fW_2069\u3002",
    "2069\uff1f\u51c0\u5316\u6548\u7387\u8fd8\u771f\u9ad8\u3002|\u901a\u8baf\u8bbe\u5907\u53ef\u7528\u5417\uff1f",
    "\u5514\uff0c\u867d\u7136\u4e0d\u662f\u5b8c\u5168\u517c\u5bb9\uff0c\u4e0d\u8fc7\u5e94\u8be5\u4e5f\u80fd\u5f3a\u884c\u4f7f\u7528\uff0c\u4e2d\u8ddd\u79bb\u7684\u901a\u8baf\u5e94\u8be5\u6ca1\u95ee\u9898\u3002",
    "\u4e2d\u8ddd\u79bb\u2026\u2026\u7f8e\u73b2\uff0c\u5411\u6240\u6709\u80fd\u591f\u63a2\u6d4b\u5230\u4fe1\u53f7\u7684\u9891\u9053\u53d1\u9001\uff0c\u201c\u54cd\u6674\u8584\u65e5\uff0c\u6d77\u5996\u5728\u6c99\u6f20\u4e4b\u4e0a\u6b4c\u5531\u3002\u201d",
    "\u597d\u3001\u597d\u7684\u3002",
    "\uff08\u6d88\u606f\u53d1\u51fa\uff0c\u4e0d\u4e45\u6709\u4e86\u56de\u590d\u3002\uff09",
    "\u6536\u5230\u4e00\u6761\u6d88\u606f\uff0c\u201c\u6676\u6761\u5907\u597d\uff0c\u963f\u5df4\u8d21\u5728\u6b64\u606d\u5019\u201d\uff0c\u8fd8\u9644\u52a0\u4e86\u4e00\u4e32\u2014\u2014\u5e94\u8be5\u662f\u901a\u8baf\u4ee3\u7801\uff1f",
    "\u8fd9\u5bb6\u4f19\u2014\u2014\u547c\u53eb\u901a\u8baf\u4ee3\u7801\u3002",
    "\uff08\u901a\u8baf\u5f88\u5feb\u5c31\u88ab\u63a5\u8d77\u3002\uff09",
    "\u54df\uff0c\u8fd9\u4e48\u665a\u4e86\uff0c\u6211\u8fd8\u4ee5\u4e3a\u662f\u8c01\u5462\uff1f",
    "\u2026\u2026\u4f69\u59ae\u6cfd\u2026\u2026",
    "\u54c8\uff0c\u8fd9\u4e0d\u662f\u6307\u6325\u5b98\u5417~",
    "\u4f1a\u7528\u8fd9\u4e2a\u6697\u53f7\u7684\uff0c\u9664\u4e86\u6211\uff0c\u8fd8\u6709\u8c01\u3002",
    "\u56af~\u770b\u6837\u5b50\u662f\u51c6\u5907\u597d\u63a5\u53d7\u963f\u5df4\u8d21\u7684\u6d17\u793c\u4e86\uff1f",
    "\u2026\u2026\u4e0d\uff0c\u53ea\u662f\u4f60\u662f\u552f\u4e00\u6070\u597d\u5728\u9644\u8fd1\u6d3b\u52a8\u7684\u53ef\u7528\u6697\u5546\u3002",
    "\u54df\uff0c\u8fd9\u8fd8\u771f\u662f\u4ee4\u4eba\u5931\u671b~|\u4f5c\u4e3a\u60e9\u7f5a\uff0c\u6211\u53ef\u8981\u591a\u6d17\u70b9\u6cb9\u6c34\u4e0b\u6765\u54af\uff1f",
    "\u2026\u2026\u5e0c\u671b\u60a8\u80fd\u591f\u2026\u2026\u600e\u4e48\u8bf4\uff0c\u9ad8\u62ac\u8d35\u624b\uff1f|\u6211\u9700\u8981\u5927\u91cf\u57fa\u5730\u8f66\u5404\u6a21\u5757\u96f6\u4ef6\u548c\u4eba\u5f62\u7d20\u4f53\u3002",
    "\u8fd9\u662f\u8981\u53d1\u5c55\u65b0\u4e1a\u52a1\uff0c\u8fd8\u662f\u2026\u2026\uff1f",
    "\u6ca1\u9519\uff0c\u6211\u88ab\u88ad\u51fb\u4e86\u3002",
    "\u54c8\uff0c\u98ce\u6c34\u8f6e\u6d41\u8f6c\u5537\u6307\u6325\u5b98~\u6ca1\u60f3\u5230\u5230\u5904\u60f9\u662f\u751f\u975e\u7684\u4f60\u4e5f\u4f1a\u7ffb\u8f66\uff1f",
    "\u8c08\u4e0d\u4e0a\u7ffb\u8f66\uff0c\u53ea\u662f\u5728\u6b63\u5e38\u5b8c\u6210\u8ba2\u5355\u3002",
    "\u867d\u7136\u8fd9\u4e2a\u8ba2\u5355\u6709\u4e9b\u5947\u602a\uff0c\u4e00\u8def\u8fc7\u6765\u9047\u5230\u4e86\u4e00\u8fde\u4e32\u5de7\u5408\uff0c\u4f46\u8d27\u7269\u73b0\u5728\u8fd8\u5728\u6211\u624b\u91cc\u3002|\u7b49\u4ea4\u4e86\u8d27\uff0c\u62ff\u5230\u916c\u91d1\uff0c\u6211\u5c31\u6765\u627e\u4f60\u7ffb\u65b0\u6211\u7684\u827e\u83ab\u53f7\u3002",
    "\u7ffb\u65b0\u827e\u83ab\u53f7\u2014\u2014\u7b49\u7b49\uff0c\u90a3\u4e2a\u916c\u91d1\u9ad8\u6602\u7684\u8ba2\u5355\uff0c\u96be\u9053\u662f\u88ab\u4f60\u2014\u2014\uff1f",
    "\u2026\u2026\u5982\u679c\u6761\u4ef6\u53ea\u662f\u916c\u91d1\u9ad8\u6602\uff0c\u90a3\u6211\u624b\u91cc\u8fd9\u4e2a\u786e\u5b9e\u662f\u3002",
    "\u54e6\u2014\u2014",
    "\u5173\u4e8e\u8fd9\u4e2a\u8ba2\u5355\uff0c\u6700\u8fd1\u6709\u4e00\u4e9b\u5947\u5999\u7684\u4f20\u8a00\u3002",
    "\u600e\u4e48\u8bf4\uff1f",
    "\u73b0\u5728\u4fdd\u5bc6\u54df~|\u5982\u679c\u4f60\u80fd\u4e0a\u95e8\u81ea\u53d6\uff0c\u987a\u4fbf\u5e2e\u6211\u89e3\u51b3\u6389\u4e00\u70b9\u5c0f\u9ebb\u70e6\uff0c\u7ed9\u4f60\u4e5d\u516b\u6298\u9644\u5e26\u514d\u8d39\u60c5\u62a5\uff1f",
    "\u2026\u2026\u73b0\u5728\u6211\u7684\u65e0\u4eba\u673a\u548c\u4eba\u5f62\u90fd\u57fa\u672c\u5168\u706d\uff0c\u8fde\u80fd\u5f00\u7684\u5409\u666e\u8f66\u90fd\u53ea\u5269\u4e00\u8f86\u4e86\u3002",
    "\u4e0d\u5de7\u8036\u6307\u6325\u5b98~\u7279\u6b8a\u65f6\u671f\uff0c\u6982\u4e0d\u9001\u8d27\uff0c\u5e76\u4e14\u514d\u8d39\u60c5\u62a5\u4e0d\u53ef\u8d2d\u4e70\u54e6~",
    "\u4f60\u9047\u5230\u9ebb\u70e6\u4e86\uff1f",
    "\u5012\u4e0d\u662f\u4ec0\u4e48\u5927\u95ee\u9898\u3002\u524d\u6bb5\u65f6\u95f4\u8fdb\u4e86\u6279\u8d27\uff0c\u6709\u7fa4\u91ce\u72d7\u95fb\u5230\u5473\u9053\u8fc7\u6765\u62a2\u98df\u800c\u5df2\u3002",
    "\u2026\u2026\u542c\u8d77\u6765\u95ee\u9898\u4f3c\u4e4e\u5f88\u4e25\u91cd\u3002",
    "\u53ea\u662f\u4e0d\u77e5\u9053\u961f\u91cc\u7684\u8c01\u505a\u4e86\u72b9\u5927\u53bb\u4eb2\u543b\u6211\u4eec\u7684\u654c\u4eba\u3002",
    "\u2026\u2026\u6211\u8fd8\u4ee5\u4e3a\u6211\u4eec\u6ca1\u6709\u5171\u540c\u7684\u654c\u4eba\uff1f",
    "\u73b0\u5728\u4e0d\u5c31\u6709\u4e86\u2014\u2014\u4f60\u90a3\u91cc\u4ec0\u4e48\u60c5\u51b5\uff1f\u6709\u6742\u97f3\u54e6\u3002",
    "\u57fa\u5730\u8f66\u7684\u901a\u8baf\u8bbe\u5907\u6545\u969c\uff0c\u73b0\u5728\u7684\u8bbe\u5907\u662f\u501f\u7528\u7684\u3002",
    "\u662f\u5417\uff0c\u8fd9\u4e2a\u6742\u97f3\u542c\u8d77\u6765\u53ef\u4e0d\u4e00\u822c\u3002|\u603b\u4e4b\uff0cTY3IU-8SSZ90\uff0c\u8c22\u8c22\u60e0\u987e\uff0c\u6307\u6325\u5b98\u3002",
    "\u2026\u2026\u7f8e\u73b2\uff0c\u6574\u7406\u7269\u8d44\u6e05\u5355\uff0c\u53d1\u7ed9\u90a3\u4e2a\u901a\u8baf\u4ee3\u7801\u3002",
    "\u597d\u3001\u5443\u3001\u6307\u6325\u5b98\uff0c\u201c\u4f69\u59ae\u6cfd\u201d\uff0c\u5e94\u8be5\u662f\u2026\u2026\uff1f",
    "\u55ef\uff0c\u5c31\u662f\u90a3\u4e2a\u601d\u7ef4\u8df3\u8131\u7684\u8001\u670b\u53cb\u2014\u2014\u7269\u8d44\u7684\u79cd\u7c7b\u548c\u8d28\u91cf\u5012\u662f\u5bf9\u5f97\u8d77\u9ad8\u6602\u7684\u4ef7\u683c\u3002",
    "\u90a3\uff0c\u5979\u90a3\u91cc\u2026\u2026\u4f1a\u4e0d\u4f1a\u6709\u2026\u2026",
    "\u7269\u8d44\u6e05\u5355\u4e2d\u52a0\u4e00\u9879\u81ea\u917f\u7c73\u9152\uff0c\u6570\u91cf10\u3002",
    "\u2014\u2014\uff01",
    "\u592a\u3001\u592a\u597d\u4e86\uff0c\u8c22\u8c22\u60a8\u6307\u6325\u5b98\uff01",
    "\u8fd8\u6709\uff0c\u901a\u8baf\u88c5\u7f6e\u8c03\u6574\u4e00\u4e0b\uff0c\u56fa\u5b9a\u901a\u8bdd\u9891\u9053\uff0c\u6ce8\u610f\u52a0\u5bc6\u3002|\u53d6\u8d27\u5c31\u62dc\u6258\u4e86\uff0c\u95ea\u7535\uff0c\u7eb3\u7f8e\u897f\u4e1d\u2014\u2014\u8bb0\u5f97\u5e26\u4e0a\u514b\u7f57\u4e3d\u79d1\u7684\u5fc3\u667a\u5907\u4efd\u3002",
    "\u4e86\u89e3\u3002"
  ]
