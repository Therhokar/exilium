    "\u2026\u2026",
    "\u5728\u521a\u624d\u90a3\u6bb5\u65f6\u95f4\uff0c\u8f66\u961f\u505c\u6446\u3001\u6218\u6597\u529b\u91cf\u5168\u90e8\u79bb\u5f00\u3001\u800c\u7bb1\u5b50\u5374\u88ab\u7559\u4e0b\u3002\u5c3d\u7ba1\u5982\u6b64\uff0c\u6211\u4eec\u5374\u6ca1\u6709\u88ab\u653b\u51fb\u3002",
    "\u4e3a\u4ec0\u4e48\u2026\u2026\u96be\u9053\u662f\uff0c\u4f5c\u4e3a\u8ffd\u8e2a\u65b9\u7684\u74e6\u826f\u683c\u5e2e\u8fd8\u9700\u8981\u66f4\u591a\u65f6\u95f4\uff1f",
    "\u7bb1\u5b50\uff1f\u7bb1\u5b50\u600e\u4e48\u4e86\uff1f",
    "\u2026\u2026\u5bd2\u51b7\u2026\u2026\u9a71\u9010\uff0c\u56f0\u5026\u4e0e\u6c38\u7720\u2026\u2026",
    "\u7bb1\u5b50\u91cc\u88c5\u7684\u662f\u4eba\uff1f\uff01",
    "\u2026\u2026\u4e5f\u4e0d\u80fd\u786e\u5b9a\u5c31\u662f\u4eba\u7c7b\u3002|\u4f46\u5982\u679c\u662f\u4eba\u7c7b\u2026\u2026\u4ee5\u5f53\u65f6\u574d\u584c\u98ce\u66b4\u7684\u5f3a\u5ea6\uff0c\u90a3\u6837\u7684\u5973\u5b69\u5b50\u5c31\u7b97\u662f\u53ea\u505c\u7559\u51e0\u79d2\u2026\u2026",
    "\u4f9d\u636e\u4f69\u59ae\u6cfd\u7684\u60c5\u62a5\uff0c\u8fd9\u4e2a\u8ba2\u5355\u7684\u59d4\u6258\u65b9\u4e5f\u5f88\u53ef\u7591\u3002",
    "\u662f\u554a\u3002|\u6211\u4eec\u73b0\u5728\u662f\u8d4f\u91d1\u730e\u4eba\uff0c\u62ff\u4eba\u94b1\u8d22\uff0c\u66ff\u4eba\u529e\u4e8b\u2014\u2014\u672c\u6765\u5e94\u8be5\u662f\u8fd9\u6837\u3002",
    "\u662f\u5426\u5e94\u8be5\u8be2\u95ee\u4e00\u4e0bB.R.I.E.F.\u5173\u4e8e\u8ba2\u5355\u7684\u66f4\u591a\u4fe1\u606f\uff1f",
    "\u7b49\u901a\u8baf\u8bbe\u5907\u4fee\u590d\uff0c\u5c31\u8054\u7cfbB.R.I.E.F.\u3002|\u2026\u2026\u5e0c\u671b\u53ea\u662f\u6211\u4eec\u591a\u5fc3\u3002",
    "\u4f20\u52a8\u8bbe\u5907\u521d\u6b65\u4fee\u590d\uff0c\u53ef\u652f\u6301\u6b63\u5e38\u884c\u9a76\u3002|\u53e6\u5916\uff0c\u7f8e\u73b2\u62a5\u544a\u7bb1\u5b50\u4f3c\u4e4e\u6709\u8f7b\u5fae\u79fb\u52a8\u7684\u8ff9\u8c61\u3002",
    "\u7acb\u523b\u8f6c\u79fb\uff01",
    "\uff08\u8f66\u4f53\u5f00\u59cb\u79fb\u52a8\uff0c\u7a97\u5916\u51fa\u73b0\u4e86\u7f13\u6162\u6697\u4e0b\u53bb\u7684\u5929\u7a7a\u3002\uff09",
    "\uff08\u5728\u9010\u6e10\u8513\u5ef6\u5f00\u6765\u7684\u9ed1\u6697\u4e2d\uff0c\u6210\u7247\u7684\u7ea2\u5149\u6b63\u5411\u7740\u827e\u83ab\u53f7\u65b9\u5411\u63a5\u8fd1\u3002\uff09",
    "\u2014\u2014\u8fd9\u4ec0\u4e48\u4e1c\u897f\uff1f\u654c\u4eba\uff1f\uff01",
    "\u679c\u7136\u8fd8\u662f\u8ffd\u4e0a\u6765\u4e86\u2026\u2026\u8fd9\u7fa4\u74e6\u826f\u683c\u5e2e\u7684\u91ce\u72d7\u3002",
    "\u7f8e\u73b2\uff0c\u51fa\u73b0\u654c\u88ad\uff0c\u56de\u6307\u6325\u5ba4\u3002",
    "\u8fd9\u4e2a\u653e\u5728\u8fd9\u91cc\uff0c\u6309\u7167\u7a0b\u5e8f\u8bbe\u7f6e\u2014\u2014\u5443\u3001\u961f\u957f\uff1f",
    "\u95ea\u7535\uff0c\u62d6\u4f4f\u4ed6\u4eec\u3002|\u57fa\u5730\u8f66\u4f1a\u5c3d\u53ef\u80fd\u8d34\u8fd1\u51c0\u5316\u533a\u3002\u6709\u957f\u57ce\u5217\u8f66\u5b88\u519b\u5728\uff0c\u74e6\u826f\u683c\u5e2e\u4e0d\u6562\u9760\u8fd1\u3002",
    "\u4e86\u89e3\u3002|\u5404\u4f5c\u6218\u5355\u4f4d\uff0c\u51c6\u5907\u4f5c\u6218\u3002"
  ]
