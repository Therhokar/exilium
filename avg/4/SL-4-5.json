[
  [
    "\u2026\u2026\u56de\u5f52\u8fd9\u79cd\u5f62\u6001\uff0c\u4f60\u8fd9\u662f\u5728\u8f7b\u89c6\u6211\uff1f",
    "\u8f7b\u89c6\u60a8\u3002",
    "\uff08\u9ed1\u8272\u4eba\u5f62\u62ac\u9ad8\u4e86\u4e0b\u988c\u3002\uff09",
    "\uff08\u5979\u5c06\u624b\u4e2d\u7684\u4f1e\u6536\u8d77\uff0c\u4f1e\u5c16\u6307\u5411\u5730\u9762\u3002\uff09",
    "\u4e0d\u77e5\u9053\u60a8\u662f\u4ee5\u4f55\u79cd\u59ff\u6001\uff0c\u5bfb\u6c42\u6211\u7684\u91cd\u89c6\uff1f",
    "\u2026\u2026\u65e0\u4eba\u673a\u4e5f\u7559\u5728\u8fd9\u91cc\uff0c\u6211\u5e76\u4e0d\u8ba4\u4e3a\u4f60\u6ca1\u6709\u201c\u91cd\u89c6\u201d\u6211\u3002",
    "\u5c06\u60a8\u59a5\u5584\u5904\u7406\u4e4b\u540e\uff0c\u518d\u8ffd\u56de\u201c\u6837\u54c1\u201d\uff0c\u5e76\u4e0d\u9700\u8981\u82b1\u8d39\u592a\u957f\u65f6\u95f4\u3002",
    "\u201c\u6837\u54c1\u201d\u2026\u2026|\u8fd9\u662f\u4f60\u4eec\u5bf9\u7bb1\u5b50\u7684\u79f0\u547c\uff1f",
    "\u5982\u60a8\u4e00\u822c\u4f4e\u52a3\u7684\u4e8c\u4ee3\u4eba\u5f62\uff0c\u5e76\u4e0d\u5177\u6709\u5411\u6211\u63d0\u95ee\u7684\u6743\u5229\u3002",
    "\u90a3\u4e48\uff0c\u5728\u8fd9\u91cc\u62e6\u4f4f\u4f60\uff0c\u4ee5\u53ca\u4f60\u7684\u65e0\u4eba\u673a\u5c31\u53ef\u4ee5\u4e86\u3002",
    "\u54e6~",
    "\u8fd9\u662f\u4e2a\u4e0e\u60a8\u7684\u8eab\u4efd\u5e76\u4e0d\u76f8\u79f0\u7684\u60f3\u6cd5\uff0c\u5b89\u5fc3\u56de\u5f52\u751f\u4ea7\u7ebf\u5982\u4f55\u3002",
    "\u6050\u6015\u8981\u8ba9\u4f60\u5931\u671b\u4e86\u3002",
    "\uff08\u95ea\u7535\u505a\u51fa\u4e86\u5907\u6218\u52a8\u4f5c\u3002\uff09",
    "\u6765\uff0c\u6211\u8d76\u65f6\u95f4\u3002"
  ],
  [
    "\uff08\u6301\u7eed\u7684\u4ea4\u6218\u7834\u574f\u4e86\u672c\u5df2\u7ecf\u6447\u6447\u6b32\u5760\u7684\u5c4b\u9876\u5e73\u53f0\uff0c\u626c\u5c18\u548c\u785d\u70df\u51cf\u5c11\u4e86\u53ef\u89c6\u8ddd\u79bb\u3002\uff09",
    "\uff08\u70df\u5c18\u4e2d\uff0c\u95ea\u7535\u4e0e\u9ed1\u8272\u4eba\u5f62\u4e00\u89e6\u5373\u5206\uff0c\u5206\u522b\u843d\u5728\u76f8\u8fd1\u7684\u4e24\u4e2a\u76f8\u5bf9\u5b8c\u6574\u7684\u5efa\u7b51\u90e8\u5206\u4e0a\u3002\uff09",
    "\uff08\u95ea\u7535\u62ac\u5934\u770b\u4e86\u4e00\u773c\u8b66\u793a\u6807\u724c\uff0c\u56e0\u4e3a\u6d32\u9645\u5217\u8f66\u7684\u9760\u8fd1\uff0c\u8b66\u793a\u6807\u724c\u5df2\u7ecf\u5f00\u59cb\u8f6c\u7ea2\u3002\uff09",
    "\u60a8\u81ea\u5df1\u7684\u72b6\u51b5\uff0c\u6211\u8ba4\u4e3a\u66f4\u9700\u8981\u5173\u6ce8\u3002",
    "\u65f6\u95f4\u2026\u2026\u5dee\u4e0d\u591a\u4e86\uff0c\u6211\u7684\u4efb\u52a1\u2026\u2026\u5373\u5c06\u8fbe\u6210\u2026\u2026",
    "\u2014\u2014\u6211\u5dee\u4e0d\u591a\u4e5f\u8981\u538c\u5026\u4e86\u3002",
    "\uff08\u9ed1\u8272\u4eba\u5f62\u76ef\u7740\u906d\u5230\u91cd\u521b\u3001\u751a\u81f3\u53d1\u58f0\u90fd\u663e\u5f97\u5c24\u4e3a\u8270\u96be\u7684\u95ea\u7535\uff0c\u4e00\u6b65\u4e00\u6b65\u5411\u5979\u8d70\u53bb\u3002\uff09",
    "\uff08\u540c\u65f6\uff0c\u5979\u80f8\u524d\u7684\u7c7b\u62df\u6001\u56fe\u8c31\u72b6\u7269\u4f53\u5f00\u59cb\u4ea7\u751f\u53d8\u5316\u3002\uff09",
    "\uff08\u968f\u7740\u7269\u4f53\u7684\u53d8\u5316\uff0c\u536b\u661f\u57ce\u5185\uff0c\u8bbe\u7f6e\u5728\u5404\u680b\u697c\u4f53\u4e0a\u7684\u574d\u584c\u8f90\u5c04\u76d1\u6d4b\u5668\u7531\u8fd1\u81f3\u8fdc\uff0c\u5f00\u59cb\u9e23\u54cd\u3002\uff09",
    "\u8f7b\u89c6\u654c\u4eba\u2026\u2026\u5728\u4efb\u4f55\u65f6\u5019\uff0c\u90fd\u4e0d\u4f1a\u662f\u2026\u2026\u660e\u667a\u7684\u505a\u6cd5\u3002",
    "\u54e6\uff0c\u654c\u4eba\uff1f|\u60a8\u662f\u6307\u50cf\u60a8\u8fd9\u6837\uff0c\u4f4e\u52a3\u7684\u4e8c\u4ee3\u4eba\u5f62\uff1f",
    "\u201c\u4e0d\u60dc\u4e00\u5207\u4ee3\u4ef7\u201d\u2026\u2026",
    "\u73b0\u5728\uff0c\u8bf7\u60a8\u8ba4\u8ba4\u771f\u771f\u5730\u4e86\u89e3\u4e00\u4e0b\uff0c\u6211\u4e0e\u60a8\u4e4b\u95f4\u7684\u5dee\u8ddd\u3002",
    "\uff08\u9ed1\u8272\u4eba\u5f62\u5f00\u59cb\u52a0\u901f\u3002\uff09",
    "\uff08\u5979\u6240\u643a\u5e26\u7684\u7c7b\u62df\u6001\u56fe\u8c31\u72b6\u7269\u4f53\u4e2d\u7684\u5fae\u5c0f\u690d\u7269\u77ac\u95f4\u5f00\u82b1\uff0c\u7a81\u7136\u91ca\u653e\u7684\u5de8\u5927\u80fd\u91cf\u4f7f\u5468\u56f4\u7684\u7a7a\u95f4\u4ea7\u751f\u4e86\u8f7b\u5fae\u626d\u66f2\uff0c\u5468\u56f4\u7684\u574d\u584c\u8f90\u5c04\u76d1\u6d4b\u5668\u54cd\u6210\u4e00\u7247\u3002\uff09",
    "\u50cf\u8fd9\u6837\uff0c\u5728\u8ddd\u79bb\u6210\u529f\u4e00\u6b65\u4e4b\u9065\u7684\u4f4d\u7f6e\u5012\u4e0b\uff0c\u662f\u4e0d\u662f\u4e00\u79cd\u975e\u5e38\u7b26\u5408\u60a8\u8eab\u4efd\u7684\u8c22\u5e55\u65b9\u5f0f\uff1f",
    "\uff08\u8fd8\u6ca1\u6709\u6765\u5f97\u53ca\u6269\u6563\u7684\u80fd\u91cf\u88ab\u9ed1\u8272\u4eba\u5f62\u5236\u7ea6\uff0c\u9010\u6e10\u91cd\u5851\u6210\u4e3a\u5de8\u5927\u5149\u5203\uff0c\u5411\u95ea\u7535\u6240\u5728\u7684\u65b9\u4f4d\u4ee5\u53ca\u66f4\u8fdc\u5904\uff0c\u5176\u4ed6\u4eba\u5f62\u4eec\u79bb\u5f00\u7684\u65b9\u5411\u88ad\u53bb\u3002\uff09",
    "\u5e0c\u671b\u60a8\uff0c\u80fd\u591f\u5fc3\u5b58\u611f\u6fc0\u3002",
    "\uff08\u4e00\u76f4\u7d27\u76ef\u9ed1\u8272\u4eba\u5f62\u7684\u95ea\u7535\uff0c\u5c06\u9632\u5fa1\u59ff\u6001\u66f4\u6362\u4e3a\u4e86\u8fdb\u653b\u59ff\u6001\u3002\uff09",
    "\uff08\u5979\u4ee5\u6700\u7ec6\u5fae\u7684\u52a8\u4f5c\u56de\u624b\u6478\u4e86\u4e00\u4e0b\u62df\u6001\u56fe\u8c31\u88c5\u7f6e\uff0c\u8bd5\u56fe\u542f\u52a8\uff0c\u4f46\u5728\u52a0\u8f7d\u7684\u8fc7\u7a0b\u4e2d\uff0c\u8fdb\u7a0b\u6b7b\u6b7b\u5361\u5728\u4e8650%\u3002\uff09",
    "\uff08\u7565\u7565\u4f4e\u5934\uff0c\u95ea\u7535\u51b3\u5b9a\u5c06\u5176\u5f3a\u5236\u6fc0\u6d3b\uff0c\u5e76\u5728\u9ed1\u8272\u4eba\u5f62\u63a5\u8fd1\u7684\u540c\u65f6\u5f15\u7206\u69b4\u5f39\u3002\uff09",
    "\u591a\u8c22\u3002"
  ]
]