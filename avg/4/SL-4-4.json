[
  [
    "\uff08\u5165\u591c\uff0c\u5965\u5fb7\u5345\u5185\u2014\u2014\uff09",
    "\uff08\u6574\u70b9\u65f6\u95f4\uff0c\u57ce\u4e2d\u5927\u697c\u697c\u4f53\u4e0a\u7684\u5927\u578b\u5c4f\u5e55\u4e2d\u65ad\u4e86\u5faa\u73af\u5e7f\u544a\uff0c\u5f00\u59cb\u64ad\u653e\u9884\u5236\u8282\u76ee\u3002\uff09",
    "\u6b22\u8fce\u6536\u770b\u665a\u95f4\u65b0\u95fb\u3002\u4eca\u5929\u662f2074\u5e744\u670825\u65e5\uff0c\u661f\u671f\u4e09\u3002\u9884\u8ba1\u5c06\u4e8e2127\u5e74\u5b8c\u6210\u7684\u6c61\u67d3\u533a\u5f00\u62d3\u8ba1\u5212\uff0c\u76ee\u524d\u7b2c\u4e00\u9636\u6bb5\u8fdb\u5c55\u6001\u52bf\u826f\u597d\u3002",
    "\u7f57\u8054\u53d1\u8a00\u4eba\u6628\u65e5\u5728\u8054\u5408\u4f1a\u8bae\u4e2d\u63d0\u5230\uff0cE_2072\u4e00\u7ebf\u51c0\u5316\u533a\u574d\u584c\u8f90\u5c04\u6d53\u5ea6\u5df2\u7ecf\u964d\u81f30.12\u5f17\u5fc5\u5b89\uff0c\u51c0\u5316\u533a\u7684\u5c45\u4f4f\u7533\u8bf7\u5165\u53e3\u5c06\u4e8e\u4e00\u5468\u540e\u5f00\u653e\u3002",
    "\uff08\u4e0b\u73ed\u65f6\u95f4\uff0c\u5404\u4e2a\u5927\u697c\u4eba\u5458\u6d8c\u51fa\uff0c\u9646\u7eed\u6c47\u6210\u8857\u4e0a\u7684\u4eba\u6d41\u3002\uff09",
    "\uff08\u7a81\u7136\uff0c\u51e0\u67b6\u65e0\u4eba\u673a\u5feb\u901f\u8d8a\u8fc7\u5927\u697c\u95f4\u7684\u7f1d\u9699\uff0c\u98de\u5411\u56fa\u5b9a\u5f0f\u9694\u79bb\u5899\u3002\uff09",
    "\u64ad\u653e\u4e00\u5219\u7d27\u6025\u65b0\u95fb\uff0cODE-01\u536b\u661f\u57ce\u76ee\u524d\u51fa\u73b0\u5c0f\u80a1\u532a\u5f92\u3002\u532a\u5f92\u5728\u88ad\u51fb\u6c11\u5c45\u540e\uff0c\u6b63\u5411Z9\u533a\u65b9\u5411\u9003\u7a9c\u3002\u622a\u6b62\u76ee\u524d\uff0c\u532a\u5f92\u7684\u884c\u52a8\u5df2\u7ecf\u9020\u62104\u95f4\u6c11\u5c45\u88ab\u6bc1\uff0c\u4f46\u5c1a\u6ca1\u6709\u9020\u6210\u4eba\u5458\u4f24\u4ea1\u3002\u5404\u4f4d\u5c45\u6c11\u5982\u6709ODE-01\u536b\u661f\u57ce\u51fa\u884c\u8ba1\u5212\uff0c\u8bf7\u6ce8\u610f\u8c03\u6574\u65f6\u95f4\uff0c\u6682\u65f6\u56de\u907f\u3002",
    "\uff08\u5965\u5fb7\u5345\uff0c\u4e00\u5ea7\u9ad8\u697c\u4e0a\u7684\u623f\u95f4\u5185\u2014\u2014\uff09",
    "\uff08\u5360\u636e\u623f\u95f4\u4e2d\u592e\u7684\u5c4f\u5e55\u4e0a\uff0c\u5409\u666e\u8f66\u7f29\u5c0f\u6210\u5730\u56fe\u4e0a\u7684\u7ea2\u70b9\u3002\uff09",
    "\uff08\u7ea2\u70b9\u95ea\u70c1\u51e0\u79d2\u540e\u6d88\u5931\uff0c\u4fe1\u53f7\u88ab\u6807\u8bc6\u4e3a\u201c\u4e22\u5931\u201d\u3002\uff09",
    "\u8ffd\u4e86\u8fd9\u4e48\u4e45\uff0c\u53d1\u73b05\u6b21\uff0c\u4e22\u59316\u6b21\uff0c\u6700\u7ec8\u7684\u7ed3\u679c\uff0c\u5c31\u662f\u4e22\u4e86\u3002|\u51fa\u52a8\u4e86\u8fd9\u4e48\u591a\u4eba\uff0c\u4f60\u4eec\u662f\u5e9f\u7269\u5417\uff1f",
    "\u60a8\u6240\u6d3e\u51fa\u7684K2\u5206\u961f\u6b63\u5728\u5168\u529b\u8ffd\u8e2a\u3002",
    "K2\uff1f|\u90a3\u7fa4\u683c\u91cc\u82ac\u7684\u6b8b\u515a\uff0c\u4e00\u5e2e\u4e0d\u4f1a\u601d\u8003\u7684\u5e9f\u7269\uff0c\u80fd\u8d77\u4ec0\u4e48\u4f5c\u7528\u3002",
    "\u4f9d\u636e\u7f57\u8054\u300a\u767d\u533a\u57ce\u5e02\u7ba1\u7406\u6761\u4f8b\u300b\u7684\u7b2c2\u6761\u7b2c43\u6b3e\uff0c\u9601\u4e0b\u7684\u8a00\u8bba\u5c06\u4f1a\u88ab\u4ee5\u201c\u5206\u5316\u57ce\u5e02\u5efa\u8bbe\u529b\u91cf\u201d\u7684\u540d\u76ee\u63d0\u51fa\u8b66\u544a\u3002",
    "\u4f60\u5728\u5a01\u80c1\u6211\uff1f",
    "\u60a8\u6709\u6240\u8bef\u89e3\uff0c\u9601\u4e0b\u3002|\u536b\u661f\u57ce\u5730\u5f62\u590d\u6742\uff0c\u800c\u76ee\u524d\uff0c\u5965\u5fb7\u5345\u7684\u6743\u529b\u4ea4\u63a5\u6b63\u5904\u4e8e\u5173\u952e\u65f6\u523b\uff0c\u536b\u661f\u57ce\u5c1a\u4e0d\u5728\u638c\u63a7\u8303\u56f4\u5185\u3002",
    "\u8fd8\u9700\u8981\u66f4\u591a\u65f6\u95f4\uff0c\u9601\u4e0b\u3002",
    "\u65f6\u95f4\u3001\u65f6\u95f4\u3001\u65f6\u95f4\uff01\u6211\u90fd\u542c\u4e86\u8fd9\u4e48\u591a\u904d\u4e86\uff01",
    "\u2026\u2026\u54fc\u3002",
    "\u5954\u6d41\u7684\u6cb3\u6c34\u53eb\u56a3\u7740\u5f80\u65e5\u7684\u79bb\u53bb\uff0c\u7070\u6697\u7684\u5929\u7a7a\u4e2d\u6709\u4e4c\u9e26\u5728\u5f98\u5f8a\u3002|\u5341\u4e5d\u4e16\u7eaa\u7684\u5df4\u9ece\uff0c\u4e0d\u77e5\u4f60\u662f\u5426\u6709\u5e78\u770b\u8fc7\u3002",
    "\u6211\u7684\u6570\u636e\u5e93\u4e2d\u5b58\u50a8\u7740\u8fc7\u5f80\u7684\u6240\u6709\u4fe1\u606f\u3002\u5982\u679c\u60a8\u5e0c\u671b\u8ba8\u8bba\u54f2\u5b66\uff0c\u73b0\u5728\u4e0d\u662f\u65f6\u5019\u3002|\u73b0\u5728\uff0c\u4efb\u52a1\u6b63\u5728\u6267\u884c\u4e2d\uff0c\u9601\u4e0b\u3002",
    "\u8ba8\u8bba\u54f2\u5b66\uff1f",
    "\u201c\u970d\u6d1b\u6208\u8ba1\u5212\u201d\u2026\u2026\u54c8\uff0c\u50cf\u4f60\u8fd9\u6837\u7684\u4e00\u4e2a\uff0c\u4e00\u4e2a\u5de5\u4e1a\u65f6\u4ee3\u7684\u6d41\u6c34\u7ebf\u590d\u5236\u4ea7\u7269\uff0c\u4e5f\u6562\u5411\u6211\u63d0\u51fa\u8b66\u544a\uff1f|\u5982\u679c\u8fd9\u6b21\u201c\u6837\u54c1\u201d\u56de\u6536\u4efb\u52a1\u5931\u8d25\uff0c\u7b49\u7740\u4f60\u7684\u662f\u57ce\u5e02\u6536\u5bb9\u6240\uff0c\u8fd8\u662f\u5e9f\u54c1\u56de\u6536\u7ad9\uff1f",
    "\u2026\u2026\u8fd9\u6b21\u4efb\u52a1\uff0c\u540d\u4e49\u4e0a\u7684\u8d1f\u8d23\u4eba\u4ecd\u7136\u662f\u9601\u4e0b\u3002|\u8fd9\u662f\u60a8\u6240\u5f3a\u8c03\u7684\uff0c\u4e5f\u662f\u201c\u5b83\u201d\u6240\u51b3\u5b9a\u7684\u3002",
    "\u90a3\u4e48\uff0c\u660e\u786e\u544a\u8bc9\u6211\uff0c\u201c\u6837\u54c1\u201d\u662f\u4ec0\u4e48\u3002",
    "\u54fc\uff0c\u6211\u57fa\u672c\u80fd\u591f\u786e\u5b9a\uff0c\u8fd9\u91cc\u9762\u5c31\u662f\u5b8c\u5168\u514d\u75ab\u4f53\u3002|\u73b0\u5728\u6211\u8981\u6c42\uff0c\u5411\u6211\u79fb\u4ea4ODE-01\u7684\u4e34\u65f6\u7ba1\u7406\u6743\uff0c\u540c\u6b65\u5f00\u542f\u6700\u9ad8\u6743\u9650\u7684\u5168\u57ce\u5c01\u9501\u3002",
    "\u5728\u76ee\u524d\u9636\u6bb5\uff0c\u4e34\u65f6\u7ba1\u7406\u6743\u7684\u8f6c\u79fb\u5c06\u9020\u6210\u4e25\u91cd\u540e\u679c\uff0c\u9601\u4e0b\u3002",
    "\u8fd9\u4e2a\u4efb\u52a1\uff0c\u8d1f\u8d23\u4eba\u662f\u6211\u3002",
    "\u2026\u2026\u786e\u8ba4\u8f6c\u79fb\u3002",
    "\uff08\u968f\u7740\u516d\u5206\u4eea\u7684\u58f0\u97f3\uff0c\u5c4f\u5e55\u4e0a\u63a5\u8fde\u51fa\u73b0ODE-01\u5404\u4e2a\u5c01\u9501\u53e3\u7684\u4f4d\u7f6e\uff0c\u7eff\u8272\u7684\u6807\u8bc6\u95ea\u70c1\u540e\u56fa\u5b9a\u4e3a\u7ea2\u8272\u3002\uff09",
    "\u5c01\u9501\u547d\u4ee4\u5df2\u7ecf\u4e0b\u8fbe\uff0c\u9601\u4e0b\u3002|\u5982\u679c\u4efb\u52a1\u6210\u529f\uff0c\u5e0c\u671b\u9601\u4e0b\u5bf9\u4e0e\u5409\u62c9\u5c14\u96c6\u56e2\u7684\u4f1a\u8c08\u6709\u6240\u51c6\u5907\u3002",
    "\u2026\u2026\u5982\u679c\u4efb\u52a1\u5931\u8d25\uff0c\u671f\u5f85\u4e0e\u9601\u4e0b\u5728\u6536\u5bb9\u6240\u76f8\u89c1\u3002",
    "\u4f60\u2026\u2026\uff01",
    "\uff08\u8d70\u51fa\u623f\u95f4\uff0c\u516d\u5206\u4eea\u6fc0\u6d3b\u5185\u7f6e\u5728\u8033\u90e8\u9644\u8fd1\u7684\u901a\u8baf\u7ec8\u7aef\u3002\uff09",
    "\u2026\u2026\u8ffd\u8e2a\u201c\u6837\u54c1\u201d\uff0c\u534f\u52a9K2\u5206\u961f\u8ffd\u51fb\u3002|\u4f60\u77e5\u9053\u5931\u8d25\u7684\u7ed3\u679c\u3002\u4e0d\u60dc\u4e00\u5207\u4ee3\u4ef7\uff0c\u201c\u6837\u54c1\u201d\u5fc5\u987b\u56de\u6765\u3002",
    "\uff08\u623f\u95f4\u5916\uff0c\u516d\u5206\u4eea\u7ad9\u5728\u539f\u5730\u3002\uff09",
    "\uff08\u56de\u5eca\u7684\u5899\u4f53\u4e0a\u6709\u4e00\u4e9b\u88c5\u9970\u6027\u7684\u7a7a\u9699\uff0c\u516d\u5206\u4eea\u770b\u7740\u90a3\u4e9b\u7a7a\u9699\uff0c\u7a7a\u9699\u95f4\uff0c\u6708\u5149\u9010\u6e10\u4eae\u4e86\u8d77\u6765\u3002\uff09",
    "\u201c\u970d\u6d1b\u6208\u8ba1\u5212\u201d\u524d\u7ebf\u706d\u53e3\u90e8\u961f\u961f\u957f\u3002|\u9ed1\u773c\u3002"
  ],
  [
    "\u64ad\u653e\u4e00\u5219\u7d27\u6025\u65b0\u95fb\uff0cODE-01\u536b\u661f\u57ce\u76ee\u524d\u51fa\u73b0\u5c0f\u80a1\u532a\u5f92\u3002\u532a\u5f92\u5728\u88ad\u51fb\u6c11\u5c45\u540e\uff0c\u6b63\u5411Z9\u533a\u65b9\u5411\u9003\u7a9c\u3002\u622a\u6b62\u76ee\u524d\uff0c\u532a\u5f92\u7684\u884c\u52a8\u5df2\u7ecf\u9020\u62104\u95f4\u6c11\u5c45\u88ab\u6bc1\uff0c\u4f46\u5c1a\u6ca1\u6709\u9020\u6210\u4eba\u5458\u4f24\u4ea1\u3002\u8ffd\u6355\u884c\u52a8\u6b63\u5728\u8fdb\u884c\u4e2d\uff0c\u73b0\u4e34\u65f6\u5b9e\u884c\u5168\u57ce\u5c01\u9501\uff0c\u8bf7\u5404\u4f4d\u5c45\u6c11\u6682\u65f6\u7559\u5728\u5ba4\u5185\uff0c\u4ee5\u514d\u906d\u5230\u6ce2\u53ca\u3002",
    "\u5c0f\u80a1\u532a\u5f92\u2026\u2026\uff1f",
    "\uff08ODE-01\uff0c\u4e0b\u6c89\u5f0f\u536b\u661f\u57ce\uff0c\u8f68\u9053\u8f66\u7ad9\u5185\u2014\u2014\uff09",
    "\uff08\u4e00\u4e2a\u4e0b\u5348\u8fc7\u53bb\uff0c\u4e1d\u6beb\u6ca1\u6709\u95ea\u7535\u5979\u4eec\u7684\u6d88\u606f\u3002\uff09",
    "\uff08\u6211\u76ef\u7740\u64ad\u653e\u8f66\u6b21\u4fe1\u606f\u7684\u5c4f\u5e55\u4e0a\u7a81\u7136\u51fa\u73b0\u7684\u65b0\u95fb\u753b\u9762\uff0c\u611f\u5230\u4e0d\u5999\u3002\uff09",
    "\uff08\u4e0b\u4e00\u4e2a\u77ac\u95f4\uff0c\u6211\u770b\u5230\u5c4f\u5e55\u4e0a\u7684\u65b0\u95fb\u5f71\u50cf\u4e2d\u51fa\u73b0\u4e86\u95ea\u7535\u7684\u8eab\u5f71\u3002\uff09",
    "\uff08\u6211\u7acb\u523b\u62ff\u51fa\u901a\u8baf\u7ec8\u7aef\uff0c\u5374\u60f3\u5230\u81ea\u4ece\u8fdb\u5165\u536b\u661f\u57ce\u8303\u56f4\u540e\uff0c\u51e0\u4e4e\u65e0\u5904\u4e0d\u5728\u7684\u901a\u8baf\u76d1\u63a7\u3002\uff09",
    "\u2026\u2026\u5567\u3002",
    "\u5c0a\u656c\u7684\u5404\u4f4d\u4e58\u5ba2\uff0c\u7531\u5965\u5fb7\u5345\u51fa\u53d1\u5f00\u5f80\u5361\u592b\u5df4\u65af\u7684OK02\u6b21\u6d32\u9645\u957f\u9014\u5ba2\u8fd0\u5217\u8f66\u5373\u5c06\u505c\u9760\u672c\u7ad9\u3002\u8bf7\u9700\u8981\u4e58\u5750\u6b64\u6b21\u5217\u8f66\u7684\u4e58\u5ba2\u6ce8\u610f\u643a\u5e26\u597d\u968f\u8eab\u884c\u674e\uff0c\u51c6\u5907\u68c0\u7968\u5165\u7ad9\u3002",
    "\uff08\u6536\u8d77\u901a\u8baf\u7ec8\u7aef\uff0c\u73af\u987e\u56db\u5468\uff0c\u6211\u53d1\u73b0\u6709\u51e0\u4e2a\u4eba\u5f71\u6b63\u8bd5\u56fe\u968f\u7740\u4eba\u6d41\u5411\u6211\u63a5\u8fd1\u3002\uff09",
    "\uff08\u6211\u7acb\u523b\u8f6c\u8eab\uff0c\u540c\u6837\u968f\u7740\u4eba\u6d41\uff0c\u6211\u8fdb\u5165\u4e86\u8f68\u9053\u533a\u3002\uff09"
  ],
  [
    "\u5567\uff0c\u8fd9\u79cd\u62d0\u6765\u62d0\u53bb\u7684\u5730\u65b9\uff0c\u522b\u8bf4\u7529\u6389\u90a3\u4e9b\u8be5\u6b7b\u7684\u5c3e\u5df4\uff0c\u6211\u4eec\u81ea\u5df1\u90fd\u5feb\u627e\u4e0d\u5230\u5730\u65b9\u4e86\uff01",
    "\uff08\u524d\u8f66\u6025\u5239\uff0c\u5bc7\u5c14\u8299\u4e00\u60ca\uff0c\u8e29\u4e86\u5239\u8f66\u5e76\u7565\u5fae\u65cb\u8f6c\u4e86\u65b9\u5411\u76d8\uff0c\u8f66\u4f53\u8f7b\u5fae\u626d\u66f2\u540e\u5927\u5e45\u5ea6\u6296\u4e86\u4e00\u4e0b\u3002\uff09",
    "\u4e0d\u8981\u8fd9\u4e48\u66b4\u8e81\u561b\u514b\u7f57\u4e3d\u79d1~",
    "\u54fc\uff0c\u66b4\u8e81\uff1f\u90fd\u4e00\u4e0b\u5348\u4e86\uff0c\u5e78\u4e8f\u8fd9\u8f66\u8fd8\u80fd\u9876\u5f97\u4f4f\u3002\u4f46\u6211\u4eec\u5f39\u836f\u90fd\u5feb\u89c1\u5e95\u4e86\u554a\uff1f\u8fd9\u8fd8\u8981\u8dd1\u591a\u4e45\uff1f",
    "\u518d\u7b49\u7b49\uff0c\u8fd8\u4e0d\u80fd\u786e\u8ba4\u8ffd\u51fb\u8005\u5df2\u7ecf\u5b8c\u5168\u6d88\u706d\u3002",
    "\u53ef\u6211\u4eec\u73b0\u5728\u8fd8\u8981\u5f80\u8f66\u7ad9\u90a3\u91cc\u8d70\uff0c\u53ea\u770b\u7740\u8fd9\u4e48\u4e2a\u767d\u67f1\u5b50\uff0c\u8fd9\u80fd\u627e\u5230\u4ec0\u4e48\u8def\uff1f\uff01|\u5567\uff0c\u4e0d\u5982\u6211\u53bb\u5077\u4e00\u67b6\u65e0\u4eba\u673a\u7b97\u4e86\uff0c\u98de\u8fc7\u53bb\u6700\u7b80\u5355\u3002",
    "\u54c7\u2014\u2014\u514b\u7f57\u4e3d\u79d1\u8fd9\u662f\u60f3\u6210\u4e3a\u536b\u661f\u57ce\u91cc\u6700\u4eae\u7684\u9776\u5b50\u54df~\uff1f",
    "\u90a3\u4f60\u8bf4\u600e\u4e48\u529e\uff0c\u6293\u4e2a\u4eba\u95ee\u95ee\u8def\uff1f\uff01",
    "\u2026\u2026\u72c2\u98ce\uff0c\u5377\u8fc7\u6d6a\u6f6e\u2026\u2026\u7ff3\u5f71\u964d\u4e34\u2026\u2026",
    "\u8c01\u6015\u4e86\u554a\uff1f\uff01\u4f60\u624d\u6015\u4e86\u5427\u7eb3\u7f8e\u897f\u4e1d\uff1f\uff01",
    "\u5728\u627e\u5230\u53bb\u8f66\u7ad9\u7684\u8def\u4e4b\u524d\uff0c\u540e\u9762\u7684\u8ffd\u51fb\u624d\u662f\u6211\u4eec\u66f4\u9700\u8981\u5173\u6ce8\u7684\u3002|\u5728\u6446\u8131\u4ed6\u4eec\u4e4b\u540e\uff0c\u6211\u4eec\u624d\u80fd\u53bb\u8f66\u7ad9\u4e0e\u6307\u6325\u5b98\u6c47\u5408\u3002",
    "\u662f\u554a\u90a3\u4e5f\u8981\u80fd\u627e\u5230\u8def\u8fc7\u53bb\u554a\uff1f\uff01",
    "\u597d\u5566\u4e0d\u8981\u5435\u4e86\u5440\uff0c\u4eba\u5bb6\u2014\u2014\u5728\u5f00\u8f66\u54df~|\u554a\u961f\u957f\uff0c\u524d\u9762\u7684\u8def\u5c01\u4e86\u8036~",
    "\u5de6\u4fa7\u8f6c\u5411\u3002",
    "\u6ca1\u95ee\u9898\uff01"
  ]
]