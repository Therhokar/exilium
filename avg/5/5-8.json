[
  [
    "\u786e\u8ba4\u5f53\u524d\u533a\u57df\u65e0\u5f02\u5e38\u3002",
    "\u884c\u52a8\u7ee7\u7eed\u3002",
    "\u5567\uff0c\u8fd9\u91cc\u5c31\u4e0d\u80fd\u6709\u70b9\u522b\u7684\uff1f|\u90a3\u4e9b\u7834\u70c2\u4e1c\u897f\u4e00\u70b9\u6218\u6597\u529b\u90fd\u6ca1\u6709\uff0c\u6570\u91cf\u8fd8\u591a\uff0c\u6253\u8d77\u6765\u70e6\u6b7b\u4e86\uff01",
    "\u2026\u2026\u6cb8\u817e\u7684\u571f\u5730\u2026\u2026\u71c3\u8d77\u55a7\u54d7\u7684\u6d6a\u6f6e\u2026\u2026",
    "\u54c8\uff1f\u4f1a\u88ab\u5077\u88ad\u7684\u662f\u4f60\u5427\uff1f\uff01",
    "\u597d\u597d~\u4e2d\u573a\u4f11\u606f\u5566\u4e24\u4f4d\u2014\u2014\u73b0\u5728\u8fd8\u5728\u6218\u6597\u4e2d\u54df\uff1f",
    "\u4e0d\u8fc7\u961f\u957f\uff0c\u73b0\u5728\u7684\u60c5\u51b5\uff0c\u6709\u70b9\u50cf\u6211\u4eec\u4e4b\u524d\u5728\u5a01\u65af\u7279\u6e2f\u91cc\u7684\u65f6\u5019\u8036~|\u4e0b\u4e00\u6b65\u96be\u9053\u662f\u2014\u2014\u901a\u8baf\u65ad\u5f00\uff1f",
    "\u5a01\u65af\u7279\u6e2f\u7684\u60c5\u51b5\uff0c\u53ef\u4e0d\u80fd\u518d\u6765\u4e00\u904d\u3002|\u4e0d\u8fc7\u73b0\u5728\u6ca1\u6709\u574d\u584c\u98ce\u66b4\uff0c\u6ca1\u6709\u7a81\u7136\u51fa\u73b0\u7684\u4f2f\u4ecb\u59c6\uff0c\u5c31\u7b97\u6d77\u4f26\u5a1c\u7684\u5916\u5c42\u7bb1\u5b50\u5bf9\u901a\u8baf\u4ea7\u751f\u4e86\u4e00\u4e9b\u5e72\u6270\uff0c\u8fd8\u4e0d\u81f3\u4e8e\u5230\u65ad\u2014\u2014",
    "\u2026\u2026\u6307\u6325\u5b98\uff1f",
    "\uff08\u4e00\u8fb9\u8b66\u60d5\u5730\u89c2\u5bdf\u5468\u56f4\uff0c\u6301\u7eed\u5411\u524d\u79fb\u52a8\uff0c\u95ea\u7535\u4e00\u8fb9\u4f7f\u7528\u901a\u8baf\u7ec8\u7aef\uff0c\u91cd\u65b0\u547c\u53eb\u3002\uff09",
    "\uff08\u4fe1\u53f7\u4e2d\u65ad\uff0c\u65e0\u6cd5\u63a5\u901a\u3002\uff09",
    "\u4eba\u5bb6\u3001\u6211\u2014\u2014\u96be\u9053\u6211\u771f\u7684\u8bfb\u53d6\u7684\u662f\u7eb3\u7f8e\u897f\u4e1d\u7684\u5fc3\u667a\u5907\u4efd\uff1f",
    "\u2014\u2014\u961f\u957f\u3002",
    "\u55ef\uff0c\u6211\u770b\u5230\u4e86\u3002",
    "\uff08\u4eba\u5f62\u4eec\u505c\u4e0b\u811a\u6b65\u3002\uff09",
    "\uff08\u51e0\u4e4e\u5728\u540c\u4e00\u65f6\u95f4\uff0c\u6709\u6c89\u95f7\u7684\u91cd\u7269\u5760\u843d\u5728\u4eba\u5f62\u4eec\u9762\u524d\uff0c\u55b7\u8584\u800c\u51fa\u7684\u70df\u5c18\u4e2d\uff0c\u5927\u7247\u7ea2\u8272\u5149\u70b9\u4eae\u8d77\uff0c\u4f46\u5374\u5e76\u4e0d\u9760\u8fd1\u4f17\u4eba\u3002\uff09",
    "\u5567\uff0c\u600e\u4e48\u53c8\u662f\u8fd9\u4e9b\u7834\u70c2\u4e1c\u897f\uff0c\u6570\u91cf\u8fd8\u771f\u591a\uff01",
    "\u2026\u2026\u6ce8\u610f\u8b66\u6212\u3002",
    "\u662f\u6307\u6325\u5b98\u7684\u901a\u8baf\u5417\uff0c\u961f\u957f\uff1f",
    "\uff08\u95ea\u7535\u76ef\u7740\u901a\u8baf\u7ec8\u7aef\u7684\u5c4f\u5e55\u770b\u4e86\u597d\u4e00\u4f1a\uff0c\u624d\u5c06\u521a\u6536\u5230\u7684\u4fe1\u606f\u5c55\u793a\u51fa\u6765\u3002\uff09",
    "\uff08\u9ed1\u8272\u7684\u5c4f\u5e55\u4e0a\uff0c\u4ee5\u5e9f\u5f03\u5de5\u5382\u4fef\u89c6\u56fe\u4e3a\u80cc\u666f\uff0c\u95ea\u7535\u5c0f\u961f\u6240\u5904\u7684\u4f4d\u7f6e\u88ab\u6807\u793a\u51fa\u6765\u3002\uff09",
    "\uff08\u53f3\u4e0a\u89d2\u7684\u4f4d\u7f6e\uff0c\u51fa\u73b0\u4e86\u4e00\u4e2a\u767d\u8272\u7684\u95ea\u70c1\u5149\u70b9\u3002\uff09"
  ]
