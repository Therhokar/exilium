[
  [
    "\uff08\u95ea\u7535\u5c0f\u961f\u8fdb\u5165\u4e86\u5de5\u5382\u3002\uff09",
    "\uff08\u5de5\u5382\u5df2\u88ab\u5e9f\u5f03\uff0c\u6b8b\u7559\u7684\u767d\u70bd\u706f\u6d12\u4e0b\u67d4\u548c\u7684\u5149\u7ebf\uff0c\u7167\u5c04\u5728\u5404\u7c7b\u6742\u4e71\u7684\u5e9f\u5f03\u673a\u68b0\u90e8\u4ef6\u4e0a\u3002\u4e0d\u8fdc\u5904\uff0c\u7855\u5927\u7684\u6247\u53f6\u4ecd\u5728\u6301\u7eed\u8fd0\u8f6c\u3002\uff09",
    "\uff08\u4eba\u5f62\u4eec\u7684\u811a\u6b65\u58f0\u5728\u7a7a\u65f7\u7684\u5de5\u5382\u5185\u6fc0\u8d77\u5c42\u53e0\u7684\u56de\u97f3\u3002\uff09",
    "\u2026\u2026\u9eef\u6de1\u7684\u4e91\u5c42\uff0c\u6c47\u805a\uff0c\u906e\u63a9\u5149\u4eae\u2026\u2026",
    "\u51b2\u4ec0\u4e48\u51b2\u4f60\u662f\u6ca1\u770b\u5230\u5f53\u65f6\u7684\u60c5\u51b5\u5417\uff1f\uff01\u5236\u5b9a\u7684\u4ec0\u4e48\u7834\u4f5c\u6218\u8ba1\u5212\uff0c\u524d\u9762\u90a3\u4e00\u5927\u5806\u90a3\u6b4c\u4ec0\u4e48\u7684\u9a6c\u4e0a\u5c31\u8981\u81ea\u7206\u4e86\u4f60\u77e5\u4e0d\u77e5\u9053\uff1f\uff01",
    "\u2026\u2026\u9690\u85cf\u5728\u7070\u70ec\u4e2d\u7684\u706b\u79cd\uff0c\u6084\u7136\u7184\u706d\u2026\u2026",
    "\u8c01\u62d6\u7d2f\u4f60\u4f60\u8bf4\u6211\uff1f\u6700\u5f00\u59cb\u6ca1\u6709\u70b9\u6740\u6210\u529f\u7684\u662f\u8c01\u554a\u7eb3\u7f8e\u897f\u4e1d\uff1f\uff01\u8981\u4e0d\u8981\u73b0\u5728\u5c31\u8fdb\u4e8c\u7ea7\u5e73\u5c42\u6211\u5e2e\u4f60\u597d\u597d\u56de\u5fc6\u4e00\u4e0b\uff1f\uff01",
    "\u7eb3\u7f8e\u897f\u4e1d\uff0c\u514b\u7f57\u4e3d\u79d1\uff0c\u6ce8\u610f\u8b66\u6212\u3002",
    "\u95ea\u7535\uff0c\u5979\u4eec\u4e24\u4e2a\u600e\u4e48\u4e86\uff1f",
    "\u2026\u2026\u53ef\u80fd\u662f\u4e4b\u524d\u5bf9\u6218\u7684\u4e8b\u60c5\u3002\u5bf9\u6297\u5404\u6709\u80dc\u8d1f\uff0c\u4f46\u5408\u4f5c\u65f6\uff0c\u7ed3\u679c\u51e0\u4e4e\u90fd\u662f\u5931\u8d25\uff0c\u8bc4\u5206\u90fd\u5728C\u4ee5\u4e0b\u3002",
    "\u662f\u554a\u90fd\u662f\u7eb3\u7f8e\u897f\u4e1d\u4e71\u63d0\u4ec0\u4e48\u4f5c\u6218\u8ba1\u5212\u554a\uff1f\uff01\u8fd9\u4e48\u4f4e\u7684\u96be\u5ea6\u8981\u4e0d\u662f\u5979\u6211\u8fd8\u80fd\u8f93\uff1f\uff01",
    "\u2026\u2026\u4f18\u5148\u5173\u6ce8\u5f53\u524d\u7684\u4efb\u52a1\uff0c\u514b\u7f57\u4e3d\u79d1\u3002",
    "\u6211\u2014\u2014\u5567\uff01",
    "\u961f\u957f\uff0c\u6307\u6325\u5b98\uff0c\u8fd9\u91cc\u2014\u2014\u597d\u50cf\u5168\u90fd\u5e9f\u5f03\u4e86\uff0c\u4ec0\u4e48\u4e5f\u6ca1\u6709\u54df\uff1f",
    "\uff08\u4eba\u5f62\u4eec\u73af\u987e\u56db\u5468\uff0c\u4f46\u5468\u56f4\u4e00\u7247\u5b89\u9759\uff0c\u6ca1\u6709\u4efb\u4f55\u5f02\u5e38\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u5f2f\u8170\u6361\u8d77\u4e00\u5757\u94c1\u7247\uff0c\u6254\u5411\u8fdc\u5904\u3002\uff09",
    "\u961f\u957f\u4f60\u770b\uff0c\u5c31\u7b97\u8fd9\u6837\u2014\u2014\u4e5f\u6ca1\u6709\u4efb\u4f55\u9700\u8981\u6211\u4eec\u8b66\u6212\u7684\u5bf9\u8c61\u2014\u2014\u554a\uff01",
    "\u505c\u4e0b\uff01",
    "\uff08\u811a\u5e95\u7a81\u7136\u4f20\u6765\u5f3a\u70c8\u7684\u9707\u611f\u3002\uff09",
    "\uff08\u539f\u672c\u5e9f\u5f03\u5728\u573a\u666f\u5404\u5904\u7684\u673a\u68b0\u4eec\u968f\u4e4b\u6d3b\u52a8\u8d77\u6765\uff0c\u5411\u7740\u53d1\u51fa\u58f0\u97f3\u7684\u95ea\u7535\u5c0f\u961f\u79fb\u52a8\u3002\uff09",
    "\u600e\u4e48\u4e86\uff1f",
    "\u6709\u654c\u65b9\u5355\u4f4d\u51fa\u73b0\uff0c\u5916\u5f62\u5224\u65ad\u4e3a\u6b8b\u7559\u5728\u5de5\u5382\u5185\u7684\u5e9f\u5f03\u673a\u68b0\uff0c\u5916\u5f62\u63a8\u6d4b\u4e3a\u6cdb\u7528\u5316\u94c1\u8840\u6280\u672f\u4ea7\u7269\u3002",
    "\u6211\uff0c\u6211\uff0c\u5bf9\u4e0d\u8d77\u2014\u2014",
    "\u54fc\uff0c\u8fd9\u7b97\u4ec0\u4e48\uff0c\u7b2c\u4e8c\u4e2a\u7eb3\u7f8e\u897f\u4e1d\uff1f",
    "\u2026\u2026\u63a0\u8fc7\u5c71\u8c37\u7684\u97f3\u8c03\uff0c\u5f92\u7136\u6d88\u6563\u2026\u2026",
    "\u4e3a\u4ec0\u4e48\uff1f\u8fd9\u90fd\u4e0d\u77e5\u9053\uff0c\u56e0\u4e3a\u4f60\u4e4c\u9e26\u5634\u554a\uff1f\u521a\u624d\u51e0\u6b21\u7167\u7740\u4f60\u7684\u4f5c\u6218\u8ba1\u5212\u8d70\u90fd\u8f93\u4e86\u554a\uff1f",
    "\u51c6\u5907\u8fce\u654c\uff01"
  ],
  [
    "\u5bf9\u4e0d\u8d77\uff0c\u961f\u957f\uff0c\u6307\u6325\u5b98\uff0c\u5927\u5bb6\uff01",
    "\u5567\uff0c\u6211\u5012\u662f\u6ca1\u4ec0\u4e48\u3002|\u6ca1\u6709\u4f60\uff0c\u8fd8\u6709\u7eb3\u7f8e\u897f\u4e1d\u4e0d\u662f\u5417\uff01",
    "\u2026\u2026\u8ffd\u5bfb\u66f2\u6298\u7684\u8def\u5f84\uff0c\u6563\u5165\u6797\u6728\u2026\u2026",
    "\u4e4b\u524d\u627e\u9519\u884c\u8fdb\u8def\u7ebf\u7684\u4e5f\u662f\u4f60\u5427\uff1f\uff01",
    "\u5bc7\u5c14\u8299\uff0c\u4e0b\u6b21\u4f5c\u6218\u65f6\uff0c\u4e0d\u8981\u64c5\u81ea\u884c\u52a8\u3002",
    "\u7edd\u5bf9\u4f1a\u7684\uff0c\u961f\u957f~\uff01",
    "\u5468\u56f4\u6709\u6ca1\u6709\u522b\u7684\u4ec0\u4e48\uff0c\u95ea\u7535\uff1f",
    "\u76ee\u524d\u8303\u56f4\u5185\u6ca1\u6709\u53d1\u73b0\u5f02\u5e38\u3002",
    "\u4ec0\u2026\u2026\u6ca1\u6709\uff1f\u2026\u2026\u8c61\u5462\uff1f",
    "\u4ec0\u4e48\uff1f",
    "\u554a\uff1f",
    "\u901a\u8baf\u6709\u5e72\u6270\uff0c\u6307\u6325\u5b98\u4f60\u521a\u624d\u8bf4\u4e86\u4ec0\u4e48\u6ca1\u542c\u5230\u3002",
    "\u5e72\u6270\u2014\u2014\u6211\u8fd9\u91cc\u4e00\u5207\u6b63\u5e38\uff1f|\u53ef\u80fd\u662f\u56e0\u4e3a\u6d77\u4f26\u5a1c\u7684\u5916\u5c42\u7bb1\u5b50\uff0c\u6211\u5df2\u7ecf\u7ed9\u7f8e\u73b2\u7559\u8a00\uff0c\u4f46\u6ca1\u6709\u6536\u5230\u56de\u590d\u3002\u7f8e\u73b2\u5e94\u8be5\u8fd8\u5728\u4f11\u606f\u3002",
    "\u6211\u662f\u8bf4\uff0c\u6709\u6ca1\u6709\u770b\u5230\u4ea4\u63a5\u5bf9\u8c61\uff1f",
    "\u6ca1\u6709\u53d1\u73b0\u3002\u5e9f\u5f03\u673a\u68b0\u4ee5\u5916\uff0c\u8fd9\u91cc\u6ca1\u6709\u4efb\u4f55\u7591\u4f3c\u201c\u4ea4\u63a5\u5bf9\u8c61\u201d\u7684\u5355\u4f4d\u3002",
    "\u2026\u2026\u7ee7\u7eed\u63a2\u7d22\uff0c\u6ce8\u610f\u5b89\u5168\u3002"
  ]
]