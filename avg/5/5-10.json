[
  [
    "\u770b\u8d77\u6765\u2014\u2014\u90a3\u4e2a\u5149\u70b9\u7684\u4f4d\u7f6e\uff0c\u5feb\u8981\u5230\u4e86\u54df~|\u6211\u4eec\u8fd8\u8981\u7ee7\u7eed\u63a5\u8fd1\u5417\uff0c\u961f\u957f\uff1f",
    "\u7ee7\u7eed\u884c\u52a8\u3002",
    "\u5567\uff0c\u8fd9\u4ec0\u4e48\u4e1c\u897f\uff0c\u795e\u795e\u79d8\u79d8\u7684\uff0c\u76f4\u63a5\u51fa\u6765\u4e0d\u884c\u554a\uff1f",
    "\u2026\u2026\u75be\u9a70\u8fc7\u5929\u9645\u7684\u6d41\u661f\uff0c\u71c3\u8d77\u957f\u957f\u7684\u706b\u7130\u2026\u2026",
    "\u54e6\uff0c\u662f\u554a\uff0c\u6211\u51b2\u51fa\u53bb\u5c31\u80fd\u628a\u8fd9\u4e0d\u77e5\u9053\u4ec0\u4e48\u4e1c\u897f\u780d\u4e86\uff0c\u4f60\u53ea\u80fd\u8eb2\u5728\u540e\u9762\u653e\u51b7\u67aa\u5427\uff1f",
    "\u597d\u5566\u597d\u5566~\u7eb3\u7f8e\u897f\u4e1d\u548c\u514b\u7f57\u4e3d\u79d1\u2014\u2014\u90fd\u5f88\u5389\u5bb3\u54df\uff1f|\u4e0b\u6b21\u6218\u6597\uff0c\u4e5f\u8981\u9ebb\u70e6\u4e24\u4f4d\u4fdd\u62a4\u4eba\u5bb6\u5566~",
    "\u54fc\uff0c\u5f53\u7136\uff01",
    "\u505c\u4e0b\u3002",
    "\uff08\u95ea\u7535\u5c0f\u961f\u505c\u5728\u4e00\u6247\u534a\u5f00\u542f\u7684\u95e8\u524d\u3002\uff09",
    "\uff08\u8fd9\u6247\u95e8\u4f3c\u4e4e\u53ea\u662f\u7528\u6765\u95f4\u9694\u7a7a\u95f4\uff0c\u4e0a\u90e8\u5e76\u6ca1\u6709\u5c01\u9876\u3002\u900f\u8fc7\u534a\u5f00\u542f\u7684\u95e8\u5411\u5185\u770b\u53bb\uff0c\u524d\u65b9\u4f3c\u4e4e\u662f\u4e00\u4e2a\u5f88\u5927\u7684\u623f\u95f4\u3002\uff09",
    "\uff08\u95ea\u7535\u624b\u4e2d\uff0c\u901a\u8baf\u7ec8\u7aef\u7684\u5c4f\u5e55\u4e0a\uff0c\u4ee3\u8868\u4eba\u5f62\u4eec\u7684\u5149\u70b9\u51e0\u4e4e\u4e0e\u4e00\u76f4\u95ea\u70c1\u7684\u767d\u8272\u5149\u70b9\u91cd\u5408\u3002\uff09",
    "\u770b\u8d77\u6765\u5c31\u662f\u8fd9\u91cc\u54df~",
    "\u7ef4\u6301\u8b66\u6212\u3002",
    "\uff08\u4ee5\u95ea\u7535\u4e3a\u9996\uff0c\u4eba\u5f62\u4eec\u8d70\u8fdb\u5f00\u542f\u7684\u95e8\u3002\uff09",
    "\uff08\u95e8\u5185\u5e94\u8be5\u662f\u4e00\u5904\u7528\u4e8e\u673a\u68b0\u7ec4\u88c5\u7684\u5382\u623f\uff0c\u9876\u90e8\u7684\u767d\u70bd\u706f\u56e0\u957f\u671f\u7684\u5e9f\u7f6e\u800c\u51fa\u73b0\u8001\u5316\uff0c\u95ea\u70c1\u7684\u706f\u5149\u6620\u5c04\u51fa\u5382\u623f\u5168\u666f\u3002\uff09",
    "\uff08\u4e2d\u592e\u9760\u540e\u7684\u4f4d\u7f6e\uff0c\u6709\u4e00\u5904\u5de8\u5927\u7684\u673a\u68b0\uff0c\u4f46\u56e0\u957f\u4e45\u7684\u5e9f\u5f03\u4ee5\u53ca\u672c\u8eab\u7684\u7ec4\u88c5\u6682\u505c\uff0c\u5e76\u4e0d\u80fd\u8fa8\u522b\u51fa\u5b83\u7684\u771f\u8c8c\u3002\uff09",
    "\uff08\u9ed1\u8272\u7684\u4eba\u5f62\u7ad9\u5728\u5de8\u5927\u7684\u673a\u68b0\u524d\uff0c\u95ea\u70c1\u7684\u706f\u5149\u6620\u51fa\u5979\u7684\u8138\uff0c\u8138\u4e0a\u7684\u8868\u60c5\u660e\u6697\u4e0d\u5b9a\u3002\uff09",
    "\u2014\u2014\u867d\u7136\u662f\u65e0\u7528\u7684\u6b8b\u6b21\u54c1\uff0c\u6765\u5230\u8fd9\u91cc\uff0c\u82b1\u8d39\u7684\u65f6\u95f4\u4ecd\u7136\u8d85\u51fa\u6211\u7684\u9884\u671f\u3002",
    "\u554a\uff0c\u8fd9\u662f\u2014\u2014\u662f\u4e4b\u524d\u9047\u5230\u7684\u90a3\u4e2a\u4eba\u5f62\uff1f",
    "\u6212\u5907\u3002",
    "\uff08\u770b\u5230\u95ea\u7535\u5c0f\u961f\u7684\u8b66\u6212\u52a8\u4f5c\uff0c\u9ed1\u8272\u4eba\u5f62\u6536\u8d77\u4e86\u4f1e\u3002\uff09",
    "\uff08\u5979\u628a\u4f1e\u5c16\u6491\u5728\u5730\u4e0a\uff0c\u7b11\u5bb9\u4e0d\u53d8\u3002\uff09",
    "\u653e\u5f03\u65e0\u8c13\u7684\u62b5\u6297\uff0c\u5b89\u5fc3\u63a5\u53d7\u5931\u8d25\u7684\u547d\u8fd0\u5982\u4f55\uff0c\u6b8b\u6b21\u54c1\u4eec\uff1f",
    "\u4f60\u6765\u5230\u8fd9\u91cc\uff0c\u96be\u9053\u4e0d\u662f\u5bf9\u6211\u4eec\u7684\u8ba4\u53ef\uff1f",
    "\u2014\u2014\u8ba4\u53ef\u3002\u54e6~\u60a8\u53ef\u80fd\u4ea7\u751f\u4e86\u4e00\u4e9b\u8bef\u89e3\u3002|\u65e0\u7528\u7684\u6b8b\u6b21\u54c1\uff0c\u4e0d\u5177\u5907\u7406\u89e3\u201c\u6211\u4eec\u201d\u884c\u52a8\u903b\u8f91\u7684\u80fd\u529b\u3002",
    "\u201c\u6211\u4eec\u201d\uff1f\u4f60\uff0c\u548c\u516d\u5206\u4eea\uff1f",
    "\u2026\u2026\u5475\u3002",
    "\u5e0c\u671b\u60a8\uff0c\u4e0d\u8981\u5c06\u6211\u4e0e\u5979\u76f8\u63d0\u5e76\u8bba\u3002",
    "\u770b\u8d77\u6765\uff0c\u4f60\u548c\u201c\u516d\u5206\u4eea\u201d\u4e0d\u5c5e\u4e8e\u5408\u4f5c\u5173\u7cfb\u3002",
    "\u90a3\u4e48\uff0c\u4f60\uff0c\u4ee5\u53ca\u201c\u4f60\u4eec\u201d\uff0c\u5728\u6211\u4eec\u4eba\u5f62\u53ea\u6709\u4e09\u4e2a\u3001\u5f39\u836f\u8865\u7ed9\u51e0\u4e4e\u4e3a\u96f6\u7684\u60c5\u51b5\u4e0b\uff0c\u6ca1\u6709\u53ca\u65f6\u56de\u6536\u4f60\u4eec\u6240\u9700\u8981\u7684\u201c\u6837\u54c1\u201d\uff0c\u800c\u662f\u4f2a\u88c5\u6210\u683c\u7433\u5a1c\uff0c\u7279\u610f\u5c06\u6211\u4eec\u5f15\u8bf1\u5230\u8fd9\u91cc\uff0c\u539f\u56e0\u662f\u4ec0\u4e48\uff1f",
    "\u7279\u610f\u5c06\u4f60\u4eec\u5f15\u8bf1\u5230\u8fd9\u91cc\uff1f",
    "\u4f9d\u636e\u6211\u4eec\u7ed9\u51fa\u7684\u6d88\u606f\uff0c\u5b8c\u5168\u4e0d\u67e5\u8bc1\u771f\u4f2a\uff0c\u4ee5\u65e0\u77e5\u800c\u611a\u6627\u7684\u9009\u62e9\u884c\u52a8\u2014\u2014\u5982\u679c\u4e4b\u524d\u8fd8\u786e\u5b9e\u82b1\u8d39\u4e86\u6211\u548c\u5979\u4e00\u4e9b\u65f6\u95f4\uff0c\u4e0d\u77e5\u73b0\u5728\uff0c\u8eab\u5728\u6b64\u5904\u7684\u60a8\u662f\u4ee5\u4f55\u79cd\u65b9\u5f0f\uff0c\u505a\u51fa\u8fd9\u4e2a\u63a8\u65ad\uff1f",
    "\u2026\u2026\u4ece\u516d\u5206\u4eea\u5230\u73b0\u5728\u7684\u201c\u4f60\u4eec\u201d\uff0c\u4f60\u66f4\u6362\u4e86\u5408\u4f5c\u5bf9\u8c61\uff1f",
    "\uff08\u76ef\u7740\u9ed1\u8272\u7684\u4eba\u5f62\uff0c\u95ea\u7535\u7684\u58f0\u97f3\u975e\u5e38\u7f13\u6162\u3002\uff09",
    "\uff08\u9ed1\u8272\u4eba\u5f62\u6ca1\u6709\u7ee7\u7eed\u8bdd\u9898\u3002\u5979\u5c06\u4f1e\u4e3e\u8d77\uff0c\u505a\u51fa\u653b\u51fb\u7684\u52a8\u4f5c\u3002\uff09",
    "\u54e6~",
    "\u65e0\u8c13\u7684\u731c\u6d4b\uff0c\u5e26\u6765\u7684\u53ea\u4f1a\u662f\u66f4\u52a0\u60e8\u70c8\u7684\u5931\u8d25\u3002",
    "\u2026\u2026\u4e0d\uff0c\u5931\u8d25\u7684\u4f1a\u662f\u4f60\u3002",
    "\u4e0a\u4e00\u6b21\u7684\u5bf9\u6218\u4ee5\u7206\u70b8\u7ed3\u675f\u3002\u6ca1\u80fd\u8ffd\u56de\u4f60\u6240\u9700\u8981\u7684\u201c\u6837\u54c1\u201d\uff0c\u4f60\u5df2\u7ecf\u8f93\u7ed9\u4e86\u6211\u3002",
    "\u2026\u2026\u4ec0\u4e48\u3002",
    "\u8fd9\u4e00\u6b21\uff0c\u4f1a\u662f\u540c\u6837\u7684\u7ed3\u679c\u3002",
    "\u2026\u2026\u4f60\uff01",
    "\u505a\u51fa\u4e0e\u81ea\u5df1\u8eab\u4efd\u5e76\u4e0d\u76f8\u7b26\u7684\u8868\u8ff0\uff0c\u5e0c\u671b\u4f60\u4eec\u80fd\u591f\u8ba4\u8bc6\u5230\u81ea\u5df1\u7684\u9519\u8bef\uff0c\u6b8b\u6b21\u54c1\u4eec\u3002",
    "\uff08\u9ed1\u8272\u4eba\u5f62\u5411\u95ea\u7535\u5c0f\u961f\u8d70\u6765\uff0c\u540c\u65f6\uff0c\u5979\u7684\u8eab\u4f53\u5f00\u59cb\u53d8\u5f62\u3002\uff09",
    "\u8fd9\u6b21\u7684\u6559\u8bad\uff0c\u5e0c\u671b\u4f60\u4eec\u80fd\u591f\u8bb0\u4f4f\u3002"
  ]
]