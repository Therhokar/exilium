[
  [
    "21%\uff0c\u4e94\u5206\u4e4b\u4e00\u7684\u57ce\u5185\u5efa\u7b51\u88ab\u6bc1\u3002",
    "\uff08\u5965\u5fb7\u5345\uff0c\u4e00\u5ea7\u9ad8\u697c\u4e0a\u7684\u623f\u95f4\u5185\u2014\u2014\uff09",
    "\uff08\u5168\u606f\u5c4f\u524d\uff0c\u59d4\u6258\u4eba\u4e0a\u4e0b\u626b\u89c6\u5730\u56fe\uff0c\u7126\u8e81\u5730\u6765\u56de\u8e31\u6b65\u3002\uff09",
    "\uff08\u516d\u5206\u4eea\u7ad9\u5728\u4ed6\u8eab\u540e\u4e0d\u8fdc\u5904\u7684\u89d2\u843d\u91cc\uff0c\u5b89\u9759\u5730\u770b\u7740\u4ed6\u3002\uff09",
    "\u5982\u60a8\u6240\u89c1\u3002",
    "\u5982\u6211\u6240\u89c1\uff1f\u4f60\u77e5\u4e0d\u77e5\u9053\u73b0\u5728ODE-01\u7684\u4e34\u65f6\u7ba1\u7406\u6743\u5728\u6211\u624b\u4e0a\uff1f\uff01",
    "\u662f\u7684\u3002",
    "\u4f60\u2014\u2014\u4f60\uff1f\uff01",
    "\u8fd9\u53ea\u662f\u4e00\u5ea7\u536b\u661f\u57ce\uff0c\u662f\u5965\u5fb7\u5345\u6240\u5c5e\u768427\u5ea7\u536b\u661f\u57ce\u5176\u4e2d\u7684\u4e00\u5ea7\uff0c\u9601\u4e0b\u3002",
    "\u4f9d\u636e\u201c\u6837\u54c1\u201d\u5728\u201c\u8ba1\u5212\u201d\u4e2d\u7684\u91cd\u8981\u7b49\u7ea7\uff0c\u201c\u4e00\u5ea7\u536b\u661f\u57ce\u7684\u4e94\u5206\u4e4b\u4e00\u201d\uff0c\u8fd9\u79cd\u7a0b\u5ea6\u7684\u635f\u574f\u4ecd\u7136\u5904\u4e8e\u5409\u62c9\u5c14\u96c6\u56e2\u53ef\u627f\u62c5\u7684\u8303\u56f4\u5185\u3002",
    "\u2026\u2026\u54fc\u3002",
    "\u90a3\u4f60\u4eec\u8fd8\u4e0d\u5feb\u53bb\u628a\u201c\u6837\u54c1\u201d\u7ed9\u6211\u5e26\u56de\u6765\uff1f",
    "\u8ffd\u8e2a\u4ecd\u5728\u8fdb\u884c\u4e2d\u3002\u552f\u4e00\u7684\u95ee\u9898\u662f\uff0cODE-01\u7684\u4e34\u65f6\u7ba1\u7406\u6743\u2014\u2014",
    "\u76ee\u524d\uff0c\u5728\u60a8\u7684\u624b\u4e0a\u3002",
    "\uff08\u516d\u5206\u4eea\u7a0d\u7a0d\u8f6c\u79fb\u4e86\u76ee\u5149\u3002\uff09",
    "\uff08\u5979\u7684\u76ee\u5149\u626b\u8fc7\u59d4\u6258\u4eba\uff0c\u504f\u5411\u4e86\u7a97\u5916\u95ea\u70c1\u7684\u9713\u8679\u706f\u3002\uff09",
    "\u201c\u8d6b\u5229\u201d\u5df2\u7ecf\u53d1\u51fa\u8b66\u544a\u3002\u53ea\u9700\u8981\u60a8\u627f\u62c5\u8d23\u4efb\uff0c\u201c\u6837\u54c1\u201d\u7684\u8ffd\u56de\u8ba1\u5212\u5c31\u80fd\u591f\u91cd\u65b0\u5f00\u59cb\u3002",
    "\u6211\u8d1f\u8d77\u8d23\u4efb\uff1f\u4f60\u60f3\u8ba9\u6211\u600e\u4e48\u627f\u62c5\u8d23\u4efb\uff1f\u6bd4\u5982\u628a\u4f60\u6254\u8fdb\u5e9f\u54c1\u56de\u6536\u7ad9\uff1f",
    "\u4f9d\u636e\u76ee\u524d\u7684\u60c5\u51b5\uff0c\u8fd9\u5e76\u4e0d\u662f\u4e00\u4e2a\u660e\u667a\u7684\u9009\u62e9\uff0c\u9601\u4e0b\u3002",
    "\u5e0c\u671b\u4f60\u7684\u6570\u636e\u4f20\u8f93\u901f\u5ea6\u8db3\u4ee5\u8ba9\u4f60\u6e05\u695a\u5730\u4e86\u89e3\u76ee\u524d\u7684\u60c5\u51b5\u3002|\u76f8\u8f83\u4e8e\u4f60\u8fd9\u79cd\u6d41\u6c34\u7ebf\u4ea7\u7269\uff0c\u6211\uff0c\u514b\u52b3\u8fea\u5bb6\u65cf\u7684\u76f4\u7cfb\u540e\u4ee3\uff0c\u8fbe\u4f26\u00b7\u514b\u52b3\u8fea\uff0c\u5982\u679c\u5bf9\u4f60\u63d0\u51fa\u8981\u6c42\uff0c\u4f60\u9700\u8981\u611f\u5230\u8363\u5e78\u3002",
    "\u2014\u2014\u8fbe\u4f26\u00b7\u514b\u52b3\u8fea\u9601\u4e0b\u3002",
    "\u9601\u4e0b\u5728\u514b\u52b3\u8fea\u96c6\u56e2\u4ee5\u53ca\u201c\u8ba1\u5212\u201d\u4e2d\u7684\u5b9e\u9645\u5730\u4f4d\uff0c\u5e94\u8be5\u4e0d\u9700\u8981\u6211\u518d\u6b21\u5411\u60a8\u5f3a\u8c03\u3002",
    "\uff08\u5b8c\u5168\u6ca1\u6709\u5bdf\u89c9\u51fa\u516d\u5206\u4eea\u7684\u4e0d\u8010\u70e6\uff0c\u59d4\u6258\u4eba\u903c\u8fd1\u516d\u5206\u4eea\uff0c\u4e00\u624b\u6293\u5411\u516d\u5206\u4eea\u7684\u9886\u53e3\u4f4d\u7f6e\u3002\uff09",
    "\uff08\u540e\u9000\u4e00\u6b65\uff0c\u516d\u5206\u4eea\u907f\u5f00\u4e86\u59d4\u6258\u4eba\u7684\u52a8\u4f5c\u3002\uff09",
    "\u4f60\uff1f\uff01",
    "\uff08\u6ca1\u60f3\u5230\u516d\u5206\u4eea\u4f1a\u8eb2\u5f00\uff0c\u59d4\u6258\u4eba\u7684\u624b\u50f5\u5728\u539f\u5730\u3002\uff09",
    "\uff08\u4ed6\u6124\u6012\u5730\u770b\u7740\u516d\u5206\u4eea\u3002\uff09",
    "\uff08\u4f46\u6ca1\u8fc7\u591a\u4e45\uff0c\u4ed6\u5c31\u653e\u4e0b\u4e86\u624b\uff0c\u8fd8\u987a\u52bf\u62cd\u4e86\u62cd\u8863\u670d\u4e0b\u6446\u3002\uff09",
    "\u4f60\u5728\u201c\u8ba1\u5212\u201d\u4e2d\u662f\u4ec0\u4e48\u5730\u4f4d\u2014\u2014\u54c8\uff0c\u9700\u8981\u6211\u518d\u5f3a\u8c03\u4e00\u904d\uff1f",
    "\u60a8\u4ecd\u7136\u5bf9\u201c\u8ba1\u5212\u201d\u6709\u6240\u8bef\u89e3\uff0c\u9601\u4e0b\u3002",
    "\u6211\u6709\u8bef\u89e3\uff1f\uff01\u4f60\u77e5\u4e0d\u77e5\u9053\uff0c\u50cf\u4f60\u8fd9\u6837\u7684\u5b9e\u9a8c\u54c1\u6839\u672c\u65e0\u5173\u7d27\u8981\uff1f",
    "\u4e4b\u524d\u5bf9\u90a3\u4e2a\u8d4f\u91d1\u730e\u4eba\u884c\u52a8\u65b9\u5411\u7684\u8bef\u5224\uff0c\u5bfc\u81f4\u5965\u5fb7\u5345\u7684\u90e8\u7f72\u88ab\u5e73\u767d\u6d6a\u8d39\uff0c\u803d\u8bef\u4e86\u201c\u6837\u54c1\u201d\u7684\u8ffd\u8e2a\u65f6\u95f4\uff0c\u8fd9\u4e2a\u8d23\u4efb\uff0c\u4f60\u5e94\u4e0d\u5e94\u8be5\u627f\u62c5\uff1f\u5982\u679c\u4e0d\u662f\u6536\u5230\u4e0a\u62a5\u6d88\u606f\uff0c\u6211\u53c8\u8981\u6c42\u5c01\u9501ODE-01\uff0c\u201c\u6837\u54c1\u201d\u5c31\u4f1a\u4e22\u5728ODE-01\uff0c\u800c\u4f60\u73b0\u5728\uff0c\u5374\u8981\u6c42\u6211\u627f\u62c5\u8d23\u4efb\uff1f",
    "\u884c\u52a8\u590d\u76d8\u53ca\u5b9a\u8d23\u5c06\u5728\u201c\u6837\u54c1\u201d\u7684\u56de\u6536\u4efb\u52a1\u5b8c\u6210\u540e\u8fdb\u884c\uff0c\u9601\u4e0b\u3002",
    "\u2014\u2014\u516d\u5206\u4eea\u3002",
    "\u6211\u5e0c\u671b\u4f60\u5bf9\u4f60\u7684\u5904\u5883\u80fd\u591f\u5efa\u7acb\u6b63\u786e\u7684\u8ba4\u77e5\u2014\u2014\u53ea\u8981\u4f5c\u4e3a\u514b\u52b3\u8fea\u5bb6\u65cf\u76f4\u7cfb\u540e\u4ee3\u7684\u6211\u53bb\u5411\u201c\u5b83\u201d\u62a5\u544a\u4f60\u7684\u65e0\u7528\u548c\u7d2f\u8d58\uff0c\u4f60\u6240\u9762\u4e34\u7684\u540e\u679c\uff0c\u53ea\u6709\u7acb\u523b\u88ab\u9500\u6bc1\u3002\u800c\u9500\u6bc1\u4e86\u4f60\uff0c\u7acb\u523b\u5c31\u80fd\u6709\u6210\u5343\u4e0a\u4e07\u4e2a\u5b9e\u9a8c\u54c1\u6765\u66ff\u4ee3\u4f60\u3002",
    "\u4f60\u2014\u2014\u4f60\u7ed9\u6211\u7ad9\u4f4f\uff01",
    "\uff08\u623f\u95f4\u5916\uff0c\u56de\u5eca\u4e0a\u7a7a\u65e0\u4e00\u4eba\u3002\uff09",
    "\uff08\u8d70\u51fa\u51e0\u6b65\uff0c\u786e\u8ba4\u5df2\u7ecf\u4e0d\u4f1a\u53d7\u5230\u623f\u95f4\u5185\u566a\u97f3\u7684\u5f71\u54cd\u540e\uff0c\u516d\u5206\u4eea\u7ad9\u4f4f\uff0c\u6309\u4e0b\u4e86\u8bbe\u7f6e\u5728\u8033\u4fa7\u7684\u901a\u8baf\u7ec8\u7aef\u3002\uff09",
    "\u2026\u2026\u516d\u5206\u4eea\uff1f",
    "\u7d20\u4f53\u91cd\u542f\u5b8c\u6210\uff1f",
    "\u5b8c\u6210\u2014\u2014\u786e\u5b9e\uff0c\u5df2\u7ecf\u5b8c\u6210\u3002\u6211\u5e0c\u671b\u8fd9\u79cd\u5ef6\u8fdf\u63a5\u542c\u901a\u8baf\u7684\u884c\u4e3a\uff0c\u4e0d\u4f1a\u88ab\u8f7d\u5165\u5e76\u4f5c\u4e3a\u60a8\u7684\u5e38\u89c4\u5e94\u5bf9\u6a21\u5f0f\u3002",
    "\u76f8\u8f83\u4e8e\u6211\u5ef6\u8fdf\u63a5\u542c\u901a\u8baf\u7684\u77ed\u66828\u79d2\uff0c\u4f60\u6bd4\u9884\u8ba1\u56de\u62a5\u65f6\u95f4\u665a\u4e8627\u520621\u79d2\u3002",
    "27\u520621\u79d2\uff1f\u5982\u679c\u6211\u63d0\u524d27\u520621\u79d2\uff0c\u6211\u662f\u5426\u80fd\u591f\u542c\u5230\u60a8\u56e0\u4e3a\u60a8\u7684\u9519\u8bef\u5224\u65ad\u5411\u6211\u9053\u6b49\uff1f",
    "\u9ed1\u773c\uff0c\u4efb\u52a1\u4ecd\u5728\u8fdb\u884c\u4e2d\u3002\u8bf7\u57285\u5206\u949f\u5185\u7ee7\u7eed\u6267\u884c\u4efb\u52a1\u3002|\u6240\u6709\u901a\u5f80\u8f66\u7ad9\u7684\u901a\u9053\u5df2\u88ab\u5c01\u9501\u3002\u7ee7\u7eed\u76d1\u63a7ODE-01\u536b\u661f\u57ce\uff0c\u201c\u6837\u54c1\u201d\u5fc5\u987b\u56de\u6765\u3002",
    "\u54e6~\u6211\u8ba4\u4e3a\u60a8\u53ef\u80fd\u9700\u8981\u5148\u4e86\u89e3\u66f4\u591a\u6b64\u524d\u53d1\u751f\u7684\u6218\u6597\u8be6\u60c5\uff1f",
    "\u8bf7\u6309\u7167\u89c4\u5b9a\u8def\u5f84\u63d0\u4ea4\u884c\u52a8\u62a5\u544a\u3002",
    "\u524d\u671f\u603b\u7ed3\u5df2\u7ecf\u63d0\u4ea4\uff0c\u8be6\u7ec6\u7684\u884c\u52a8\u62a5\u544a\u7a0d\u540e\u5c31\u4f1a\u53d1\u9001\uff0c\u5e0c\u671b\u60a8\u4f1a\u8be6\u7ec6\u9605\u8bfb\u3002\u4f46\u73b0\u5728\u6709\u66f4\u91cd\u8981\u7684\u4e8b\u60c5\u2014\u2014",
    "\u516d\u5206\u4eea\uff0c\u201c\u5728\u672a\u80fd\u83b7\u53d6\u5b8c\u6574\u4fe1\u606f\u7684\u524d\u63d0\u4e0b\u505a\u51fa\u5224\u65ad\u201d\uff0c\u8fd9\u79cd\u9c81\u83bd\u800c\u8f7b\u7387\u7684\u884c\u4e3a\uff0c\u6211\u5e76\u4e0d\u8ba4\u4e3a\u80fd\u591f\u88ab\u5f52\u5165\u4e09\u4ee3\u4eba\u5f62\u7684\u5e38\u89c4\u5e94\u5bf9\u6a21\u5f0f\u4e2d\u3002\u7531\u6b64\u800c\u4ea7\u751f\u7684\u5931\u8d25\uff0c\u6211\u5e0c\u671b\u770b\u5230\u60a8\u662f\u5982\u4f55\u5408\u7406\u800c\u5b8c\u6574\u5730\u627f\u62c5\uff1f",
    "\u9ed1\u773c\u3002",
    "\uff08\u63a5\u8fd1\u5348\u591c\uff0c\u7a97\u5916\u5076\u5c14\u63a0\u8fc7\u8f66\u8f86\u5f80\u6765\u7684\u58f0\u97f3\uff0c\u6a21\u7cca\u5f97\u50cf\u9694\u7740\u4e00\u5c42\u6c34\u6ce1\u3002\uff09",
    "\uff08\u516d\u5206\u4eea\u8eab\u540e\uff0c\u56de\u5eca\u5c3d\u5934\u7684\u623f\u95f4\u5185\uff0c\u8fbe\u4f26\u00b7\u514b\u52b3\u8fea\u4f3c\u4e4e\u5b89\u9759\u4e86\u4e0b\u6765\u3002\uff09",
    "\u201c\u8d38\u7136\u4f7f\u7528\u6218\u6597\u578b\u6001\u201d\uff0c\u4ee5\u53ca\u201c\u5728\u672a\u80fd\u51c6\u786e\u9884\u5224\u654c\u65b9\u884c\u4e3a\u7684\u524d\u63d0\u4e0b\u653e\u4efb\u654c\u65b9\u884c\u52a8\u201d\uff0c\u9488\u5bf9\u8fd9\u4e24\u9879\u884c\u4e3a\u7684\u89e3\u91ca\uff0c\u4ee5\u53ca\u53ef\u80fd\u7531\u6b64\u4ea7\u751f\u7684\u5584\u540e\u5de5\u4f5c\uff0c\u6211\u5e0c\u671b\u80fd\u591f\u5728\u4f60\u7a0d\u540e\u7684\u8be6\u7ec6\u884c\u52a8\u62a5\u544a\u4e2d\u770b\u5230\u3002",
    "\u2026\u2026\u54c8~",
    "\u76d1\u63a7\u3001\u8ffd\u51fb\u3001\u5c01\u9501\uff0c\u5982\u679c\u60a8\u6b64\u524d\u7684\u884c\u4e3a\u6709\u6548\uff0c\u60a8\u5c31\u4e0d\u4f1a\u6765\u5bfb\u6c42\u6211\u7684\u5e2e\u52a9\u3002|\u9762\u5bf9\u5931\u8d25\uff0c\u60a8\u6240\u505a\u51fa\u7684\u5224\u65ad\uff0c\u53ea\u662f\u518d\u4e00\u6b21\u7684\u76d1\u63a7\u3001\u8ffd\u51fb\u3001\u5c01\u9501\uff1f",
    "\u8bf7\u6267\u884c\u547d\u4ee4\uff0c\u9ed1\u773c\u3002\u8bf7\u57283\u520650\u79d2\u5185\u5f00\u59cb\u884c\u52a8\u3002",
    "\u6211\u671f\u5f85\u8046\u542c\u60a8\u66f4\u52a0\u6709\u6548\u7684\u5224\u65ad\u3002|\u201c\u970d\u6d1b\u6208\u8ba1\u5212\u201d\u60c5\u62a5\u4e2d\u53f0\u6267\u884c\u957f\uff0c\u516d\u5206\u4eea\u3002",
    "\uff08\u53cc\u65b9\u50f5\u6301\u4e0d\u4e0b\uff0c\u9677\u5165\u77ed\u6682\u6c89\u9ed8\u3002\uff09",
    "\uff08\u516d\u5206\u4eea\u8d70\u5411\u56de\u5eca\u8fb9\u7f18\uff0c\u900f\u8fc7\u539a\u5b9e\u7684\u73bb\u7483\u7a97\u770b\u5411\u4e0b\u65b9\u9010\u6e10\u6c89\u5bc2\u7684\u57ce\u5e02\u3002\uff09",
    "\uff08\u5728\u5404\u7c7b\u4ecd\u88ab\u70b9\u4eae\u7684\u9713\u8679\u706f\u4e4b\u5916\uff0c\u6570\u4e4b\u4e0d\u5c3d\u7684\u6444\u50cf\u5934\u4e5f\u5728\u6301\u7eed\u8fd0\u8f6c\u3002\uff09",
    "\u201c\u8d6b\u5229\u201d\u53d1\u51fa\u7684\u8b66\u544a\u5185\u5bb9\uff0c\u4f60\u5e94\u8be5\u5df2\u7ecf\u77e5\u9053\u3002",
    "\u201c\u6837\u54c1\u201d\u9700\u8981\u8ffd\u56de\uff0c\u201c\u5965\u5fb7\u5345\u201d\u4e5f\u5fc5\u987b\u5904\u4e8e\u5409\u62c9\u5c14\u96c6\u56e2\u7684\u638c\u63a7\u4e0b\u3002|\u6211\u4eec\u7684\u884c\u52a8\uff0c\u5fc5\u987b\u7ef4\u6301\u5728\u201c\u4e2d\u98ce\u9669\u201d\u53ca\u4ee5\u4e0b\u7684\u5224\u5b9a\u8303\u56f4\u5185\u3002\u4f60\u6240\u5426\u5b9a\u7684\u76d1\u63a7\u3001\u8ffd\u51fb\u3001\u5c01\u9501\uff0c\u662f\u76ee\u524d\u7684\u6700\u4f18\u9009\u62e9\u3002",
