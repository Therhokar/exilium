[
  [
    "\uff08\u6309\u7167\u6df7\u6df7\u6307\u660e\u7684\u65b9\u5411\uff0c\u4e09\u4eba\u6765\u5230\u68da\u5c4b\u65c1\u5c0f\u8def\u91cc\u7684\u5783\u573e\u5806\u9644\u8fd1\u3002\uff09",
    "\uff08\u4e09\u4eba\u9762\u524d\uff0c\u5c42\u53e0\u7684\u5783\u573e\u5806\u5728\u5468\u56f4\u9713\u8679\u706f\u7684\u6620\u7167\u4e0b\uff0c\u5448\u73b0\u51fa\u6d53\u91cd\u7684\u9ed1\u7eff\u8272\uff0c\u4ece\u5916\u89c2\u57fa\u672c\u65e0\u6cd5\u5224\u65ad\u5185\u5bb9\u7269\u7684\u79cd\u7c7b\u3002\uff09",
    "\u554a\uff0c\u5783\u573e\u5806\uff01\u5e94\u8be5\u5c31\u662f\u8fd9\u91cc\u4e86\u54df~",
    "\u8fd9\u79cd\u5473\u9053\u2014\u2014\u514b\u7f57\u4e3d\u79d1\uff0c\u4f60\u7684\u670b\u53cb\uff0c\u8be5\u4e0d\u4f1a\u559c\u6b22\u6234\u82cd\u8747\u7684\u8033\u6735\u5427\u2014\u2014\u5514\uff0c\u5e94\u8be5\u662f\u201c\u82cd\u8747\u7684\u89e6\u89d2\u201d\uff1f",
    "\u8c01\u8bf4\u5979\u662f\u6211\u670b\u53cb\uff1f\uff01",
    "\u90a3\u4e48\u2014\u2014\u4e0d\u662f\u670b\u53cb\uff0c\u662f\u4ec0\u4e48\u5462~\uff1f",
    "\u2026\u2026\u5567\uff0c\u95ee\u4ec0\u4e48\u95ee\uff0c\u8fd9\u8ddf\u4f60\u6709\u5173\u7cfb\u5417\u3002",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u7387\u5148\u7ed5\u7740\u5783\u573e\u5806\u8d70\u4e86\u4e00\u5708\uff0c\u9762\u65e0\u8868\u60c5\u3002\uff09",
    "\u662f\u8fd9\u4e2a\u5427\u3002",
    "\uff08\u5783\u573e\u5806\u7684\u4e00\u89d2\uff0c\u6709\u4e2a\u5229\u7528\u51e0\u5757\u5e9f\u5f03\u7684\u91d1\u5c5e\u677f\u642d\u8d77\u7684\u5c0f\u5e73\u53f0\uff0c\u4e0a\u9762\u6446\u7740\u4e00\u4e2a\u767d\u8272\u7684\u76d2\u5b50\uff0c\u76d2\u5b50\u4e0a\u751a\u81f3\u7528\u5e9f\u5f03\u7684\u5851\u6599\u5e03\u7cfb\u4e86\u4e00\u4e2a\u8774\u8776\u7ed3\u3002\uff09",
    "\uff08\u8774\u8776\u7ed3\u7684\u4e24\u4e2a\u5706\u5708\u626d\u66f2\u6210\u5947\u602a\u7684\u5f62\u72b6\uff0c\u4f3c\u4e4e\u662f\u9075\u5faa\u67d0\u79cd\u89c4\u5f8b\u51fa\u73b0\u7684\u5f27\u5f62\u3002\uff09",
    "\uff08\u9ed1\u7eff\u8272\u7684\u5783\u573e\u5806\u3001\u4e94\u989c\u516d\u8272\u7684\u9713\u8679\u706f\uff0c\u4e0e\u767d\u8272\u7684\u76d2\u5b50\uff0c\u642d\u914d\u8d77\u6765\u683c\u5916\u602a\u5f02\u3002\uff09",
    "\u8fd9\u770b\u8d77\u6765\uff0c\u7edd\u5bf9\u2014\u2014\u662f\u9677\u9631\u54df~\uff1f",
    "\u90a3\u4e5f\u8981\u8bd5\u8bd5\u624d\u77e5\u9053\u3002",
    "\uff08\u770b\u5230\u8774\u8776\u7ed3\uff0c\u514b\u7f57\u4e3d\u79d1\u8868\u73b0\u51fa\u660e\u663e\u7684\u70e6\u8e81\u3002)",
    "\uff08\u5979\u76ef\u7740\u76d2\u5b50\uff0c\u968f\u540e\u8d70\u5230\u5783\u573e\u5806\u8fb9\u7f18\u3002\uff09",
    "\uff08\u4f3c\u4e4e\u662f\u8ba1\u7b97\u597d\u7684\uff0c\u514b\u7f57\u4e3d\u79d1\u8e2e\u8d77\u811a\uff0c\u4f38\u957f\u4e86\u624b\u81c2\uff0c\u521a\u521a\u597d\u80fd\u628a\u76d2\u5b50\u62ff\u5728\u624b\u91cc\u3002\uff09",
    "\u2026\u2026\u65e0\u8272\u7684\u5f62\u4f53\u2026\u2026\u5760\u843d\uff0c\u6cbe\u67d3\u989c\u8272\u2026\u2026",
    "\u6211\u8fd9\u4e0d\u662f\u597d\u597d\u62ff\u5230\u4e86\u5417\uff0c\u4f60\u624d\u4f1a\u6454\u5230\u5783\u573e\u5806\u91cc\u3002",
    "\u2026\u2026\u8352\u91ce\u9000\u5374\uff0c\u571f\u5730\u56db\u6563\u2026\u2026",
    "\u54fc\uff0c\u6db2\u538b\u4f20\u52a8\u7cfb\u7edf\u8fd0\u884c\u4e0d\u7a33\u7684\u662f\u4f60\u5427\u7eb3\u7f8e\u897f\u4e1d\uff1f",
    "\u597d\u5566\u5148\u62c6\u5f00\u770b\u770b\u5427\uff0c\u57f9\u517b\u4eb2\u5bc6\u5ea6\u7684\u5bf9\u8bdd\u8bad\u7ec3\u53ef\u4ee5\u518d\u7b49\u7b49\u54df~",
    "\u8c01\u8ddf\u5979\u57f9\u517b\u4eb2\u5bc6\u5ea6\uff1f\uff01",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u6fc0\u52a8\u5730\u6325\u821e\u7740\u624b\uff0c\u5979\u62ce\u7740\u7684\u76d2\u5b50\u6b63\u597d\u51fa\u73b0\u5728\u5bc7\u5c14\u8299\u773c\u524d\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u987a\u52bf\u5c06\u76d2\u5b50\u63a5\u8fc7\u6765\uff0c\u6258\u5728\u624b\u91cc\u6643\u4e86\u6643\u3002\uff09",
    "\u8fd9\u4e2a\u91cd\u91cf\uff0c\u5c31\u7b97\u662f\u91cc\u9762\u88c5\u7684\u662f\u5371\u9669\u54c1\uff0c\u5a01\u529b\u4e5f\u4e0d\u4f1a\u5f88\u5927\u7684\u6837\u5b50~|\u51c6\u5907\u597d\u4e86\u5417\uff0c\u5927\u5bb6\u2014\u2014",
    "\uff08\u5bc7\u5c14\u8299\u4f5c\u52bf\u5c31\u8981\u53bb\u626f\u76d2\u5b50\u4e0a\u626d\u66f2\u7684\u8774\u8776\u7ed3\u3002\uff09",
    "\uff08\u6ca1\u770b\u5230\u514b\u7f57\u4e3d\u79d1\u5236\u6b62\u7684\u52a8\u4f5c\uff0c\u5bc7\u5c14\u8299\u53cd\u800c\u6709\u4e9b\u5947\u602a\u5730\u770b\u4e86\u770b\u514b\u7f57\u4e3d\u79d1\u3002\uff09",
    "\u514b\u7f57\u4e3d\u79d1\uff1f\u76d2\u5b50\uff0c\u4eba\u5bb6\u62c6\u4e86\u54df\uff1f",
    "\u55ef\u3002",
    "\uff08\u76d2\u5b50\u88ab\u5bc7\u5c14\u8299\u6253\u5f00\u4e86\u3002\uff09",
    "\uff08\u653e\u5728\u76d2\u5b50\u91cc\u7684\uff0c\u662f\u4e00\u4e2a\u7834\u65e7\u7684\u901a\u8baf\u88c5\u7f6e\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u6b63\u8981\u4f38\u624b\u53bb\u62ff\uff0c\u514b\u7f57\u4e3d\u79d1\u5374\u5c06\u76d2\u5b50\u62a2\u4e86\u8fc7\u6765\u3002\uff09",
    "\u73b0\u5728\u624d\u6765\u62a2\uff0c\u662f\u4e0d\u662f\u2014\u2014\u665a\u4e86\u4e00\u70b9\uff1f|\u4eba\u5bb6\u770b\u5230\u4e86\uff0c\u91cc\u9762\u662f\u4e2a\u901a\u8baf\u88c5\u7f6e\uff0c\u4e0a\u9762\u8fd8\u6709\u884c\u5b57\u54df~",
    "\u2026\u2026",
    "\u514b\u7f57\u4e3d\u79d1\uff1f\u514b\u7f57\u4e3d\u79d1\uff1f\u4e0a\u9762\u5199\u4e86\u4ec0\u4e48\u5440\uff1f",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u7684\u795e\u8272\u4ece\u70e6\u8e81\u9010\u6e10\u53d8\u4e3a\u54ac\u7259\u5207\u9f7f\u3002\uff09",
    "\uff08\u5979\u72e0\u72e0\u5730\u76ef\u7740\u901a\u8baf\u88c5\u7f6e\u7684\u5c4f\u5e55\uff0c\u968f\u540e\u6781\u5feb\u5730\u62ff\u8d77\u653e\u6709\u901a\u8baf\u88c5\u7f6e\u7684\u76d2\u5b50\uff0c\u5c31\u8981\u6454\u5230\u5730\u4e0a\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u8d76\u7d27\u62e6\u4f4f\u5979\u3002\uff09",
    "\u4f60\u5c45\u7136\u8eb2\u5728\u8fd9\u79cd\u5730\u65b9\uff0c\u6211\u53ef\u771f\u662f\u6ca1\u60f3\u5230\uff01",
    "\u554a\u4e0d\u8981\u6fc0\u52a8\u2014\u2014\u514b\u7f57\u4e3d\u79d1\uff1f",
    "\uff08\u8eab\u9ad8\u4e0d\u53ca\u514b\u7f57\u4e3d\u79d1\uff0c\u5bc7\u5c14\u8299\u53ea\u80fd\u5f92\u52b3\u5730\u62b1\u4f4f\u514b\u7f57\u4e3d\u79d1\u7684\u624b\u81c2\u3002\uff09",
    "\uff08\u7eb3\u7f8e\u897f\u4e1d\u987a\u52bf\u4ece\u514b\u7f57\u4e3d\u79d1\u7684\u624b\u4e2d\u62ff\u8d70\u4e86\u76d2\u5b50\u3002\uff09",
    "\u2026\u2026\u6697\u6d41\uff0c\u649e\u51fb\u704c\u6728\u2026\u2026\u6c89\u6d6e\u7684\u7a7a\u6c14\u4e4b\u4e2d\uff0c\u65e0\u6240\u51ed\u4f9d\u2026\u2026",
    "\u4f60\uff1f\uff01",
    "\u597d\u5566\u597d\u5566\uff0c\u8ba9\u7eb3\u7f8e\u897f\u4e1d\u5148\u4fdd\u7ba1\u4e00\u4e0b~|\u7eb3\u7f8e\u897f\u4e1d\uff0c\u653e\u4f4e\u70b9\u561b\uff0c\u8ba9\u4eba\u5bb6\u770b\u770b\u2014\u2014\u8fd9\u4e0a\u9762\u5199\u4e86\u4ec0\u4e48\u5440\uff0c\u514b\u7f57\u4e3d\u79d1\u8fd9\u2014\u2014\u4e48\u751f\u6c14~",
    "\u5514\uff0c\u201c\u52a0\u5bc6\u9891\u9053\uff0c\u653e\u5fc3\u4f7f\u7528\u201d\uff1f\u8fd8\u6709\u4e00\u884c\u5c0f\u5b57\uff0c\u201c\u66f4\u5feb\u79bb\u5f00\uff0c\u4e0d\u5982\u8bd5\u8bd5\u5f53\u65f6\u6211\u4eec\u79bb\u5f00\u7684\u8def\u201d\uff1f\u8fd9\u4e0b\u9762\u7684\u7b26\u53f7\u662f\u4ec0\u4e48\u2014\u2014\u4e00\u4e2a\u5706\u5708\uff0c\u548c\u4e0a\u9762\u7684\u4e24\u4e2a\u5947\u602a\u7684\u6ce2\u6d6a\u7ebf\uff1f\u770b\u8d77\u6765\u2014\u2014\u6709\u70b9\u50cf\u76d2\u5b50\u5916\u9762\u7684\u8774\u8776\u7ed3\u54df\uff1f",
    "\u662f\u732b\u8033\u3002",
    "\u732b\u8033\u2014\u2014\u53ef\u662f\u4e0d\u7ba1\u600e\u4e48\u770b\uff0c\u4e5f\u4e0d\u50cf\u662f\u732b\u8033\u5440\uff1f",
    "\u5c31\u662f\u732b\u8033\uff01\u8fd9\u4e1c\u897f\uff0c\u6211\u5c31\u7b97\u7d20\u4f53\u91cd\u542f\u4e00\u767e\u6b21\u90fd\u80fd\u8ba4\u51fa\u6765\uff01",
    "\uff08\u76d2\u5b50\u548c\u901a\u8baf\u88c5\u7f6e\u90fd\u5728\u7eb3\u7f8e\u897f\u4e1d\u624b\u91cc\uff0c\u514b\u7f57\u4e3d\u79d1\u4e5f\u6ca1\u6709\u8bd5\u56fe\u62a2\u56de\u53bb\u3002\uff09",
    "\uff08\u5979\u53cc\u624b\u62b1\u81c2\uff0c\u5934\u626d\u5411\u4e00\u4fa7\u3002\uff09",
    "\u8fd9\u4e48\u8bf4\u8d77\u6765\uff0c\u90a3\u4e2a\u201c\u6234\u7740\u4e0d\u77e5\u9053\u4ec0\u4e48\u8033\u6735\u7684\u4eba\u5f62\u201d\uff0c\u5176\u5b9e\u2014\u2014\u5c31\u662f\u732b\u8033\u4e86\u54df\uff1f",
    "\u6211\u7ba1\u5979\u6234\u4ec0\u4e48\u4e1c\u897f\uff01\u8fd9\u76d2\u5b50\u91cc\u7684\u901a\u8baf\u88c5\u7f6e\u80fd\u7528\uff0c\u5feb\u70b9\u8054\u7cfb\u6307\u6325\u5b98\u95ee\u95ee\u60c5\u51b5\u3002",
    "\u53ef\u4e4b\u524d\u961f\u957f\u8981\u6c42\u6211\u4eec\u201c\u4e0d\u8981\u4f7f\u7528\u901a\u8baf\u201d\u5440~|\u5e94\u8be5\u662f\u5305\u62ec\u4e86\u6240\u6709\u7684\u901a\u8baf\u88c5\u7f6e\uff0c\u4ee5\u53ca\u80fd\u591f\u8fde\u63a5\u5230\u7684\u6240\u6709\u901a\u8baf\u9891\u9053\u54df\uff1f",
    "\u8fd9\u4e2a\u88c5\u7f6e\u2014\u2014\u53ef\u4ee5\u4f7f\u7528\u3002\u662f\u5417\uff0c\u514b\u7f57\u4e3d\u79d1\uff1f",
    "\u2014\u2014\u5f53\u7136\u80fd\u7528\uff01\u7edd\u5bf9\u80fd\u7528\uff01\u5982\u679c\u90a3\u4e2a\u6df7\u86cb\u6ca1\u9000\u5316\uff01",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u54ac\u7259\u5207\u9f7f\u5730\u76ef\u7740\u76d2\u5b50\u4e2d\u7834\u65e7\u7684\u901a\u8baf\u88c5\u7f6e\uff0c\u4f3c\u4e4e\u4e0b\u4e00\u523b\u5c31\u8981\u5c06\u5b83\u593a\u8fc7\u6765\u6454\u5230\u5730\u4e0a\u3002)",
    "\uff08\u5bc7\u5c14\u8299\u770b\u7740\u514b\u7f57\u4e3d\u79d1\uff0c\u826f\u4e45\uff0c\u9732\u51fa\u5b89\u629a\u7684\u7b11\u5bb9\u3002\uff09",
    "\uff08\u5979\u4ece\u76d2\u5b50\u91cc\u62ff\u51fa\u901a\u8baf\u88c5\u7f6e\uff0c\u5f00\u59cb\u64cd\u4f5c\u3002\uff09",
    "\u597d\u5566\u597d\u5566\uff0c\u514b\u7f57\u4e3d\u79d1\u522b\u751f\u6c14~|\u5514\uff0c\u8fd9\u4e2a\u578b\u53f7\u6709\u70b9\u9ebb\u70e6\uff0c\u7f8e\u73b2\u662f\u8fd9\u4e48\u8bf4\u7684\uff0c\u5e94\u8be5\u8fd9\u6837\u2026\u2026",
    "\u554a\uff0c\u6210\u529f\u4e86\u54df~",
    "\u6211\u8bf4\uff0c\u6211\u4eec\u5148\u4ece\u8fd9\u5730\u65b9\u51fa\u53bb\u5427\u3002",
    "\u5148\u7b49\u4e0b\u6307\u6325\u5b98\u2014\u2014\u6bd4\u8f83\u597d\u54df\uff1f",
    "\u90fd\u54cd\u534a\u5929\u4e86\uff0c\u8fd8\u8981\u591a\u4e45\u554a\uff1f",
    "\u518d\u7b49\u7b49\u561b\uff0c\u6307\u6325\u5b98\u90a3\u91cc\u7684\u60c5\u51b5\u2014\u2014\u6211\u4eec\u90fd\u4e0d\u77e5\u9053\u7684\u5440~|\u554a\u3002",
    "\u5582\u2026\u2026\uff1f"
  ],
  [
    "\u961f\u957f\u2026\u2026\uff1f",
