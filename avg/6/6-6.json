    "\u4e0d\u786e\u5b9a\u3002\u7b49\u683c\u7433\u59d0\u5f00\u5b8c\u4f1a\uff0c\u6211\u53bb\u95ee\u95ee\u3002",
    "\u5514\uff0c\u6307\u6325\u5b98\uff0c\u867d\u7136\u6211\u4eec\u5df2\u7ecf\u628a\u8fd9\u4e2a\u4e8b\u4ef6\u60f3\u5f97\u5f88\u4e25\u91cd\u4e86\uff0c\u4f46\u7ee7\u7eed\u67e5\u4e0b\u53bb\uff0c\u611f\u89c9\u8fd8\u662f\u8d85\u51fa\u4e86\u6211\u4eec\u7684\u9884\u671f\u3002\u901a\u8baf\u662f\u7528\u4e86\u52a0\u5bc6\u7ebf\u8def\u6ca1\u9519\u5566\uff0c\u53ef\u8fd8\u662f\u6709\u88ab\u7a83\u542c\u7684\u98ce\u9669\u2026\u2026\u63a5\u4e0b\u6765\u7684\u4e00\u6bb5\u65f6\u95f4\u4f30\u8ba1\u90fd\u4e0d\u80fd\u4e92\u76f8\u8054\u7cfb\u5566\u3002",
    "\u4f60\u8981\u6ce8\u610f\u5b89\u5168\u54e6\uff0c\u6211\u4eec\u5728\u57fa\u8f85\u7b49\u4f60\u3002",
    "\u5982\u679c\u89c1\u5230\u516d\u5206\u4eea\uff1f",
    "\u516d\u5206\u4eea\u2026\u2026\u516d\u5206\u4eea\u2026\u2026\u5982\u679c\u78b0\u5230\u5979\uff0c\u8054\u7cfb\u4e00\u4e0b\u6211\uff0c\u5e94\u8be5\u8fd8\u662f\u2026\u2026\u6ca1\u95ee\u9898\u7684\u2026\u2026\u5427\u2026\u2026",
    "\u7b97\u4e86\u7b97\u4e86\uff0c\u9047\u5230\u516d\u5206\u4eea\uff0c\u5c31\u53ea\u80fd\u8bf7\u6307\u6325\u5b98\u4f60\u5148\u5e94\u4ed8\u4e00\u4e0b\u5979\uff0c\u7136\u540e\u628a\u60c5\u51b5\u8bb0\u5f55\u4e0b\u6765\u5566\u3002|\u80fd\u628a\u9ed1\u773c\u5e72\u6389\uff0c\u516d\u5206\u4eea\u5e94\u8be5\u4e5f\u662f\u6ca1\u95ee\u9898\u7684\uff1f",
    "\u6b64\u524d\uff0c\u6211\u4eec\u4e0e\u9ed1\u773c\u6709\u8fc7\u591a\u6b21\u5bf9\u6218\uff0c\u5bf9\u4e8e\u5979\u7684\u6218\u6597\u65b9\u5f0f\u53ca\u884c\u4e3a\u6a21\u5f0f\u6211\u4eec\u90fd\u6709\u9884\u671f\u3002\u4f46\u5bf9\u4e8e\u516d\u5206\u4eea\uff0c\u6211\u4eec\u5e76\u6ca1\u6709\u592a\u591a\u4fe1\u606f\uff0c\u5982\u679c\u5979\u4e5f\u5177\u6709\u4e0e\u8868\u9762\u4e0d\u540c\u7684\u578b\u6001\uff0c\u6211\u4eec\u53ef\u80fd\u4f1a\u5f88\u96be\u5bf9\u4ed8\u3002",
    "\u5514\u2026\u2026\u516d\u5206\u4eea\u7684\u8d44\u6599\uff0c\u6211\u4eec\u8fd9\u91cc\u4e5f\u6ca1\u6709\u3002|\u4e0d\u8fc7\u4e5f\u4e0d\u8981\u592a\u62c5\u5fc3\uff0c\u6309\u7167\u4f60\u73b0\u5728\u7684\u4f4d\u7f6e\uff0c\u53bb\u5f80\u57fa\u8f85\u7684\u8def\u7ebf\uff0c\u6211\u4eec\u57fa\u672c\u90fd\u627e\u8fc7\u5566\uff0c\u5e94\u8be5\u662f\u78b0\u4e0d\u5230\u7684\u3002\u683c\u7433\u59d0\u8fd9\u8fb9\u6211\u4f1a\u770b\u7740\u7684\uff0c\u53ef\u4ee5\u7684\u8bdd\uff0c\u6211\u4f1a\u7acb\u523b\u8054\u7cfb\u4f60\u7684~",
    "\u6307\u6325\u5b98\uff0c\u8981\u6709\u4e00\u6bb5\u65f6\u95f4\u89c1\u4e0d\u5230\u5566\uff0c\u6709\u4ec0\u4e48\u8bdd\u8981\u6211\u5e26\u7ed9\u683c\u7433\u59d0\u5417\uff1f",
    "\u2026\u2026\u63d0\u9192\u683c\u7433\uff0c\u5c0f\u5fc3\u884c\u4e8b\uff0c\u5979\u548c\u4ee5\u524d\u7684\u8eab\u4efd\u4e0d\u540c\u4e86\u3002",
    "\u6536\u5230\uff01\u7edd\u5bf9\u4f20\u8fbe\u5230~|\u7b49\u6211\u8054\u7cfb\u54e6\uff0c\u6307\u6325\u5b98\u266a",
    "\u5c0f\u77f3\u5934\u3001\u6307\u6325\u5b98\uff0c\u5c0f\u77f3\u5934\uff01",
    "\uff08\u6d77\u4f26\u5a1c\u62fd\u7740\u6211\u4e0d\u653e\u3002\uff09",
    "\uff08\u6211\u62c9\u7740\u5979\u8d70\u5230\u4f11\u606f\u533a\uff0c\u5c06\u5979\u91cd\u65b0\u5b89\u7f6e\u5728\u5ea7\u4f4d\u4e0a\u3002\uff09",
    "\u95ea\u7535\uff0c\u5bc7\u5c14\u8299\u5462\uff1f",
    "\u6b63\u5728\u5e2e\u52a9\u7f8e\u73b2\u6574\u7406\u548c\u6e05\u70b9\u76ee\u524d\u4ed3\u5e93\u5185\u7684\u7269\u8d44\uff0c\u77ed\u65f6\u95f4\u5185\u5e94\u8be5\u4e0d\u80fd\u6765\u6307\u6325\u5ba4\u3002",
    "\u2026\u2026\u5567\u3002",
    "\uff08\u968f\u7740\u827e\u83ab\u53f7\u7684\u884c\u8fdb\uff0c\u524d\u65b9\u7684\u5929\u8272\u88ab\u906e\u6321\uff0c\u6307\u6325\u5ba4\u5185\u6697\u4e86\u4e0b\u6765\u3002\uff09",
    "\u5373\u5c06\u8fdb\u5165\u6bd4\u970d\u5c14\u5ce1\u8c37\u3002",
    "\u5ce1\u8c37\uff1f\u5750\u6807\u70b9\u8981\u7ecf\u8fc7\u5ce1\u8c37\uff1f",
    "\u76ee\u7684\u5730\u663e\u793a\u5728\u5ce1\u8c37\u5185\u3002",
    "\u2026\u2026\u63d0\u9ad8\u884c\u9a76\u901f\u5ea6\uff0c\u5feb\u70b9\u901a\u8fc7\u3002|\u8fd9\u6b21\uff0c\u4e0d\u4f1a\u518d\u548c\u4e0a\u6b21\u4e00\u6837\u4e86\u2014\u2014",
