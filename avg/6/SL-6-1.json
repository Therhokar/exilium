[
  [
    "\u901a\u8baf\u5e72\u6270\u6d88\u5931\u5df2\u786e\u8ba4\uff0c\u901a\u8baf\u8bbe\u5907\u4f1a\u57280001\u540e\u5c1d\u8bd5\u91cd\u542f\uff01",
    "\u95ea\u7535\u5979\u4eec\u5462\uff1f",
    "\u961f\u957f\u5df2\u7ecf\u5e2e\u6211\u4eec\u628a\u56f4\u653b\u57fa\u5730\u8f66\u7684\u751f\u9ab8\u90fd\u6e05\u7406\u6389\u4e86\uff0c\u57280016\u524d\u5df2\u7ecf\u56de\u5f52\u57fa\u5730\u8f66\u3002|\u6e05\u9664\u4e00\u4e0b\u574d\u584c\u8f90\u5c04\uff0c\u5e94\u8be5\u5f88\u5feb\u5c31\u80fd\u8fc7\u6765\u4e86\uff01",
    "\u55ef\uff0c\u56de\u6765\u5f97\u8fd8\u771f\u662f\u53ca\u65f6\u3002",
    "\u867d\u7136\u8fd9\u4e9b\u751f\u9ab8\u8fd8\u7b97\u597d\u5bf9\u4ed8\uff0c\u53ef\u6570\u91cf\u4e5f\u592a\u591a\u4e86\u3002\u518d\u665a\u4e00\u70b9\uff0c\u6211\u4eec\u7684\u827e\u83ab\u53f7\u4f30\u8ba1\u90fd\u8981\u88ab\u5b83\u4eec\u7ed9\u5543\u5149\u3002",
    "\u5543\u3001\u5543\u5149\uff1f\uff01|\u5443\uff0c\u4e0d\u4f1a\u7684\u5427\u2026\u2026\u5982\u679c\u5543\u5149\u4e86\uff0c\u5c31\u8981\u6574\u4f53\u7ef4\u4fee\u2026\u2026\u4e0d\u8981\u8bf4\u8fd9\u4e48\u53ef\u6015\u7684\u8bdd\u9898\u554a\u6307\u6325\u5b98\uff01",
    "\u8fd9\u90fd\u8981\u611f\u8c22\u95ea\u7535\u961f\u957f\u56de\u6765\u5f97\u53ca\u65f6\u554a\u3002|\u5206\u6790\u73af\u5883\u60c5\u62a5\uff0c\u627e\u4e00\u6761\u4e0d\u4f1a\u88ab\u574d\u584c\u98ce\u66b4\u5f71\u54cd\u7684\u8def\u3002\u6211\u4eec\u5148\u79bb\u5f00\u8fd9\u91cc\uff0c\u7f8e\u73b2\u3002",
    "\u554a\uff0c\u554a\u2026\u2026\u597d\u7684\u3002|\u73af\u5883\u60c5\u62a5\u89e3\u6790\u5f00\u59cb\u2026\u2026\u8def\u7ebf\u786e\u8ba4\uff0c\u5411\u529b\u5f15\u64ce\u542f\u52a8\uff0c\u901f\u5ea6\u8bbe\u7f6e\u4e3a45KM/H\uff01",
    "\uff08\u827e\u83ab\u53f7\u542f\u52a8\uff0c\u5f00\u59cb\u884c\u9a76\u3002\uff09",
    "\uff08\u6307\u6325\u5ba4\u5165\u53e3\u7684\u6ed1\u95e8\u5f00\u542f\uff0c\u603b\u63a7\u53f0\u524d\u7684\u7f8e\u73b2\u542c\u5230\u58f0\u97f3\uff0c\u8f6c\u8fc7\u5934\u3002\uff09",
    "\u554a\uff0c\u662f\u961f\u957f\uff0c\u8fd8\u6709\u5bc7\u5c14\u8299\uff01",
    "\u4eba\u5bb6\u56de\u6765\u4e86\u54df\uff0c\u7f8e\u73b2\u2014\u2014\u6709\u6ca1\u6709\u60f3\u4eba\u5bb6~\uff1f",
    "\u5443\u2026\u2026\u5443\uff1f\u73b0\u5728\u4e0d\u662f\u8bf4\u8fd9\u4e2a\u7684\u65f6\u5019\u5427\u2026\u2026\u5927\u5bb6\u90fd\u6ca1\u4e8b\uff1f\u7eb3\u7f8e\u897f\u4e1d\u548c\u514b\u7f57\u4e3d\u79d1\u5462\uff1f",
    "\u7d20\u4f53\u635f\u4f24\uff0c\u76ee\u524d\u5728\u6574\u5907\u5ba4\u3002",
    "\u53c8\u8981\u8f9b\u82e6\u7f8e\u73b2\u4e86\u54df~|\u4eba\u5bb6\u867d\u7136\u4e5f\u6709\u70b9\u635f\u4f24\u2014\u2014\u4e0d\u8fc7\u57fa\u672c\u90fd\u662f\u5c0f\u95ee\u9898\u5566\uff0c\u4eba\u5bb6\u81ea\u5df1\u5c31\u80fd\u89e3\u51b3~",
    "\u554a\uff0c\u6211\u9a6c\u4e0a\u53bb\u5e2e\u5979\u4eec\u5904\u7406\uff01|\u5bc7\u5c14\u8299\u4f60\u5982\u679c\u6709\u95ee\u9898\u4e00\u5b9a\u8981\u544a\u8bc9\u6211\uff01",
    "\u6ca1\u95ee\u9898~",
    "\u987a\u4fbf\u53bb\u770b\u770b\u6d77\u4f26\u5a1c\u7684\u60c5\u51b5\uff0c\u7f8e\u73b2\u3002",
    "\u597d\uff01",
    "\uff08\u63a5\u66ff\u79bb\u5f00\u7684\u7f8e\u73b2\uff0c\u95ea\u7535\u8d70\u5230\u603b\u63a7\u53f0\u65c1\uff0c\u7ee7\u7eed\u76d1\u63a7\u827e\u83ab\u53f7\u7684\u8fd0\u884c\u3002\uff09",
    "\u95ea\u7535\uff0c\u4f60\u7684\u60c5\u51b5\uff1f",
    "\u76ee\u524d\u72b6\u6001\u826f\u597d\u3002",
    "\u8fd9\u6b21\u4eba\u5bb6\u6709\u597d\u597d\u4fdd\u62a4\u961f\u957f\u54df~",
    "\u5bc7\u5c14\u8299\uff0c\u8fd9\u6b21\u7684\u4f5c\u6218\uff0c\u8d1f\u8d23\u63f4\u62a4\u7684\u662f\u514b\u7f57\u4e3d\u79d1\u548c\u7eb3\u7f8e\u897f\u4e1d\u3002",
    "\u4f46\u4eba\u5bb6\u4e5f\u6709\u8d77\u5230\u5927\u2014\u2014\u4f5c\u7528\u561b\uff0c\u662f\u4e0d\u662f\uff0c\u961f\u957f\uff1f",
    "\u2026\u2026\u55ef\uff0c\u662f\u7684\u3002",
    "\u4eba\u5bb6\u5e2e\u4e0a\u961f\u957f\u4e86\u2014\u2014\u4eba\u5bb6\u53d8\u5f3a\u4e86\uff01|\u961f\u957f\uff0c\u4eba\u5bb6\u53d8\u5f3a\u4e86\u54df~",
    "\u56de\u5f52\u57fa\u5730\u8f66\u7684\u8fc7\u7a0b\u4e2d\uff0c\u4f60\u4f3c\u4e4e\u4e00\u76f4\u90fd\u5904\u4e8e\u6e38\u79bb\u72b6\u6001\uff0c\u662f\u5728\u60f3\u8fd9\u4e2a\uff1f",
    "\u662f\u7684\u5440\uff0c\u80fd\u591f\u6709\u66f4\u5f3a\u7684\u6218\u6597\u80fd\u529b\uff0c\u4eba\u5bb6\u2014\u2014\u4e00\u76f4\u5f88\u671f\u5f85\u7684\u561b~|\u5514\uff0c\u8981\u611f\u8c22\u7f8e\u73b2\uff0c\u8fd8\u6709\u6307\u6325\u5b98\u7684\u670b\u53cb\u201c\u5e15\u65af\u5361\u201d~",
    "\u5e15\u65af\u5361\uff1f\u4f60\u4eec\u7528\u62df\u6001\u56fe\u8c31\u4e86\uff1f\u5b9e\u6218\u6548\u679c\u600e\u4e48\u6837\uff1f",
    "\u662f\u7684\u3002\u542f\u7528\u62df\u6001\u56fe\u8c31\u540e\uff0c\u6bd4\u5bf9\u53d1\u751f\u5728ODE-01\u536b\u661f\u57ce\u7684\u3001\u7f16\u53f7\u4e3a20740533S\u7684\u884c\u52a8\u62a5\u544a\uff0c\u6b64\u6b21\u884c\u52a8\u4e2d\uff0c\u6211\u65b9\u5404\u4f5c\u6218\u5355\u4f4d\u8868\u73b0\u51fa\u7684\u6218\u529b\u6709\u8f83\u5927\u5e45\u5ea6\u63d0\u5347\u3002\u6570\u503c\u6682\u65f6\u65e0\u6cd5\u9884\u4f30\uff0c\u8be6\u7ec6\u7684\u6218\u529b\u5206\u6790\u53ca\u884c\u52a8\u62a5\u544a\u5c06\u4f1a\u57280030\u540e\u63d0\u4ea4\uff0c\u6863\u6848\u7f16\u53f720740542S\u3002",
    "\u8fd9\u4e48\u5feb\u5c31\u7528\u62df\u6001\u56fe\u8c31\u2014\u2014\u90a3\u4e2a\u770b\u8d77\u6765\u50cf\u516d\u5206\u4eea\u7684\u4eba\u5f62\uff0c\u7528\u5979\u90a3\u4e2a\u9ebb\u70e6\u7684\u975e\u4eba\u5f62\u578b\u6001\u5bf9\u4ed8\u4f60\u4eec\u4e86\uff1f\u4f60\u4eec\u600e\u4e48\u89c1\u5230\u5979\u7684\uff1f",
    "\u55ef\uff0c\u654c\u65b9\u7684\u5916\u89c2\u4e0e\u6b64\u524d\u514b\u7f57\u4e3d\u79d1\u5728\u884c\u52a8\u62a5\u544a\u4e2d\u6240\u63cf\u8ff0\u7684\u578b\u6001\u5b8c\u5168\u76f8\u540c\u3002|\u901a\u8baf\u4e2d\u65ad\u540e\uff0c\u63a8\u6d4b\u7c7b\u4f3c\u516d\u5206\u4eea\u7684\u9ed1\u8272\u4eba\u5f62\u5c06\u5979\u6240\u5728\u5730\u5750\u6807\u53d1\u9001\u7ed9\u6211\u4eec\uff0c\u5e76\u5f15\u5bfc\u6211\u4eec\u5230\u8fbe\u5979\u6240\u5728\u7684\u5750\u6807\u70b9\u3002",
    "\u5904\u4e8e\u5750\u6807\u70b9\u7684\u9ed1\u8272\u4eba\u5f62\u5728\u4e0e\u6211\u4eec\u89c1\u9762\u540e\u5207\u6362\u81f3\u4e0e\u8868\u9762\u4e0d\u540c\u7684\u578b\u6001\u3002|\u5176\u540e\uff0c\u6211\u4eec\u4ee568%\u7684\u529f\u7387\u542f\u52a8\u62df\u6001\u56fe\u8c31\uff0c\u963b\u62e6\u5e76\u7834\u574f\u9ed1\u8272\u4eba\u5f62\uff0c\u6210\u529f\u8131\u79bb\u5f53\u524d\u73af\u5883\uff0c\u8fd4\u56de\u57fa\u5730\u8f66\u3002",
    "\u2026\u2026\u542c\u8d77\u6765\u8fd9\u4e2a\u540d\u5b57\u62d7\u53e3\u7684\u88c5\u7f6e\u8fd8\u633a\u597d\u7528\u3002\u5e15\u65af\u5361\u8fd9\u5bb6\u4f19\uff0c\u603b\u7b97\u662f\u505a\u4e86\u4ef6\u597d\u4e8b\u3002",
    "\u5e15\u65af\u5361\u8fd9\u6837\u7684\u670b\u53cb\u2014\u2014\u5982\u679c\u80fd\u591a\u591a\u51fa\u73b0\u5c31\u597d\u5566\uff0c\u6307\u6325\u5b98~|\u4eba\u5bb6\u2014\u2014\u4e00\u76f4\u90fd\u60f3\u66f4\u5f3a\u4e00\u70b9\u54df~\uff1f",
    "\u8fd9\u53ef\u4e0d\u4e00\u5b9a\u662f\u4e2a\u597d\u4e8b\u2026\u2026\u5e15\u65af\u5361\u62ff\u6765\u7684\u6280\u672f\uff0c\u8c01\u77e5\u9053\u7ed3\u679c\u4f1a\u662f\u4ec0\u4e48\u6837\u3002\u8fd9\u6b21\u8981\u4e0d\u662f\u7f8e\u73b2\uff0c\u62df\u6001\u56fe\u8c31\u4e5f\u4e0d\u80fd\u7528\u3002",
    "\uff08\u7565\u5fae\u653e\u677e\u4e0b\u6765\uff0c\u6211\u8d70\u5230\u65c1\u8fb9\u7684\u4f11\u606f\u533a\uff0c\u5012\u4e86\u676f\u5496\u5561\u3002\uff09",
    "\uff08\u7aef\u7740\u6ee1\u676f\u7684\u5496\u5561\uff0c\u6211\u56de\u5230\u6307\u6325\u53f0\u524d\u7684\u6905\u5b50\u5750\u4e0b\u3002)",
    "\u2026\u2026\u867d\u7136\u73b0\u5728\u786e\u5b9e\u662f\u9700\u8981\u5979\u8d76\u7d27\u51fa\u6765\u89e3\u51b3\u4e00\u4e0b68%\u7684\u6fc0\u53d1\u9650\u5236\uff0c\u4e0d\u8fc7\u95ee\u9898\u7b97\u662f\u6682\u65f6\u89e3\u51b3\u4e86\uff0c\u5728\u7f8e\u73b2\u5b8c\u6210\u7d20\u4f53\u7ef4\u62a4\u4e4b\u524d\uff0c\u4e2d\u573a\u4f11\u606f\u4e00\u4e0b\u3002",
    "\u5bf9\u4e86\uff0c\u901a\u8baf\u65ad\u5f00\u540e\uff0c\u6211\u8fd9\u91cc\u4e5f\u6536\u5230\u4e86\u90a3\u4e2a\u9ed1\u8272\u4eba\u5f62\u7684\u901a\u8baf\u2014\u2014",
    "\u5514\u2014\u2014\u6307\u6325\u5b98\u63d0\u5230\u901a\u8baf\uff0c\u5c31\u6709\u901a\u8baf\u63a5\u5165\u8036~|\u96be\u9053\u662f\u2014\u2014\u9ed1\u8272\u4eba\u5f62\uff1f",
    "\u2026\u2026\u4e0d\uff0c\u5979\u521a\u88ab\u4f60\u4eec\u7834\u574f\u4e86\uff0c\u662f\u8c01\uff1f\u603b\u4e0d\u80fd\u662f\u2014\u2014",
    "\u95ea\u7535\uff0c\u901a\u8baf\u65b9\uff1f",
    "\u52a0\u5bc6\u7ebf\u8def\uff0c\u901a\u8baf\u65b9\u4e0d\u660e\u3002",
    "\u63a5\u901a\uff0c\u51c6\u5907\u56de\u6eaf\u901a\u8baf\u4fe1\u53f7\u3002",
    "\uff08\u6211\u7ad9\u5230\u4e86\u6295\u5f71\u5c4f\u524d\u3002\uff09",
    "\uff08\u901a\u8baf\u63a5\u901a\uff0c\u4e00\u4e2a\u719f\u6089\u7684\u9762\u5b54\u51fa\u73b0\u5728\u6295\u5f71\u5c4f\u4e0a\u3002\uff09",
    "\u2026\u2026\u683c\u7433\uff1f\u600e\u4e48\u662f\u4f60\uff1f\u7b49\u7b49\uff0c\u4f60\u2014\u2014\u683c\u7433\uff0c\u90a3\u4e2a\u2026\u2026\u624b\u3001\u624b\u63e1\u950b\u8292\uff1f",
    "\u70b9\u71c3\u9ece\u660e\uff1f",
    "\uff08\u5728\u6211\u8eab\u540e\uff0c\u5bc7\u5c14\u8299\u6084\u6084\u9000\u5230\u4e86\u843d\u5730\u7a97\u9644\u8fd1\uff0c\u4fa7\u5934\u770b\u7740\u6295\u5f71\u5c4f\u4e0a\u7684\u683c\u7433\u3002\uff09",
    "\u4e0d\u5bf9\uff0c\u4ed6\u4eec\u4e5f\u77e5\u9053\u8fd9\u4e2a\u6697\u53f7\u2026\u2026\u683c\u7433\uff0c\u4f60\u3001\u4f60\u2014\u2014",
    "\u2026\u2026\u662f\u683c\u7433\u5a1c\u3002\u8c01\u77e5\u9053\u6697\u53f7\uff0c\u4f60\u90a3\u91cc\u53d1\u751f\u4ec0\u4e48\u4e86\uff1f",
    "\u5443\uff0c\u51fa\u4e86\u4e9b\u95ee\u9898\u2026\u2026\u603b\u4e4b\uff0c\u683c\u7433\u5a1c\u2026\u2026\u662f\u683c\u7433\u5a1c\u5427\uff1f",
    "\u6ca1\u731c\u9519\u7684\u8bdd\uff0c\u4f60\u4eec\u6536\u5230\u4e86\u5305\u542b\u6697\u53f7\u7684\u3001\u4ee5\u6211\u7684\u540d\u4e49\u53d1\u51fa\u7684\u4f2a\u9020\u4fe1\u606f\uff1f",
    "\u2026\u2026\u55ef\u3002\u901a\u8baf\u65b9\u4e0d\u660e\uff0c\u6d88\u606f\u4e2d\uff0c\u8fd8\u5305\u542b\u4e86\u4e00\u4e2a\u5750\u6807\uff0c\u6211\u4eec\u73b0\u5728\u5c31\u5728\u8fd9\u4e2a\u5750\u6807\u70b9\u3002",
    "\u2026\u2026",
    "\u4f60\u4eec\u73b0\u5728\u5728\u54ea\uff1f",
    "\u628a\u5750\u6807\u53d1\u7ed9\u683c\u7433\u5a1c\uff0c\u95ea\u7535\u3002",
    "\u4e86\u89e3\u3002",
    "\uff08\u64cd\u4f5c\u603b\u63a7\u53f0\uff0c\u95ea\u7535\u5c06\u5750\u6807\u4f4d\u7f6e\u901a\u8fc7\u52a0\u5bc6\u7ebf\u8def\u53d1\u7ed9\u4e86\u683c\u7433\u3002\uff09",
    "\u55ef\uff0c\u770b\u5230\u4e86\u2014\u2014\u8fd9\u4e48\u8fdc\uff1f\u8fd9\u5b8c\u5168\u662f\u57fa\u8f85\u7684\u53cd\u65b9\u5411\u5427\uff1f\u8fd8\u5728\u9ec4\u533a\uff1f",
    "\u662f\u3002\u867d\u7136\u5f88\u53ef\u7591\uff0c\u4f46\u4fe1\u606f\u4e2d\u5305\u542b\u4e86\u4f60\u7ed9\u6211\u7684\u6697\u53f7\u3002",
    "\u2026\u2026\u6697\u53f7\uff0c\u662f\u83b1\u5a1c\u5f53\u9762\u7ed9\u4f60\u7684\u3002",
    "\u5f53\u65f6\u7684\u73af\u5883\u4e0b\uff0c\u5468\u56f4\u6ca1\u6709\u4efb\u4f55\u53ef\u7591\u5bf9\u8c61\u3002|\u83b1\u5a1c\u662f\u600e\u4e48\u77e5\u9053\u6697\u53f7\u7684\uff1f",
    "\u8fdb\u5165ODE-01\u4e4b\u524d\uff0c\u6211\u4eec\u5df2\u7ecf\u7ea6\u5b9a\u8fc7\u4e86\u6697\u53f7\u2026\u2026\u901a\u8fc7\u901a\u8baf\u3002",
    "\u2026\u2026\u4f60\u6240\u5efa\u7acb\u7684\u901a\u8baf\u9891\u9053\uff0c\u8fd9\u4e48\u65e9\u5c31\u88ab\u5b9a\u4f4d\u76d1\u542c\u4e86\uff1f",
    "\u8fd9\u662f\u552f\u4e00\u7684\u7ed3\u8bba\u3002",
    "\u6211\u4eec\u5728\u8ffd\u67e5\u4e4b\u524d\u5e72\u6270\u901a\u8baf\u9891\u9053\u7684\u5bf9\u8c61\u65f6\uff0c\u83b7\u5f97\u4e86\u4e00\u4e9b\u7ebf\u7d22\uff0c\u6709\u4e86\u4e00\u4e9b\u660e\u786e\u7684\u6307\u5411\u3002\u4f46\u4ed6\u4eec\u5bf9\u5965\u5fb7\u5345\u4ee5\u53ca\u5468\u8fb9\u536b\u661f\u57ce\u7684\u4fb5\u8680\u8fdc\u8d85\u6211\u4eec\u4e4b\u524d\u7684\u9884\u6599\u2026\u2026\u73b0\u5728\u770b\u8d77\u6765\uff0c\u65f6\u95f4\u70b9\u4e5f\u4e0e\u6211\u4eec\u9884\u671f\u7684\u4e0d\u540c\u3002",
    "\u4ed6\u4eec\uff1f\u5409\u62c9\u5c14\u96c6\u56e2\uff1f",
    "\u51c6\u786e\u6765\u8bf4\uff0c\u662f\u5305\u62ec\u5409\u62c9\u5c14\u96c6\u56e2\u5728\u5185\u7684\u591a\u4e2a\u5e55\u540e\u52bf\u529b\uff0c\u6211\u4eec\u76ee\u524d\u53ea\u80fd\u521d\u6b65\u786e\u8ba4\u5409\u62c9\u5c14\u96c6\u56e2\u7275\u6d89\u5176\u4e2d\u3002",
    "\u4f60\u4eec\u5728ODE-01\u4e2d\u9047\u5230\u7684\u673a\u68b0\u5355\u4f4d\uff0c\u6240\u5c5e\u6743\u767b\u8bb0\u5728\u7531\u5409\u62c9\u5c14\u96c6\u56e2\u6240\u96c7\u4f63\u7684P.M.C.\u540d\u4e0b\u3002\u4ed6\u4eec\u5728\u5965\u5fb7\u5345\u53ca\u5468\u8fb9\u533a\u57df\u7684\u51c0\u5316\u671f\u7ba1\u8f96\u6743\u5df2\u7ecf\u5230\u671f\uff0c\u7ecf\u7531\u4ed6\u4eec\u6240\u63d0\u4ea4\u7684\u7533\u8bf7\uff0c\u51c0\u5316\u533a\u7684\u7ba1\u8f96\u6743\u5373\u5c06\u8f6c\u53d8\u4e3a\u6b63\u5f0f\u7684\u884c\u653f\u7ba1\u7406\u6743\u3002",
    "\u6536\u5230\u7533\u8bf7\u540e\uff0c\u6309\u7167\u65e2\u5b9a\u6d41\u7a0b\uff0c\u4e2d\u592e\u653f\u5e9c\u6b63\u5728\u5bf9\u4ed6\u4eec\u8fdb\u884c\u4e3a\u671f\u4e09\u4e2a\u6708\u7684\u8003\u5bdf\u2014\u2014\u4f46\u56fd\u5b89\u5c40\u63d0\u4f9b\u7684\u6d88\u606f\u8868\u660e\u4ed6\u4eec\u6301\u6709\u5f3a\u6740\u4f24\u529b\u79c1\u8bbe\u6b66\u88c5\uff0c\u8fd9\u4e9b\u6b66\u88c5\u529b\u91cf\u5e76\u6ca1\u6709\u5728\u975e\u519b\u4e8b\u529b\u91cf\u7ba1\u7406\u5c40\u767b\u8bb0\uff0c\u56e0\u6b64\u6211\u4eec\u624d\u4f1a\u53bbODE-01\u3002",
    "\u55ef\uff0c\u4f60\u4e4b\u524d\u5df2\u7ecf\u89e3\u91ca\u8fc7\u3002\u5f3a\u6740\u4f24\u529b\u79c1\u8bbe\u6b66\u88c5\u2014\u2014\u662f\u8bf4\u516d\u5206\u4eea\uff1f",
    "\u8fd8\u6709\u9ed1\u773c\uff0c\u4e00\u4e2a\u6301\u6709\u4f1e\u5f62\u6b66\u88c5\u3001\u5916\u8868\u9ed1\u8272\u4e3a\u4e3b\u7684\u4eba\u5f62\u3002|\u6211\u4eec\u83b7\u5f97\u4e86\u5979\u79bb\u5f00\u5965\u5fb7\u5345\u7684\u60c5\u62a5\uff0c\u4e0d\u77e5\u9053\u662f\u4e0d\u662f\u4e0e\u4f60\u6b64\u524d\u6536\u5230\u7684\u4f2a\u9020\u4fe1\u606f\u6709\u5173\uff0c\u4f60\u4eec\u9047\u5230\u8fc7\u5417\uff1f",
    "\u4e0d\u53ea\u662f\u9047\u5230\u8fc7\u3002\u6211\u4eec\u5728\u5230\u8fbe\u8fd9\u91cc\u4e4b\u540e\uff0c\u5c31\u63a5\u5230\u4e86\u901a\u8baf\u3002\u6211\u6240\u770b\u5230\u7684\u901a\u8baf\u65b9\u5f62\u8c61\uff0c\u4e0e\u4f60\u521a\u624d\u7684\u63cf\u8ff0\u76f8\u7b26\u3002",
    "\u8fd9\u5c31\u662f\u8bf4\uff0c\u4f2a\u9020\u7684\u4fe1\u606f\uff0c\u662f\u9ed1\u773c\u53d1\u7684\uff1f",
    "\u4e0d\uff0c\u4e0d\u662f\u5979\u3002\u8fd9\u4e5f\u6b63\u662f\u5728\u4f60\u7684\u901a\u8baf\u63a5\u5165\u524d\uff0c\u6211\u6b63\u8981\u548c\u95ea\u7535\u5979\u4eec\u63d0\u5230\u7684\u3002|\u901a\u8baf\u4e2d\uff0c\u5979\u4e00\u76f4\u5728\u5f3a\u8c03\u8ba9\u6211\u201c\u4ea4\u51fa\u6837\u54c1\uff0c\u7ed3\u675f\u4efb\u52a1\u201d\uff0c\u53ef\u6211\u89c9\u5f97\u2026\u2026\u6837\u54c1\u5bf9\u5979\uff0c\u6216\u8005\u8bf4\u73b0\u9636\u6bb5\u7684\u5979\uff0c\u5e76\u4e0d\u50cf\u4e4b\u524d\u4e00\u6837\u91cd\u8981\u3002",
    "\u5728ODE-01\u7684\u906d\u9047\u6218\u4e2d\uff0c\u9ed1\u773c\u51e0\u4e4e\u4e00\u5207\u884c\u52a8\u90fd\u662f\u4ee5\u201c\u6837\u54c1\u201d\u4e3a\u4e2d\u5fc3\u5c55\u5f00\uff0c\u5982\u679c\u4e0d\u662f\u95ea\u7535\u6700\u540e\u2026\u2026\u54b3\uff0c\u53ef\u80fd\u6d77\u4f26\u5a1c\u771f\u5c31\u88ab\u5979\u5e26\u8d70\u4e86\u3002",
    "\uff08\u6211\u54b3\u55fd\u4e86\u4e00\u58f0\uff0c\u63a9\u76d6\u4e86\u201c\u7559\u4e0b\u6765\u62e6\u4f4f\u5979\u201d\u51e0\u4e2a\u5b57\u3002\uff09",
    "\uff08\u4e00\u65c1\u7684\u95ea\u7535\u4f1a\u610f\u5730\u5782\u4e0b\u773c\u775b\uff0c\u7b11\u4e86\u4e00\u4e0b\uff0c\u4ece\u603b\u63a7\u53f0\u524d\u8d70\u5230\u4e86\u6211\u8eab\u540e\u3002\uff09",
    "\u5bf9\u6218\u524d\uff0c\u5728\u6211\u4eec\u4e0e\u5979\u7684\u5bf9\u8bdd\u4e2d\uff0c\u4e5f\u51fa\u73b0\u4e86\u4e00\u4e9b\u4fe1\u606f\u3002",
    "\u7b49\u4e00\u4e0b\uff0c\u5bf9\u6218\uff0c\u95ea\u7535\uff0c\u4f60\u4eec\u548c\u9ed1\u773c\u5bf9\u4e0a\u4e86\uff1f",
    "\u5df2\u786e\u8ba4\u5c06\u5176\u51fb\u6bc1\u3002",
    "\u51fb\u6bc1\uff1f\u4f60\u4eec\u7528\u4e86\u201c\u62df\u6001\u56fe\u8c31\u201d\uff1f",
    "\u662f\u7684\u3002\u6211\u4eec\u4ee568%\u7684\u6fc0\u53d1\u72b6\u6001\u542f\u52a8\u4e86\u62df\u6001\u56fe\u8c31\uff0c\u6210\u529f\u7834\u574f\u4e0e\u4f60\u63cf\u8ff0\u76f8\u7b26\u7684\u9ed1\u8272\u4eba\u5f62\u3002",
    "68%\uff1f",
    "\u7f8e\u73b2\u5728\u89e3\u6790\u5e15\u65af\u5361\u7559\u4e0b\u7684\u4fe1\u606f\u65f6\u51fa\u73b0\u4e86\u610f\u5916\uff0c\u4f9d\u636e\u73b0\u6709\u4fe1\u606f\uff0c\u4fbf\u643a\u5b58\u50a8\u4f53\u4e2d\u7684\u574d\u584c\u6619\u82b1\u80fd\u91cf\u53ea\u80fd\u6fc0\u53d1\u523068%\u3002",
    "\u8fd8\u4e0d\u786e\u5b9a\u662f\u56e0\u4e3a\u5e15\u65af\u5361\u63d0\u4f9b\u7684\u6570\u636e\u51fa\u73b0\u95ee\u9898\uff0c\u8fd8\u662f\u56e0\u4e3a\u8c03\u8bd5\u88c5\u7f6e\u65f6\u4f7f\u7528\u4e86\u4e00\u4e9b\u66ff\u4ee3\u96f6\u4ef6\uff0c\u4f46\u96f6\u4ef6\u73b0\u5728\u6ca1\u529e\u6cd5\u89e3\u51b3\uff0c\u4f60\u80fd\u8054\u7edc\u5230\u5e15\u65af\u5361\u5417\uff1f",
    "\u2026\u2026\u5e15\u65af\u5361\u6700\u8fd1\u5728\u7814\u7a76\u65b0\u6280\u672f\uff0c\u65f6\u95f4\u8fd8\u4e0d\u786e\u5b9a\uff0c\u77ed\u671f\u5185\u6211\u4e5f\u8054\u7cfb\u4e0d\u4e0a\u5979\u3002\u5148\u5c1d\u8bd5\u66f4\u6362\u96f6\u4ef6\uff1f",
    "\u6ca1\u94b1\u3002\u7ed9\u6211\u63d0\u4f9b\u8865\u7ed9\u7684\u6697\u5546\u90a3\u91cc\u6ca1\u6709\u76f8\u5173\u96f6\u4ef6\uff0c\u8bf7\u5979\u8c03\u8d27\u53ef\u662f\u9700\u8981\u4e00\u5927\u7b14\u8d44\u91d1\u3002",
    "\u2026\u2026\u6211\u4f1a\u8054\u7cfb\u5176\u4ed6\u6697\u5546\u3002",
    "\u90a3\u5c31\u62dc\u6258\u4e86\uff0c\u683c\u7433\u2026\u2026\u554a\u2026\u2026\u90a3\u4e2a\u2026\u2026",
    "\u55ef\uff0c\u4f60\u4eec\u7f3a\u5c11\u4ec0\u4e48\u96f6\u4ef6\uff1f",
    "\u95ea\u7535\uff0c\u8054\u7cfb\u7f8e\u73b2\uff0c\u5c06\u9700\u6c42\u7684\u6e05\u5355\u5217\u8868\u53d1\u7ed9\u683c\u7433\u3002",
    "\u4e86\u89e3\u3002",
    "\u683c\u7433\uff0c\u6ce8\u610f\u67e5\u6536\u3002",
    "\u4f9d\u636e\u4f60\u4eec\u9700\u8981\u96f6\u4ef6\u79cd\u7c7b\u7684\u4e0d\u540c\uff0c\u80fd\u591f\u63d0\u4f9b\u7269\u8d44\u7684\u4eba\u5458\u9700\u8981\u4e00\u70b9\u65f6\u95f4\u786e\u8ba4\u3002\u786e\u8ba4\u540e\u4f1a\u901a\u77e5\u4f60\u3002|\u7ee7\u7eed\u4e4b\u524d\u7684\u8bdd\u9898\uff0c\u95ea\u7535\uff0c\u5728\u548c\u9ed1\u773c\u7684\u5bf9\u8bdd\u4e2d\uff0c\u4f60\u53d1\u73b0\u4ec0\u4e48\u4e86\uff1f",
    "\u6211\u7684\u5224\u65ad\u4e0e\u6307\u6325\u5b98\u76f8\u540c\u3002\u5728\u4e0e\u6211\u4eec\u5bf9\u8bdd\u65f6\uff0c\u5979\u5168\u7a0b\u6ca1\u6709\u63d0\u53ca\u201c\u6837\u54c1\u201d\uff0c\u8fd9\u786e\u5b9e\u4e0eODE-01\u65f6\u671f\u7684\u5979\u5b8c\u5168\u4e0d\u540c\u3002",
    "\u5982\u679c\u5c06\u66fe\u7ecf\u51fa\u73b0\u8fc7\u7684\u516d\u5206\u4eea\u89c6\u4e3a\u5979\u5728ODE-01\u65f6\u671f\u7684\u5408\u4f5c\u5bf9\u8c61\uff0c\u9020\u6210\u8fd9\u4e00\u6539\u53d8\u7684\u539f\u56e0\uff0c\u6211\u8ba4\u4e3a\u662f\u5979\u7684\u5408\u4f5c\u5bf9\u8c61\u4ea7\u751f\u4e86\u53d8\u5316\u3002",
    "\u55ef\uff0c\u4ece\u5979\u7684\u8868\u8ff0\u4e2d\uff0c\u6211\u4e5f\u83b7\u5f97\u4e86\u7c7b\u4f3c\u7684\u4fe1\u606f\u2014\u2014\u9ed1\u773c\u66f4\u6362\u4e86\u5408\u4f5c\u5bf9\u8c61\u3002|\u4f2a\u9020\u4fe1\u606f\u7684\u53d1\u51fa\u65b9\uff0c\u6211\u8ba4\u4e3a\u5c31\u662f\u5979\u66f4\u6362\u540e\u7684\u5408\u4f5c\u5bf9\u8c61\uff0c\u6216\u8005\u8bf4\u5408\u4f5c\u96c6\u56e2\u3002",
    "\u2026\u2026\u9ed1\u773c\u5df2\u7ecf\u8131\u79bb\u4e86\u5409\u62c9\u5c14\u96c6\u56e2\uff0c\u66f4\u6362\u540e\u7684\u6240\u5c5e\u4e0d\u660e\uff0c\u6211\u4eec\u8fd8\u5728\u8ffd\u67e5\u3002",
    "\u90a3\u4ed6\u4eec\u662f\u5229\u7528\u4e86\u4f60\u53bb\u8ffd\u67e5\u7684\u8fd9\u4e09\u5929\uff0c\u62e6\u622a\u4e86\u6211\u7684\u901a\u8baf\uff0c\u5e76\u4e14\u7ed9\u6211\u53d1\u4e86\u4f2a\u9020\u7684\u4fe1\u606f\uff1f",
    "\u2026\u2026",
    "\u2026\u2026\u8c03\u67e5\u8fdb\u7a0b\u7275\u6d89\u5230\u4e86\u591a\u4e2a\u4e0d\u540c\u673a\u6784\uff0c\u9700\u8981\u96c6\u4e2d\u534f\u8c03\u3002\u4e0d\u8fc7\u597d\u5728\u6211\u4eec\u53d1\u73b0\u4e86\u5176\u4ed6\u8ffd\u67e5\u8fd9\u4e00\u4e8b\u4ef6\u7684\u4eba\u7269\uff0c\u5979\u5728\u786e\u5b9a\u8bc1\u636e\u7684\u8fc7\u7a0b\u4e2d\u7ed9\u6211\u4eec\u63d0\u4f9b\u4e86\u4e0d\u5c11\u5e2e\u52a9\uff0c\u6211\u4eec\u624d\u80fd\u5728\u4e09\u5929\u4e4b\u5185\u638c\u63e1\u5409\u62c9\u5c14\u96c6\u56e2\u6301\u6709\u975e\u6cd5\u6b66\u88c5\u529b\u91cf\u7684\u5207\u5b9e\u8bc1\u636e\u3002",
    "\u76ee\u524d\uff0c\u6211\u4eec\u6240\u638c\u63e1\u7684\u8bc1\u636e\u5df2\u7ecf\u63d0\u4ea4\u7ed9\u4e2d\u592e\u653f\u5e9c\uff0c\u4e2d\u592e\u653f\u5e9c\u5c06\u4ee5\u6b64\u62d2\u7edd\u5409\u62c9\u5c14\u96c6\u56e2\u5bf9\u5965\u5fb7\u5345\u53ca\u5468\u8fb9\u5730\u533a\u7684\u884c\u653f\u7ba1\u7406\u6743\u7533\u8bf7\u3002",
    "\u2026\u2026\u6ca1\u6709\u53ca\u65f6\u901a\u77e5\u4f60\uff0c\u8fd8\u8ba9\u4ed6\u4eec\u5229\u7528\u4e86\u8fd9\u4e2a\u65f6\u95f4\u6bb5\uff0c\u662f\u6211\u7684\u95ee\u9898\u3002\u62b1\u6b49\uff0c\u6307\u6325\u5b98\u3002",
    "\u554a\u2026\u2026\u5443\u2026\u2026\u5e2e\u4f60\u4eec\u7684\u662f\u8c01\uff0c\u80fd\u786e\u5b9a\u5417\uff1f",
    "\u76ee\u524d\u8fd8\u5728\u548c\u5176\u4ed6\u90e8\u95e8\u7684\u540c\u4e8b\u4e00\u8d77\u786e\u8ba4\u5bf9\u65b9\u7684\u8eab\u4efd\u53ca\u5177\u4f53\u76ee\u7684\uff0c\u9ed1\u773c\u65b0\u7684\u6240\u5c5e\u4e5f\u5728\u8c03\u67e5\u8fdb\u7a0b\u4e2d\u3002\u83b1\u5a1c\u4f1a\u6301\u7eed\u5173\u6ce8\u4f60\u8fd9\u8fb9\u7684\u901a\u8baf\u60c5\u51b5\uff0c\u4e0d\u4f1a\u518d\u51fa\u73b0\u901a\u8baf\u88ab\u62e6\u622a\u7684\u60c5\u51b5\u4e86\u3002",
    "\u201c\u975e\u519b\u4e8b\u529b\u91cf\u7ba1\u7406\u5c40\u201d\u2026\u2026\u8ffd\u67e5\u8fd9\u79cd\u4e8b\u60c5\uff0c\u4e5f\u5728\u4f60\u4eec\u7684\u7ba1\u7406\u8303\u56f4\u5185\uff1f",
    "\u53ea\u662f\u6d89\u53ca\u975e\u6cd5\u6b66\u88c5\u529b\u91cf\u7684\u90e8\u5206\u3002",
    "\u2026\u2026\u4ece\u628a\u8ba2\u5355\u7ed9\u6211\u5f00\u59cb\uff0c\u5230\u73b0\u5728\u4e3a\u6b62\uff0c\u8fd9\u4e00\u7cfb\u5217\u4e8b\u60c5\u6d89\u53ca\u5230\u7684\u53ef\u8fdc\u8fdc\u4e0d\u53ea\u662f\u201c\u975e\u6cd5\u6b66\u88c5\u529b\u91cf\u201d\u3002|\u683c\u7433\uff0c\u4f60\u73b0\u5728\u5230\u5e95\u5728\u5e72\u4ec0\u4e48\uff1f",
    "\uff08\u683c\u7433\u8c03\u6574\u4e86\u4e00\u4e0b\u901a\u8baf\u88c5\u7f6e\u7684\u4f4d\u7f6e\uff0c\u53ef\u80fd\u662f\u89e6\u6478\u5230\u4e86\u6536\u97f3\u6a21\u5757\uff0c\u6211\u542c\u5230\u4e86\u4e00\u4e9b\u6742\u97f3\u3002\uff09",
    "\u201c\u975e\u519b\u4e8b\u529b\u91cf\u7ba1\u7406\u201d\u2026\u2026\u6b66\u88c5\u529b\u91cf\uff0c\u4e0d\u7ba1\u662f\u4e0d\u662f\u519b\u4e8b\u529b\u91cf\uff0c\u90fd\u4f1a\u6216\u4e3b\u52a8\u3001\u6216\u88ab\u52a8\u5730\u88ab\u7275\u626f\u8fdb\u66f4\u591a\u7684\u4e8b\u60c5\u3002\u8fd9\u4e00\u70b9\uff0c\u4f60\u5e94\u8be5\u5f88\u6e05\u695a\uff0c\u6307\u6325\u5b98\u3002",
    "\u73b0\u5728\uff0c\u6211\u53ea\u80fd\u544a\u8bc9\u4f60\uff0c\u6211\u7684\u8eab\u4efd\u662f\u201c\u975e\u519b\u4e8b\u529b\u91cf\u7ba1\u7406\u5c40\u7684\u9ad8\u7ea7\u53c2\u4e8b\u201d\u3002",
    "\u2026\u2026\u4e39\u5fb7\u83b1\u5462\uff0c\u5979\u80fd\u89e3\u51b3ODE-01\u7684\u57ce\u5e02\u5c01\u9501\uff0c\u96be\u9053\u5979\u4e5f\u6709\u5176\u4ed6\u8eab\u4efd\uff1f\u603b\u4e0d\u80fd\u662f\u9ed1\u4e86\u7f57\u8054\u7684\u57ce\u5e02\u7ba1\u7406\u7cfb\u7edf\u5427\uff1f",
    "\u4e0d\uff0c\u5979\u548c\u6211\u4e0d\u4e00\u6837\uff0c\u5979\u672c\u4eba\u4e0d\u5728\u7f57\u8054\u4f53\u7cfb\u4e2d\u627f\u62c5\u4efb\u4f55\u804c\u8d23\u3002\u73b0\u5728\u7684\u4e39\u5fb7\u83b1\uff0c\u786e\u786e\u5b9e\u5b9e\u662f\u5728\u5ea6\u5047\u3002|\u5979\u80fd\u89e3\u9664\u5c01\u9501\uff0c\u867d\u7136\u7b97\u4e0d\u4e0a\u662f\u4f7f\u7528\u4e86\u5e38\u89c4\u624b\u6bb5\u2026\u2026\u4f46\u4e5f\u7b97\u662f\u88ab\u7f57\u8054\u5141\u8bb8\uff0c\u4e0d\u7136\u6211\u4e5f\u4f1a\u9047\u5230\u9ebb\u70e6\u3002",
    "\u5443\uff0c\u4e00\u79cd\u88ab\u7f57\u8054\u5141\u8bb8\u7684\u975e\u5e38\u89c4\u624b\u6bb5\uff1f",
    "\u5177\u4f53\u7684\u65b9\u5f0f\uff0c\u73b0\u5728\u4e0d\u80fd\u544a\u8bc9\u4f60\u3002\u8fd9\u6d89\u53ca\u5230\u4e00\u4e9b\u673a\u5bc6\u4fe1\u606f\uff0c\u4f60\u5df2\u7ecf\u79bb\u5f00\u2014\u2014",
    "\u4f60\u73b0\u5728\u5e94\u8be5\u6682\u65f6\u7528\u4e0d\u5230\u8fd9\u4e9b\u4fe1\u606f\u3002",
    "\uff08\u683c\u7433\u72b9\u8c6b\u4e86\u4e00\u4e0b\uff0c\u79fb\u5f00\u4e86\u76ee\u5149\u3002\uff09",
    "\u2026\u2026\u597d\u5427\uff0c\u90a3\u5c31\u7b49\u7528\u5f97\u4e0a\u7684\u65f6\u5019\uff0c\u518d\u8bf7\u4f60\u544a\u8bc9\u6211\u4e86\uff0c\u683c\u7433\u3002",
    "\u55ef\uff0c\u7b49\u4ee5\u540e\u3002|\u6d77\u4f26\u5a1c\u600e\u4e48\u6837\uff0c\u5979\u73b0\u5728\u5e94\u8be5\u5728\u4f60\u7684\u57fa\u5730\u8f66\u4e0a\uff1f",
    "\u8eab\u4f53\u72b6\u51b5\u6076\u5316\uff0c\u56de\u6765\u4e4b\u540e\u4e00\u76f4\u5904\u4e8e\u7761\u7720\u72b6\u6001\u3002|\u7ecf\u8fc7\u5bc7\u5c14\u8299\u7684\u6cbb\u7597\u540e\uff0c\u5df2\u7ecf\u5f00\u59cb\u6062\u590d\uff0c\u5e94\u8be5\u5f88\u5feb\u5c31\u80fd\u9192\u8fc7\u6765\u3002",
    "\u8eab\u4f53\u72b6\u51b5\u6076\u5316\uff1f\u56e0\u4e3a\u574d\u584c\u8f90\u5c04\uff1f",
    "\u56e0\u4e3a\u7f3a\u5c11\u574d\u584c\u8f90\u5c04\u3002",
    "\u2026\u2026\u68c0\u6d4b\u6570\u636e\u4e0e\u7ed3\u8bba\u53ef\u786e\u8ba4\u5417\uff1f",
    "\u9535\u9535~\u6253\u65ad\u4e00\u4e0b\uff0c\u5230\u4eba\u5bb6\u56de\u7b54\u7684\u65f6\u95f4\u4e86\u54df~",
    "\uff08\u7ad9\u5728\u843d\u5730\u7a97\u524d\uff0c\u6b6a\u7740\u5934\u5b89\u9759\u542c\u6211\u4eec\u5bf9\u8bdd\u7684\u5bc7\u5c14\u8299\u4e3e\u8d77\u4e86\u624b\u3002\uff09",
    "\u5df2\u7ecf\u68c0\u6d4b\u8fc7\u7684\u9879\u76ee\uff0c\u6570\u636e\u90fd\u662f\u53ef\u786e\u8ba4\u7684\uff0c\u6bd5\u7adf\u2014\u2014\u4eba\u5bb6\u662f\u533b\u7597\u4eba\u5f62\u561b~|\u4e0d\u8fc7\u7ed3\u8bba\u53ea\u662f\u4eba\u5bb6\u7684\u63a8\u6d4b\u54df\uff0c\u8fd8\u6709\u597d\u591a\u68c0\u6d4b\u505a\u4e0d\u4e86\uff0c\u827e\u83ab\u53f7\u4e0a\u7684\u8bbe\u5907\u4e0d\u662f\u5f88\u9f50\u5168\u561b~",
    "\u4e2d\u592e\u9646\u519b\u5373\u65f6\u54cd\u5e94\u6551\u63f4\u7f16\u5236\uff0c\u7b2c\u5341\u4e00\u533b\u62a4\u9632\u5316\u8425\u7b2c\u4e09\u533b\u7597\u961f\u6240\u5c5e\u533b\u7597\u4eba\u5f62\uff0cTS-K-2076\uff0c\u5bc7\u5c14\u8299\uff1f",
    "\u5bf9\u7684\u54df\u2014\u2014\u662f\u4eba\u5bb6~",
    "\u5514\uff0c\u8fd9\u4e48\u5feb\u5c31\u731c\u51fa\u6765\u2014\u2014\u679c\u7136\u662f\u6307\u6325\u5b98\u7edd\u5bf9\u53ef\u4fe1\u7684\u201c\u8001\u719f\u4eba\u201d~",
    "\uff08\u6295\u5f71\u5c4f\u4e0a\u7684\u683c\u7433\u4f4e\u4e0b\u5934\uff0c\u770b\u8d77\u6765\u662f\u5728\u786e\u8ba4\u901a\u8baf\u7ec8\u7aef\u7684\u72b6\u6001\u3002\uff09",
    "\u54b3\uff0c\u5bc7\u5c14\u8299\uff0c\u5982\u679c\u4f60\u7684\u63a8\u6d4b\u6210\u7acb\uff0c\u90a3\u4e48\u6d77\u4f26\u5a1c\u5c31\u4e0d\u662f\u2014\u2014",
    "\u4e0d\u662f\u771f\u7684\u201c\u5b8c\u5168\u514d\u75ab\u4f53\u201d\u3002",
    "\u6d77\u4f26\u5a1c\u5bf9\u574d\u584c\u8f90\u5c04\u7684\u514d\u75ab\u53ef\u80fd\u8868\u73b0\u5728\u4e00\u5b9a\u7684\u574d\u584c\u8f90\u5c04\u6d53\u5ea6\u8303\u56f4\u5185\uff0c\u76ee\u524d\u53ea\u77e5\u9053\u5979\u80fd\u591f\u627f\u53d7\u7b49\u540c\u4e8e\u9ec4\u533a\u574d\u584c\u98ce\u66b4\u7a0b\u5ea6\u7684\u574d\u584c\u8f90\u5c04\u6d53\u5ea6\uff1b\u4f4e\u4e8e\u7b49\u540c\u4e8e\u7eff\u533a\u901a\u5e38\u7a0b\u5ea6\u7684\u574d\u584c\u8f90\u5c04\u6d53\u5ea6\u73af\u5883\uff0c\u4f1a\u4f7f\u5979\u7684\u8eab\u4f53\u72b6\u51b5\u51fa\u73b0\u6076\u5316\u3002",
    "\u2026\u2026\u6d77\u4f26\u5a1c\u4e4b\u524d\u66fe\u7ecf\u88ab\u4ed6\u4eec\u77ed\u6682\u5e26\u8d70\u3002\u5982\u679c\u6d77\u4f26\u5a1c\u662f\u201c\u5b8c\u5168\u514d\u75ab\u4f53\u201d\uff0c\u628a\u5979\u5e26\u8d70\uff0c\u5728\u77ed\u65f6\u95f4\u5185\u83b7\u53d6\u5979\u7684\u751f\u7269\u4fe1\u606f\u3001\u63d0\u53d6\u8db3\u591f\u7684\u5b9e\u9a8c\u6837\u672c\uff0c\u5bf9\u4e8e\u4ed6\u4eec\u6765\u8bf4\uff0c\u6216\u8bb8\u8fd8\u662f\u8db3\u591f\u7684\u2026\u2026",
    "\u4f46\u5982\u679c\u6d77\u4f26\u5a1c\u5e76\u4e0d\u662f\u201c\u5b8c\u5168\u514d\u75ab\u4f53\u201d\uff0c\u4ed6\u4eec\u53ef\u80fd\u66f4\u9700\u8981\u83b7\u5f97\u5bf9\u4e8e\u4ed6\u4eec\u800c\u8a00\uff0c\u88ab\u4f5c\u4e3a\u5b9e\u9a8c\u5bf9\u8c61\u7684\u201c\u6d77\u4f26\u5a1c\u201d\u3002",
    "\u81f3\u4e8e\u4ed6\u4eec\u7684\u76ee\u7684\u2014\u2014\u6216\u8005\u8bf4\u76ee\u7684\u4e4b\u4e00\uff0c\u662f\u5e0c\u671b\u4ee5\u6d77\u4f26\u5a1c\u4e3a\u201c\u6837\u54c1\u201d\uff0c\u6d4b\u8bd5\u5e76\u7814\u7a76\u6d77\u4f26\u5a1c\u7684\u7279\u6b8a\u6027\uff1f\u6bd4\u5982\u751f\u7269\u4f53\u5728\u5f3a\u574d\u584c\u8f90\u5c04\u73af\u5883\u4e0b\u7684\u9002\u5e94\u65b9\u5f0f\uff1f",
    "\u2026\u2026\u4f5c\u4e3a\u6211\u4eec\u7684\u4e3b\u8981\u8ffd\u67e5\u76ee\u6807\uff0c\u5409\u62c9\u5c14\u96c6\u56e2\u7684\u7ec4\u7ec7\u67b6\u6784\u4e2d\uff0c\u6ca1\u6709\u6ce8\u518c\u4e8e\u751f\u7269\u79d1\u6280\u76f8\u5173\u5206\u91ce\u7684\u5b50\u516c\u53f8\u3002",
    "\uff08\u95ea\u7535\u8c03\u51fa\u8d44\u6599\u5e93\uff0c\u67e5\u9605\u4e86\u4e00\u4e0b\u4fe1\u606f\u3002\uff09",
    "\u6b64\u5916\uff0c\u5728\u5409\u62c9\u5c14\u96c6\u56e2\u516c\u5f00\u7684\u5546\u4e1a\u4fe1\u606f\u4e2d\uff0c\u6ca1\u6709\u53d1\u73b0\u4efb\u4f55\u6d89\u53ca\u751f\u7269\u79d1\u6280\u7684\u5408\u4f5c\u96c6\u56e2\u4ee5\u53ca\u5408\u4f5c\u9879\u76ee\u3002",
    "\u4ee5\u4ed6\u4eec\u7684\u884c\u4e3a\u5224\u65ad\uff0c\u73b0\u5728\uff0c\u4ed6\u4eec\u5df2\u7ecf\u4e0d\u9700\u8981\u4e00\u4e2a\u4f5c\u4e3a\u5b9e\u9a8c\u5bf9\u8c61\u7684\u201c\u6d77\u4f26\u5a1c\u201d\u3002\u683c\u7433\uff0c\u4ed6\u4eec\u7684\u5173\u6ce8\u70b9\u5df2\u7ecf\u8f6c\u79fb\u4e86\u3002",
    "\u2026\u2026\u6211\u7a81\u7136\u60f3\u5230\u4e00\u4e2a\u4e8b\u4ef6\uff0c\u60c5\u51b5\u6709\u4e9b\u76f8\u4f3c\u3002",
    "\u5177\u4f53\u662f\uff1f",
    "\u7f57\u514b\u8428\u7279\u4e3b\u4e49\u5408\u4f17\u56fd\u8054\u76df\u653f\u5e9c\u673a\u8981\u6863\u6848\u5e93\u4e2d\uff0c\u6709\u4e00\u4efd\u7f16\u53f7\u4e3aZMQ_038\u7684\u6863\u6848\uff0c\u8bb0\u5f55\u4e86\u4e00\u8d77\u4e0e\u51c0\u5316\u5de5\u7a0b\u76f8\u5173\u7684\u4e8b\u4ef6\u3002",
    "\u5728\u94fa\u8bbe\u51c0\u5316\u533a\u7684\u8fc7\u7a0b\u4e2d\uff0c\u8d1f\u8d23\u5f53\u524d\u533a\u57df\u7684\u8f68\u9053\u519b\u5728\u6e05\u9664\u4e00\u5904\u6c61\u6c34\u5904\u7406\u6c60\u65f6\uff0c\u53d1\u73b0\u6c60\u4e2d\u4ea4\u53e0\u5806\u79ef\u4e86\u5927\u91cf\u9ab8\u9aa8\u3002",
    "\u7ecf\u8fc7\u67e5\u8bc1\uff0c\u6b64\u5904\u66fe\u7531\u4e00\u5bb6\u751f\u7269\u5236\u836f\u516c\u53f8\u63a7\u5236\uff0c\u8be5\u516c\u53f8\u5728\u7814\u53d1\u836f\u54c1\u65f6\uff0c\u4e3a\u4e86\u4fdd\u8bc1\u7814\u53d1\u8fdb\u5ea6\u53ca\u836f\u54c1\u6548\u679c\uff0c\u8fdd\u6cd5\u4f7f\u7528\u4e86\u514b\u9686\u4f53\u2014\u2014\u800c\u8fd9\u4e9b\u514b\u9686\u4f53\uff0c\u90fd\u6709\u76f8\u4f3c\u7684\u8eab\u4f53\u6761\u4ef6\u3002",
    "\u2026\u2026E_\u6d77\u4f26\u5a1c_O43\u3002",
    "\u8fd9\u662f\uff1f",
    "\u662f\u201c\u6d77\u4f26\u5a1c\u201d\u7684\u540d\u5b57\u6765\u6e90\u54df\u2014\u2014\u662f\u7f8e\u73b2\u68c0\u67e5\u6d77\u4f26\u5a1c\u5e26\u7684\u90a3\u4e9b\u7403\u4f53\u5185\u90e8\u7a0b\u5f0f\u65f6\u770b\u5230\u7684~|\u55ef\u2014\u2014\u8fd9\u4e48\u8bf4\uff0c\u662f\u4e0d\u662f\u201c\u6d77\u4f26\u5a1c\u201d\u4e5f\u6709\u597d\u591a\u4e2a\u5440~\uff1f",
    "\u2026\u2026\u6211\u4eec\u76ee\u524d\u5e76\u6ca1\u6709\u53d1\u73b0\u7b2c\u4e8c\u4e2a \u201c\u6d77\u4f26\u5a1c\u201d\u3002",
    "\u53ea\u662f\u201c\u6ca1\u6709\u53d1\u73b0\u201d\u3002",
    "\u2026\u2026\u55ef\u3002",
    "\u8fd9\u53ef\u80fd\u662f\u4e2a\u7a81\u7834\u53e3\uff0c\u6211\u4eec\u5728\u8ffd\u67e5\u65f6\u4f1a\u6ce8\u610f\u3002|\u6307\u6325\u5b98\uff0c\u8fd9\u4e2a\u4e8b\u4ef6\u6bd4\u6211\u4eec\u6b64\u524d\u9884\u60f3\u7684\u66f4\u4e3a\u590d\u6742\uff0c\u9700\u8981\u4f60\u786e\u4fdd\u6d77\u4f26\u5a1c\u7684\u5b89\u5168\u3002",
    "\u5b89\u5168\u6211\u4f1a\u4fdd\u8bc1\uff0c\u4f46\u6211\u8ba4\u4e3a\uff0c\u5728\u201c\u6d77\u4f26\u5a1c\u201d\u53ef\u80fd\u5b58\u5728\u590d\u6570\u4e2a\u7684\u524d\u63d0\u4e0b\uff0c\u6211\u4eec\u7684\u201c\u6d77\u4f26\u5a1c\u201d\u53ef\u80fd\u5df2\u7ecf\u4e0d\u5177\u6709\u8db3\u591f\u7684\u91cd\u8981\u6027\u3002",
    "\u4ed6\u4eec\u5df2\u7ecf\u83b7\u5f97\u4e86\u5176\u4ed6\u7684\u201c\u6d77\u4f26\u5a1c\u201d\uff0c\u800c\u6211\u4eec\u7684\u201c\u6d77\u4f26\u5a1c\u201d\uff0c\u539f\u672c\u53ea\u662f\u4ed6\u4eec\u6240\u8ffd\u8e2a\u7684\u76ee\u6807\u4e4b\u4e00\u3002\u9ed1\u773c\u7684\u6001\u5ea6\u53d8\u5316\uff0c\u5c31\u53ef\u4ee5\u4f50\u8bc1\u8fd9\u4e00\u70b9\u3002",
    "\u4e0d\uff0c\u8fd8\u4e0d\u80fd\u786e\u5b9a\u3002",
    "\u4e0e\u6d77\u4f26\u5a1c\u76f8\u5173\u7684\u4e8b\u4ef6\uff0c\u6211\u4eec\u638c\u63e1\u7684\u4fe1\u606f\u5f88\u53ef\u80fd\u53ea\u662f\u6574\u4e2a\u4e8b\u4ef6\u5f88\u5c0f\u7684\u4e00\u90e8\u5206\u3002\u5728\u6574\u4e2a\u4e8b\u4ef6\u4e2d\uff0c\u5979\u662f\u6211\u4eec\u552f\u4e00\u80fd\u591f\u88ab\u786e\u5b9a\u4e3a\u5177\u6709\u660e\u786e\u4ef7\u503c\u7684\u76ee\u6807\u7269\u3002",
