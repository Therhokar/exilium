    "\u54e6\u2014\u2014\u54e6\uff0c\u4f60\u7684\u57fa\u5730\u8f66\uff0c\u662f\u8fd9\u4e48\u5927\u4e00\u4e2a\u5bb6\u4f19\uff1f\u7ef4\u62a4\u8d77\u6765\u8981\u82b1\u4e0d\u5c11\u94b1\u5427\uff1f",
    "\u8fd8\u597d\u3002",
    "\u6211\u8981\u662f\u4f60\uff0c\u6709\u8fd9\u4e48\u4e2a\u5927\u5bb6\u4f19\uff0c\u65e9\u5c31\u4e0d\u505a\u8d4f\u91d1\u730e\u4eba\u4e86\u3002|\u5356\u6389\u6362\u4e00\u5927\u7b14\u94b1\uff0c\u627e\u4e2a\u7eff\u533a\u57ce\u5e02\uff0c\u80fd\u7528\u4e00\u8f88\u5b50\u3002",
    "\u53bb\u7eff\u533a\u6bd5\u7adf\u8981\u529e\u4e0d\u5c11\u624b\u7eed\uff0c\u4e5f\u4e0d\u662f\u94b1\u80fd\u5b8c\u5168\u89e3\u51b3\u7684\u3002",
    "\u54c8\uff0c\u8bf4\u5f97\u4e5f\u662f\u3002\u6765\u6765\u8fd9\u8fb9\uff0c\u6211\u5e26\u4f60\u4eec\u53bb\u4ed3\u5e93\u3002|\u53c2\u4e8b\u5973\u58eb\u8981\u7ed9\u4f60\u7684\u7269\u8d44\u4e0d\u5c11\uff0c\u8fd0\u9001\u7684\u8bdd\uff0c\u4f60\u7684\u4eba\u5f62\u2014\u2014\u554a\u3002",
    "\uff08\u8428\u73c8\u770b\u4e86\u773c\u8ddf\u5728\u6211\u540e\u9762\u7684\u95ea\u7535\u5c0f\u961f\uff0c\u773c\u5149\u5728\u5bc7\u5c14\u8299\u5934\u53d1\u4e0a\u505c\u7559\u4e86\u4e00\u77ac\uff0c\u5f88\u5feb\u79fb\u5f00\u3002\uff09",
    "\u6709\u56db\u4e2a\uff0c\u591f\u4e86\u3002",
    "\u55ef\uff0c\u8f9b\u82e6\u4e86\u3002",
    "\u4f60\u7684\u8fd9\u8f86\u8f66\u8fd8\u771f\u662f\u4e2a\u5927\u5bb6\u4f19\uff0c\u5982\u679c\u518d\u5927\u4e00\u70b9\uff0c\u53ef\u80fd\u5c31\u7528\u4e0d\u4e86\u5347\u964d\u68af\u4e86\u3002\u7ef4\u62a4\u8d77\u6765\u5f88\u9ebb\u70e6\u5427\uff1f",
    "\u786e\u5b9e\u9700\u8981\u82b1\u8d39\u4e0d\u5c11\u65f6\u95f4\uff0c\u4e0d\u8fc7\u6211\u6709\u503c\u5f97\u4fe1\u8d56\u7684\u673a\u68b0\u5e08\u3002",
    "\u8fd9\u91cc\u5de6\u8f6c\uff0c\u5c31\u5728\u524d\u9762\u4e86\u3002",
    "\u597d\u7684\u3002",
    "\u8bf4\u8d77\u6765\uff0c\u542c\u8bf4\u4f60\u4ee5\u524d\u8fd8\u662f\u4e2a\u6307\u6325\u8fc7\u5927\u573a\u9762\u7684\u5389\u5bb3\u4eba\u7269\uff1f",
    "\u2026\u2026\u8c01\u8bf4\u7684\uff0c\u201c\u53c2\u4e8b\u5973\u58eb\u201d\uff1f",
    "\u600e\u4e48\u53ef\u80fd\u3002\u53c2\u4e8b\u5973\u58eb\u6bcf\u6b21\u7684\u8054\u7edc\u90fd\u975e\u5e38\u7b80\u77ed\uff0c\u57fa\u672c\u4e0a\u9664\u4e86\u7269\u8d44\u540d\u79f0\u548c\u4ea4\u4ed8\u5bf9\u8c61\u7684\u8bc6\u522b\u65b9\u5f0f\uff0c\u5c31\u53ea\u6709\u94b1\u3002",
    "\u544a\u8bc9\u6211\u7684\u4e5f\u662f\u4e2a\u6697\u5546\uff0c\u4f60\u4eec\u5e94\u8be5\u5f88\u719f\u6089\uff0c\u4f69\u59ae\u6cfd\u3002",
    "\u4f60\u8ba4\u8bc6\u4f69\u59ae\u6cfd\uff1f\u5979\u4e0d\u5728\u8fd9\u4e2a\u533a\u57df\u6d3b\u52a8\u5427\uff1f",
    "\u7b97\u4e0d\u4e0a\u8ba4\u8bc6\uff0c\u53ea\u662f\u542c\u8bf4\u8fc7\u3002\u6bd5\u7adf\u5979\u662f\u6211\u4eec\u6697\u5546\u91cc\u7684\u201c\u4ee3\u884c\u4eba\u201d\uff0c\u5f88\u6709\u540d\u6c14\u7684\u3002",
    "\u591a\u4e8f\u4e86\u8fd9\u4e9b\u4ee3\u884c\u4eba\u4eec\u5efa\u7acb\u7684\u6697\u884c\uff0c\u6211\u4eec\u505a\u5c0f\u89c4\u6a21\u751f\u610f\u7684\u6697\u5546\u624d\u6709\u4e86\u6d3b\u8def\uff0c\u4e5f\u6709\u4e86\u50cf\u4f60\u73b0\u5728\u5728\u7684\u8fd9\u79cd\u5904\u4e8e\u5730\u4e0b\u7684\u6697\u8857\uff0c\u7528\u6765\u73b0\u573a\u4ee5\u7269\u6613\u7269\u2014\u2014\u773c\u89c1\u4e3a\u5b9e\u561b\u3002|\u4e0d\u4e45\u4e4b\u524d\u5979\u5230\u8fc7\u8fd9\u91cc\uff0c\u51d1\u5de7\u9047\u5230\u5979\uff0c\u5c31\u542c\u5979\u63d0\u8d77\u4e86\u4f60\u3002",
    "\u5979\u4f1a\u6765\u8fd9\u91cc\uff0c\u4e0d\u4f1a\u662f\u5728\u8865\u8d27\u5427\uff1f",
    "\u6211\u4eec\u6697\u5546\u8fdb\u8d27\u6e20\u9053\u662f\u4fdd\u5bc6\u7684\u3002",
    "\u4e0d\u8fc7\u4f69\u59ae\u6cfd\u6765\u8fd9\u91cc\uff0c\u4e3b\u8981\u662f\u60f3\u6253\u542c\u4e00\u4e9b\u548c\u74e6\u826f\u683c\u5e2e\u76f8\u5173\u7684\u6d88\u606f\u2014\u2014\u54e6\uff0c\u8bf4\u662f\u8981\u514d\u8d39\u9001\u7ed9\u4e00\u4f4d\u66fe\u7ecf\u4e3b\u5bfc\u8fc7\u5f88\u591a\u5927\u573a\u9762\u7684\u6307\u6325\u5b98\u3002|\u80fd\u4ece\u4f69\u59ae\u6cfd\u624b\u4e2d\u83b7\u5f97\u514d\u8d39\u6d88\u606f\uff0c\u5979\u5230\u5e95\u6b20\u4e86\u4f60\u591a\u5927\u7684\u4eba\u60c5\uff1f",
    "\u2026\u2026\u6ca1\u6709\uff0c\u5979\u7ed9\u4e86\u6211\u4e0d\u5c11\u5e2e\u52a9\u3002",
    "\uff08\u6211\u7684\u76ee\u5149\u4e0d\u7531\u81ea\u4e3b\u5730\u77a5\u5411\u4e86\u5bc7\u5c14\u8299\u3002\uff09",
    "\uff08\u8428\u73c8\u8ddf\u968f\u6211\uff0c\u770b\u5411\u4e86\u5bc7\u5c14\u8299\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u6c89\u9ed8\u5730\u8ddf\u5728\u6211\u540e\u9762\uff0c\u6ca1\u6709\u6ce8\u610f\u5230\u6211\u4eec\u3002\uff09",
    "\u2014\u2014\u548c\u8d4f\u91d1\u730e\u4eba\u641e\u597d\u5173\u7cfb\u786e\u5b9e\u5f88\u91cd\u8981\u3002|\u6307\u6325\u5b98\uff0c\u6211\u4eec\u5230\u4e86\u3002",
    "\uff08\u6211\u4eec\u505c\u5728\u4e00\u5904\u88ab\u4f2a\u88c5\u6210\u5ca9\u58c1\u7684\u4ed3\u5e93\u524d\u3002\uff09",
    "\uff08\u8428\u73c8\u6309\u4e0b\u6309\u94ae\uff0c\u968f\u7740\u8f74\u627f\u7684\u8fd0\u8f6c\uff0c\u4ed3\u5e93\u7684\u95e8\u88ab\u6253\u5f00\u3002\uff09",
    "\u53c2\u4e8b\u5973\u58eb\u8981\u4ea4\u7ed9\u4f60\u7684\u7269\u8d44\u90fd\u5728\u91cc\u9762\u4e86\u3002",
    "\u95ea\u7535\uff0c\u514b\u7f57\u4e3d\u79d1\uff0c\u7eb3\u7f8e\u897f\u4e1d\uff0c\u5bc7\u5c14\u8299\uff0c\u9ebb\u70e6\u4f60\u4eec\u628a\u7269\u8d44\u9001\u56de\u53bb\u5427\u3002",
    "\u53ef\u80fd\u9700\u8981\u4f60\u7559\u4e0b\u4e24\u4e2a\u3002",
    "\u2026\u2026\uff1f",
    "\u5165\u53e3\u7684\u5149\u5b66\u4f2a\u88c5\u6ee4\u5149\u88c5\u7f6e\u867d\u7136\u6709\u5907\u4efd\uff0c\u53ef\u662f\u5e03\u7f6e\u548c\u542f\u52a8\u90fd\u9700\u8981\u65f6\u95f4\u3002",
    "\u4f7f\u7528\u5907\u4efd\u7684\u6210\u672c\u4f1a\u7b97\u5728\u53c2\u4e8b\u5973\u58eb\u540d\u4e0b\uff0c\u5229\u606f\u7684\u8bdd\u2026\u2026\u6211\u4eec\u4ee5\u7269\u6613\u7269\uff0c\u5728\u8fd9\u4e00\u6bb5\u65f6\u95f4\u91cc\uff0c\u6697\u8857\u7684\u9632\u536b\u5c31\u4ea4\u7ed9\u4f60\u7684\u4eba\u5f62\uff0c\u600e\u4e48\u6837\uff1f",
    "\u2026\u2026\u62b1\u6b49\uff0c\u8fd9\u786e\u5b9e\u662f\u6211\u7684\u5931\u8bef\u3002\u9700\u8981\u591a\u4e45\uff1f",
    "\u548c\u4f60\u4eec\u642c\u8fd0\u7269\u8d44\u7684\u65f6\u95f4\u5dee\u4e0d\u591a\u3002",
    "\u55ef\u3002\u514b\u7f57\u4e3d\u79d1\uff0c\u7eb3\u7f8e\u897f\u4e1d\uff0c\u4f60\u4eec\u5148\u5f00\u59cb\u8fd0\u9001\u7269\u8d44\u3002\u95ea\u7535\uff0c\u5bc7\u5c14\u8299\uff0c\u4f60\u4eec\u2014\u2014",
    "\uff08\u4e00\u9635\u523a\u8033\u7684\u566a\u97f3\u6253\u65ad\u4e86\u6211\u7684\u8bdd\u3002\uff09",
    "\uff08\u566a\u97f3\u542c\u8d77\u6765\u7c7b\u4f3c\u97f3\u54cd\u542f\u52a8\u524d\u7684\u8c03\u8bd5\u58f0\u6ce2\uff0c\u5728\u523a\u8033\u7684\u566a\u97f3\u505c\u606f\u540e\uff0c\u53e6\u4e00\u4e2a\u6e05\u8106\u7684\u58f0\u97f3\u54cd\u4e86\u8d77\u6765\u3002\uff09",
    "\u4e00\u8d77\u6765\u542c\u7ef4\u666e\u857e\u7684\u6b4c\u58f0\u5427\uff0c\u5927\u5bb6\u2606",
    "\u53c8\u662f\u4f60\u53c8\u662f\u4f60\uff0c\u522b\u8df3\u4e86\uff0c\u5435\u6b7b\u4e86\uff01\u4e0b\u6765\u4e0b\u6765\uff01",
    "\u5566\u5566\u5566\u2606~",
    "\u4f60\u95ed\u5634\uff01\u8428\u73c8\u2014\u2014\uff01",
    "\u554a\u2014\u2014\uff01\u9a6c\u4e0a\u8fc7\u53bb\uff01",
    "\u8fd9\u662f\uff1f",
    "\u6211\u8fd9\u91cc\u6709\u4e2a\u4eba\u5f62\uff0c\u4f60\u542c\u5230\u4e86\uff0c\u662f\u53eb\u7ef4\u666e\u857e\u3002\u53ea\u8981\u6211\u4e00\u4e0d\u6ce8\u610f\uff0c\u5979\u5c31\u559c\u6b22\u8dd1\u53bb\u6697\u8857\u4e2d\u95f4\u90a3\u91cc\u7684\u4e00\u5757\u5927\u77f3\u5934\u4e0a\u5531\u6b4c\u3002|\u5bf9\u4e86\uff0c\u4f60\u80fd\u4e0d\u80fd\u987a\u4fbf\u5e2e\u6211\u628a\u5979\u5e26\u56de\u6765\uff1f",
    "\u2026\u2026\u8fd9\u5e94\u8be5\u4e0d\u80fd\u88ab\u7b97\u5728\u6697\u8857\u7684\u9632\u536b\u5de5\u4f5c\u91cc\uff1f",
    "\u8fd9\u6837\u5427\u3002\u867d\u7136\u662f\u53c2\u4e8b\u5973\u58eb\u4ed8\u94b1\uff0c\u4f46\u8fd9\u6b21\u4f60\u786e\u5b9e\u8ba2\u8d2d\u4e86\u4e0d\u5c11\u7269\u8d44\u3002\u4f5c\u4e3a\u5927\u5b97\u8d27\u54c1\u7684\u9644\u8d60\uff0c\u5982\u679c\u4f60\u613f\u610f\u8ba9\u4f60\u7684\u4eba\u5f62\u53bb\u628a\u7ef4\u666e\u857e\u5e26\u56de\u6765\uff0c\u6211\u5c31\u628a\u7ef4\u666e\u857e\u7ed9\u4f60\uff0c\u600e\u4e48\u6837\uff1f",
    "\u2026\u2026\u662f\u5426\u9700\u8981\u8be2\u95ee\u4e00\u4e0b\u8fd9\u4f4d\u540d\u5b57\u662f\u201c\u7ef4\u666e\u857e\u201d\u7684\u4eba\u5f62\uff1f",
    "\u7b49\u4f60\u628a\u5979\u5e26\u56de\u6765\u540e\uff0c\u53ef\u4ee5\u5148\u95ee\u95ee\u5979\u3002|\u5979\u4f1a\u613f\u610f\u7684\u3002"
  ]
]