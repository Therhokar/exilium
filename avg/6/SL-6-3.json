[
  [
    "\u554a\uff0c\u961f\u957f\uff0c\u8fd9\u91cc\u8fd9\u91cc\u2014\u2014\u8fd9\u91cc\u6709\u6761\u8def\u54df~|\u867d\u7136\u8bf4\u662f\u4e2a\u5ce1\u8c37\uff0c\u53ef\u662f\u6709\u597d\u2014\u2014\u591a\u5947\u602a\u7684\u673a\u5668\u6324\u5728\u4e00\u8d77\uff0c\u8fd9\u91cc\u4ee5\u524d\u662f\u4e2a\u673a\u68b0\u5236\u9020\u5382\u561b\uff1f",
    "\uff08\u4e24\u4eba\u7a7f\u8fc7\u673a\u5668\u95f4\u7684\u7f1d\u9699\uff0c\u4e00\u8def\u5411\u5ce1\u8c37\u4e2d\u6bb5\u4f4d\u7f6e\u79fb\u52a8\u3002\uff09",
    "\u7834\u788e\u673a\u3001\u5fae\u7c89\u78e8\uff0c\u8fd9\u91cc\u66fe\u7ecf\u662f\u4e2a\u77ff\u573a\u3002|\u4ee5\u66b4\u9732\u5728\u5916\u7684\u673a\u5668\u635f\u6bc1\u7a0b\u5ea6\u5224\u65ad\uff0c\u5e9f\u5f03\u65f6\u95f4\u5e94\u8be5\u5728\u4e8c\u5341\u5e74\u4ee5\u4e0a\u3002",
    "\u554a\uff01",
    "\uff08\u4e00\u8fb9\u542c\u7740\u95ea\u7535\u7684\u89e3\u91ca\uff0c\u5bc7\u5c14\u8299\u4e00\u8fb9\u9976\u6709\u5174\u81f4\u5730\u56de\u5934\u770b\u7740\u95ea\u7535\uff0c\u6ca1\u80fd\u6ce8\u610f\u5230\u811a\u4e0b\u9508\u8680\u7684\u94a2\u677f\uff0c\u4e00\u811a\u8e29\u7a7a\u3002\uff09",
    "\uff08\u540e\u65b9\u7684\u95ea\u7535\u53ca\u65f6\u4f38\u51fa\u624b\uff0c\u5c06\u5979\u62c9\u4f4f\u3002\uff09",
    "\u5c0f\u5fc3\u3002\u8fd9\u91cc\u7684\u5730\u9762\u7ed3\u6784\u592a\u4e45\u6ca1\u6709\u4fdd\u517b\uff0c\u9508\u8680\u7a0b\u5ea6\u5f88\u9ad8\u3002",
    "\u8c22\u8c22\u961f\u957f~",
    "\u55ef\u2014\u2014\u5e9f\u5f03\u77ff\u573a\uff0c\u542c\u8d77\u6765\uff0c\u597d\u50cf\u662f\u4e2a\u5f88\u5371\u9669\u7684\u5730\u65b9\u3002|\u74e6\u826f\u683c\u5e2e\u4e3a\u4ec0\u4e48\u8981\u5446\u5728\u8fd9\u91cc\u5440\uff1f",
    "\u74e6\u826f\u683c\u5e2e\u7684\u884c\u52a8\u6a21\u5f0f\uff0c\u5e76\u4e0d\u80fd\u4ee5\u5e38\u7406\u5224\u65ad\u3002|\u505c\u4e0b\uff01",
    "\uff08\u7f1d\u9699\u4e2d\uff0c\u95ea\u7535\u6ce8\u610f\u5230\u4e86\u4e00\u5904\u4e0e\u5468\u8fb9\u73af\u5883\u4e0d\u540c\u7684\u8272\u5f69\u3002\uff09",
    "\uff08\u4e24\u4eba\u653e\u6162\u811a\u6b65\uff0c\u5411\u7f1d\u9699\u5904\u79fb\u52a8\u3002\uff09",
    "\u554a\uff0c\u627e\u5230\u4e86~",
    "\u6ce8\u610f\u89c2\u5bdf\u3002",
    "\uff08\u4ece\u4e24\u4eba\u6240\u5904\u4f4d\u7f6e\u770b\u8fc7\u53bb\uff0c\u662f\u4e00\u5904\u88ab\u56f4\u5728\u4f17\u591a\u5e9f\u5f03\u673a\u68b0\u4e2d\u7684\u534a\u9732\u5929\u7a7a\u95f4\u3002\uff09",
    "\uff08\u7a7a\u95f4\u4f4d\u7f6e\u8f83\u4f4e\uff0c\u4ee5\u4e24\u4eba\u6240\u5904\u7684\u4f4d\u7f6e\u8ba1\u7b97\uff0c\u7ea6\u6709\u5341\u7c73\u4ee5\u4e0a\u7684\u6df1\u5ea6\u3002\uff09",
    "\u770b\u770b\uff0c\u770b\u5230\u6ca1\uff1f",
    "\u8001\u5927\uff0c\u6211\u4eec\u62a2\u4e86\u8fd9\u4e48\u591a\uff0c\u90fd\u4ea4\u4e0a\u53bb\uff0c\u4e0b\u6b21\u53bb\u56f4\u730e\uff0c\u6211\u4eec\u4e00\u5b9a\u80fd\u6709\u4e2a\u597d\u4f4d\u7f6e\u5427\uff01",
    "\u8bf4\u4ec0\u4e48\u5462\uff01\u4ec0\u4e48\u53eb\u62a2\uff0c\u660e\u660e\u662f\u90a3\u4e9b\u8822\u8d27\u81ea\u5df1\u5b88\u4e0d\u4f4f\uff0c\u8fd8\u56b7\u56b7\u7740\u8981\u56de\u6765\u627e\u7237\u7237\u4eec\u62a5\u4ec7\uff0c\u54c8\u54c8\u54c8\u54c8\u54c8\uff0c\u51c0\u626f\u86cb\uff01\u4eba\u90fd\u6ca1\u4e86\uff0c\u62a5\u4ec0\u4e48\u4ec7\uff01",
    "\u4e00\u961f\u4f60\u4eec\u77e5\u9053\u5427\uff1f",
    "\u77e5\u9053\uff0c\u524d\u4e00\u6bb5\u4e0d\u77e5\u9053\u600e\u4e48\u60f3\u7684\uff0c\u8dd1\u53bb\u5e2e\u4e94\u5341\u4e03\u961f\u90a3\u5e2e\u5e9f\u7269\u3002\u7ed3\u679c\u600e\u4e48\u6837\uff1f\u54c8\u54c8\u54c8\u54c8\u54c8\uff0c\u4e00\u4e0b\u5b50\u5c31\u6ca1\u4e86\u4e09\u6210\uff0c\u90a3\u53ef\u662f\u4e09\u6210\uff01\u4e94\u5341\u4e03\u7684\u8001\u5927\u53eb\u4ec0\u4e48\u6765\u7740\uff1f",
    "\u5e03\u745f\u5c14\uff1f",
    "\u5bf9\u5bf9\uff0c\u5e03\u745f\u5c14\u3002\u90a3\u4e2a\u5e9f\u7269\uff0c\u4ed7\u7740\u80fd\u8ddf\u7f57\u8054\u7684\u996d\u6876\u642d\u4e0a\u5173\u7cfb\uff0c\u8fd8\u6709\u4e2a\u4e11\u5f97\u8981\u6b7b\u7684\u4ec0\u4e48\u2026\u2026\u63a0\u593a\u8005\uff1f\u8d70\u8def\u90a3\u4e2a\u6837\u5b50\u54df\uff0c\u7237\u7237\u6211\u65e9\u770b\u4ed6\u4e0d\u987a\u773c\u4e86\u3002\u8fd9\u4e0d\uff0c\u6574\u4e2a\u4e94\u5341\u4e03\u961f\u90fd\u6ca1\u4e86\uff01\u54c8\u54c8\u54c8\u54c8\u54c8\u54c8\uff01",
    "\u8fd9\u4e9b\u4e1c\u897f\u6211\u4eec\u90fd\u8981\u4ea4\u4e0a\u53bb\u5417\uff0c\u8001\u5927\uff1f",
    "\u5f53\u7136\u4e0d\u3002\u4e00\u961f\u4e0d\u662f\u53f7\u79f0\u88c5\u5907\u9f50\u5168\u6218\u529b\u51fa\u4f17\u5417\uff0c\u78b0\u4e0a\u4e2a\u786c\u832c\u5b50\uff0c\u6709\u7528\uff1f",
    "\u6211\u5c31\u8bf4\uff0c\u9760\u683c\u62c9\u5e0c\u5c14\u8fd8\u4e0d\u5982\u9760\u6211\u4eec\u81ea\u5df1\uff01\u5446\u5728\u8fd9\u5730\u65b9\uff0c\u8981\u4ec0\u4e48\u6ca1\u6709\uff01\u53bb\uff0c\u53bb\u5206\u5206\uff0c\u9760\u8c31\u7684\u88c5\u5907\u6211\u4eec\u81ea\u5df1\u7559\u7740\uff0c\u56f4\u730e\u62ff\u4e2a\u4e2d\u95f4\u7684\u4f4d\u7f6e\u5c31\u884c\uff0c\u8d70\u524d\u9762\u7684\u4e0d\u5c31\u662f\u53bb\u7ed9\u4e2d\u95f4\u7684\u57ab\u80cc\u5417\uff01",
    "\u5c31\u662f\u5c31\u662f\u3002\u7b49\u6211\u4eec\u88c5\u5907\u4e0a\u6765\u4e86\uff0c\u5148\u53bb\u8fb9\u4e0a\u90a3\u51e0\u4e2a\u70b9\u8e29\u8e29\u3002\u90a3\u51e0\u4e2a\u70b9\u80a5\u5f97\u6d41\u6cb9\u8fd8\u4e0d\u7ed9\u7237\u4e0a\u8d21\uff0c\u7237\u65e9\u5c31\u770b\u90a3\u51e0\u4e2a\u70b9\u7684\u4eba\u4e0d\u987a\u773c\u4e86\uff01",
    "\u8981\u3001\u8981\u6740\u4e86\u4ed6\u4eec\u5417\uff1f",
    "\u4e0d\u7136\u8fd8\u80fd\u653e\u4e86\u4ed6\u4eec\uff0c\u7b49\u4ed6\u4eec\u56de\u6765\u62a5\u4ec7\uff1f\u54c8\u54c8\u54c8\u54c8\u54c8\uff01",
    "\u5feb\u70b9\u5feb\u70b9\uff0c\u5206\u5b8c\u4e86\u6ca1\uff0c\u5206\u5b8c\u4e86\u5c31\u7b49\u4e0b\u4e00\u6279\u6765\u9001\u6b7b\u7684\u3002\u5165\u53e3\u90a3\u8fb9\u600e\u4e48\u6ca1\u6d88\u606f\u4e86\uff0c\u53bb\u95ee\u95ee\uff01\u6709\u65b0\u7684\u80a5\u7f8a\u8d76\u7d27\u62a5\u8fc7\u6765\uff0c\u6211\u4eec\u518d\u53bb\u5bb0\u4e00\u53ea\uff0c\u5feb\u53bb\u5feb\u53bb\uff01",
    "\uff08\u76ef\u7740\u5ce1\u8c37\u4e2d\u5f97\u610f\u6d0b\u6d0b\u7684\u74e6\u826f\u683c\u5e2e\u6210\u5458\u4eec\uff0c\u5bc7\u5c14\u8299\u795e\u8272\u51b7\u6de1\u4e86\u4e0b\u6765\u3002\uff09",
    "\u5bc7\u5c14\u8299\uff1f",
    "\u961f\u957f\uff0c\u4ed6\u4eec\u662f\u5728\u8bf4\u2014\u2014\u8981\u53bb\u6740\u6389\u4f4f\u5728\u9644\u8fd1\u7684\u4eba\uff1f",
    "\u2026\u2026\u8fd9\u7b26\u5408\u4ed6\u4eec\u7684\u884c\u4e3a\u6a21\u5f0f\u3002",
    "\u90a3\u6211\u4eec\u2014\u2014\u8981\u628a\u4ed6\u4eec\u90fd\u5e72\u6389\u5417\uff1f",
    "\u4ed6\u4eec\u6240\u5728\u4f4d\u7f6e\u4f1a\u5f71\u54cd\u5230\u57fa\u5730\u8f66\u7684\u884c\u8fdb\u3002|\u8054\u7edc\u57fa\u5730\u8f66\uff0c\u6211\u4eec\u9700\u8981\u514b\u7f57\u4e3d\u79d1\u548c\u7eb3\u7f8e\u897f\u4e1d\u7684\u589e\u63f4\u3002\u4ee5\u4ed6\u4eec\u7684\u706b\u529b\u914d\u7f6e\u548c\u4eba\u5458\u6570\u91cf\uff0c\u6211\u4eec\u65e0\u6cd5\u89e3\u51b3\u3002",
    "\u4f46\u4ed6\u4eec\u5f88\u53ef\u80fd\u62ff\u5230\u4e86\u8db3\u591f\u7684\u88c5\u5907\u5c31\u4f1a\u9003\u8d70\u2014\u2014",
    "\u4ece\u4ed6\u4eec\u7684\u5bf9\u8bdd\u4e2d\u5224\u65ad\uff0c\u4ed6\u4eec\u8fd8\u4f1a\u505c\u7559\u5728\u8fd9\u91cc\u4e00\u6bb5\u65f6\u95f4\u3002|\u5bc7\u5c14\u8299\uff0c\u4f60\u600e\u4e48\u4e86\uff1f",
    "\u2026\u2026",
    "\u2026\u2026\u6ca1\u4ec0\u4e48\uff0c\u90a3\u5c31\u2026\u2026",
    "\u90a3\u5c31\u8d76\u5feb\u8054\u7edc\u5427\uff0c\u961f\u957f\uff01"
  ]
]