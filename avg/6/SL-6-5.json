[
  [
    "\u4f60\u522b\u52a8\u2014\u2014\u4f60\u522b\u52a8\uff01\u90fd\u8bf4\u4e86\u4f60\u522b\u52a8\u884c\u4e0d\u884c\uff01",
    "\u6f14\u51fa\u5931\u8d25\uff01\u7ef4\u666e\u857e\u3001\u8981\u91cd\u65b0\u5f00\u59cb\uff01",
    "\u2026\u2026\u5fae\u5c0f\u7684\u788e\u77f3\uff0c\u6253\u7834\u5c71\u8c37\u7684\u660f\u6697\u2026\u2026",
    "\u54c8\uff1f\uff01\u90a3\u4f60\u6765\uff01\u4f60\u6765\u6309\u4f4f\u5979\u8bd5\u8bd5\uff01\u8fd9\u5c0f\u4e2a\u5b50\u662f\u600e\u4e48\u56de\u4e8b\uff0c\u6db2\u538b\u6746\u600e\u4e48\u88c5\u7684\u8fd8\u6ca1\u5bc7\u5c14\u8299\u9ad8\u529b\u6c14\u600e\u4e48\u6bd4\u5bc7\u5c14\u8299\u8fd8\u5927\uff01",
    "\u2026\u2026",
    "\uff08\u5bc7\u5c14\u8299\u8d70\u5728\u6700\u540e\u9762\uff0c\u4e00\u8a00\u4e0d\u53d1\u3002\uff09",
    "\uff08\u542c\u5230\u81ea\u5df1\u7684\u540d\u5b57\uff0c\u5979\u4e5f\u53ea\u662f\u62ac\u5934\u770b\u4e86\u770b\u514b\u7f57\u4e3d\u79d1\u6240\u5728\u7684\u65b9\u5411\uff0c\u5e76\u6ca1\u6709\u50cf\u5f80\u5e38\u4e00\u6837\u63a5\u4e0a\u514b\u7f57\u4e3d\u79d1\u7684\u8bdd\u3002\uff09",
    "\u6307\u6325\u5b98\u3002",
    "\u8f9b\u82e6\u4e86\u3002",
    "\u653e\u5f00\u7ef4\u666e\u857e\u5566\u2014\u2014\u554a\uff0c\u8428\u73c8\u3001\u8428\u73c8\uff01",
    "\uff08\u770b\u5230\u8428\u73c8\uff0c\u7ef4\u666e\u857e\u518d\u6b21\u8bd5\u56fe\u6323\u8131\u514b\u7f57\u4e3d\u79d1\u7684\u63a7\u5236\u3002\uff09",
    "\uff08\u6211\u5411\u514b\u7f57\u4e3d\u79d1\u70b9\u4e86\u70b9\u5934\uff0c\u514b\u7f57\u4e3d\u79d1\u677e\u624b\u3002\uff09",
    "\u8428\u73c8\u2014\u2014\uff01",
    "\u554a\u4f60\u8f7b\u4e00\u70b9\u8f7b\u4e00\u70b9\u2026\u2026\u75bc\u75bc\u2026\u2026",
    "\u4eca\u5929\u7684\u6f14\u5531\u4f1a\u6ca1\u6709\u6210\u529f\uff0c\u4f46\u662f~",
    "\u7ef4\u666e\u857e\uff0c\u5c1d\u8bd5\u4e86\u53ef\u7231\u98ce\u683c\u7684\u6f14\u51fa\u2606\u8428\u73c8\uff0c\u5408\u4e0d\u5408\u9002\uff1f",
    "\u90fd\u8bf4\u4e86\u591a\u5c11\u6b21\u4e86\uff0c\u8fd9\u91cc\u4e0d\u80fd\u7ed9\u4f60\u5f00\u6f14\u5531\u4f1a\u3002\u7ef4\u666e\u857e\uff0c\u4f60\u80fd\u4e0d\u80fd\u542c\u8bdd\u4e00\u70b9\u3002",
    "\u6211\u8bf4\u3002",
    "\uff08\u5bc7\u5c14\u8299\u7a81\u7136\u5f00\u53e3\u3002\uff09",
    "\uff08\u5979\u9634\u6c89\u5730\u76ef\u7740\u8428\u73c8\uff0c\u4ee5\u53ca\u4ecd\u7136\u62b1\u7740\u8428\u73c8\u6c42\u5938\u5956\u7684\u7ef4\u666e\u857e\u3002\uff09",
    "\u5982\u679c\u60f3\u5b8c\u5168\u63a7\u5236\u4eba\u5f62\u7684\u884c\u52a8\uff0c\u6211\u6709\u4e00\u4e2a\u597d\u529e\u6cd5\u3002",
    "\u5443\uff0c\u662f\u4ec0\u4e48\uff1f",
    "\u5173\u673a\u3002",
    "\u5c06\u4eba\u5f62\u5173\u673a\uff0c\u5220\u9664\u5fc3\u667a\u5907\u4efd\u3002|\u6309\u7167\u4f60\u7684\u8981\u6c42\uff0c\u91cd\u65b0\u5efa\u7acb\u5fc3\u667a\u5b58\u6863\u7684\u201c\u7ef4\u666e\u857e\u201d\uff0c\u4e00\u5b9a\u662f\u4f60\u5e0c\u671b\u770b\u5230\u7684\u201c\u7ef4\u666e\u857e\u201d\u3002",
    "\u554a\u2026\u2026\u54c8\u54c8\uff0c\u6211\u662f\u5728\u6c61\u67d3\u533a\u8ba8\u751f\u6d3b\uff0c\u8fd8\u8981\u4f9d\u8d56\u6211\u7684\u4eba\u5f62\u3002\u867d\u7136\u60f9\u4e86\u70b9\u9ebb\u70e6\uff0c\u4f46\u4e5f\u6ca1\u6709\u9020\u6210\u592a\u5927\u635f\u5931\uff0c\u5173\u673a\u2026\u2026\u4e0d\u81f3\u4e8e\u4e0d\u81f3\u4e8e\u3002",
    "\u6ca1\u6709\u9020\u6210\u592a\u5927\u635f\u5931\uff0c\u4f60\u5c31\u8981\u628a\u4f60\u4f9d\u8d56\u7684\u4eba\u5f62\u9001\u8d70\uff1f",
    "\u554a\uff0c\u5443\uff0c\u5bc7\u5c14\u8299\u2014\u2014",
    "\u5bc7\u5c14\u8299\uff0c\u6307\u6325\u5b98\u4e0d\u4f1a\u8fd9\u4e48\u505a\u3002",
    "\u54c8\uff0c\u6211\u8bf4\u7684\u662f\u7ef4\u666e\u857e\u3002",
    "\uff08\u542c\u5230\u81ea\u5df1\u7684\u540d\u5b57\u88ab\u6301\u7eed\u63d0\u8d77\uff0c\u7f20\u7740\u8428\u73c8\u7684\u7ef4\u666e\u857e\u597d\u5947\u5730\u8f6c\u5934\u3002\uff09",
    "\u4f60\u597d\uff0c\u6211\u662f\u7ef4\u666e\u857e~|\u4f60\u5934\u53d1\u7684\u989c\u8272\u597d\u597d\u770b\uff0c\u50cf\u5927\u6d77\uff01\u7ef4\u666e\u857e\u3001\u8bb0\u5f97\u4f60\u7684\u540d\u5b57\uff0c\u662f\u5bc7\u5c14\u8299\uff01",
    "\u6211\u662f\u5728\u5bf9\u4f60\u7684\u6240\u6709\u8005\u8bf4\uff0c\u628a\u4f60\u5173\u673a\uff0c\u5220\u9664\u4f60\u7684\u5fc3\u667a\u5907\u4efd\uff01",
    "\u7ef4\u666e\u857e\u3001\u6709\u597d\u597d\u505a\u5907\u4efd\u7ba1\u7406\uff0c\u4e5f\u6709\u597d\u597d\u5173\u673a\u4f11\u606f\u5440\uff1f|\u5bc7\u5c14\u8299\uff0c\u4e0d\u5f00\u5fc3\u5417\uff1f",
    "\u2026\u2026\u2026\u2026",
    "\uff08\u6ca1\u6709\u5f97\u5230\u56de\u7b54\uff0c\u7ef4\u666e\u857e\u6709\u4e9b\u56f0\u60d1\u3002\uff09",
    "\uff08\u6765\u56de\u770b\u7740\u8428\u73c8\u548c\u5bc7\u5c14\u8299\uff0c\u5979\u60f3\u4e86\u60f3\uff0c\u8ba4\u771f\u63d0\u8bae\u3002\uff09",
    "\u7ef4\u666e\u857e\u7ed9\u4f60\u5531\u9996\u6b4c\u597d\u4e0d\u597d\uff1f|\u6709\u4e0d\u5f00\u5fc3\u7684\u4e8b\u60c5\uff0c\u5c31\u6765\u627e\u7ef4\u666e\u857e\u5427~",
    "\u2026\u2026\u2026\u2026\u2026\u2026\u2026\u2026",
    "\uff08\u773c\u89c1\u5bc7\u5c14\u8299\u7684\u8138\u8272\u8d8a\u6765\u8d8a\u9634\u6c89\uff0c\u6211\u8fde\u5fd9\u9694\u5230\u4e24\u4eba\u4e2d\u95f4\u3002\uff09",
    "\u554a\u90a3\u4ec0\u4e48\uff0c\u8428\u73c8\uff0c\u6211\u4eec\u5148\u53bb\u642c\u7269\u8d44\uff0c\u7ef4\u666e\u857e\u5c31\u7559\u5728\u4f60\u8fd9\u91cc\u5427\u3002|\u770b\u8d77\u6765\uff0c\u7ef4\u666e\u857e\u5e76\u6ca1\u6709\u79bb\u5f00\u7684\u60f3\u6cd5\uff1f",
    "\u2026\u2026\u90a3\u4e2a\u2026\u2026\u672c\u6765\u4e0d\u60f3\u8bf4\u7684\u2026\u2026\u5012\u4e5f\u4e0d\u662f\u8ba9\u7ef4\u666e\u857e\u542c\u8bdd\u3002",
    "\u6211\uff0c\u6211\u89c9\u5f97\u2026\u2026\u7ef4\u666e\u857e\u5728\u4f60\u90a3\u91cc\uff0c\u5927\u6982\u80fd\u81ea\u7531\u4e00\u70b9\uff1f\u6bd4\u5982\u6bcf\u5929\u5728\u4f60\u90a3\u4e2a\u57fa\u5730\u8f66\u4e0a\u5531\u5531\u6b4c\u4ec0\u4e48\u7684\uff1f\u4f60\u7684\u57fa\u5730\u8f66\u90a3\u4e48\u5927\uff0c\u603b\u80fd\u627e\u5230\u4e00\u4e2a\u5730\u65b9\u8ba9\u5979\u5531\u6b4c\u5427\uff1f|\u5728\u6211\u8fd9\u91cc\uff0c\u4f60\u4e5f\u770b\u5230\u4e86\u2026\u2026\u5927\u5bb6\u90fd\u4e0d\u592a\u80fd\u63a5\u53d7\u7ef4\u666e\u857e\u5531\u6b4c\u3002",
    "\u7ef4\u666e\u857e\uff0c\u6362\u4e00\u4e2a\u80fd\u6bcf\u5929\u81ea\u7531\u5531\u6b4c\u7684\u5730\u65b9\uff0c\u597d\u4e0d\u597d\uff1f",
    "\u597d\uff01\u7ef4\u666e\u857e\uff0c\u8981\u5531\u6b4c\u2606",
    "\u4f60\u770b\uff0c\u7ef4\u666e\u857e\u613f\u610f\u3002",
    "\u2026\u2026\u81ea\u7531\u7684\u5730\u65b9\u2026\u2026",
    "\u800c\u4e14\u2026\u2026\u6211\u8fd9\u91cc\u867d\u7136\u6709\u5404\u79cd\u96f6\u4ef6\uff0c\u4f46\u6ca1\u6709\u4eba\u5f62\u7ef4\u62a4\u8bbe\u5907\u3002\u73b0\u5728\u7ef4\u666e\u857e\u7684\u7d20\u4f53\u4e5f\u51fa\u4e86\u4e0d\u5c11\u95ee\u9898\uff0c\u673a\u80fd\u8d8a\u6765\u8d8a\u5dee\uff0c\u518d\u8fd9\u4e48\u4e0b\u53bb\uff0c\u53ef\u80fd\u7ef4\u666e\u857e\u53ea\u80fd\u88ab\u8feb\u6c38\u4e45\u5173\u673a\u4e86\u3002",
    "\u4f60\u6709\u8fd9\u4e48\u591a\u6218\u672f\u4eba\u5f62\uff0c\u518d\u589e\u52a0\u4e00\u4e2a\uff0c\u5e94\u8be5\u4e5f\u6ca1\u4ec0\u4e48\u95ee\u9898\u5427\uff1f\u5982\u679c\u4f60\u4e0d\u60f3\u8ba9\u5979\u7559\u5728\u4f60\u90a3\u91cc\uff0c\u7ef4\u4fee\u5b8c\u6210\uff0c\u628a\u5979\u653e\u5728\u54ea\u4e2a\u5b89\u5168\u70b9\u7684\u5730\u65b9\uff0c\u4e5f\u662f\u53ef\u4ee5\u7684\uff1f",
    "\u884c\u4e86\u884c\u4e86\uff0c\u5148\u522b\u7ba1\u90a3\u4e2a\u53eb\u4ec0\u4e48\u7ef4\u666e\u857e\u7684\uff0c\u6307\u6325\u5b98\uff0c\u6211\u5148\u53bb\u642c\u7269\u8d44\u4e86\uff01",
    "\u2026\u2026\u90a3\u5c31\u9ebb\u70e6\u4f60\u4e86\u3002",
    "\uff08\u8d70\u5411\u4ed3\u5e93\u65f6\uff0c\u514b\u7f57\u4e3d\u79d1\u8def\u8fc7\u7eb3\u7f8e\u897f\u4e1d\u8eab\u8fb9\uff0c\u9732\u51fa\u4e86\u5632\u8bbd\u7684\u7b11\u5bb9\u3002\uff09",
    "\u54fc\uff0c\u7eb3\u7f8e\u897f\u4e1d\uff0c\u6562\u4e0d\u6562\u6765\u6bd4\u6bd4\u8c01\u642c\u5f97\u5feb\uff1f",
    "\u2026\u2026\u5206\u6563\u7684\u5149\u8292\uff0c\u65e0\u6cd5\u8fa8\u8bc6\u7684\u955c\u4e2d\u5f71\u50cf\u2026\u2026",
    "\u6211\u5e7c\u7a1a\uff1f\uff01\u6211\u770b\u4f60\u662f\u4e0d\u6562\u5427\uff1f\uff01",
    "\u2026\u2026\u65e2\u5b58\u4e8e\u4e16\u7684\u5f71\u50cf\uff0c\u91cd\u65b0\u51dd\u805a\u2026\u2026",
    "\u554a\u554a\u662f\u554a\u4f60\u6562\u90a3\u4f60\u6765\u554a\uff1f\u6bd4\u6bd4\u554a\uff1f\uff01",
    "\uff08\u4e24\u4eba\u5435\u95f9\u7740\u8d70\u5411\u4ed3\u5e93\u91cc\u7684\u7269\u8d44\u5806\uff0c\u5206\u522b\u625b\u4e86\u4e00\u4e2a\u8d85\u5927\u7684\u7bb1\u5b50\uff0c\u5411\u827e\u83ab\u53f7\u7684\u65b9\u5411\u8d70\u53bb\u3002\uff09",
    "\u2026\u2026\u6211\u4e5f\u53bb\u642c\u7269\u8d44\u3002",
    "\uff08\u6ca1\u6709\u7b49\u5f85\u6211\u7684\u7b54\u590d\uff0c\u5bc7\u5c14\u8299\u5f84\u81ea\u8ddf\u4e0a\u514b\u7f57\u4e3d\u79d1\u548c\u7eb3\u7f8e\u897f\u4e1d\u7684\u811a\u6b65\uff0c\u8d70\u5411\u4e86\u7269\u8d44\u5806\u3002\uff09",
    "\uff08\u770b\u7740\u5bc7\u5c14\u8299\u79bb\u53bb\u7684\u80cc\u5f71\uff0c\u8428\u73c8\u5fcd\u4e86\u5fcd\uff0c\u7ec8\u4e8e\u5fcd\u4e0d\u4f4f\u95ee\u51fa\u53e3\u3002\uff09",
    "\u4f60\u7684\u8fd9\u4e2a\u84dd\u5934\u53d1\u4eba\u5f62\uff0c\u5bc7\u5c14\u8299\u662f\u5427\uff0c\u5e73\u65f6\u4e5f\u8fd9\u6837\u5417\uff1f",
    "\u2026\u2026\u5e73\u65f6\u4e0d\u592a\u4e00\u6837\u2026\u2026\u662f\u53e6\u4e00\u79cd\u610f\u4e49\u4e0a\u7684\u4e0d\u592a\u4e00\u6837\u3002",
    "\u597d\u5427\u597d\u5427\u3002\u90a3\u7ef4\u666e\u857e\u7684\u95ee\u9898\uff1f",
    "\u5728\uff01",
    "\uff08\u542c\u5230\u81ea\u5df1\u7684\u540d\u5b57\uff0c\u7ef4\u666e\u857e\u5e94\u58f0\u4e3e\u8d77\u624b\uff0c\u548c\u8428\u73c8\u4e00\u8d77\u770b\u5411\u6211\u3002\uff09",
    "\u63a5\u4e0b\u6765\u7684\u8ba2\u5355\u6709\u70b9\u9ebb\u70e6\u2026\u2026\u6211\u8fd9\u91cc\u4e5f\u786e\u5b9e\u662f\u7f3a\u4eba\u624b\u3002\u4f46\u662f\u4e0d\u662f\u8981\u6765\u6211\u8fd9\u91cc\u3001\u662f\u4e0d\u662f\u8981\u4ece\u6211\u8fd9\u91cc\u79bb\u5f00\uff0c\u90fd\u8981\u770b\u7ef4\u666e\u857e\u81ea\u5df1\u7684\u60f3\u6cd5\u3002",
    "\u7ef4\u666e\u857e\uff0c\u4f60\u613f\u610f\u6765\u6211\u7684\u8f66\u961f\u5417\uff1f\u7d20\u4f53\u7ef4\u62a4\u6ca1\u95ee\u9898\uff0c\u6bcf\u5929\u8ba9\u4f60\u81ea\u7531\u5531\u6b4c\u867d\u7136\u505a\u4e0d\u5230\uff0c\u4f46\u4e5f\u4e0d\u4f1a\u6709\u592a\u591a\u9650\u5236\u3002",
    "\u597d~|\u7ef4\u666e\u857e\u3001\u5f00\u59cb\u65b0\u7684\u65c5\u9014\u2606",
    "\u8c22\u8c22\u60e0\u987e\uff0c\u4e00\u6839\u6676\u6761\u3002|\u554a\uff0c\u7279\u522b\u8bf4\u660e\uff0c\u4f5c\u4e3a\u6697\u5546\uff0c\u6211\u8fd8\u6ca1\u6709\u81ea\u5df1\u7684\u6676\u6761\u3002\u4f69\u59ae\u6cfd\u7684\u6676\u6761\u5728\u6211\u8fd9\u91cc\u662f\u80fd\u7528\u7684\u3002",
    "\u2026\u2026\u6ca1\u8bb0\u9519\u7684\u8bdd\uff0c\u60a8\u4e4b\u524d\u8bf4\u7684\u662f\u201c\u9644\u8d60\u201d\u3002",
    "\u6211\u53ef\u6ca1\u8bf4\u662f\u514d\u8d39\u9644\u8d60\uff0c\u4eba\u5f62\u6743\u9650\u7684\u8f6c\u79fb\u4e5f\u7a0d\u7a0d\u9700\u8981\u4e00\u70b9\u70b9\u6210\u672c\u4e0d\u662f\u2014\u2014",
    "\u6307\u6325\u5b98\uff0c\u4f60\u60f3\u60f3\uff0c\u867d\u7136\u7d20\u4f53\u635f\u8017\u7a0b\u5ea6\u6709\u70b9\u9ad8\uff0c\u4f46\u53ea\u7528\u4e00\u6839\u6676\u6761\uff0c\u5c31\u80fd\u83b7\u5f97\u4e00\u4e2a\u52a0\u88c5\u8fc7\u706b\u63a7\u6838\u5fc3\u7684\u6218\u672f\u4eba\u5f62\uff0c\u7ef4\u666e\u857e\u7acb\u523b\u5c31\u80fd\u6210\u4e3a\u4f60\u7684\u6709\u751f\u529b\u91cf\uff0c\u8fd9\u53ef\u662f\u975e\u5e38\u5212\u7b97\u7684\u3002",
    "\u4ee5\u53ef\u80fd\u5b58\u5728\u7684\u6df1\u5ea6\u7ef4\u62a4\u4e0e\u7d20\u4f53\u66f4\u6362\u4e3a\u524d\u63d0\uff1f",
    "\u2014\u2014\u8865\u5145\uff0c\u6211\u4f1a\u53e6\u5916\u7ed9\u4f60\u4ecb\u7ecd\u51e0\u4e2a\u8ba2\u5355\uff0c\u6027\u4ef7\u6bd4\u7edd\u5bf9\u4f1a\u6bd4\u4f60\u53bbB.R.I.E.F.\u90a3\u91cc\u63a5\u53d6\u7684\u8ba2\u5355\u8981\u9ad8\u5f88\u591a\u3002",
    "\u60a8\u7684\u884c\u4e3a\u6a21\u5f0f\u548c\u4f69\u59ae\u6cfd\u975e\u5e38\u76f8\u4f3c\u3002",
    "\u80fd\u548c\u4ee3\u884c\u4eba\u76f8\u4f3c\uff0c\u662f\u6211\u7684\u8363\u5e78\u3002",
    "\u2026\u2026\u597d\u5427\uff0c\u4e00\u6839\u6676\u6761\u3002\u7a0d\u540e\u62ff\u7ed9\u60a8\u3002",
    "\u6b22\u8fce\u4e0b\u6b21\u5149\u4e34\u3002",
    "\u7ef4\u666e\u857e\uff0c\u6709\u5730\u65b9\u5531\u6b4c\u4e86\uff0c\u5f00\u5fc3\u5417\uff1f",
    "\u5f00\u5fc3\uff01",
    "\u95ea\u7535\uff0c\u8054\u7cfb\u7f8e\u73b2\uff0c\u51c6\u5907\u4e00\u4e0b\u7ef4\u666e\u857e\u7684\u5f52\u5c5e\u767b\u8bb0\u548c\u7269\u8d44\u5206\u914d\u3002|\u6697\u8857\u7684\u9632\u536b\u2014\u2014",
    "\uff08\u4e0d\u8fdc\u5904\uff0c\u5bc7\u5c14\u8299\u91cd\u65b0\u625b\u8d77\u4e86\u4e00\u4e2a\u7269\u8d44\u7bb1\u3002\uff09",
    "\uff08\u4ece\u6211\u6240\u5728\u5730\u70b9\u770b\u8fc7\u53bb\uff0c\u5bc7\u5c14\u8299\u7684\u8138\u5b8c\u5168\u63a9\u85cf\u5728\u7269\u8d44\u7bb1\u7684\u9634\u5f71\u4e0b\uff0c\u9634\u5f71\u7684\u8fb9\u7f18\u6253\u5728\u5979\u7684\u4e0b\u988c\u4f4d\u7f6e\uff0c\u968f\u7740\u5979\u7684\u6301\u7eed\u79fb\u52a8\u7f13\u6162\u53d8\u5316\u3002\uff09",
    "\u5148\u4ea4\u7ed9\u4f60\u548c\u7ef4\u666e\u857e\u3002\u6267\u884c\u9632\u536b\u4efb\u52a1\u7684\u540c\u65f6\uff0c\u4e5f\u6ce8\u610f\u652f\u63f4\u514b\u7f57\u4e3d\u79d1\u5979\u4eec\u3002",
    "\u4e86\u89e3\u3002|\u7ef4\u666e\u857e\uff0c\u8ddf\u6211\u6765\u3002",
    "\u6f14\u51fa\u3001\u548c\u961f\u957f\u4e00\u8d77\u2606",
    "\u4e0d\u662f\u6f14\u51fa\u3002\u7d20\u4f53\u72b6\u51b5\u548c\u5f39\u836f\u4f59\u91cf\u6b63\u5e38\u5417\uff1f",
    "\u4e00\u5207\u6b63\u5e38\uff0c\u7ef4\u666e\u857e\u3001\u968f\u65f6STAND BY\uff01",
    "\uff08\u8ddf\u5728\u95ea\u7535\u540e\u9762\uff0c\u4e24\u4eba\u5411\u5165\u53e3\u7684\u5347\u964d\u68af\u4f4d\u7f6e\u8d70\u53bb\u3002\uff09",
    "\u79bb\u7269\u8d44\u642c\u8fd0\u5b8c\u6210\u8fd8\u65e9\u3002\u6307\u6325\u5b98\uff0c\u4f11\u606f\u4e00\u4e0b\uff1f|\u65f6\u95f4\u4e0a\u4e5f\u8be5\u5403\u665a\u996d\u4e86\uff0c\u8981\u4e0d\u8981\u4e00\u8d77\u6765\uff1f\u867d\u7136\u53e3\u611f\u4e0d\u80fd\u4fdd\u8bc1\uff0c\u5403\u9971\u8fd8\u662f\u6ca1\u95ee\u9898\u7684\uff1f",
    "\u2026\u2026\u8fd9\u6b21\u662f\u771f\u7684\u514d\u8d39\u5427\uff1f",
    "\u522b\u62c5\u5fc3\u3002",
    "\uff08\u5341\u5206\u949f\u540e\u2014\u2014\uff09",
    "\uff08\u5728\u8428\u73c8\u7684\u5e26\u9886\u4e0b\uff0c\u6211\u4eec\u6765\u5230\u4e86\u5979\u7684\u644a\u4f4d\u524d\u3002\uff09",
    "\u5750\u5427\uff0c\u6211\u53bb\u62ff\u665a\u996d\u3002",
    "\u8c22\u8c22 \u3002",
    "\uff08\u6211\u5750\u5728\u644a\u4f4d\u4e00\u4fa7\u7684\u6905\u5b50\u4e0a\uff0c\u73af\u89c6\u56db\u5468\u3002\uff09",
    "\uff08\u8fd9\u4e00\u5904\u5730\u4e0b\u6697\u8857\u4e3b\u8981\u4ee5\u706f\u5149\u7167\u660e\uff0c\u5404\u4e2a\u644a\u4f4d\u65c1\u5927\u591a\u4eae\u7740\u706f\u3002\uff09",
    "\uff08\u661f\u661f\u70b9\u70b9\u7684\u706f\u5149\u88c5\u9970\u7740\u660f\u6697\u7684\u73af\u5883\uff0c\u5e7f\u9614\u7684\u5730\u4e0b\u7a7a\u95f4\u4eff\u4f5b\u88ab\u706f\u5149\u5206\u9694\u6210\u6570\u4e2a\u5927\u5c0f\u4e0d\u7b49\u7684\u533a\u5757\u3002\uff09",
    "\u6765\u4e86\uff0c\u8fd9\u662f\u201c\u739b\u742a\u201d\uff0c\u6211\u4eec\u8fd9\u91cc\u7684\u7279\u4ea7\u3002",
    "\uff08\u8428\u73c8\u7aef\u7740\u4e00\u76d8\u770b\u8d77\u6765\u50cf\u662f\u70e4\u8089\u6761\u7684\u4e1c\u897f\u653e\u5728\u4e86\u6211\u9762\u524d\u3002\uff09",
    "\u8089\uff1f\u8fd9\u4e0d\u4f1a\u662f\u571f\u8c46\u505a\u7684\u5427\uff1f",
    "\u4e0d\u662f\u571f\u8c46\uff0c\u4f60\u5c1d\u5c1d\u3002",
    "\u597d\u7684\u3002",
    "\uff08\u6211\u62ff\u8d77\u4e86\u4e00\u4e2a\u8089\u6761\u201c\u739b\u742a\u201d\uff0c\u585e\u8fdb\u5634\u91cc\u3002\uff09",
    "\uff08\u8089\u6761\u7684\u53e3\u611f\u975e\u5e38\u5947\u5999\uff0c\u5916\u5c42\u5728\u98ce\u5e72\u4e4b\u540e\u5f88\u6709\u97e7\u6027\uff0c\u4f46\u5df2\u7ecf\u88ab\u70e4\u8106\u4e86\u3002\u54ac\u4e0b\u53bb\u4e4b\u540e\uff0c\u5916\u5c42\u88c2\u5f00\uff0c\u88ab\u5305\u88f9\u5728\u91cc\u9762\u7684\u5185\u9985\u5f88\u5feb\u878d\u5316\uff0c\u6709\u4e9b\u50cf\u9a6c\u82cf\u91cc\u62c9\u5976\u916a\u3002\uff09",
    "\u5473\u9053\u600e\u4e48\u6837\uff1f",
    "\u8fd8\u4e0d\u9519\u3002|\u80fd\u7528\u8089\u7c7b\u4f5c\u4e3a\u665a\u9910\uff0c\u8fd1\u671f\u7684\u6548\u76ca\u4e0d\u9519\uff1f",
    "\u8fd9\u90fd\u8981\u611f\u8c22\u6c61\u67d3\u533a\u5f00\u62d3\u8ba1\u5212\u3002",
    "\u6211\u8fd8\u4ee5\u4e3a\u8fd9\u4e2a\u8ba1\u5212\u4f1a\u5bfc\u81f4\u4f60\u4eec\u5229\u6da6\u964d\u4f4e\uff1f",
    "\u8fd9\u5012\u6ca1\u6709\u3002\u51c0\u5316\u533a\u7684\u63a8\u8fdb\u7f29\u51cf\u4e86\u6c61\u67d3\u533a\u539f\u4f4f\u6c11\u4eec\u7684\u6d3b\u52a8\u8303\u56f4\uff0c\u867d\u7136\u751f\u9ab8\u662f\u8d8a\u6765\u8d8a\u6d3b\u8dc3\u4e86\u2026\u2026\u4f46\u7269\u8d44\u4ef7\u683c\u4e0a\u6da8\uff0c\u6211\u4eec\u90fd\u8d5a\u4e86\u4e0d\u5c11\u3002",
    "\uff08\u8bf4\u8bdd\u95f4\uff0c\u6211\u5403\u5b8c\u4e86\u4e00\u4e2a\u201c\u739b\u742a\u201d\u3002\uff09",
    "\u6709\u6ca1\u6709\u8fd9\u4e2a\u201c\u739b\u742a\u201d\u7684\u5236\u4f5c\u65b9\u5f0f\uff1f\u53ef\u4ee5\u7684\u8bdd\uff0c\u6211\u60f3\u5e26\u56de\u53bb\u7ed9\u6211\u7684\u961f\u957f\u5236\u4f5c\u8bd5\u8bd5\u3002",
    "\u6211\u8fd8\u4ee5\u4e3a\u4f60\u4f1a\u5403\u4e0d\u60ef\u3002",
    "\u8fd9\u4e0d\u5c31\u662f\u666e\u901a\u7684\u8089\u7c7b\uff1f",
    "\u867d\u7136\u80fd\u88ab\u5f52\u4e3a\u201c\u8089\u7c7b\u201d\uff0c\u4f46\u539f\u6750\u6599\u5e76\u4e0d\u666e\u901a\u3002",
    "\u8fd9\u51e0\u5e74\uff0c\u6211\u4eec\u8fd9\u4e2a\u533a\u57df\u53d1\u73b0\u4e86\u4e00\u79cd\u751f\u6d3b\u5728\u5730\u4e0b\u7684\u866b\u5b50\u3002\u5b83\u5bf9\u574d\u584c\u8f90\u5c04\u6709\u4e00\u5b9a\u6297\u6027\uff0c\u4f4f\u6c11\u4eec\u90fd\u76f8\u4fe1\u591a\u5403\u8fd9\u79cd\u866b\u5b50\uff0c\u80fd\u591f\u66f4\u597d\u5730\u62b5\u6297\u574d\u584c\u8f90\u5c04\u3002",
    "\u201c\u739b\u742a\u201d\u5c31\u662f\u4ee5\u5b83\u4e3a\u539f\u6750\u6599\u5236\u4f5c\u7684\uff0c\u5728\u6211\u4eec\u8fd9\u91cc\u5f88\u53d7\u6b22\u8fce\uff0c\u4e0d\u8fc7\u5927\u591a\u6570\u5916\u6765\u4eba\u5458\u90fd\u89c9\u5f97\u53e3\u611f\u5947\u7279\uff0c\u4e0d\u592a\u80fd\u63a5\u53d7\u3002",
    "\u2026\u2026\u866b\u5b50\uff1f",
    "\u662f\u7684\u3002\u5982\u679c\u4f60\u559c\u6b22\uff0c\u4ef7\u683c\u662f\u2014\u2014",
    "\u4e0d\u7528\u4e86\uff0c\u8c22\u8c22\u3002",
    "\u597d\u5427\u3002",
    "\u611f\u8c22\u62db\u5f85\u3002\u65f6\u95f4\u5dee\u4e0d\u591a\u4e86\uff0c\u6211\u53bb\u62ff\u6676\u6761\u3002|\u7ef4\u666e\u857e\u7684\u6240\u5c5e\u767b\u8bb0\u548c\u6743\u9650\u4ea4\u63a5\u9700\u8981\u5728\u6676\u6761\u4ea4\u5230\u4f60\u624b\u4e2d\u4e4b\u540e\uff1f",
    "\u611f\u8c22\u7406\u89e3\u3002|\u6162\u8d70\uff0c\u76f4\u884c\u5de6\u62d0\u90a3\u6761\u8def\u706f\u5149\u5206\u5e03\u6bd4\u8f83\u5bc6\u96c6\u3002",
    "\uff08\u6211\u7ad9\u8d77\u6765\uff0c\u5411\u8428\u73c8\u70b9\u4e86\u70b9\u5934\uff0c\u51c6\u5907\u56de\u5230\u827e\u83ab\u53f7\u3002\uff09",
    "\uff08\u4e0e\u6b64\u540c\u65f6\uff0c\u6697\u8857\u7684\u5165\u53e3\u4f4d\u7f6e\u4f20\u6765\u4e00\u58f0\u5de8\u54cd\uff0c\u6d53\u70df\u548c\u706b\u5149\u968f\u7740\u5de8\u54cd\u70b9\u4eae\u4e86\u5165\u53e3\u9644\u8fd1\u7684\u7a7a\u95f4\u2014\u2014\uff09",
    "\u88c5\u5907\u5462\uff0c\u7269\u8d44\u5462\uff0c\u90fd\u7ed9\u8001\u5b50\u62ff\u51fa\u6765\uff01",
    "\u2026\u2026\u74e6\u826f\u683c\u5e2e\uff1f"
  ]
]