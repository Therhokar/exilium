[
  [
    "\u6362\u88c5\u5df2\u5b8c\u6210\uff0c\u5c0f\u961f\u5df2\u7ecf\u5230\u8fbe\u6307\u5b9a\u4f4d\u7f6e\u3002",
    "\u62a5\u544a\u5f02\u5e38\u3002",
    "\u76ee\u524d\u65e0\u5f02\u5e38\u3002\u5404\u4f5c\u6218\u5355\u4f4d\u7d20\u4f53\u65e0\u635f\u4f24\uff0c\u5f39\u836f\u5e73\u5747\u635f\u8017\u7ea6\u572812%\u3002\u201c\u5f92\u4ece\u201d\u7684\u670d\u88c5\u80fd\u591f\u906e\u76d6\u5927\u90e8\u5206\u4f53\u8868\u7279\u5f81\uff0c\u6f5c\u5165\u884c\u52a8\u6b63\u5e38\u8fdb\u884c\u4e2d\u3002",
    "\u4ee5\u83b7\u53d6\u4fe1\u606f\u4e3a\u9996\u8981\u6761\u4ef6\u3002\u57fa\u5730\u8f66\u4f4d\u7f6e\u5750\u6807\u7f8e\u73b2\u7a0d\u540e\u4f1a\u53d1\u9001\u7ed9\u4f60\u4eec\uff0c\u9047\u5230\u5371\u9669\u5c3d\u5feb\u64a4\u9000\u3002",
    "\u6211\u4f1a\u8c28\u614e\u5224\u65ad\u3002",
    "\u4e00\u770b\u5230\u90a3\u79cd\u5947\u602a\u7684\u5854\u5f62\u5efa\u7b51\uff0c\u6d77\u4f26\u5a1c\u5c31\u575a\u6301\u8981\u53bb\u2026\u2026\u6211\u5df2\u7ecf\u53ee\u5631\u5979\u542c\u4f60\u7684\u547d\u4ee4\uff0c\u63a7\u5236\u597d\u81ea\u5df1\u7684\u9632\u5fa1\u7cfb\u7edf\uff0c\u4e0d\u8981\u8d38\u7136\u542f\u52a8\u3002\u4f60\u4eec\u884c\u52a8\u65f6\u6ce8\u610f\u4fdd\u8bc1\u6d77\u4f26\u5a1c\u7684\u5b89\u5168\u3002",
    "\u5b9e\u9645\u63a5\u89e6\u8fd9\u4e9b\u5efa\u7b51\uff0c\u6d77\u4f26\u5a1c\u4e5f\u8bb8\u80fd\u591f\u56de\u60f3\u8d77\u66f4\u591a\u4fe1\u606f\u3002",
    "\u55ef\uff0c\u6211\u4e5f\u8fd9\u4e48\u60f3\u3002\u628a\u5979\u5b89\u5168\u5e26\u56de\u6765\uff0c\u95ea\u7535\u3002",
    "\u4e86\u89e3\u3002",
    "\u867d\u7136\u4f7f\u7528\u7684\u662f\u7f8e\u73b2\u8bbe\u7f6e\u7684\u52a0\u5bc6\u7ebf\u8def\uff0c\u4e3a\u4e86\u9632\u6b62\u51fa\u73b0\u4e4b\u524d\u7684\u60c5\u51b5\uff0c\u6211\u4f1a\u6682\u65f6\u65ad\u5f00\u901a\u8baf\u3002|\u6ce8\u610f\u4fdd\u8bc1\u5b9a\u65f6\u8054\u7edc\u3002\u5b89\u5168\u4fe1\u53f722\uff0c\u6c42\u63f449\u3002\u6536\u5230\u6c42\u63f4\u4fe1\u53f7\u540e\uff0c\u6211\u4f1a\u6d3e\u65e0\u4eba\u673a\u8fc7\u6765\u3002",
    "\u597d\u3002",
    "\uff08\u95ea\u7535\u6572\u4e86\u6572\u81ea\u5df1\u7684\u901a\u8baf\u7ec8\u7aef\uff0c\u65ad\u5f00\u4e86\u901a\u8baf\u3002\uff09",
    "\uff08\u636e\u70b9\u4e2d\u4eba\u7fa4\u7199\u6518\uff0c\u5f92\u4ece\u4eec\u5927\u591a\u8eab\u7a7f\u957f\u888d\uff0c\u6210\u7fa4\u7ed3\u961f\u3002\uff09",
    "\uff08\u95ea\u7535\u5c0f\u961f\u7a7f\u7740\u4e0e\u5f92\u4ece\u4eec\u76f8\u4f3c\u7684\u957f\u888d\uff0c\u6df7\u5728\u4ed6\u4eec\u4e4b\u4e2d\uff0c\u4e00\u8d77\u7f13\u6162\u5730\u5411\u7740\u4e0d\u8fdc\u5904\u6700\u5927\u7684\u5c16\u9876\u5efa\u7b51\u8d70\u53bb\u3002\uff09",
    "\uff08\u8d70\u5728\u4e2d\u95f4\u4f4d\u7f6e\u7684\u6d77\u4f26\u5a1c\u65f6\u4e0d\u65f6\u505c\u4e0b\u811a\u6b65\uff0c\u770b\u7740\u9010\u6e10\u63a5\u8fd1\u7684\u5854\u578b\u5efa\u7b51\u53d1\u5446\u3002\uff09",
    "\uff08\u540e\u65b9\u7684\u514b\u7f57\u4e3d\u79d1\u88ab\u515c\u5e3d\u906e\u6321\u4e86\u89c6\u7ebf\uff0c\u4e0d\u5c0f\u5fc3\u649e\u5728\u4e86\u6d77\u4f26\u5a1c\u8eab\u4e0a\u3002\uff09",
    "\u5567\uff0c\u8d70\u554a\uff0c\u7ad9\u7740\u4e0d\u52a8\u5e72\u4ec0\u4e48\uff1f",
    "\u2026\u2026",
    "\u524d\u9762\u7684\u5012\u4e09\u89d2\u5c16\u9876\u5efa\u7b51\uff0c\u662f\u201c\u6ce2\u6797\u5361\u201d\u5417\uff0c\u6d77\u4f26\u5a1c\uff1f",
    "\u4e0d\u662f\uff01\u6ce2\u6797\u5361\u3001\u8981\u597d\u5927\u2026\u2026\u5514\u2026\u2026",
    "\u662f\u900f\u660e\u3001\u5c16\u5c16\u7684\u3001\u6ce2\u6797\u5361\u2026\u2026",
    "\u900f\u660e\u7684\u3001\u5c16\u5c16\u7684\uff0c\u540d\u5b57\u5f88\u76f8\u4f3c\u7684\u4e1c\u897f\uff0c\u7ef4\u666e\u857e\u4ee5\u524d\u89c1\u8fc7\u7684\u54e6\uff01",
    "\uff08\u6700\u524d\u65b9\u7684\u7ef4\u666e\u857e\u56de\u5934\uff0c\u4f38\u51fa\u98df\u6307\u70b9\u4e86\u70b9\u989d\u5934\u3002\uff09",
    "\u4f60\u89c1\u8fc7\uff1f",
    "\u6ca1\u9519\uff01\u5514\u2014\u2014\u8ba9\u7ef4\u666e\u857e\u60f3\u4e00\u60f3\u2026\u2026",
    "\u554a\uff01\u5bf9\u4e86\uff0c\u662f\u5728\u300a\u65e9\u6668\u7684\u68a6\u60f3\u2606TIME\uff01\u300b\u7684\u9996\u6f14\u73b0\u573a\u2606\u96ea\u5c71\u8d85\u7ea7\u6f02\u4eae\u7684\uff01\u573a\u5730\u8d44\u6599\u91cc\u8bf4\uff0c\u96ea\u5c71\u4e0b\u7684\u73bb\u7483\u623f\u5b50\u91cc\u7684\u5c31\u662f\u201c\u683c\u5229\u5361\u201d\u54e6\uff01\u4e0d\u8fc7\u597d\u9057\u61be\uff0c\u6f14\u51fa\u6ca1\u6709\u987a\u5229\u8fdb\u884c\u5566\u2026\u2026\u5927\u5bb6\u8981\u4e0d\u8981\u542c\uff01\u6b4c\u8bcd\u548c\u52a8\u4f5c\u7ef4\u666e\u857e\u90fd\u8bb0\u5f97\uff0c\u73b0\u5728\u5c31\u80fd\u5531\uff01",
    "\u5531\u4ec0\u4e48\u554a\u2014\u2014\u7ef4\u666e\u857e\uff0c\u6211\u4eec\u73b0\u5728\u662f\u5728\u6267\u884c\u4efb\u52a1\uff01\u683c\u5229\u5361\uff0c\u90a3\u662f\u690d\u7269\u7684\u540d\u5b57\u5427\uff01",
    "\u662f\u5417\u662f\u5417\uff1f\u5514\uff0c\u539f\u6765\u8fd8\u4f1a\u6709\u8fd9\u79cd\u53d6\u540d\u65b9\u5f0f\u2026\u2026",
    "\u2026\u2026\u7fa4\u5c71\u4e0e\u6fc0\u6d41\uff0c\u904d\u5386\u6d77\u6d0b\u2026\u2026",
    "\u4e0d\u8981\u968f\u968f\u4fbf\u4fbf\u5b66\u4e60\u554a\u7eb3\u7f8e\u897f\u4e1d\uff01\u5e2e\u4f60\u7ffb\u8bd1\u5df2\u7ecf\u591f\u70e6\u4e86\u522b\u7ed9\u6211\u589e\u52a0\u5de5\u4f5c\u96be\u5ea6\u597d\u4e0d\u597d\uff1f",
    "\u54e6\u54e6\uff01\u5c0f\u7eb3\u7f8e\u4e5f\u89c9\u5f97\u5f88\u6709\u610f\u601d\u5bf9\u4e0d\u5bf9\uff1f\u7ef4\u666e\u857e\u2606\u4e5f\u8981\u5b66\u4e60\uff01",
    "\u5c0f\u7eb3\u7f8e\uff1f\u4f60\u90a3\u662f\u4ec0\u4e48\u53eb\u6cd5\uff1f",
    "\u201c\u7eb3\u7f8e\u897f\u4e1d\u201d\u8fd9\u4e2a\u540d\u5b57\u592a\u957f\u4e86\u5566\uff0c\u53eb\u8d77\u6765\u5c31\u5f88\u4e0d\u53ef\u7231\u2026\u2026\u201c\u5c0f\u7eb3\u7f8e\u201d\u4e0d\u662f\u4f1a\u66f4\u53ef\u7231\u5417\uff0c\u97f3\u8282\u77ed\u4e00\u4e9b\uff0c\u89c2\u4f17\u4e5f\u80fd\u66f4\u5feb\u8bb0\u4f4f\u54df\uff01\u5514\u2026\u2026\u201c\u5c0f\u514b\u7f57\u201d\u3001\u4e5f\u5f88\u53ef\u7231\u2606",
    "\u5c0f\u3001\u5c0f\u514b\u7f57\uff1f\uff01",
    "\u5618\uff01\u4f60\u4eec\u5b89\u9759\u4e00\u70b9\u2014\u2014\u8981\u88ab\u53d1\u73b0\u4e86\uff01",
    "\uff08\u51e0\u4eba\u7acb\u523b\u4f4e\u4e0b\u5934\uff0c\u8bd5\u56fe\u9690\u85cf\u81ea\u5df1\u3002\uff09",
    "\uff08\u2014\u2014\u53ea\u6709\u6d77\u4f26\u5a1c\u4f8b\u5916\u3002\u73bb\u7483\u5c16\u9876\u53cd\u5c04\u51fa\u7684\u67d4\u548c\u5149\u8292\uff0c\u5728\u6d77\u4f26\u5a1c\u7684\u773c\u4e2d\u71a0\u71a0\u751f\u8f89\u3002\uff09",
    "\u2026\u2026\u201c\u8001\u5e08\u201d\u2026\u2026",
    "\uff08\u7ad9\u5728\u6d77\u4f26\u5a1c\u8eab\u540e\uff0c\u514b\u7f57\u4e3d\u79d1\u4e00\u5df4\u638c\u62cd\u4e0a\u6d77\u4f26\u5a1c\u7684\u5934\u3002\uff09",
    "\u4f4e\u5934\uff0c\u542c\u5230\u6ca1\u6709\uff0c\u6d77\u4f26\u5a1c\uff01",
    "\u4f60\u624d\u662f\u5e94\u8be5\u4f4e\u5934\u7684\u5427\uff01\u515c\u5e3d\u90fd\u88ab\u4f60\u7684\u8033\u6735\u6491\u8d77\u6765\u4e86\uff01",
    "\u8fd9\u80fd\u602a\u6211\uff1f\u8c01\u77e5\u9053\u8fd9\u8863\u670d\u8fd8\u6709\u5e3d\u5b50\u554a\uff1f\uff01",
    "\u2026\u2026\u4ee5\u6c38\u4e45\u4e0d\u6613\u7684\u754c\u7891\uff0c\u8d5e\u9882\u575a\u56fa\u7684\u987d\u77f3\u2026\u2026",
    "\u4e0d\u80fd\u6458\u6389\u7684\u597d\u5427\uff1f\uff01",
    "\u55ef\u55ef\u55ef\uff01\u7ef4\u666e\u857e\u3001\u660e\u767d\u7684\u54e6\uff01\u5154\u8033\u662f\u5c0f\u514b\u7f57\u6709\u4eba\u6c14\u7684\u79d8\u8bc0\u2606\u6ca1\u6709\u5154\u8033\uff0c\u5c0f\u514b\u7f57\u5c31\u548c\u522b\u7684\u5076\u50cf\u4e00\u6837\u4e86\uff0c\u8fd9\u6837\u7edd\u5bf9\u4e0d\u53ef\u4ee5\uff01",
    "\uff08\u7ef4\u666e\u857e\u6084\u6084\u8f6c\u5934\uff0c\u5411\u514b\u7f57\u4e3d\u79d1\u4f38\u51fa\u4e86\u5927\u62c7\u6307\u3002\uff09",
    "\u522b\u7528\u8fd9\u4e2a\u540d\u5b57\u53eb\u6211\u800c\u4e14\u8fd9\u4e0d\u662f\u5154\u8033\u800c\u4e14\u4e0d\u8981\u628a\u4f60\u90a3\u4e00\u5957\u653e\u5728\u6211\u8eab\u4e0a\u554a\uff1f\uff01",
    "\u5b89\u9759\uff01",
    "\uff08\u5904\u4e8e\u961f\u4f0d\u524d\u65b9\u3001\u4ed4\u7ec6\u89c2\u5bdf\u5468\u56f4\u73af\u5883\u7684\u95ea\u7535\u7ec8\u4e8e\u5fcd\u8010\u4e0d\u4f4f\uff0c\u5979\u4fa7\u8fc7\u5934\uff0c\u63a5\u5165\u4e86\u4e8c\u7ea7\u5e73\u5c42\u3002\uff09"
  ],
  [
    "\u4fdd\u6301\u5b89\u9759\uff0c\u4e0d\u8981\u5f15\u8d77\u5176\u4ed6\u4eba\u6ce8\u610f\u3002\u5982\u679c\u9700\u8981\u6c9f\u901a\uff0c\u4f7f\u7528\u4e8c\u7ea7\u5e73\u5c42\u901a\u4fe1\u3002",
    "\u54e6\u2014\u2014\u54e6\uff01\u4e8c\u7ea7\u5e73\u5c42\u901a\u4fe1\uff1f\u8fd8\u80fd\u8fd9\u6837\uff1f",
    "\u55ef\u3002\u961f\u4f0d\u505c\u4e0b\u6765\u4e86\u3002\u6ce8\u610f\u8b66\u6212\u3002",
    "\u8fd9\u662f\u5728\u6392\u961f\u5427\uff1f\u6392\u961f\u8fdb\u90a3\u4e2a\u957f\u5f97\u50cf\u4e2a\u6f0f\u6597\u7684\u5efa\u7b51\u91cc\uff1f\u90a3\u4e2a\u51c0\u5316\u4eea\u5f0f\u5c31\u5728\u91cc\u8fb9\uff1f",
    "\u8fd8\u4e0d\u80fd\u5b8c\u5168\u786e\u5b9a\uff0c\u4f46\u5b9a\u4f4d\u56de\u4f20\u88c5\u7f6e\u663e\u793a\u7684\u4f4d\u7f6e\u5c31\u5728\u8fd9\u4e2a\u533a\u57df\u5185\u3002\u4ee5\u5b83\u7684\u89c4\u6a21\u548c\u5916\u5f62\u8003\u8651\uff0c\u6709\u5f88\u5927\u7684\u53ef\u80fd\u3002",
    "\u800c\u4e14\u5468\u56f4\u6392\u961f\u7684\u5927\u5bb6\u770b\u8d77\u6765\u90fd\u662f\u8ddf\u7740\u81ea\u5df1\u7684\u9886\u961f\u54e6\u2014\u2014\u5514\uff0c\u9886\u961f\u4eec\u957f\u5f97\u90fd\u5f88\u50cf\u4e4b\u524d\u6211\u4eec\u89c1\u8fc7\u7684\u201c\u59ae\u7279\u201d\u5462\uff01",
    "\u4e0d\u6b62\u662f\u50cf\u5427\u90a3\u5c31\u662f\u554a\uff01\u201c\u59ae\u7279\u201d\u662f\u91cf\u4ea7\u578b\u53f7\uff1f",
    "\u59ae\u7279No.249\uff0c\u548c\u5b83\u4e00\u6837\u7684\u6709248\u4e2a\u4ee5\u4e0a\uff0c\u514b\u7f57\u4e3d\u79d1\u4f60\u4e4b\u524d\u4e0d\u662f\u77e5\u9053\u7684\u5417\uff1f",
    "\u6211\u77e5\u9053\uff01\u7ad9\u8fd9\u91cc\u4ec0\u4e48\u4e5f\u4e0d\u5e72\u7b49\u592a\u4e45\u4e86\u4e0d\u968f\u4fbf\u8bf4\u70b9\u4ec0\u4e48\u5f88\u70e6\u7684\u597d\u5427\u2014\u2014\u5bc7\u5c14\u8299\uff0c\u6211\u600e\u4e48\u89c9\u5f97\u4f60\u56de\u6765\u4e4b\u540e\u8bf4\u8bdd\u98ce\u683c\u4e5f\u53d8\u4e86\u8fd8\u5c31\u9488\u5bf9\u6211\uff1f",
    "\u6211\u3001\u6211\u6ca1\u6709\u9488\u5bf9\u4f60\u2014\u2014"
  ],
  [
    "\u4f60\u4ee5\u524d\u90a3\u79cd\u5047\u5230\u4e0d\u884c\u7684\u8bed\u6c14\u54ea\u53bb\u4e86\uff1f",
    "\u5b89\u9759\uff01",
    "\u4f7f\u7528\u4e8c\u7ea7\u5e73\u5c42\u901a\u4fe1\u7684\u65f6\u5019\u4e0d\u80fd\u505c\u6ede\u5728\u539f\u5730\uff0c\u6ce8\u610f\u80a2\u4f53\u52a8\u4f5c\u2026\u2026\u5982\u679c\u4e0d\u4e60\u60ef\u7528\uff0c\u90a3\u5c31\u4e0d\u8981\u7528\u4e86\uff0c\u66f4\u4e0d\u8981\u7528\u5b83\u804a\u5929\u3002",
    "\u5404\u4f4d\uff0c\u6211\u4eec\u73b0\u5728\u5728\u4efb\u52a1\u4e2d\u3002",
    "\uff08\u9065\u8fdc\u7684\u54cd\u58f0\u6253\u65ad\u4e86\u51e0\u4eba\u7684\u5bf9\u8bdd\u3002\uff09",
    "\uff08\u968f\u7740\u54cd\u58f0\uff0c\u5c16\u9876\u5efa\u7b51\u4e0a\u6d41\u8f6c\u7684\u8f89\u5149\u95ea\u70c1\u4e86\u4e00\u4e0b\uff0c\u4ece\u539f\u5148\u67d4\u548c\u7684\u767d\u8272\u8f6c\u53d8\u4e3a\u4e86\u7565\u5e26\u950b\u5229\u611f\u7684\u94f6\u8272\u3002\uff09",
    "\uff08\u770b\u5230\u5149\u8292\u7684\u53d8\u5316\uff0c\u5468\u56f4\u7684\u4eba\u7fa4\u505c\u4e86\u4e0b\u6765\uff0c\u6446\u51fa\u4e86\u53cc\u624b\u4ea4\u53c9\u7684\u59ff\u52bf\uff0c\u53c8\u90fd\u62ff\u51fa\u4e86\u4e00\u4e2a\u88c5\u5728\u900f\u660e\u5bb9\u5668\u91cc\u7684\u4e1c\u897f\uff0c\u6367\u5728\u624b\u91cc\uff0c\u9ad8\u4e3e\u8fc7\u5934\u3002\uff09",
