    "\uff08\u961f\u4f0d\u518d\u6b21\u505c\u4e86\u4e0b\u6765\u3002\uff09",
    "\uff08\u5927\u91cf\u7684\u5f92\u4ece\u805a\u96c6\u4e86\u8fc7\u6765\uff0c\u4f46\u56e0\u4e3a\u5efa\u7b51\u8fc7\u9ad8\uff0c\u6d8c\u52a8\u7684\u4eba\u7fa4\u5e76\u6ca1\u6709\u5c06\u5927\u5385\u7684\u7a7a\u65f7\u611f\u51cf\u5f31\u534a\u5206\u3002\uff09",
    "\uff08\u51e0\u4eba\u5c0f\u5fc3\u89c2\u5bdf\u7740\u5468\u56f4\u7684\u73af\u5883\uff0c\u89c2\u5bdf\u7740\u90a3\u4e2a\u5de8\u5927\u7684\u96d5\u5851\u3002\uff09",
    "\uff08\u8bf4\u662f\u96d5\u5851\uff0c\u5176\u5b9e\u5e76\u4e0d\u786e\u5207\u3002\u5b83\u7684\u5916\u58c1\u7531\u7834\u65e7\u7684\u7ba1\u98ce\u7434\u7ba1\u9053\u6784\u6210\uff0c\u9876\u7aef\u88c5\u9970\u7740\u4e00\u4e2a\u505a\u5de5\u62d9\u52a3\u7684\u7269\u54c1\uff0c\u770b\u4e0d\u51fa\u5177\u4f53\u7684\u5f62\u8c8c\u3002\uff09",
    "\u2026\u2026\u6811\u4e1b\u9ed1\u6697\uff0c\u71c3\u8d77\u901a\u5f80\u9ece\u660e\u7684\u706b\u7130\u2026\u2026",
    "\u90a3\u662f\u4f2f\u4ecb\u59c6\uff1f\u600e\u4e48\u53ef\u80fd\uff1f\u4f60\u662f\u8bf4\u5b83\u7684\u539f\u578b\u662f\u4f2f\u4ecb\u59c6\u5427\uff1f",
    "\uff08\u7a79\u9876\u900f\u51fa\u7684\u5149\u7ebf\u7167\u5c04\u5728\u5b83\u62d9\u52a3\u7684\u5916\u5f62\u4e0a\uff0c\u5149\u7ebf\u968f\u7740\u7a7a\u6c14\u4e2d\u6643\u52a8\u7684\u5c18\u57c3\uff0c\u8f7b\u67d4\u5730\u629a\u6478\u96d5\u5851\u7684\u5916\u8868\uff0c\u7ed9\u8fd9\u4e2a\u770b\u8d77\u6765\u975e\u5e38\u53e4\u602a\u800c\u4e11\u964b\u7684\u9020\u7269\u589e\u6dfb\u4e86\u4e00\u4e1d\u8be1\u5f02\u7684\u795e\u5723\u611f\u3002\uff09",
    "\u2026\u2026\uff01",
    "\uff08\u96d5\u5851\u65c1\u8fb9\u4e0d\u8fdc\uff0c\u6709\u4e2a\u7a7f\u7740\u957f\u888d\u3001\u5916\u89c2\u770b\u8d77\u6765\u4e0e\u5176\u4ed6\u4eba\u975e\u5e38\u76f8\u4f3c\u7684\u5f92\u4ece\u3002\uff09",
    "\u662f\u8d6b\u745e\u5c24\u2014\u2014\u961f\u957f\uff01",
    "\u554a\uff01\u771f\u7684\u662f\uff01\u5bc7\u5c14\u8299\u597d\u5389\u5bb3\u2014\u2014",
    "\u5bc7\u5c14\u8299\uff0c\u522b\u52a8\uff01",
    "\u2026\u2026\u6211\u53ea\u9700\u8981\u4e00\u5206\u949f\uff01\u6211\u4f1a\u5c0f\u5fc3\u7684\u2026\u2026\u6211\u8fc7\u53bb\uff0c\u6ce8\u5c04\u836f\u5242\u53ea\u8981\u4e00\u5206\u949f\u2014\u2014\u7edd\u5bf9\u4e0d\u4f1a\u88ab\u53d1\u73b0\uff01",
    "\u73b0\u5728\u6211\u4eec\u5468\u8fb9\u6ca1\u6709\u6d3b\u52a8\u5bf9\u8c61\uff0c\u8d38\u7136\u884c\u52a8\u76ee\u6807\u592a\u5927\u4e86\uff0c\u5bc7\u5c14\u8299\uff01",
    "\u2026\u2026",
    "\uff08\u73af\u987e\u56db\u5468\uff0c\u95ea\u7535\u626f\u4e86\u626f\u515c\u5e3d\uff0c\u76d6\u4f4f\u4e86\u81ea\u5df1\u7684\u8138\u3002\uff09",
    "\u51c6\u5907\u63a5\u5165\u4e8c\u7ea7\u5e73\u5c42\uff0c\u63a7\u5236\u59ae\u7279\u3002|\u6211\u4eec\u4e0d\u80fd\u884c\u52a8\uff0c\u5c31\u8ba9\u4ed6\u4eec\u8fc7\u6765\uff01"
  ]
]