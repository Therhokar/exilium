    "\u4f46\u9a6c\u4e0a\u5c31\u8981\u8003\u8651\u5206\u914d\u4e86\uff0c\u53bb\u665a\u4e86\u8bf4\u4e0d\u5b9a\u7f8e\u73b2\u5c31\u4f1a\u62ff\u8d70\u2026\u2026",
    "\u6211\uff0c\u6211\u6ca1\u6709\u2014\u2014",
    "\u53bb\u62ff\uff01\u4e0d\u62ff\u8d70\uff01",
    "\uff08\u6d77\u4f26\u5a1c\u7acb\u523b\u51b2\u5411\u6307\u6325\u5ba4\u5165\u53e3\u7684\u6ed1\u95e8\u3002\uff09",
    "\u5443\uff0c\u6307\u6325\u5b98\uff0c\u90a3\u4e9b\u4e0d\u662f\u5411\u5927\u5bb6\u6536\u96c6\u7684\u9884\u5b9a\u8981\u4e22\u6389\u7684\u6742\u7269\u5417\uff1f",
    "\u8bf4\u4e0d\u5b9a\u80fd\u627e\u5230\u4ec0\u4e48\u5b9d\u85cf\uff1f|\u73b0\u5728\uff0c\u91cd\u8981\u7684\u662f\u2014\u2014\u6d77\u4f26\u5a1c\u4e0d\u80fd\u7559\u5728\u8fd9\u91cc\u3002",
    "\u554a\uff0c\u5bf9\u3001\u5bf9\uff0c\u6d77\u4f26\u5a1c\u5bb3\u6015\u90a3\u4e9b\u4eba\u2026\u2026\u961f\u957f\u5979\u4eec\u9a6c\u4e0a\u5c31\u4f1a\u5e26\u7740\u90a3\u4e2a\u9ed1\u8272\u7684\u4eba\u56de\u6765\uff0c\u8fd9\u4e48\u8fd1\u7684\u8ddd\u79bb\u770b\u5230\uff0c\u6d77\u4f26\u5a1c\u4f1a\u5f88\u5bb3\u6015\u7684\u5427\u2026\u2026\u547c\u3002",
    "\u600e\u4e48\u4e86\uff1f",
    "\u5c31\u662f\u2026\u2026\u6307\u6325\u5b98\uff0c\u548c\u90a3\u65f6\u5019\u7684\u6307\u6325\u5b98\u4e00\u6837\uff0c\u679c\u7136\u6ca1\u6709\u53d8\u561b\u3002",
    "\u2026\u2026\uff1f",
    "\u6307\u6325\u5b98\u8bf4\u3001\u8bf4\u8981\u6ce8\u610f\u52a0\u5bc6\u901a\u8baf\uff0c\u5bf9\u5427\uff1f\u4ea4\u7ed9\u6211\uff0c\u6211\u4f1a\u6ce8\u610f\u7684\uff01\u554a\uff0c\u8bf4\u8d77\u6765\uff0c\u5f88\u957f\u65f6\u95f4\u4e86\uff0c\u961f\u957f\u5979\u4eec\u8fd8\u6ca1\u6709\u56de\u6765\u5417\u2026\u2026",
    "\uff08\u4ee5\u95ea\u7535\u4e3a\u9996\uff0c\u4eba\u5f62\u4eec\u8d70\u5165\u4e86\u6307\u6325\u5ba4\u3002\uff09",
    "\uff08\u8d70\u5728\u4e2d\u95f4\u7684\u514b\u7f57\u4e3d\u79d1\u548c\u7eb3\u7f8e\u897f\u4e1d\u4e00\u5de6\u4e00\u53f3\u63a7\u5236\u7740\u9ed1\u8272\u4eba\u5f71\uff0c\u514b\u7f57\u4e3d\u79d1\u7d27\u7d27\u6342\u4f4f\u5979\u7684\u5634\u3002\uff09",
    "\uff08\u7ef4\u666e\u857e\u8ddf\u5728\u6700\u540e\uff0c\u7d27\u7d27\u62c9\u4f4f\u5bc7\u5c14\u8299\u7684\u624b\uff0c\u5bc7\u5c14\u8299\u770b\u8d77\u6765\u60f3\u7529\u5f00\uff0c\u4f46\u8fd8\u662f\u4f4e\u5934\u8ddf\u4e0a\u3002\uff09",
    "\u4e0d\u8981\u4e71\u8dd1\u54e6\uff0c\u8981\u597d\u597d\u5730\u8ddf\u4e0a\u7ef4\u666e\u857e\uff01",
    "\u2026\u2026\u6211\u5728\u540e\u9762\u3002",
    "\u6307\u6325\u5b98\u3002",
    "\u8f9b\u82e6\u4e86\u3002\u4f60\u4eec\u8fd9\u662f\uff1f",
    "\uff08\u6211\u770b\u4e86\u773c\u88ab\u514b\u7f57\u4e3d\u79d1\u7d27\u7d27\u6342\u4f4f\u5634\u7684\u9ed1\u8272\u4eba\u5f71\u3002\uff09",
    "\u592a\u5435\u4e86\uff01\u8fd9\u4ec0\u4e48\u4e1c\u897f\u554a\uff0c\u957f\u5f97\u50cf\u4e2a\u4eba\u53c8\u91cd\u5230\u4e0d\u50cf\u4eba\uff0c\u6bd4\u7eb3\u7f8e\u897f\u4e1d\u8fd8\u5435\uff01",
    "\u2026\u2026\u6df7\u6742\u5347\u8d77\u7684\u5929\uff0c\u6e05\u6717\u6d01\u51c0\u2026\u2026",
    "\u5c31\u662f\u8fd9\u79cd\uff01\u5570\u91cc\u5570\u55e6\u7a00\u5947\u53e4\u602a\u8c01\u77e5\u9053\u4f60\u4eec\u5230\u5e95\u8981\u8bf4\u4ec0\u4e48\u554a\uff1f\uff01",
    "\u2026\u2026\u8fde\u63a5\u8230\u8239\u7684\u5c71\u5cf0\uff0c\u4e8e\u8fbd\u9614\u7684\u6d77\u6d0b\u8513\u5ef6\u2026\u2026",
    "\u4f60\u8ddf\u5979\u4e0d\u4e00\u6837\u80fd\u597d\u597d\u8bf4\u51fa\u6765\u90a3\u4f60\u5012\u662f\u8fd9\u4e48\u505a\u554a\uff1f\u6bcf\u6b21\u90fd\u8981\u6211\u518d\u91cd\u590d\u4e00\u904d\u4f60\u70e6\u4e0d\u70e6\uff1f\uff01",
    "\uff08\u7784\u51c6\u7a7a\u9699\uff0c\u9ed1\u8272\u4eba\u5f71\u7684\u5173\u8282\u53cd\u5411\u79fb\u52a8\uff0c\u7565\u5fae\u6323\u8131\u4e86\u4e24\u4eba\u7684\u63a7\u5236\u3002\uff09",
    "\u7942\u5c06\u5524\u8d77\u81f3\u9ad8\u8005\u7684\u795d\u798f\u3002",
    "\uff08\u9ed1\u8272\u4eba\u5f71\u6ca1\u6709\u7ee7\u7eed\u6323\u624e\uff0c\u524d\u989d\u68d5\u8272\u7684\u5934\u53d1\u5782\u5728\u8863\u9886\u7684\u4f4d\u7f6e\uff0c\u770b\u8d77\u6765\u4f3c\u4e4e\u662f\u5973\u6027\u7684\u8138\u4e0a\u9732\u51fa\u67d4\u548c\u7684\u7b11\u5bb9\u3002\uff09",
    "\uff08\u5b83\u7a7f\u7740\u7684\u8863\u670d\u67d4\u8f6f\u800c\u6709\u5149\u6cfd\uff0c\u56db\u80a2\u4e5f\u88ab\u540c\u6837\u6750\u8d28\u7684\u5e03\u6599\u8986\u76d6\uff0c\u4f46\u5728\u5173\u8282\u5904\uff0c\u5374\u975e\u5e38\u5947\u7279\u5730\u51fa\u73b0\u4e86\u95ea\u70c1\u7740\u91d1\u5c5e\u5149\u6cfd\u7684\u51f8\u51fa\u7ed3\u6784\u3002\uff09",
    "\u8bf7\u8fdc\u79bb\uff0c\u771f\u7406\u7684\u6069\u8d50\u5c06\u4e0d\u4f1a\u964d\u4e34\u5728\u62d2\u7edd\u8005\u6240\u5728\u4e4b\u5904\u3002",
    "\uff08\u5b83\u5fae\u5fae\u62ac\u8d77\u4e86\u624b\u3002\uff09",
    "\u4f60\u522b\u52a8\uff01\u5582\u5582\u7eb3\u7f8e\u897f\u4e1d\uff0c\u4f60\u6293\u597d\u554a\uff01",
    "\u2026\u2026\u575a\u5b9e\u7684\u7269\u4f53\uff0c\u56e0\u529b\u91cf\u7684\u6c47\u805a\u800c\u6d41\u52a8\u2026\u2026",
    "\uff08\u7eb3\u7f8e\u897f\u4e1d\u91cd\u65b0\u6293\u7262\u4e86\u9ed1\u8272\u4eba\u5f71\u7684\u624b\u81c2\uff0c\u5b83\u624b\u8155\u5904\u8986\u76d6\u7684\u5e03\u6599\u7565\u5fae\u6ed1\u843d\uff0c\u6211\u6ce8\u610f\u5230\u4e86\u5e03\u6599\u4e0b\u7684\u673a\u68b0\u7ed3\u6784\u3002\uff09",
    "\uff08\u2026\u2026\u867d\u7136\u5728\u6c61\u67d3\u533a\u4e2d\uff0c\u4ee5\u673a\u68b0\u7ed3\u6784\u66ff\u4ee3\u67d0\u4e9b\u56e0\u6545\u7f3a\u5931\u7684\u8eab\u4f53\u90e8\u4ef6\u662f\u4e00\u79cd\u5e38\u89c1\u7684\u60c5\u51b5\uff0c\u4f46\u8054\u5408\u6b64\u524d\u770b\u5230\u7684\u6807\u5fd7\u3001\u5bf9\u957f\u888d\u4eba\u7fa4\u8eab\u4efd\u7684\u731c\u6d4b\u2026\u2026\uff09",
    "\u2026\u2026\u6d85\u6258\uff1f",
    "\u662f\u59ae\u7279\u3002",
    "\u662f\u5305\u5b50\u5934\u5c0f\u59d0\u59d0\u7684\u540d\u5b57\u5417\uff1f\u597d\u53ef\u7231\uff01",
    "\u55ef\u3002",
    "\uff08\u7ef4\u666e\u857e\u8e66\u8e66\u8df3\u8df3\u5730\u4e0a\u524d\u51e0\u6b65\uff0c\u4e00\u76f4\u88ab\u5979\u62c9\u7740\u7684\u5bc7\u5c14\u8299\u88ab\u8feb\u548c\u5979\u4e00\u8d77\u5411\u524d\u79fb\u52a8\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u7529\u4e86\u7529\u624b\uff0c\u6ca1\u80fd\u7529\u8131\u7ef4\u666e\u857e\u3002\uff09",
    "\u2026\u2026\u653e\u5f00\u6211\u3002",
    "\u5514\uff0c\u53ef\u662f\u961f\u957f\u544a\u8bc9\u7ef4\u666e\u857e\u4e0d\u80fd\u653e\u5f00\u4f60\u54e6\u2014\u2014\u554a\uff0c\u7ef4\u666e\u857e\u77e5\u9053\u4e86\uff01\u662f\u4e0d\u662f\u8981\u653e\u677e\u4e00\u70b9\u70b9\uff1f\u53ef\u7ef4\u666e\u857e\u4e5f\u6ca1\u6709\u5f88\u7528\u529b\u5440\u2026\u2026",
    "\uff08\u8d8a\u8fc7\u514b\u7f57\u4e3d\u79d1\uff0c\u5bc7\u5c14\u8299\u548c\u7ef4\u666e\u857e\u4e00\u540c\u6765\u5230\u4e86\u95ea\u7535\u65c1\u8fb9\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u62ac\u5934\uff0c\u89c6\u7ebf\u6781\u5feb\u5730\u626b\u8fc7\u88ab\u5979\u5c06\u540d\u79f0\u66f4\u6b63\u4e3a\u201c\u59ae\u7279\u201d\u7684\u9ed1\u8272\u4eba\u5f71\uff0c\u4f59\u5149\u77a5\u8fc7\u7ad9\u5728\u59ae\u7279\u65c1\u8fb9\u7684\u95ea\u7535\u3002\uff09",
    "\uff08\u5979\u7684\u89c6\u7ebf\u5728\u6211\u8eab\u4e0a\u77ed\u6682\u505c\u7559\u4e86\u51e0\u79d2\uff0c\u5728\u4e0e\u6211\u7684\u89c6\u7ebf\u5bf9\u4e0a\u524d\uff0c\u5bc7\u5c14\u8299\u91cd\u65b0\u4f4e\u4e0b\u5934\u3002\uff09",
    "\u2026\u2026",
    "\u90a3\u3001\u90a3\u7ef4\u666e\u857e\u653e\u677e\u4e00\u70b9\u70b9\uff1f\u5bc7\u5c14\u8299\u4e0d\u8981\u9003\u8d70\u54e6~",
    "\u597d\u5566\u597d\u5566~\u90a3\u4e48\u2014\u2014",
    "\u521d\u6b21\u89c1\u9762\uff0c\u53ef\u7231\u7684\u5305\u5b50\u5934\u5c0f\u59d0\u59d0\uff01\u4f60\u662f\u4e0d\u662f\u4e5f\u8981\u52a0\u5165\u6211\u4eec\u4e00\u8d77\u6f14\u51fa\u5440\uff1f\u7ef4\u666e\u857e\u3001\u60f3\u5230\u4e86\u4e00\u79cd\u8d85\u5389\u5bb3\u7684\u821e\u8e48\u52a8\u4f5c\u54e6\uff01\u4f60\u60f3\u4e0d\u60f3\u770b\u5440\uff1f",
    "\u8fd9\u4e0d\u662f\u6211\u4eec\u7684\u540c\u4f34\uff0c\u7ef4\u666e\u857e\u3002",
    "\u53ef\u662f\u961f\u957f\uff0c\u4e4b\u524d\u514b\u7f57\u4e3d\u79d1\u4e5f\u662f\u8fd9\u6837\u6293\u7740\u7ef4\u666e\u857e\u54e6\uff1f",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u7684\u7709\u6bdb\u8df3\u52a8\u4e86\u4e00\u4e0b\u3002\uff09",
    "\u5fc3\u667a\u56de\u8def\u6ca1\u642d\u597d\u4f60\u5c31\u53bb\u2014\u2014",
    "\u2026\u2026\u60c5\u51b5\uff0c\u7a0d\u5fae\u6709\u4e9b\u4e0d\u4e00\u6837\u3002",
    "\uff08\u88ab\u4f17\u4eba\u73af\u7ed5\u7684\u59ae\u7279\u7ef4\u6301\u7740\u67d4\u548c\u7684\u5fae\u7b11\u3002\uff09",
    "\uff08\u5b83\u5782\u4e0b\u773c\u775b\uff0c\u76ee\u5149\u51dd\u5b9a\u5728\u4e0d\u8fdc\u5904\u7ad9\u5728\u603b\u63a7\u53f0\u524d\u7684\u7f8e\u73b2\u8eab\u4e0a\uff0c\u7136\u540e\u8f6c\u5411\u4e86\u603b\u63a7\u53f0\u3002\uff09",
    "\uff08\u5bdf\u89c9\u5230\u5b83\u89c6\u7ebf\u7684\u504f\u79fb\uff0c\u6211\u968f\u7740\u5b83\u770b\u5411\u4e86\u603b\u63a7\u53f0\u3002\uff09",
    "\uff08\u2026\u2026\u5b83\u770b\u8d77\u6765\u5e76\u6ca1\u6709\u53d1\u52a8\u653b\u51fb\u7684\u610f\u56fe\u2014\u2014\u6211\u8fd8\u5728\u76ef\u7740\u5b83\u601d\u8003\uff0c\u827e\u83ab\u53f7\u5c31\u54cd\u8d77\u4e86\u523a\u8033\u7684\u8b66\u62a5\u58f0\u3002\uff09",
    "\u554a\u3001\u554a\u2014\u2014\u57fa\u5730\u8f66\u63a7\u5236\u7cfb\u7edf\u906d\u5230\u5165\u4fb5\uff01\u6743\u9650\u53d8\u66f4\u3001\u6307\u4ee4\u5931\u6548\u3001\u5411\u529b\u5f15\u64ce\u505c\u6b62\u8fd0\u884c\u3001\u5b89\u5168\u7cfb\u7edf\u505c\u6b62\u8fd0\u884c\u3001\u5f3a\u5236\u6062\u590d\u65e0\u54cd\u5e94\u2014\u2014\u8bbf\u95ee\u5730\u5740\u6765\u6e90\u2014\u2014\u672c\u5730\uff1f\uff01",
    "\u963b\u65ad\u653b\u51fb\uff01"
  ]
]