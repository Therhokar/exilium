    "\uff08\u2026\u2026\uff09",
    "\u62ff\u57fa\u672c\u770b\u4e0d\u51fa\u6765\u7684\u4f24\u53e3\u7ed9\u6211\u5904\u7406\uff0c\u4f60\u60f3\u8981\u8ddf\u6211\u8bf4\u8bdd\uff0c\u5df2\u7ecf\u9700\u8981\u627e\u8fd9\u79cd\u501f\u53e3\u4e86\uff1f",
    "\uff08\u5f53\u65f6\u7684\u5bc7\u5c14\u8299\u867d\u7136\u9732\u51fa\u4e86\u8bbd\u523a\u7684\u773c\u795e\uff0c\u4f46\u624b\u4e0a\u7684\u52a8\u4f5c\u4ecd\u7136\u975e\u5e38\u6e29\u67d4\u3002\uff09",
    "\u6d77\u4f26\u5a1c\uff0c\u6682\u65f6\u4e0d\u8981\u78b0\u5230\u6c34\uff0c\u4e5f\u4e0d\u8981\u4e71\u52a8\u3002",
    "\uff08\u2026\u2026\uff09",
    "\u2026\u2026\u4e0d\u8981\u4e71\u52a8\u3002",
    "\uff08\u6211\u6293\u4f4f\u4e86\u6d77\u4f26\u5a1c\u4e71\u52a8\u7684\u624b\u81c2\uff0c\u6d77\u4f26\u5a1c\u52aa\u529b\u4e86\u4e00\u4e0b\uff0c\u6ca1\u80fd\u6323\u5f00\u3002\u5979\u7684\u76ee\u5149\u8f6c\u800c\u96c6\u4e2d\u5230\u7f8e\u73b2\u9762\u524d\u7684\u5c4f\u5e55\u4e0a\u3002\uff09",
    "\u7f8e\u73b2\u3001\u5927\u65b9\u5757\u3001\u95ea\u95ea\uff01",
    "\u554a\u3001\u554a\u2014\u2014\u662f\uff0c\u662f\u961f\u957f\u7684\u6d88\u606f\uff01|\u5443\uff0c\u6211\u3001\u6211\u521a\u624d\u6ca1\u6ce8\u610f\u2026\u2026\u961f\u957f\u8bf4\uff0c\u5df2\u7ecf\u6293\u4f4f\u76ee\u6807\u5bf9\u8c61\uff0c\u5979\u4eec\u9a6c\u4e0a\u5c31\u4f1a\u56de\u6765\uff01",
    "\u635f\u4f24\u60c5\u51b5\uff1f",
    "\u9664\u4e86\u5f39\u836f\u635f\u8017\u5916\uff0c\u7d20\u4f53\u90fd\u6ca1\u95ee\u9898\uff01",
    "\u56de\u5f52\u57fa\u5730\u8f66\u540e\uff0c\u901a\u77e5\u5979\u4eec\u7acb\u523b\u6765\u6307\u6325\u5ba4\u3002\u540c\u6b65\u542f\u52a8\u5411\u529b\u5f15\u64ce\uff0c\u5728\u4fdd\u8bc1\u5411\u529b\u5f15\u64ce\u80fd\u591f\u6b63\u5e38\u8fd0\u8f6c\u7684\u524d\u63d0\u4e0b\u4ee5\u6700\u9ad8\u901f\u5ea6\u8fd0\u884c\u3002",
    "\u6211\u4eec\u53bb\u54ea\u91cc\uff1f",
    "\u5148\u79bb\u5f00\u8fd9\u91cc\u3002",
    "\u597d\u7684\uff01",
    "\uff08\u7f8e\u73b2\u4f4e\u4e0b\u5934\uff0c\u4e13\u5fc3\u64cd\u4f5c\u603b\u63a7\u53f0\u3002\uff09",
    "\uff08\u2026\u2026\u6e29\u6696\uff0c\u67d4\u8f6f\uff0c\u6d77\u4f26\u5a1c\u76ae\u80a4\u7684\u89e6\u611f\u4e0e\u5176\u4ed6\u4eba\u7c7b\u5e76\u6ca1\u6709\u4e0d\u540c\u3002\uff09",
