    "\u53ea\u8981\u60a8\u3001\u53ea\u8981\u961f\u957f\u4e0b\u8fbe\u547d\u4ee4\uff0c\u6211\u5c31\u53ef\u4ee5\u6218\u6597\u3002\u6211\u4e0d\u8981\u5de5\u8d44\uff0c\u73b0\u5728\u7684\u7d20\u4f53\uff0c\u6211\u4e5f\u4f1a\u5c0f\u5fc3\u4f7f\u7528\uff0c\u5e0c\u671b\u60a8\u80fd\u591f\u7ed9\u6211\u63d0\u4f9b\u57fa\u7840\u7684\u7d20\u4f53\u7ef4\u62a4\u548c\u7269\u8d44\u8865\u7ed9\u2026\u2026\u53ea\u8981\u6211\u80fd\u591f\u6218\u6597\uff0c\u6211\u5c31\u53ef\u4ee5\u6218\u6597\u3002",
    "\u60a8\u7684\u547d\u4ee4\uff0c\u6211\u65e0\u8bba\u5982\u4f55\u90fd\u4f1a\u8fbe\u6210\u3002",
    "\u53ea\u4f9d\u9760\u4f60\u73b0\u5728\u7684\u706b\u63a7\u6838\u5fc3\u548c\u70d9\u5370\u67aa\u79cd\uff0c\u4f60\u80fd\u505a\u5230\u7684\u4e8b\u60c5\u53ef\u662f\u975e\u5e38\u6709\u9650\u7684\u2014\u2014\u55ef\uff0c\u6211\u60f3\u60f3\uff0c\u5982\u679c\u6d3e\u4f60\u53bb\u6267\u884c\u4e00\u4e9b\u96be\u5ea6\u8f83\u9ad8\u7684\u4efb\u52a1\uff0c\u53ef\u80fd\u8fd8\u9700\u8981\u522b\u7684\u4eba\u5f62\u8ddf\u4f60\u4e00\u8d77\u53bb\u3002\u8fd9\u4e48\u7b97\u4e0b\u6765\uff0c\u6d88\u8017\u5c31\u66f4\u5927\u4e86\u3002",
    "\u2026\u2026\u6211\u2026\u2026",
    "\u5982\u679c\u6709\u5fc5\u8981\u6539\u88c5\u2026\u2026\u5982\u679c\u60a8\u80fd\u591f\u63d0\u4f9b\u6539\u88c5\uff0c\u53ea\u8981\u7ed9\u6211\u7559\u4e0b\u8db3\u591f\u7684\u7a7a\u95f4\u5b58\u50a8\u73b0\u6709\u5fc3\u667a\u8bb0\u5f55\uff0c\u6211\u6ca1\u6709\u4efb\u4f55\u610f\u89c1\uff01",
    "\uff08\u5bc7\u5c14\u8299\u7684\u8eab\u4f53\u6296\u4e86\u4e00\u4e0b\u3002\uff09",
    "\uff08\u5979\u5f3a\u8feb\u81ea\u5df1\u7741\u5927\u4e86\u773c\u775b\uff0c\u7ee7\u7eed\u76f4\u89c6\u6211\u3002\u663e\u793a\u5728\u5979\u77b3\u5b54\u4e0a\u7684\u8bc6\u522b\u7801\u683c\u5916\u660e\u663e\u3002\uff09",
    "\uff08\u6211\u770b\u4e86\u5979\u4e00\u4f1a\u3002\uff09",
    "\u7b97\u4e86\u7b97\u4e86\u3002\u6b63\u597d\uff0c\u5c31\u5728\u4f60\u4eec\u56de\u6765\u524d\u4e0d\u4e45\uff0c\u4f69\u59ae\u6cfd\u628a\u201c\u8d27\u7269\u201d\u9001\u5230\u4e86\u3002",
    "\u5e94\u8be5\u5c31\u5728\u8fd9\u8fb9\u5427\u2026\u2026\u55ef\uff0c\u7269\u8d44\u7bb1\u7f16\u53f7\u2026\u2026\u5bf9\uff0c\u662f\u8fd9\u4e2a\u3002",
    "\u8fd8\u633a\u91cd\u2026\u2026\u7ed9\uff0c\u4f60\u5148\u770b\u770b\u3002",
    "\uff08\u5bc7\u5c14\u8299\u6123\u4e86\u4e00\u4e0b\uff0c\u8bd5\u63a2\u7740\u4f38\u51fa\u624b\uff0c\u63a5\u8fc7\u4e86\u6211\u9012\u8fc7\u6765\u7684\u4e00\u4e2a\u91d1\u5c5e\u7bb1\u3002\uff09",
    "\u83b7\u53d6\u60c5\u62a5\u548c\u5bfb\u627e\u7ebf\u7d22\u6709\u70b9\u56f0\u96be\u3002\u4f69\u59ae\u6cfd\u8bf4\u627e\u56de\u6765\u7684\u65f6\u5019\u7834\u635f\u6709\u70b9\u4e25\u91cd\uff0c\u5979\u987a\u624b\u4fee\u4e86\u4fee\uff0c\u5927\u81f4\u7684\u611f\u89c9\u5e94\u8be5\u5dee\u4e0d\u591a\u3002",
    "\uff08\u7bb1\u5b50\u6253\u5f00\uff0c\u662f\u4e00\u5957\u8863\u670d\u3002\uff09",
    "\u2026\u2026\uff01",
    "\uff08\u5bc7\u5c14\u8299\u6014\u6014\u5730\u770b\u7740\u5b83\u3002\uff09",
    "\uff08\u8fd9\u5957\u8863\u670d\u7684\u8272\u5f69\u4e0e\u5bc7\u5c14\u8299\u73b0\u5728\u6240\u7a7f\u7740\u7684\u975e\u5e38\u76f8\u4f3c\uff0c\u6446\u5728\u5c42\u53e0\u7684\u8863\u6599\u4e4b\u4e0b\u7684\uff0c\u662f\u4e00\u5957\u590d\u6742\u7684\u673a\u68b0\u81c2\u3002\uff09",
    "\uff08\u5979\u5c0f\u5fc3\u7ffc\u7ffc\u5730\u4f38\u51fa\u624b\uff0c\u8bd5\u56fe\u89e6\u78b0\u5b83\uff0c\u4f46\u5728\u5373\u5c06\u89e6\u53ca\u8863\u6599\u8868\u9762\u65f6\uff0c\u53c8\u6781\u5feb\u5730\u7f29\u56de\u4e86\u624b\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u7684\u624b\u60ac\u5728\u4e86\u534a\u7a7a\u3002\uff09",
    "\u4f69\u59ae\u6cfd\u8bf4\u5979\u5c3d\u529b\u4e86\uff0c\u5f88\u591a\u5730\u65b9\u4fee\u590d\u7684\u65f6\u5019\u4e5f\u6ca1\u6709\u53c2\u8003\uff0c\u5de5\u827a\u3001\u9020\u578b\u3001\u8272\u5f69\u4e0a\u5df2\u7ecf\u5c3d\u53ef\u80fd\u4fdd\u6301\u539f\u6837\uff0c\u4e5f\u56e0\u6b64\u591a\u82b1\u4e86\u4e0d\u5c11\u65f6\u95f4\u548c\u9884\u7b97\u2026\u2026\u8fd9\u4e2a\u963f\u5df4\u8d21\uff0c\u5c31\u4f1a\u60f3\u65b9\u8bbe\u6cd5\u8ddf\u6211\u8981\u6676\u6761\u3002",
    "\u2026\u2026\u4e0d\u2026\u2026",
    "\uff08\u518d\u4e09\u8bd5\u63a2\uff0c\u5bc7\u5c14\u8299\u7684\u624b\u7ec8\u4e8e\u653e\u5728\u4e86\u8863\u6599\u4e0a\u3002\uff09",
    "\uff08\u8863\u6599\u7684\u89e6\u611f\u67d4\u8f6f\u800c\u51b0\u51b7\u3002\u5979\u7684\u624b\u7f13\u6162\u629a\u6478\u8fc7\u8896\u5b50\u548c\u80f8\u53e3\u3002\uff09",
    "\u8fd9\u91cc\uff0c\u548c\u8fd9\u91cc\u2026\u2026\u5e94\u8be5\u662f\u2026\u2026\u6709\u8840\u7684\u3002\u662f\u2026\u2026\u5979\u7684\u8840\u3002\u5979\u63e1\u7740\u6211\u7684\u624b\uff0c\u628a\u6211\u6309\u8fdb\u5409\u666e\u8f66\u3002\u6211\u62b1\u7740\u5979\u7684\u624b\uff0c\u6211\u2026\u2026\u4e0d\u60f3\u653e\u5f00\u3002",
    "\u2026\u2026\u53ef\u6211\uff0c\u53ea\u80fd\u653e\u5f00\u3002",
    "\u4f60\u8bf4\u4ec0\u4e48\uff1f\u662f\u54ea\u91cc\u89c9\u5f97\u4e0d\u5bf9\u5417\uff1f\u4e0d\u7136\u4f60\u5148\u8bd5\u8bd5\uff0c\u628a\u4e0d\u5bf9\u7684\u5730\u65b9\u6574\u7406\u4e2a\u5217\u8868\u7ed9\u6211\uff0c\u6211\u53bb\u8ba9\u4f69\u59ae\u6cfd\u53bb\u60f3\u529e\u6cd5\u3002",
    "\uff08\u5bc7\u5c14\u8299\u731b\u7136\u62ac\u5934\u770b\u6211\uff0c\u624b\u4e00\u677e\uff0c\u7bb1\u5b50\u5c31\u8981\u6389\u5728\u5730\u4e0a\u3002\uff09",
    "\uff08\u5979\u7acb\u523b\u4f38\u624b\uff0c\u60f3\u8981\u62b1\u4f4f\u7bb1\u5b50\uff0c\u5374\u6ca1\u80fd\u628a\u63e1\u597d\u91cd\u5fc3\uff0c\u6700\u7ec8\u62b1\u7740\u7bb1\u5b50\u91cd\u91cd\u5730\u8dea\u5728\u4e86\u5730\u4e0a\u3002\uff09",
    "\uff08\u6211\u4e00\u60ca\uff0c\u7acb\u523b\u4f38\u624b\u53bb\u5e2e\u5979\u6276\u7bb1\u5b50\uff0c\u5e76\u987a\u624b\u60f3\u628a\u5979\u62c9\u8d77\u6765\u3002\uff09",
    "\u600e\u4e48\u4e86\u8fd9\u662f\u2014\u2014\u7d20\u4f53\u8fd8\u662f\u53d7\u635f\u4e86\uff1f\u5e73\u8861\u6746\u7684\u95ee\u9898\uff1f",
    "\u2026\u2026\u4e0d\uff0c\u6ca1\u6709\uff0c\u6211\u5f88\u597d\u2026\u2026",
    "\uff08\u8dea\u5728\u5730\u4e0a\uff0c\u5bc7\u5c14\u8299\u65e0\u610f\u8bc6\u5730\u7d27\u7d27\u62b1\u4f4f\u7bb1\u5b50\u3002\uff09",
    "\uff08\u6211\u62fd\u4e86\u5bc7\u5c14\u8299\u4e00\u4e0b\uff0c\u6ca1\u80fd\u62fd\u52a8\u3002\uff09",
    "\u2026\u2026",
    "\u6d77\u4f26\u5a1c\u8bf4\u8981\u9001\u4f60\u793c\u7269\uff0c\u6211\u60f3\u4e86\u60f3\uff0c\u81ea\u4ece\u4f60\u52a0\u5165\u6211\u7684\u8f66\u961f\uff0c\u6211\u9664\u4e86\u8ba9\u7f8e\u73b2\u5e2e\u4f60\u6539\u88c5\uff0c\u5176\u4ed6\u4e5f\u6ca1\u505a\u4ec0\u4e48\u2026\u2026\u6d77\u4f26\u5a1c\u7684\u4e8b\uff0c\u4f60\u5e2e\u4e86\u6211\u4e0d\u5c11\u2026\u2026\u4f46\u6211\u4e5f\u4e0d\u77e5\u9053\u4f60\u60f3\u8981\u4ec0\u4e48\u3002",
    "\u65e2\u7136\u4f60\u5f88\u770b\u91cd\u4f60\u66fe\u7ecf\u5728\u7b2c\u5341\u4e00\u533b\u62a4\u9632\u5316\u8425\u7684\u90a3\u6bb5\u7ecf\u5386\uff0c\u4f60\u5f53\u65f6\u7a7f\u7684\u8863\u670d\u6211\u5c31\u8ba9\u4f69\u59ae\u6cfd\u53bb\u627e\u4e86\u627e\uff0c\u5979\u7528\u5979\u4f5c\u4e3a\u6697\u5546\u7684\u4fe1\u8a89\u5411\u6211\u4fdd\u8bc1\u5c31\u662f\u8fd9\u4e00\u5957\u2014\u2014\u5e0c\u671b\u4f60\u80fd\u559c\u6b22\u3002",
    "\u2026\u2026",
    "\u5443\uff0c\u90a3\u4e2a\u2026\u2026\u4e4b\u524d\u6211\u63d0\u8d77\u7ea2\u7ebf\uff0c\u662f\u60f3\u8bf4\uff0c\u662f\u6211\u758f\u5ffd\u4e86\uff0c\u5076\u5c14\u6211\u4e5f\u66f4\u5e94\u8be5\u4e3b\u52a8\u5173\u5fc3\u4e00\u4e0b\u6211\u7684\u6210\u5458\u4eec\u3002\u55ef\u2026\u2026\u8981\u600e\u4e48\u8bf4\uff0c\u5bc7\u5c14\u8299\uff0c\u6211\u60f3\u60f3\u2026\u2026",
    "\u6211\u4e5f\u2026\u2026\u4e0d\u64c5\u957f\u8fd9\u4e2a\u554a\u3002\u603b\u4e4b\u5c31\u662f\u2026\u2026\u5bc7\u5c14\u8299\uff0c\u5728\u4f60\u73b0\u6709\u7684\u5fc3\u667a\u8bb0\u5f55\u4e2d\uff0c\u6709\u597d\u7684\u4e8b\u60c5\uff0c\u4e5f\u6709\u574f\u7684\u4e8b\u60c5\uff0c\u800c\u574f\u7684\u4e8b\u60c5\u5f80\u5f80\u4f1a\u6bd4\u597d\u7684\u4e8b\u60c5\u66f4\u8ba9\u4f60\u5ff5\u5ff5\u4e0d\u5fd8\u3002",
    "\u4f46\u8bb0\u4f4f\u8fd9\u4e9b\u574f\u7684\u4e8b\u60c5\uff0c\u4e0d\u80fd\u88ab\u770b\u4f5c\u662f\u4e00\u4ef6\u574f\u4e8b\u2026\u2026\u8fd9\u8ba9\u4f60\u80fd\u591f\u66f4\u597d\u5730\u5728\u8fd9\u4e2a\u4e16\u754c\u4e0a\u751f\u5b58\u4e0b\u53bb\u3002\u4f46\u53ea\u8bb0\u4f4f\u597d\u7684\u4e8b\u60c5\uff0c\u6216\u8005\u53ea\u8bb0\u4f4f\u574f\u7684\u4e8b\u60c5\uff0c\u90fd\u662f\u4e0d\u884c\u7684\u3002\u4f60\u5fc3\u667a\u8bb0\u5f55\u4e2d\u7684\u6240\u6709\u4e8b\u60c5\uff0c\u9700\u8981\u2014\u2014\u4e5f\u5fc5\u987b\u90fd\u6210\u4e3a\u4f60\u751f\u5b58\u7684\u9a71\u52a8\u529b\u3002",
    "\u5bc7\u5c14\u8299\uff0c\u4f60\u6709\u4e24\u6761\u817f\uff0c\u60f3\u8981\u5f80\u524d\u8d70\u7684\u65f6\u5019\uff0c\u4e24\u6761\u817f\u90fd\u8981\u7528\u8d77\u6765\uff0c\u662f\u4e0d\u662f\uff1f\u5982\u679c\u53ea\u7528\u5de6\u817f\uff0c\u6216\u8005\u53ea\u7528\u53f3\u817f\uff0c\u4f60\u8fd8\u80fd\u5f80\u524d\u8d70\u5417\uff1f",
    "\uff08\u8fd9\u4e9b\u8bdd\uff0c\u6211\u4eff\u4f5b\u4e5f\u5728\u8bf4\u7ed9\u6211\u81ea\u5df1\u3002\uff09",
    "\uff08\u62cd\u4e86\u62cd\u5bc7\u5c14\u8299\u7684\u80a9\u8180\uff0c\u6211\u5e2e\u5979\u6574\u7406\u4e86\u4e00\u4e0b\u6709\u4e9b\u51cc\u4e71\u7684\u8863\u9886\u3002\uff09",
    "\u6709\u60f3\u6cd5\uff0c\u6709\u8981\u6c42\uff0c\u90fd\u53ef\u4ee5\u5728\u786e\u5b9a\u81ea\u5df1\u7684\u771f\u5b9e\u60f3\u6cd5\u4e4b\u540e\uff0c\u660e\u786e\u5730\u8bf4\u51fa\u6765\u3002\u4f60\u60f3\u77e5\u9053\u6211\u7684\u7ecf\u5386\uff0c\u4f60\u5c31\u53ef\u4ee5\u95ee\u6211\uff0c\u5982\u679c\u9700\u8981\u4f60\u77e5\u9053\uff0c\u6211\u4e5f\u4f1a\u4e3b\u52a8\u544a\u8bc9\u4f60\u2014\u2014\u4ee5\u53ca\u57fa\u5730\u8f66\u4e0a\u7684\u5176\u4ed6\u6210\u5458\u3002\u8fd9\u5bf9\u4e8e\u4f60\u4e5f\u662f\u540c\u6837\u3002",
    "\u4f60\u4e3a\u4ec0\u4e48\u8981\u590d\u4ec7\uff0c\u4e3a\u4ec0\u4e48\u5fc5\u987b\u590d\u4ec7\uff0c\u5982\u679c\u4f60\u4e0d\u8bf4\uff0c\u6211\u5c31\u4e0d\u77e5\u9053\u2026\u2026\u8fd9\u8981\u6211\u600e\u4e48\u5e2e\u4f60\uff1f|\u5bc7\u5c14\u8299\uff0c\u8981\u518d\u591a\u4fe1\u4efb\u4f60\u7684\u6307\u6325\u5b98\u4e00\u4e9b\u554a\u2026\u2026",
    "\uff08\u53c8\u62fd\u4e86\u4e00\u4e0b\u5bc7\u5c14\u8299\uff0c\u8fd8\u662f\u6ca1\u80fd\u62fd\u52a8\uff0c\u6211\u9732\u51fa\u82e6\u7b11\u3002\uff09",
    "\u6211\u6ca1\u90a3\u4e48\u4e0d\u503c\u5f97\u4fe1\u4efb\u5427\uff1f",
    "\u2026\u2026\u6307\u6325\u5b98\u3002",
    "\u5979\u53eb\u5b89\u83f2\u5a05\u3002\u5b89\u83f2\u5a05\u00b7\u838e\u62c9\u6ce2\u5a03\uff0c\u4e2d\u592e\u9646\u519b\u5373\u65f6\u54cd\u5e94\u6551\u63f4\u7f16\u5236\uff0c\u7b2c\u5341\u4e00\u533b\u62a4\u9632\u5316\u8425\u7b2c\u4e09\u533b\u7597\u961f\u533b\u7597\u7ec4\u957f\u3002\u662f\u6211\u7684\u59d0\u59d0\uff0c\u662f\u7ed9\u4e86\u6211\u201c\u5bc7\u5c14\u8299\u201d\u8fd9\u4e2a\u540d\u5b57\u7684\u4eba\u3002",
    "2073\u5e7410\u6708\u2026\u202622\u65e5\uff0c\u4ece\u90a3\u5929\u4ee5\u540e\uff0c\u6211\u518d\u4e5f\u6ca1\u6709\u89c1\u5230\u8fc7\u5979\u3002\u6211\u76f8\u4fe1\u5979\u53ea\u662f\u5931\u8e2a\u4e86\uff0c\u53ea\u8981\u6211\u52aa\u529b\u53bb\u627e\uff0c\u4e00\u5b9a\u80fd\u591f\u627e\u5230\u5979\uff0c\u80fd\u591f\u6551\u5979\u3002\u53ef\u5e0c\u671b\u8d8a\u6765\u8d8a\u5c0f\uff0c\u8d8a\u6765\u8d8a\u5c0f\u2026\u2026\u6211\u8be5\u600e\u4e48\u529e\uff0c\u6211\u8be5\u600e\u4e48\u529e\u2026\u2026\u6211\u8be5\u600e\u4e48\u529e\u554a\uff01",
    "\u6211\u53ea\u662f\u4e00\u4e2a\u4eba\u5f62\uff0c\u6211\u53ea\u662f\u4e00\u4e2a\u4eba\u5f62\u554a\uff01\u9664\u4e86\u60f3\u5c3d\u4e00\u5207\u529e\u6cd5\u3001\u6293\u4f4f\u4e00\u5207\u673a\u4f1a\u7ed9\u5979\u62a5\u4ec7\u2014\u2014\u6211\u8fd8\u80fd\u505a\u4ec0\u4e48\u2014\u2014\u6211\u8fd8\u80fd\u505a\u4ec0\u4e48\u554a\uff01\u6211\u8fd8\u80fd\u505a\u4ec0\u4e48\uff0c\u6211\u8fd8\u9700\u8981\u505a\u4ec0\u4e48\uff0c\u60a8\u80fd\u4e0d\u80fd\u544a\u8bc9\u6211\u2014\u2014\u6307\u6325\u5b98\uff01",
    "\uff08\u770b\u7740\u5bc7\u5c14\u8299\uff0c\u6211\u4eff\u4f5b\u770b\u5230\u4e86\u66fe\u7ecf\u7684\u81ea\u5df1\u3002\uff09",
    "\uff08\u72b9\u8c6b\u4e86\u4e00\u4e0b\uff0c\u518d\u6b21\u62cd\u4e86\u62cd\u5bc7\u5c14\u8299\u7684\u80a9\u8180\uff0c\u6211\u4e5f\u8dea\u4e86\u4e0b\u6765\u3002\uff09",
    "\u2026\u2026\u6211\u4e5f\u4e0d\u77e5\u9053\u3002",
    "\u2026\u2026\u545c\u2026\u2026",
    "\u4eba\u5f62\u4f1a\u54ed\uff0c\u4e5f\u4f1a\u7b11\u3002\u4f60\u4eec\u6bcf\u4e00\u4e2a\u4eba\uff0c\u5bf9\u4e8e\u6211\u6765\u8bf4\uff0c\u90fd\u662f\u4e0d\u540c\u7684\u201c\u4eba\u201d\u3002\u76f8\u4fe1\u5979\u8fd8\u6d3b\u7740\uff0c\u90a3\u5c31\u53bb\u627e\u3002\u8ba4\u4e3a\u5979\u4e0d\u5728\u4e86\uff0c\u5c31\u53bb\u590d\u4ec7\u3002\u505a\u51fa\u9009\u62e9\uff0c\u800c\u4e0d\u662f\u9003\u907f\u2026\u2026",
    "\u4f5c\u4e3a\u4e00\u4e2a\u201c\u4eba\u201d\uff0c\u4f60\u6bd4\u6211\u52c7\u6562\u3002",
    "\u5bc7\u5c14\u8299\uff0c\u8be5\u600e\u4e48\u505a\uff0c\u6211\u7ed9\u4e0d\u4e86\u4f60\u7b54\u6848\u3002",
    "\u5bf9\u4e0d\u8d77\uff0c\u6211\u7ed9\u4e0d\u4e86\u4f60\u7b54\u6848\u3002",
    "\u2026\u2026",
    "\u2026\u2026\u8be5\u8bf4\u5bf9\u4e0d\u8d77\u7684\uff0c\u662f\u6211\u624d\u5bf9\u5427\u3002",
    "\uff08\u5c0f\u5fc3\u5730\u5c06\u91d1\u5c5e\u7bb1\u7684\u91cd\u5fc3\u79fb\u5230\u4e00\u53ea\u624b\u4e0a\uff0c\u5bc7\u5c14\u8299\u614e\u91cd\u5730\u4f38\u51fa\u53e6\u4e00\u53ea\u624b\uff0c\u56de\u62b1\u4e86\u6211\u3002\uff09",
    "\uff08\u2026\u2026\u5979\u7684\u624b\u5728\u98a4\u6296\uff0c\u9694\u7740\u80cc\u90e8\u7684\u8863\u6599\uff0c\u6211\u80fd\u591f\u6e05\u6670\u5730\u611f\u53d7\u5230\u3002\uff09",
    "\u5bf9\u4e0d\u8d77\uff0c\u7ed9\u60a8\u5236\u9020\u4e86\u5f88\u591a\u9ebb\u70e6\u3002\u8c22\u8c22\uff0c\u5e2e\u6211\u6539\u88c5\u7d20\u4f53\uff0c\u8ba9\u6211\u80fd\u591f\u7ef4\u6301\u7d20\u4f53\u8fd0\u8f6c\u3002\u518d\u89c1\uff0c\u6211\u4e0d\u60f3\u4e0d\u544a\u800c\u522b\u2026\u2026\u4f46\u6211\u65e2\u7136\u9009\u62e9\u79bb\u5f00\uff0c\u5c31\u4e0d\u5e94\u8be5\u5728\u57fa\u5730\u8f66\u4e0a\u4fdd\u7559\u5fc3\u667a\u2026\u2026\u7136\u540e\uff0c\u6211\u56de\u6765\u4e86\u3002",
    "\u5bf9\u4e0d\u8d77\u3001\u8c22\u8c22\u3001\u518d\u89c1\u3001\u6211\u56de\u6765\u4e86\uff0c\u8fd9\u4e9b\u2014\u2014\u90fd\u5e94\u8be5\u6211\u5bf9\u60a8\u8bf4\uff0c\u6307\u6325\u5b98\uff01",
    "\u679c\u7136\uff0c\u4f60\u6bd4\u6211\u52c7\u6562\uff0c\u5bc7\u5c14\u8299\u3002",
    "\uff08\u5bc7\u5c14\u8299\u518d\u6b21\u56de\u62b1\u4e86\u4e00\u4e0b\u6211\uff0c\u7136\u540e\u62b9\u4e86\u62b9\u773c\u775b\uff0c\u5c06\u7bb1\u5b50\u653e\u5728\u4e00\u65c1\uff0c\u7ad9\u4e86\u8d77\u6765\u3002\uff09",
    "\uff08\u5979\u5411\u6211\u5f2f\u4e0b\u8170\uff0c\u90d1\u91cd\u5730\u97a0\u8eac\u3002\uff09",
    "\u8c22\u8c22\u60a8\uff0c\u80fd\u5141\u8bb8\u6211\u56de\u6765\u3002\u6211\u56de\u6765\u4e86\u2014\u2014\u6709\u9700\u8981\u6211\u5e2e\u5fd9\u7684\uff0c\u8bf7\u544a\u8bc9\u6211\uff0c\u6307\u6325\u5b98\u3002",
    "\u5443\uff0c\u6211\u60f3\u60f3\u2026\u2026\u5176\u5b9e\u5c31\u50cf\u5e73\u5e38\u90a3\u6837\u5c31\u884c\u4e86\u3002|\u5982\u679c\u4f60\u6709\u4ec0\u4e48\u8981\u6c42\uff0c\u4e5f\u6765\u544a\u8bc9\u6211\uff0c\u6709\u95ee\u9898\u7684\u8bdd\uff0c\u6211\u4e5f\u4f1a\u76f4\u63a5\u8ddf\u4f60\u8bf4\u7684\u3002",
    "\uff08\u4fdd\u6301\u5f2f\u8170\u7684\u59ff\u52bf\uff0c\u5bc7\u5c14\u8299\u5411\u8dea\u5728\u5730\u4e0a\u7684\u6211\u4f38\u51fa\u624b\uff0c\u5c06\u6211\u62c9\u4e86\u8d77\u6765\u3002\uff09",
    "\uff08\u7136\u540e\uff0c\u5979\u73cd\u800c\u91cd\u4e4b\u5730\u53cc\u624b\u6258\u8d77\u4e86\u91d1\u5c5e\u7bb1\u3002\uff09",
    "\u6211\u8bb0\u4f4f\u4e86\u3002\u8c22\u8c22\u60a8\u7684\u793c\u7269\uff0c\u6211\u5f88\u559c\u6b22\u3002",
    "\u6d77\u4f26\u5a1c\u90a3\u8fb9\u6211\u9a6c\u4e0a\u53bb\u770b\u4e00\u4e0b\uff01",
    "\u8f9b\u82e6\u4f60\u4e86\u3002",
    "\u6ca1\u4e8b\u7684\u3002",
    "\uff08\u8dd1\u5f00\u4e24\u6b65\uff0c\u5bc7\u5c14\u8299\u505c\u4f4f\u4e86\u811a\u6b65\uff0c\u8f6c\u8eab\u5411\u6211\u9732\u51fa\u4e00\u4e2a\u548c\u5f80\u5e38\u4e00\u6837\u7684\u3001\u707f\u70c2\u7684\u7b11\u5bb9\u3002\uff09",
    "\u4eca\u5929\u4f60\u8bf4\u7684\u8bdd\uff0c\u4eba\u5bb6\u2014\u2014\u53ef\u662f\u4f1a\u2014\u2014\u597d\u597d\u8bb0\u4f4f\u7684\u54df\uff0c\u6307\u00b7\u6325\u00b7\u5b98~",
    "\uff08\u5411\u5bc7\u5c14\u8299\u6325\u4e86\u6325\u624b\uff0c\u6211\u76ee\u9001\u5979\u62b1\u7740\u91d1\u5c5e\u7bb1\u79bb\u5f00\u4e86\u4e2d\u67a2\u533a\u3002\uff09",
    "\u597d\u3002\u7b97\u7b97\u65f6\u95f4\uff0c\u4e5f\u5dee\u4e0d\u591a\u4e86\u3002\u63a5\u4e0b\u6765\u2026\u2026"
  ],
  [
    "\u6307\u6325\u5b98\u3002",
    "\u55ef\u3002\u7ef4\u62a4\u5b8c\u6210\u4e86\uff1f",
    "\u5df2\u7ecf\u5b8c\u6210\u3002\u884c\u52a8\u62a5\u544a\u6211\u6b63\u5728\u6574\u7406\uff0c\u9884\u8ba10024\u540e\u4e0a\u4f20\u81f3\u6307\u5b9a\u8def\u5f84\u3002",
    "\u2026\u2026\u8fc7\u6765\u7684\u8def\u4e0a\u6211\u60f3\u4e86\u5f88\u591a\u95ee\u9898\uff0c\u53ef\u73b0\u5728\uff0c\u6211\u4e5f\u4e0d\u77e5\u9053\u600e\u4e48\u5f00\u59cb\u95ee\u6bd4\u8f83\u597d\u3002\u6216\u8005\u6211\u76f4\u63a5\u53bb\u9605\u8bfb\u884c\u52a8\u62a5\u544a\u4f1a\u6bd4\u8f83\u5408\u9002\uff1f",
    "\u662f\u5bc7\u5c14\u8299\u90a3\u91cc\u53d1\u751f\u4ec0\u4e48\u4e8b\u4e86\u5417\uff1f",
    "\u7b97\u4e0d\u4e0a\u3002\u53ea\u662f\u2026\u2026\u6211\u5728\u60f3\uff0c\u6216\u8bb8\u6211\u4e5f\u53ef\u4ee5\u66f4\u52a0\u79ef\u6781\u4e00\u70b9\u2026\u2026",
    "\u6307\u6325\u5b98\u4e00\u76f4\u5f88\u79ef\u6781\u3002\u6bd4\u5982\uff0c\u5728\u63d0\u51fa\u60f3\u8981\u5403\u4ec0\u4e48\u7684\u65f6\u5019\u3002",
    "\u4f60\u77e5\u9053\u6211\u4e0d\u662f\u5728\u8bf4\u8fd9\u4e2a\u3002\u55ef\uff0c\u90a3\u6211\u5c31\u5f00\u59cb\u95ee\u5427\u3002",
    "\u597d\u3002",
    "\u2026\u2026\u55ef\uff0c\u4f60\u4eec\u9047\u5230\u4ec0\u4e48\u4e86\uff1f\u53d7\u635f\u60c5\u51b5\u8d85\u51fa\u4e86\u6211\u7684\u9884\u671f\uff0c\u6211\u672c\u6765\u4ee5\u4e3a\uff0c\u8fd9\u53ea\u662f\u4e00\u4e2a\u9700\u8981\u614e\u91cd\u5bf9\u5f85\u7684\u6f5c\u5165\u4efb\u52a1\u3002",
    "\u662f\u6d77\u4f26\u5a1c\u3002\u6d77\u4f26\u5a1c\u6ca1\u80fd\u63a7\u5236\u4f4f\u5979\u7684\u9632\u5fa1\u88c5\u7f6e\uff0c\u5979\u5931\u63a7\u4e86\uff0c\u514b\u7f57\u4e3d\u79d1\u6253\u6655\u4e86\u5979\u2014\u2014\u5979\u548c\u5e15\u62c9\u8482\u65af\u3001\u548c\u51c0\u5316\u4eea\u5f0f\u6709\u5173\u7cfb\uff0c\u6211\u8ba4\u4e3a\u5979\u5c31\u662f\u201c\u79d1\u672a\u5854\u201d\uff0c\u6307\u6325\u5b98\u3002",
    "\u6211\u60f3\u77e5\u9053\u8be6\u60c5\u3002",
    "\u5931\u63a7\u524d\uff0c\u6d77\u4f26\u5a1c\u63d0\u5230\u4e86\u201c\u4eea\u5f0f\u201d\u3001\u201c\u5b9e\u9a8c\u201d\uff0c\u5979\u5931\u63a7\u7684\u65f6\u95f4\u70b9\uff0c\u4e5f\u4e0e\u51c0\u5316\u4eea\u5f0f\u5f00\u59cb\u7684\u65f6\u95f4\u91cd\u5408\u3002",
    "\u800c\u5728\u51c0\u5316\u4eea\u5f0f\u4e0a\uff0c\u6d85\u6258\u63d0\u5230\u4e86\u201c\u6d01\u51c0\u7684\u79d1\u672a\u5854\u201d\uff0c\u7efc\u5408\u6d85\u6258\u548c\u59ae\u7279\u8868\u8ff0\u51fa\u7684\u5e15\u62c9\u8482\u65af\u5bf9\u4e8e\u574d\u584c\u8f90\u5c04\u7684\u6001\u5ea6\uff0c\u6211\u8ba4\u4e3a\uff0c\u201c\u79d1\u672a\u5854\u201d\u6307\u7684\u5c31\u662f\u201c\u5b8c\u5168\u514d\u75ab\u4f53\u201d\u2014\u2014\u4e5f\u5c31\u662f\u6d77\u4f26\u5a1c\u3002",
    "\u4f60\u4eec\u89c1\u5230\u6d85\u6258\u4e86\uff1f",
    "\u5f92\u4ece\u4eec\u79f0\u547c\u4e3e\u884c\u51c0\u5316\u4eea\u5f0f\u7684\u5bf9\u8c61\u4e3a\u201c\u6d85\u6258\u201d\uff0c\u56fe\u50cf\u7a0d\u540e\u6211\u4f1a\u9644\u5728\u884c\u52a8\u62a5\u544a\u4e2d\u3002",
    "\u55ef\u3002\u5c31\u7b97\u201c\u79d1\u672a\u5854\u201d\u662f\u6307\u5b8c\u5168\u514d\u75ab\u4f53\uff0c\u6d77\u4f26\u5a1c\u4e5f\u4e0d\u4e00\u5b9a\u5c31\u662f\u201c\u79d1\u672a\u5854\u201d\u2014\u2014\u6d77\u4f26\u5a1c\u4e0d\u662f\u771f\u7684\u201c\u5b8c\u5168\u514d\u75ab\u4f53\u201d\u3002\u867d\u7136\u548c\u6211\u4ee5\u524d\u89c1\u8fc7\u7684\u90a3\u4e9b\u4e2a\u4f53\u76f8\u6bd4\uff0c\u6d77\u4f26\u5a1c\u7684\u8868\u73b0\u975e\u5e38\u4e0d\u540c\u2026\u2026\u4f46\u5979\u5728\u65e0\u574d\u584c\u8f90\u5c04\u73af\u5883\u4e0b\u8868\u73b0\u51fa\u4e86\u8eab\u4f53\u673a\u80fd\u6076\u5316\u2026\u2026",
    "\u4e5f\u53ef\u80fd\u9700\u8981\u7279\u6b8a\u7684\u89e6\u53d1\u6761\u4ef6\uff0c\u6216\u8005\u662f\u4e00\u4e9b\u6211\u4eec\u73b0\u5728\u8fd8\u4e0d\u77e5\u9053\u7684\u7406\u7531\u3002|\u6307\u6325\u5b98\uff0c\u201c\u6d88\u5931\u7684\u79d1\u672a\u5854\u201d\u3002\u6d85\u6258\u548c\u59ae\u7279\u90fd\u5728\u8981\u6c42\u5b83\u4eec\u7684\u5f92\u4ece\u5bfb\u627e\u201c\u6d88\u5931\u7684\u79d1\u672a\u5854\u201d\uff0c\u800c\u6d77\u4f26\u5a1c\u6070\u597d\u5c31\u88ab\u4ea4\u5230\u4e86\u6211\u4eec\u624b\u4e0a\uff0c\u6211\u4e0d\u8ba4\u4e3a\u8fd9\u53ea\u662f\u4e00\u4e2a\u5de7\u5408\u3002",
    "\u4e0d\uff0c\u65e2\u7136\u6d77\u4f26\u5a1c\u53ef\u80fd\u5b58\u5728\u590d\u6570\u4e2a\uff0c\u5e15\u62c9\u8482\u65af\u5f88\u53ef\u80fd\u8fd8\u5236\u9020\u4e86\u5176\u4ed6\u79cd\u7c7b\u7684\u4e2a\u4f53\u3002\u5982\u679c\u5728\u540c\u4e00\u65f6\u95f4\uff0c\u8fd8\u6709\u5176\u4ed6\u79cd\u7c7b\u7684\u4e2a\u4f53\u6d88\u5931\uff0c\u90a3\u201c\u6d88\u5931\u7684\u79d1\u672a\u5854\u201d\uff0c\u6307\u7684\u5c31\u4e0d\u4e00\u5b9a\u662f\u6d77\u4f26\u5a1c\u3002",
    "\u2026\u2026\u8fd9\u4e2a\u5305\u542b\u4e86\u6d77\u4f26\u5a1c\u7684\u8ba2\u5355\uff0c\u59d4\u6258\u65b9\u548c\u63a5\u6536\u8ba2\u5355\u7684\u59d4\u6258\u4eba\uff0c\u90fd\u6ca1\u6709\u76f4\u63a5\u4e0e\u5e15\u62c9\u8482\u65af\u63a5\u89e6\u7684\u8bc1\u636e\u2026\u2026\u4f46\u6d77\u4f26\u5a1c\u7684\u7bb1\u5b50\uff0c\u7528\u7684\u662f\u5e15\u62c9\u8482\u65af\u7684\u79d1\u6280\u3002",
    "\u90a3\u4e2a\u201c\u751f\u7269\u79d1\u6280\u76f8\u5173\u5206\u91ce\u201d\u7684\u516c\u53f8\uff0c\u6709\u53ef\u80fd\u662f\u5e15\u62c9\u8482\u65af\uff1f\u6216\u8005\uff0c\u9664\u4e86\u74e6\u826f\u683c\u5e2e\u548c\u5409\u62c9\u5c14\u96c6\u56e2\u4e4b\u5916\uff0c\u6211\u4eec\u624b\u4e2d\u7684\u8ba2\u5355\u7275\u626f\u5230\u7684\u90a3\u4e2a\u5c5e\u4e8e\u751f\u7269\u79d1\u6280\u76f8\u5173\u5206\u91ce\u7684\u516c\u53f8\uff0c\u80fd\u591f\u4e0e\u5e15\u62c9\u8482\u65af\u76f4\u63a5\u4ea7\u751f\u8054\u7cfb\uff1f",
    "\u6211\u8ba4\u4e3a\u662f\u540e\u8005\u2026\u2026\u5982\u679c\u5e15\u62c9\u8482\u65af\u5728\u767d\u533a\u6709\u6d3b\u52a8\uff0c\u683c\u7433\u4e0d\u4f1a\u4e0d\u544a\u8bc9\u6211\u3002",
    "\u8fd9\u6b21\u6f5c\u5165\u884c\u52a8\uff0c\u6d77\u4f26\u5a1c\u76f4\u63a5\u66b4\u9732\u5728\u4e86\u6d85\u6258\u9762\u524d\u3002\u5982\u679c\u6211\u4eec\u7684\u63a8\u6d4b\u6210\u7acb\uff0c\u8fd9\u4e00\u60c5\u51b5\u4ea7\u751f\u7684\u5f71\u54cd\u5c06\u65e0\u6cd5\u9884\u4f30\u2014\u2014\u6211\u4eec\u5e94\u8be5\u7acb\u523b\u5411\u683c\u7433\u5a1c\u6c42\u63f4\uff0c\u6307\u6325\u5b98\u3002",
    "\u683c\u7433\u90a3\u8fb9\u7684\u60c5\u51b5\u6211\u5e76\u4e0d\u6e05\u695a\u3002\u5982\u679c\u5979\u6ca1\u6709\u4e3b\u52a8\u8054\u7edc\u6211\u2026\u2026\u73b0\u5728\u6211\u4e0d\u80fd\u8054\u7edc\u5979\u3002|\u4f60\u4eec\u79bb\u5f00\u540e\uff0c\u6d85\u6258\u6ca1\u6709\u8ffd\u51fb\u4f60\u4eec\uff1f",
    "\u6ca1\u6709\u3002\u5b83\u6ca1\u6709\u653b\u51fb\u6211\u4eec\u3002",
    "\u7b49\u4e00\u4e0b\uff0c\u6211\u8fd8\u4ee5\u4e3a\u4f60\u4eec\u7684\u635f\u4f24\u662f\u56e0\u4e3a\u201c\u6d85\u6258\u201d\u7684\u6218\u6597\u529b\u63d0\u5347\u4e86\uff1f",
    "\u662f\u516d\u5206\u4eea\u3002",
    "\u516d\u5206\u4eea\uff1f\uff01|\u2014\u2014\u5979\u600e\u4e48\u5728\u5e15\u62c9\u8482\u65af\uff1f\uff01",
    "\u4e0d\u6e05\u695a\u3002",
    "\u6211\u4eec\u5230\u8fbe\u4e4b\u524d\uff0c\u5979\u5c31\u5df2\u7ecf\u5728\u573a\u5730\u5185\u3002\u4f46\u662f\u76f8\u8f83\u4e4b\u524d\uff0c\u5979\u6240\u8868\u73b0\u51fa\u7684\u6218\u6597\u529b\u5f31\u4e86\u5f88\u591a\u3002",
    "\u4f60\u7ed9\u6211\u7684\u547d\u4ee4\u662f\u201c\u574d\u584c\u8f90\u5c04\u63a2\u6d4b\u4eea\u663e\u793a\u51fa\u7684\u6570\u503c\u5230\u8fbe\u5371\u9669\u8303\u56f4\u540e\u64a4\u9000\u201d\uff0c\u867d\u7136\u6570\u503c\u4e00\u76f4\u90fd\u5728\u5b89\u5168\u8303\u56f4\u5185\uff0c\u4f46\u6211\u8ba4\u4e3a\u5f53\u65f6\u5df2\u7ecf\u4e0d\u53ef\u80fd\u83b7\u5f97\u66f4\u591a\u4fe1\u606f\uff0c\u5c31\u6ca1\u6709\u8981\u6c42\u5404\u6218\u6597\u5355\u4f4d\u4f7f\u7528\u62df\u6001\u56fe\u8c31\uff0c\u800c\u662f\u4e0b\u8fbe\u4e86\u64a4\u9000\u547d\u4ee4\u3002",
    "\u4e0e\u4f60\u4eec\u6218\u6597\u4e4b\u540e\uff0c\u516d\u5206\u4eea\u4e5f\u6ca1\u6709\u8ffd\u51fb\u4f60\u4eec\uff1f",
    "\u5f53\u65f6\u60c5\u51b5\u6df7\u4e71\uff0c\u6211\u4eec\u64a4\u79bb\u540e\u6ca1\u6709\u53d1\u73b0\u5979\u7684\u884c\u8e2a\u3002",
    "\u2026\u2026\u55ef\u3002",
    "\u6d85\u6258\u7684\u4fe1\u606f\u3001\u4e0e\u516d\u5206\u4eea\u7684\u4ea4\u6218\u60c5\u51b5\uff0c\u9644\u5728\u884c\u52a8\u62a5\u544a\u4e2d\uff0c\u5c3d\u5feb\u63d0\u4ea4\u3002|\u53e6\u5916\uff0c\u901a\u77e5\u7f8e\u73b2\uff0c\u5c1d\u8bd5\u53cd\u5411\u8ffd\u8e2a\u683c\u7433\u6b64\u524d\u4f7f\u7528\u7684\u7ebf\u8def\u2026\u2026\u867d\u7136\u8ffd\u8e2a\u6210\u529f\u7684\u53ef\u80fd\u6027\u4e0d\u9ad8\uff0c\u59d1\u4e14\u8bd5\u4e00\u8bd5\u3002",
    "\u597d\u3002",
    "\u7eb3\u7f8e\u897f\u4e1d\u5462\uff0c\u5979\u6709\u6ca1\u6709\u4ec0\u4e48\u503c\u5f97\u6ce8\u610f\u7684\u5730\u65b9\uff1f",
    "\u548c\u5f80\u5e38\u4e00\u6837\u3002",
    "\u2026\u2026",
    "\u6bd4\u5bf9\u73af\u5883\u60c5\u62a5\uff0c\u6211\u4eec\u73b0\u5728\u5230\u54ea\u91cc\u4e86\uff1f",
    "\u5f53\u524d\u5750\u6807\u70b97WE2WX3-EO021R4\uff0c\u8ddd\u79bb\u51c0\u5316\u4eea\u5f0f\u6240\u5728\u533a\u57df\u7ea6290KM\u3002\u73af\u5883\u60c5\u62a5\u663e\u793a\uff0c\u524d\u65b9\u5c5e\u4e8e\u574d\u584c\u98ce\u66b4\u9891\u53d1\u533a\u57df\uff0c\u76ee\u524d\u6211\u4eec\u6b63\u5728\u5c1d\u8bd5\u7ed5\u884c\u6b64\u533a\u57df\u3002\u9884\u8ba10006\u540e\u8fdb\u5165\u4e00\u5904\u6807\u793a\u6df1\u5ea6\u4e3a430M\u7684\u5ce1\u8c37\u3002",
    "\u53c8\u662f\u574d\u584c\u98ce\u66b4\u3001\u53c8\u662f\u5ce1\u8c37\uff1f|\u8ddd\u79bb\u62c9\u81f3500KM\u540e\u5bfb\u627e\u63a9\u4f53\uff0c\u51c6\u5907\u4fee\u6574\u3002",
    "\u654c\u88ad\uff01\u57fa\u5730\u8f66\u6307\u6325\u5ba4\u4e0a\u90e8\u7684\u6307\u5411\u6027\u70b8\u836f\u5df2\u88ab\u5f15\u7206\uff01",
    "\u2026\u2026\u53c8\u6765\uff1f"
  ]
]