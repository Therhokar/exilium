[
  [
    "\u5927\u77f3\u5934\u3001\u6709\u597d\u591a\uff01",
    "\uff08\u6d77\u4f26\u5a1c\u7ad9\u5728\u9760\u8fd1\u5e73\u53f0\u8fb9\u7f18\u7684\u4f4d\u7f6e\uff0c\u5979\u8d8a\u8fc7\u62a4\u680f\uff0c\u7262\u7262\u76ef\u7740\u8f66\u4f53\u8fb9\u7684\u5ca9\u77f3\u3002\uff09",
    "\uff08\u770b\u7740\u5979\u7684\u80cc\u5f71\uff0c\u6211\u601d\u7eea\u6709\u4e9b\u98d8\u6563\u3002\uff09",
    "\u592a\u9760\u8fb9\u4e86\uff0c\u4e0d\u8981\u6389\u4e0b\u53bb\uff0c\u6d77\u4f26\u5a1c\u3002",
    "\uff08\u770b\u5979\u60f3\u5c06\u624b\u4f38\u51fa\u62a4\u680f\uff0c\u53bb\u6478\u65c1\u8fb9\u7684\u5ca9\u77f3\uff0c\u6211\u4e00\u8fb9\u7ee7\u7eed\u601d\u8003\uff0c\u4e00\u8fb9\u4f38\u624b\u62c9\u4f4f\u4e86\u5979\u3002\uff09",
    "\u5927\u77f3\u5934\uff01|\u989c\u8272\u3001\u597d\u591a\u3001\u9001\u793c\u7269\uff01",
    "\u9001\u793c\u7269\u2014\u2014\u4f60\u8981\u9001\u7ed9\u8c01\uff0c\u5bc7\u5c14\u8299\uff1f",
    "\u5bc7\u5c14\u8299\u3001\u7ef4\u666e\u857e\uff01\u5c0f\u77f3\u5934\u3001\u8774\u8776\u7ed3\uff01\u90fd\u559c\u6b22\uff01",
    "\u55ef\uff0c\u90fd\u559c\u6b22\u3002",
    "\uff08\u2026\u2026\u5bc7\u5c14\u8299\u7684\u95ee\u9898\u9700\u8981\u89e3\u51b3\uff0c\u8bf4\u8d77\u6765\uff0c\u4e4b\u524d\u7eb3\u7f8e\u897f\u4e1d\u8bf4\u89c1\u8fc7\u90a3\u4e2a\u7b26\u53f7\uff0c\u8fd8\u6709\uff0c\u95ea\u7535\u2026\u2026\uff09",
    "\uff08\u2026\u2026\u6700\u8fd1\u53d1\u751f\u4e86\u592a\u591a\u4e8b\uff0c\u4e5f\u90fd\u5f88\u91cd\u8981\uff0c\u4f46\u5728\u89e3\u51b3\u7684\u8fc7\u7a0b\u4e2d\uff0c\u5c31\u4f1a\u6709\u65b0\u7684\u4e8b\u60c5\u53d1\u751f\uff0c\u4e5f\u5b9e\u5728\u662f\u2026\u2026\uff09",
    "\u6307\u3001\u6325\u3001\u5b98\uff01",
    "\u2026\u2026\u55ef\uff1f",
    "\u8981\u5927\u77f3\u5934\uff01\u4e00\u3001\u4e8c\u3001\u4e09\u3001\u56db\u3001\u4e94\uff01\u7ed9\u5bc7\u5c14\u8299\u3001\u7ed9\u7ef4\u666e\u857e\uff01",
    "\u4f4d\u7f6e\u592a\u9ad8\u4e86\uff0c\u4f1a\u6709\u5371\u9669\u3002",
    "\u5514\u2026\u2026\u62b1\uff01",
    "\uff08\u6d77\u4f26\u5a1c\u5411\u6211\u4f38\u51fa\u53cc\u624b\u3002\uff09",
    "\u2026\u2026\u8fd9\u53c8\u662f\u8c01\u6559\u4f60\u7684\uff1f\u4e0d\u80fd\u662f\u5bc7\u5c14\u8299\u5427\uff1f",
    "\u7ef4\u666e\u857e\uff01\u65b0\u2026\u2026\u65b0\u8d70\u4f4d\uff1f\u79d8\u5bc6\u6b66\u5668\uff01",
    "\u2026\u2026\u5982\u679c\u4f60\u7684\u610f\u601d\u662f\uff0c\u8fd9\u662f\u5979\u9700\u8981\u7528\u5728\u4e0b\u6b21\u6218\u6597\u4e2d\u7684\u79d8\u5bc6\u6b66\u5668\uff0c\u6211\u5e0c\u671b\u4e0b\u6b21\u6218\u6597\u6765\u5f97\u8d8a\u665a\u8d8a\u597d\u3002",
    "\u62b1\u8d77\u6765\u3001\u53ef\u7231\uff1f\u6307\u6325\u5b98\u3001\u4e0d\u559c\u6b22\uff1f",
    "\u4e0d\uff0c\u91cd\u70b9\u4e0d\u5728\u8fd9\u91cc\u3002",
    "\uff08\u2026\u2026\u683c\u7433\u90a3\u8fb9\u8fd8\u4e0d\u77e5\u9053\u53d1\u751f\u4e86\u4ec0\u4e48\uff0c\u4e0d\u8fc7\u65e2\u7136\u6ca1\u6709\u8054\u7cfb\u6211\uff0c\u90a3\u5c31\u8bf4\u660e\u60c5\u51b5\u53ef\u63a7\u2026\u2026\uff09",
    "\uff08\u76ef\u7740\u6211\u770b\u4e86\u4e00\u4f1a\uff0c\u53d1\u73b0\u6211\u6ca1\u6709\u7406\u4f1a\u5979\uff0c\u6d77\u4f26\u5a1c\u6487\u4e86\u6487\u5634\uff0c\u653e\u4e0b\u624b\u8d70\u5230\u6211\u8eab\u8fb9\uff0c\u62fd\u4f4f\u4e86\u6211\u7684\u8896\u5b50\u3002\uff09",
    "\u4e0d\u62b1\u3001\u8981\u5927\u77f3\u5934\u3001\u6307\u6325\u5b98\uff01",
    "\u4f60\u7b49\u4e00\u4e0b\uff0c\u6211\u770b\u770b\u901a\u8baf\u2014\u2014\u95ea\u7535\uff1f",
    "\uff08\u9002\u65f6\u54cd\u8d77\u7684\u901a\u8baf\u63d0\u793a\u77ed\u6682\u8f6c\u79fb\u4e86\u6d77\u4f26\u5a1c\u7684\u6ce8\u610f\u529b\uff0c\u6d77\u4f26\u5a1c\u6643\u52a8\u6211\u8896\u5b50\u7684\u52a8\u4f5c\u505c\u4e86\u4e0b\u6765\u3002\uff09",
    "\u514b\u7f57\u4e3d\u79d1\u4f20\u6765\u4e86\u901a\u8baf\u3002",
    "\u76d1\u63a7\u6709\u6d88\u606f\u4e86\uff1f",
    "\u662f\u7684\u3002\u5979\u548c\u7eb3\u7f8e\u897f\u4e1d\u53d1\u73b0\u6b64\u524d\u51fa\u73b0\u5728\u56de\u4f20\u5f71\u50cf\u4e2d\u7684\u9ed1\u8272\u4eba\u5f71\u5355\u72ec\u5916\u51fa\uff0c\u6240\u5904\u4f4d\u7f6e\u8ddd\u79bb\u6b64\u524d\u7591\u4f3c\u5e15\u62c9\u8482\u65af\u6210\u5458\u7684\u5c45\u4f4f\u70b9\u7ea6\u670918KM\uff0c\u63a8\u6d4b\u77ed\u671f\u5185\u4e0d\u4f1a\u56de\u5f52\u3002",
    "\u5916\u51fa\u7684\u76ee\u7684\u662f\uff1f",
    "\u4e0d\u660e\uff0c\u76ee\u6807\u5bf9\u8c61\u4ecd\u5904\u4e8e\u6301\u7eed\u79fb\u52a8\u4e2d\u3002|\u53e6\u5916\u2026\u2026",
    "\u4ec0\u4e48\uff1f",
    "\u514b\u7f57\u4e3d\u79d1\u8865\u5145\uff0c\u5979\u770b\u5230\u6b64\u524d\u6211\u4eec\u7a81\u5165\u7684\u5c45\u4f4f\u70b9\u4e2d\uff0c\u5e38\u4f4f\u4eba\u7fa4\u6b63\u5728\u6301\u7eed\u589e\u591a\uff0c\u5c45\u4f4f\u70b9\u5185\u4e5f\u5728\u540c\u4e00\u65f6\u95f4\u65b0\u589e\u5927\u91cf\u201c\u5854\u201d\u5f62\u5efa\u7b51\u3002",
    "\u4f9d\u636e\u5979\u6240\u89c2\u6d4b\u5230\u7684\u60c5\u51b5\uff0c\u5979\u8ba4\u4e3a\uff0c\u4ee5\u6b64\u5c45\u4f4f\u70b9\u4e3a\u6838\u5fc3\uff0c\u5468\u56f4\u4eba\u7fa4\u6b63\u5728\u4e0d\u65ad\u805a\u96c6\u3002\u8fd9\u662f\u5979\u548c\u7eb3\u7f8e\u897f\u4e1d\u76d1\u63a7\u4e00\u5468\u4ee5\u6765\u9996\u6b21\u53d1\u73b0\u76ee\u6807\u5bf9\u8c61\u5355\u72ec\u5916\u51fa\uff0c\u5982\u679c\u9700\u8981\u5c06\u5176\u6355\u83b7\uff0c\u5979\u5efa\u8bae\u6211\u4eec\u7acb\u5373\u884c\u52a8\u3002",
    "\u5bc7\u5c14\u8299\u7684\u6743\u9650\u767b\u8bb0\u5b8c\u6210\u4e86\u5417\uff1f",
    "\u8fd8\u6ca1\u6709\u3002|\u2026\u2026\u5bc7\u5c14\u8299\u2026\u2026",
    "\u2026\u2026\u867d\u7136\u7f8e\u73b2\u4e00\u76f4\u548c\u5bc7\u5c14\u8299\u5728\u4e00\u8d77\u2026\u2026\u5bc7\u5c14\u8299\uff0c\u5979\u4ecd\u7136\u62d2\u7edd\u91cd\u65b0\u767b\u8bb0\u6743\u9650\u5f52\u5c5e\u3002",
    "\u597d\u5427\u3002\u51c6\u5907\u884c\u52a8\uff0c\u6ce8\u610f\u4fdd\u8bc1\u5bc7\u5c14\u8299\u5b89\u5168\u3002",
    "\u4e86\u89e3\u3002",
    "\uff08\u901a\u8baf\u7ed3\u675f\uff0c\u5b89\u9759\u4e86\u51e0\u5206\u949f\u7684\u6d77\u4f26\u5a1c\u7acb\u523b\u5927\u529b\u6325\u624b\uff0c\u8bd5\u56fe\u5438\u5f15\u6211\u7684\u6ce8\u610f\u529b\u3002\uff09",
    "\u6307\u6325\u5b98\u3001\u6307\u6325\u5b98\uff01",
    "\u95ea\u7535\u5979\u4eec\u9a6c\u4e0a\u5c31\u4f1a\u79bb\u5f00\u57fa\u5730\u8f66\uff0c\u6211\u4eec\u9700\u8981\u56de\u53bb\u4e86\u3002",
    "\u56de\u53bb\u3001\u4e0d\u56de\u53bb\u2014\u2014\u8981\u793c\u7269\uff01",
    "\u2026\u2026\u5514\u2026\u2026",
    "\u5bc7\u5c14\u8299\u3001\u4e0d\u5f00\u5fc3\u3001\u8981\u793c\u7269\uff01|\u9001\u793c\u7269\u3001\u597d\u5173\u7cfb\u3001\u90fd\u5f00\u5fc3\uff01",
    "\uff08\u98ce\u6c99\u63a0\u8fc7\u827e\u83ab\u53f7\u9876\u7aef\uff0c\u5439\u5165\u5ce1\u8c37\uff0c\u4e00\u5757\u7565\u5927\u7684\u788e\u77f3\u968f\u7740\u5ce1\u8c37\u4e2d\u731b\u70c8\u7684\u98ce\u7838\u5411\u6d77\u4f26\u5a1c\u3002\uff09",
    "\uff08\u6211\u7acb\u523b\u5c06\u5979\u62c9\u5f00\u3002\uff09",
    "\uff08\u77f3\u5757\u5728\u5979\u7684\u624b\u81c2\u4e0a\u7559\u4e0b\u4e86\u4e00\u9053\u7ec6\u5fae\u7684\u5212\u75d5\uff0c\u6d77\u4f26\u5a1c\u4f3c\u4e4e\u6ca1\u6709\u5bdf\u89c9\u5230\uff0c\u7ad9\u7a33\u540e\uff0c\u5979\u7591\u60d1\u5730\u770b\u7740\u6211\u3002\uff09",
    "\u8981\u8df3\u821e\uff1f",
    "\u2026\u2026\u4e0d\u2026\u2026\u4f60\u2026\u2026",
    "\uff08\u6211\u6307\u4e86\u6307\u90a3\u9053\u5212\u75d5\u3002\uff09",
    "\uff08\u6d77\u4f26\u5a1c\u987a\u7740\u6211\u7684\u52a8\u4f5c\u770b\u4e86\u4e00\u773c\uff0c\u7acb\u523b\u7529\u5f00\u6211\u7684\u624b\uff0c\u540e\u8df3\u4e00\u6b65\uff0c\u6342\u4f4f\u5212\u75d5\u3002\uff09",
    "\u554a\u3001\u4e0d\u75bc\u3001\u6ca1\u95ee\u9898\u3001\u53ef\u4ee5\u2026\u2026\u53ef\u4ee5\u4f7f\u7528\uff01",
    "\u4ec0\u4e48\u53ef\u4ee5\u4f7f\u7528\uff1f\u4f24\u53e3\u770b\u8d77\u6765\u4e0d\u6df1\uff0c\u4e0d\u8fc7\u8fd8\u662f\u56de\u53bb\u8ba9\u5bc7\u5c14\u8299\u5e2e\u4f60\u5904\u7406\u4e00\u4e0b\u5427\u3002",
    "\u56de\u53bb\u3001\u8981\u56de\u53bb\uff01\u6307\u6325\u5b98\u3001\u73b0\u5728\u56de\u53bb\u2014\u2014\u554a\u3001\u793c\u7269\u3001\u5bc7\u5c14\u8299\uff01",
    "\uff08\u6d77\u4f26\u5a1c\u8868\u73b0\u5f97\u975e\u5e38\u614c\u4e71\u3002\uff09",
    "\uff08\u5979\u770b\u4e86\u770b\u62a4\u680f\u8fb9\u7684\u5927\u77f3\u5934\uff0c\u53c8\u770b\u4e86\u770b\u6211\u3002\uff09",
    "\uff08\u6211\u7684\u6ce8\u610f\u529b\u4ecd\u7136\u5728\u6d77\u4f26\u5a1c\u624b\u81c2\u7684\u4f24\u53e3\u4e0a\u2014\u2014\uff09",
    "\uff08\u2026\u2026\u90a3\u9053\u4e4b\u524d\u5fae\u5fae\u6e17\u8840\u7684\u4f24\u53e3\uff0c\u5df2\u7ecf\u51fa\u73b0\u4e86\u6108\u5408\u7684\u8ff9\u8c61\u3002\uff09",
    "\u2026\u2026\u793c\u7269\u2026\u2026",
    "\u793c\u7269\u6211\u6765\u60f3\u529e\u6cd5\u3002\u6211\u4eec\u5148\u56de\u53bb\u3002",
    "\u597d\u597d\u3001\u56de\u53bb\uff01",
    "\uff08\u6d77\u4f26\u5a1c\u7acb\u523b\u54cd\u5e94\u6211\u7684\u8bdd\uff0c\u8f6c\u8eab\u5c31\u5411\u7740\u5347\u964d\u53f0\u51fa\u53e3\u8dd1\u53bb\u3002\uff09",
    "\uff08\u7ad9\u5728\u539f\u5730\uff0c\u6211\u770b\u7740\u5979\u79bb\u5f00\u7684\u80cc\u5f71\u3002\uff09",
    "\u2026\u2026\u6d88\u5931\u7684\u2026\u2026\u79d1\u672a\u5854\u2026\u2026\uff1f"
  ]
]