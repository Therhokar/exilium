[
  [
    "\u4e34\u65f6\u51bb\u7ed3\u53d6\u6d88\u5b8c\u6210\u3001\u5411\u529b\u5f15\u64ce\u91cd\u542f\u5b8c\u6210\u3001\u5f53\u524d\u901f\u5ea660KM/H\u2026\u2026\u5168\u7cfb\u7edf\u635f\u4f2417%\u2026\u2026\u91cd\u542f\u5931\u8d25\uff0c\u5373\u5c06\u542f\u52a8\u4fee\u590d\u7a0b\u5e8f\u2026\u2026\u76ee\u524d\u8fdb\u5ea63%\uff0c\u9884\u8ba10259\u540e\u5b8c\u6210\u4fee\u590d\uff01",
    "\u65ad\u5f00\u57fa\u5730\u8f66\u7f51\u7edc\u8fde\u63a5\uff0c\u5c4f\u853d\u5168\u6ce2\u6bb5\u4fe1\u53f7\uff0c\u5e76\u9488\u5bf9\u59ae\u7279\u67b6\u8bbe\u4fe1\u53f7\u5c4f\u853d\u88c5\u7f6e\u3002",
    "\u4fe1\u53f7\u5c4f\u853d\u88c5\u7f6e\uff1f\u6211\u3001\u6211\u8bb0\u5f97\u4e0a\u6b21\u8bf4\u8981\u7ed9\u6d77\u4f26\u5a1c\u7684\u7bb1\u5b50\u7528\uff0c\u96f6\u4ef6\u6211\u90fd\u5b9a\u4e86\u2026\u2026\u662f\u8fd8\u5728\u4ed3\u5e93\u91cc\u5417\uff0c\u6211\u627e\u627e\u8bb0\u5f55\u2026\u2026",
    "\uff08\u5165\u4fb5\u88ab\u963b\u65ad\uff0c\u59ae\u7279\u534a\u8eba\u5728\u5730\u4e0a\uff0c\u8138\u4e0a\u4ecd\u7136\u4fdd\u6301\u7740\u67d4\u548c\u7684\u5fae\u7b11\uff0c\u4f46\u76ee\u5149\u5df2\u7ecf\u660e\u663e\u653e\u7a7a\u3002\uff09",
    "\u7eb3\u7f8e\u897f\u4e1d\uff0c\u514b\u7f57\u4e3d\u79d1\uff0c\u63a7\u5236\u4f4f\u201c\u59ae\u7279\u201d\u3002|\u201c\u59ae\u7279\u201d\uff0c\u56de\u7b54\u6211\u7684\u95ee\u9898\u3002",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u5f2f\u8170\u5c06\u59ae\u7279\u62c9\u8d77\u6765\uff0c\u6309\u5230\u4e00\u8fb9\u7684\u5899\u58c1\u4e0a\u3002\uff09",
    "\u5567\uff0c\u53eb\u4f60\u5462\u542c\u5230\u6ca1\uff0c\u521a\u624d\u4e0d\u8fd8\u4e71\u52a8\u5417\u600e\u4e48\u5c31\u4e0d\u52a8\u4e86\uff1f",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u62cd\u4e86\u62cd\u59ae\u7279\u7684\u80cc\uff0c\u59ae\u7279\u7ef4\u6301\u5fae\u7b11\u7684\u8868\u60c5\uff0c\u6beb\u65e0\u53cd\u5e94\u3002\uff09",
    "\u2026\u2026\u82e6\u75db\u8513\u5ef6\u8fc7\u6d77\u9762\u2026\u2026\u63a5\u53d7\u751f\u547d\u7684\u5b89\u606f\u2026\u2026",
    "\u8c01\u592a\u5927\u52b2\u4e86\uff1f\uff01\u4f60\u51e0\u67aa\u90fd\u6ca1\u6253\u4e2d\u8ba9\u5979\u4e71\u8dd1\u6700\u540e\u51b2\u6211\u8fc7\u6765\u8ddd\u79bb\u8fd9\u4e48\u8fd1\u6211\u53ea\u80fd\u4e0a\u624b\u4e86\u597d\u5427\uff1f\uff01",
    "\u2026\u2026\u4e1b\u751f\u7684\u8346\u68d8\uff0c\u663e\u51fa\u5947\u5f02\u8f89\u5f69\u2026\u2026",
    "\u90fd\u8bf4\u4e86\u4e0d\u662f\u6211\u7684\u95ee\u9898\u884c\u4e0d\u884c\uff1f\u5979\u521a\u624d\u8fd8\u5728\u52a8\u5462\u80af\u5b9a\u6ca1\u574f\uff01",
    "\u5582\uff0c\u5582\uff0c\u5305\u5b50\u5934\uff0c\u542c\u5f97\u5230\u5417\uff1f",
    "\uff08\u59ae\u7279\u6beb\u65e0\u58f0\u606f\u3002\uff09",
    "\u2026\u2026\u63a7\u5236\u4f4f\u5b83\u7684\u52a8\u4f5c\uff0c\u514b\u7f57\u4e3d\u79d1\u3002\u7f8e\u73b2\uff0c\u627e\u5230\u4e86\u5417\uff1f",
    "\u8fd8\u5728\u627e\uff0c\u5e94\u8be5\u662f\u5728\u8fd9\u91cc\u6ca1\u9519\u2026\u2026\u554a\uff0c\u627e\u5230\u4e86\uff01\u5728\u4e2d\u67a2\u533a\u7684\u7269\u8d44\u7bb1\u91cc\uff01\u6211\u53bb\u62ff\u96f6\u4ef6\uff0c\u961f\u957f\uff0c\u4fee\u590d\u7a0b\u5e8f\u7684\u76d1\u63a7\u9ebb\u70e6\u4f60\u4e86\uff01",
    "\u2026\u2026",
    "\u961f\u957f\uff1f",
    "\u2014\u2014\u662f\uff1f",
    "\u662f\u521a\u624d\u7684\u6218\u6597\u9020\u6210\u635f\u4f24\u4e86\u5417\uff0c\u6211\u3001\u6211\u5e2e\u4f60\u68c0\u4fee\u4e00\u4e0b\uff0c\u961f\u957f\uff1f",
    "\u6211\u6ca1\u4e8b\uff0c\u662f\u6211\u6ca1\u6ce8\u610f\u5230\u3002\u7f8e\u73b2\uff0c\u62b1\u6b49\uff0c\u9700\u8981\u6211\u505a\u4ec0\u4e48\uff1f",
    "\u57fa\u5730\u8f66\u7684\u4fee\u590d\u7a0b\u5e8f\u6b63\u5728\u8fd0\u884c\uff0c\u4f46\u6211\u8981\u53bb\u4e2d\u67a2\u533a\u627e\u4e00\u627e\u96f6\u4ef6\uff0c\u961f\u957f\uff0c\u80fd\u9ebb\u70e6\u4f60\u5e2e\u6211\u6ce8\u610f\u4e00\u4e0b\u5417\uff1f",
    "\u597d\u3002",
    "\uff08\u95ea\u7535\u8d70\u5230\u603b\u63a7\u53f0\u524d\uff0c\u63a5\u66ff\u4e86\u7f8e\u73b2\u7684\u5de5\u4f5c\u3002\uff09",
    "\uff08\u7f8e\u73b2\u6709\u4e9b\u614c\u5f20\u5730\u5411\u7740\u6307\u6325\u5ba4\u7684\u5165\u53e3\u6ed1\u95e8\u8dd1\u53bb\uff0c\u4e2d\u9014\u7ecf\u8fc7\u59ae\u7279\u8fd8\u7279\u610f\u653e\u8f7b\u4e86\u811a\u6b65\u3002\uff09",
    "\u6211\u9a6c\u4e0a\u5c31\u56de\u6765\uff01\u961f\u957f\uff0c\u6709\u54ea\u91cc\u4e0d\u5bf9\uff0c\u8981\u7acb\u523b\u544a\u8bc9\u6211\uff01",
    "\u55ef\u3002",
    "\u9700\u4e0d\u9700\u8981\u7ef4\u666e\u857e\u5e2e\u4f60\u642c\u8bbe\u5907\uff1f",
    "\u4ec0\u4e48\u5440\u2014\u2014\u6307\u6325\u5b98\u5728\u8bf4\u4ec0\u4e48\u5440\uff01\u7ef4\u666e\u857e\u4f1a\u642c\u4e0d\u52a8\u7684\u5566\uff01",
    "\u7ef4\u666e\u857e\u4e0d\u613f\u610f\u5c31\u4e0d\u7528\u4e86\u5427\uff1f\u4e0d\u662f\u975e\u5e38\u91cd\uff0c\u6211\u81ea\u5df1\u53bb\u6ca1\u95ee\u9898\u7684\u2026\u2026",
    "\u4f1a\u5f88\u91cd\u5417\uff1f\u5514\u2026\u2026\u90a3\u7ef4\u666e\u857e\u8fd8\u662f\u548c\u4f60\u4e00\u8d77\u53bb\u5427\uff01|\u5bc7\u5c14\u8299\uff0c\u7ef4\u666e\u857e\u4e0d\u5728\u7684\u65f6\u5019\uff0c\u4f60\u8981\u597d\u597d\u7559\u5728\u8fd9\u91cc\u7b49\u7ef4\u666e\u857e\u54e6\uff01",
    "\uff08\u6309\u4f4f\u5934\u4e0a\u6709\u4e9b\u677e\u52a8\u7684\u8774\u8776\u7ed3\uff0c\u7ef4\u666e\u857e\u677e\u5f00\u4e86\u62c9\u7740\u5bc7\u5c14\u8299\u7684\u624b\u3002\uff09",
    "\uff08\u5979\u5c0f\u8dd1\u51e0\u6b65\uff0c\u8ffd\u4e0a\u4e86\u5df2\u7ecf\u7ad9\u5230\u4e86\u6307\u6325\u5ba4\u5165\u53e3\u6ed1\u95e8\u8fb9\u7684\u7f8e\u73b2\u3002\uff09",
    "\u7f8e\u73b2\uff0c\u6211\u4eec\u8d70\u5427~",
    "\u554a\u3001\u53ef\u4ee5\u7684\u5417\uff1f\u8c22\u8c22\u4f60\u7ef4\u666e\u857e\uff0c\u6211\u4eec\u53bb\u4ed3\u5e93\uff01",
    "\u597d\u5440\uff01\u5bf9\u4e86\u5bf9\u4e86\uff0c\u7f8e\u73b2\uff0c\u4f60\u5e2e\u7ef4\u666e\u857e\u770b\u770b\u5934\u4e0a\u7684\u8fd9\u4e2a\u8774\u8776\u7ed3\u597d\u4e0d\u597d\uff0c\u5b83\u7fd8\u8d77\u6765\u4e86\uff0c\u7ef4\u666e\u857e\u6ca1\u6709\u529e\u6cd5\u628a\u5b83\u6574\u7406\u597d\u5440\u2026\u2026",
    "\uff08\u5bc7\u5c14\u8299\u7684\u624b\u505c\u5728\u534a\u7a7a\u4e2d\u3002\u5979\u7684\u624b\u6307\u8737\u7f29\u8d77\u6765\uff0c\u7136\u540e\u624b\u81c2\u5782\u5230\u4e86\u8eab\u4fa7\u3002\uff09",
    "\uff08\u95ea\u7535\u7ad9\u5728\u603b\u63a7\u53f0\u524d\uff0c\u5e73\u9759\u5730\u76ef\u7740\u663e\u793a\u5c4f\uff0c\u4f3c\u4e4e\u6ca1\u6709\u6ce8\u610f\u5230\u6211\u8fd9\u91cc\u7684\u72b6\u51b5\u3002\uff09",
    "\u5bc7\u5c14\u8299\u3002",
    "\uff08\u731b\u7136\u88ab\u6211\u53eb\u4f4f\uff0c\u5bc7\u5c14\u8299\u731d\u7136\u62ac\u5934\uff0c\u4e0e\u6211\u5bf9\u4e0a\u4e86\u89c6\u7ebf\u3002\uff09",
    "\u662f\u2014\u2014\u662f\uff1f",
    "\u5173\u4e8e\u201c\u59ae\u7279\u201d\uff0c\u4f60\u8fd8\u77e5\u9053\u4ec0\u4e48\uff1f",
    "\u2026\u2026\u4f60\u60f3\u95ee\u4ec0\u4e48\u3002",
    "\uff08\u5bc7\u5c14\u8299\u7acb\u523b\u907f\u5f00\u4e86\u6211\u7684\u89c6\u7ebf\uff0c\u5979\u5c06\u76ee\u5149\u653e\u5728\u88ab\u514b\u7f57\u4e3d\u79d1\u538b\u5728\u5899\u4e0a\u7684\u59ae\u7279\u8eab\u4e0a\u3002\uff09",
    "\uff08\u7ecf\u5386\u4e86\u77ed\u6682\u7684\u6218\u6597\uff0c\u59ae\u7279\u4e00\u90e8\u5206\u8863\u670d\u6ed1\u843d\uff0c\u9732\u51fa\u88ab\u63a9\u85cf\u8d77\u6765\u7684\u76ae\u80a4\u2014\u2014\u989c\u8272\u4e0e\u8d28\u611f\u90fd\u65e0\u9650\u63a5\u8fd1\u4e8e\u4eba\u7c7b\u7684\u76ae\u80a4\u3002\uff09",
    "\u6240\u6709\u3002",
    "\u5b83\u5728\u5e15\u62c9\u8482\u65af\u4f53\u7cfb\u4e2d\u7684\u5730\u4f4d\u3001\u88ab\u5b89\u7f6e\u5728\u8fd9\u91cc\u7684\u76ee\u7684\u3001\u51fa\u73b0\u73b0\u5728\u8fd9\u79cd\u5bf9\u5916\u754c\u63a5\u89e6\u65e0\u53cd\u5e94\u60c5\u51b5\u7684\u539f\u56e0\uff0c\u4ee5\u53ca\u2014\u2014\u5b83\u548c\u5b83\u5468\u8fb9\u7684\u90a3\u4e9b\u7a7f\u7740\u957f\u888d\u7684\u4eba\uff0c\u662f\u4e0d\u662f\u771f\u7684\u5c5e\u4e8e\u201c\u5e15\u62c9\u8482\u65af\u201d\u3002",
    "\u6211\u4e0d\u77e5\u9053\u3002",
    "\u5567\uff0c\u77e5\u9053\u4f60\u5c31\u8bf4\u554a\uff0c\u8bf4\u51fa\u6765\u6307\u6325\u5b98\u4e5f\u4e0d\u4f1a\u8ba9\u7f8e\u73b2\u62c6\u4e86\u4f60\u3002",
    "\u514b\u7f57\u4e3d\u79d1\uff01",
    "\u2026\u2026\u72ed\u7a84\u7684\u6811\u4e1b\uff0c\u65e0\u6cd5\u5bb9\u7eb3\u7684\u6c79\u6d8c\u6d6a\u6f6e\u2026\u2026",
    "\u884c\u884c\u6211\u95ed\u5634\u597d\u4e86\u5427\uff0c\u4f60\u4eec\u95ee\u4f60\u4eec\u95ee\u3002",
    "\u5bc7\u5c14\u8299\u3002",
    "\u2026\u2026\u5bc7\u5c14\u8299\uff0c\u4f60\u76ee\u524d\u6240\u77e5\u9053\u7684\u4fe1\u606f\uff0c\u5bf9\u4e8e\u89e3\u51b3\u8d6b\u745e\u5c24\u662f\u5fc5\u987b\u7684\u3002",
    "\u83b7\u5f97\u8db3\u591f\u7684\u4fe1\u606f\uff0c\u6211\u4eec\u624d\u80fd\u4fdd\u8bc1\u987a\u5229\u89e3\u51b3\u4ed6\u4eec\u2014\u2014\u5305\u62ec\u8d6b\u745e\u5c24\u3002",
    "\u2026\u2026\u6211\u4e0d\u77e5\u9053\u3002",
    "\u6211\u4e0d\u77e5\u9053\u2026\u2026\u6211\u5728\u90a3\u91cc\u7684\u65f6\u5019\uff0c\u6ca1\u6709\u542c\u8fc7\u201c\u5e15\u62c9\u8482\u65af\u201d\u8fd9\u4e2a\u540d\u5b57\u3002",
    "\u90a3\u4e48\uff0c\u8fd9\u4e2a\u7b26\u53f7\uff0c\u4f60\u89c1\u8fc7\u6ca1\u6709\uff1f",
    "\uff08\u6211\u64cd\u4f5c\u4e86\u4e00\u4e0b\u901a\u8baf\u7ec8\u7aef\uff0c\u4f9d\u7167\u6211\u7684\u8bb0\u5fc6\uff0c\u5c06\u4e00\u4e2a\u4ee5\u5706\u5f62\u548c\u5c16\u89d2\u7ec4\u6210\u7684\u56fe\u6848\u753b\u4e86\u51fa\u6765\u3002\uff09",
    "\u5bc7\u5c14\u8299\uff0c\u4f60\u7684\u901a\u8baf\u4ee3\u7801\uff1f",
    "\u2026\u2026",
    "\uff08\u72b9\u8c6b\u4e86\u4e00\u4e0b\uff0c\u5bc7\u5c14\u8299\u8fd8\u662f\u5c06\u624b\u653e\u5728\u4e86\u81ea\u5df1\u7684\u901a\u8baf\u7ec8\u7aef\u4e0a\u3002\uff09",
    "\u6536\u5230\u4e86\u3002\u7b26\u53f7\u6211\u53d1\u7ed9\u4f60\u3002",
    "\uff08\u6211\u66f4\u65b0\u4e86\u4e00\u4e0b\u8bb0\u5f55\u5728\u201c\u5bc7\u5c14\u8299\u201d\u540d\u5b57\u4e0b\u7684\u901a\u8baf\u4ee3\u7801\uff0c\u7136\u540e\u5c06\u6211\u6b64\u524d\u753b\u7684\u56fe\u6848\u53d1\u7ed9\u4e86\u5bc7\u5c14\u8299\u3002\uff09",
    "\u89c1\u8fc7\u3002",
    "\uff08\u5bc7\u5c14\u8299\u7acb\u523b\u70b9\u5f00\u4e86\u6d88\u606f\uff0c\u53c8\u523b\u610f\u653e\u7f13\u4e86\u9605\u8bfb\u6d88\u606f\u7684\u52a8\u4f5c\u3002\uff09",
    "\uff08\u5173\u95ed\u6d88\u606f\u754c\u9762\uff0c\u5bc7\u5c14\u8299\u6469\u6332\u4e86\u4e00\u4e0b\u81ea\u5df1\u7684\u901a\u8baf\u7ec8\u7aef\uff0c\u62ac\u8d77\u5934\uff0c\u76f4\u89c6\u6211\u3002\uff09",
    "\u5728\u90a3\u4e2a\u4ed6\u4eec\u79f0\u4f5c\u201c\u805a\u843d\u201d\u7684\u5730\u70b9\uff0c\u4e00\u4e9b\u5efa\u7b51\u7684\u5899\u58c1\u4e0a\u3001\u5730\u9762\u4e0a\uff0c\u6211\u90fd\u770b\u5230\u4e86\u548c\u5b83\u5f88\u50cf\u7684\u56fe\u6848\u3002|\u201c\u59ae\u7279\u201d\uff0c\u6211\u731c\u5979\u662f\u201c\u805a\u843d\u201d\u7684\u2026\u2026\u7ba1\u7406\u8005\uff1f",
    "\u4ec0\u4e48\u610f\u601d\uff1f",
    "\u6211\u5728\u201c\u805a\u843d\u201d\u7684\u65f6\u95f4\u592a\u77ed\uff0c\u4ed6\u4eec\u8bf4\u7684\u8bdd\u5f88\u591a\u6211\u90fd\u542c\u4e0d\u61c2\u3002\u53ea\u4e0d\u8fc7\uff0c\u6240\u6709\u65b0\u6765\u7684\u4eba\uff0c\u90fd\u4f1a\u88ab\u96c6\u4e2d\u8d77\u6765\u5e26\u53bb\u89c1\u201c\u59ae\u7279\u201d\u2014\u2014\u5c31\u662f\u514b\u7f57\u4e3d\u79d1\u4f60\u4eec\u53c2\u52a0\u7684\u90a3\u79cd\u2026\u2026",
    "\u8d6b\u745e\u5c24\u4e5f\u5728\u91cc\u9762\u2026\u2026\u4ed6\u4e00\u76f4\u90fd\u5728\u59ae\u7279\u8eab\u8fb9\uff0c\u5982\u679c\u6211\u7684\u706b\u63a7\u6838\u5fc3\u2014\u2014\u6211\u65e9\u5c31\u8be5\u2014\u2014",
    "\u5443\uff0c\u5bc7\u5c14\u8299\uff0c\u4f60\u4e5f\u770b\u5230\u4e86\uff0c\u59ae\u7279\u73b0\u5728\u6beb\u65e0\u53cd\u5e94\uff0c\u8fd9\u79cd\u60c5\u51b5\u4f60\u770b\u5230\u8fc7\u5417\uff1f",
    "\u2026\u2026\u6211\u4e0d\u77e5\u9053\u3002",
    "\u6211\u4e0d\u77e5\u9053\uff0c\u4f46\u5728\u201c\u805a\u843d\u201d\u7684\u65f6\u5019\uff0c\u6211\u6709\u65f6\u5019\u4f1a\u770b\u5230\u5979\u7ef4\u6301\u540c\u6837\u7684\u59ff\u52bf\u5f88\u957f\u65f6\u95f4\u3002",
    "\u5728\u505a\u4ec0\u4e48\uff1f",
    "\u6709\u65f6\u5019\u662f\u5728\u770b\u7740\u4ec0\u4e48\uff0c\u6709\u65f6\u5019\u5c31\u53ea\u662f\u7ad9\u5728\u539f\u5730\uff0c\u6709\u65f6\u5019\u662f\u5728\u548c\u5f92\u4ece\u5bf9\u8bdd\u2014\u2014\u4ed6\u4eec\u7684\u4e92\u76f8\u79f0\u547c\u662f\u201c\u5f92\u4ece\u201d\u3002",
    "\u5f92\u4ece\u2026\u2026",
    "\u65f6\u95f4\u3001\u5730\u70b9\uff0c\u6709\u6ca1\u6709\u7279\u522b\u7684\uff1f",
    "\u6ca1\u6709\u3002|\u5979\u51fa\u73b0\u8fd9\u79cd\u72b6\u51b5\u7684\u6b21\u6570\u975e\u5e38\u5c11\uff0c\u6211\u4e5f\u6ca1\u6709\u7279\u522b\u6ce8\u610f\u3002",
    "\u662f\u600e\u4e48\u6062\u590d\u8fc7\u6765\u7684\uff1f",
    "\u4e0d\u77e5\u9053\u3002\u5728\u6211\u770b\u6765\uff0c\u5c31\u662f\u666e\u666e\u901a\u901a\u6062\u590d\u4e86\u6b63\u5e38\u2014\u2014\u6211\u8fd8\u4ee5\u4e3a\u5979\u5728\u53d1\u5446\u3002",
    "\u5982\u679c\u5979\u80fd\u5165\u4fb5\u4e8c\u7ea7\u5e73\u5c42\u2014\u2014\u5979\u2014\u2014\u662f\u4eba\u5f62\uff1f\u4e0d\u8fc7\u521a\u624d\uff0c\u6211\u4eec\u63a5\u5165\u7684\u4e8c\u7ea7\u5e73\u5c42\uff0c\u770b\u8d77\u6765\u6709\u70b9\u4e0d\u4e00\u6837\u2014\u2014\u770b\u8d77\u6765\uff0c\u5f88\u5947\u602a\u2014\u2014",
    "\u662f\u8bf4\u90a3\u91cc\u9762\u4e71\u4e03\u516b\u7cdf\u7684\u4e1c\u897f\u5427\uff1f\u5567\uff0c\u4f4d\u7f6e\u4e71\u4e03\u516b\u7cdf\uff0c\u989c\u8272\u4e5f\u4e71\u4e03\u516b\u7cdf\uff0c\u8fd8\u627e\u4e0d\u7740\u8def\uff0c\u70e6\u90fd\u70e6\u6b7b\u4e86\u3002|\u54e6\u5bf9\u4e86\uff0c\u961f\u957f\u521a\u624d\u65ad\u5f00\u6620\u5c04\u7684\u65f6\u5019\u8bf4\u597d\u50cf\u53d1\u73b0\u4e86\u4ec0\u4e48\u2026\u2026",
    "\u95ea\u7535\uff1f",
    "\u2026\u2026\u6b64\u6b21\u63a5\u5165\u4e8c\u7ea7\u5e73\u5c42\u540e\uff0c\u6211\u4eec\u5728\u73af\u5883\u5404\u5904\u89c2\u5bdf\u5230\u4e0d\u89c4\u5f8b\u5206\u5e03\u7684\u5927\u91cf\u7269\u4ef6\u3002\u7269\u4ef6\u8868\u9762\u51fa\u73b0\u8272\u5f69\u6df7\u6742\uff0c\u7c7b\u578b\u548c\u72b6\u6001\u4e0d\u80fd\u5b8c\u5168\u5206\u8fa8\u3002",
    "\u53e6\u5916\uff0c\u5728\u4e0e\u4e8c\u7ea7\u5e73\u5c42\u4e2d\u7684\u201c\u59ae\u7279\u201d\u63a5\u89e6\u65f6\uff0c\u6211\u5c1d\u8bd5\u5165\u4fb5\u4e86\u5979\u7684\u5fc3\u667a\uff0c\u4f46\u5f88\u5feb\u5c31\u88ab\u6392\u9664\uff0c\u539f\u56e0\u4e0d\u660e\uff0c\u63a8\u6d4b\u4e0e\u4e8c\u7ea7\u5e73\u5c42\u51fa\u73b0\u7684\u5f02\u5e38\u72b6\u51b5\u6709\u5173\u3002|\u867d\u7136\u65f6\u95f4\u77ed\u6682\uff0c\u6211\u4e5f\u83b7\u5f97\u4e86\u4e00\u4e9b\u4fe1\u606f\u3002\u4fe1\u606f\u91cf\u975e\u5e38\u5927\uff0c\u6211\u82b1\u4e86\u4e00\u4e9b\u65f6\u95f4\u5904\u7406\u3002",
    "\u54ea\u4e9b\u4fe1\u606f\u2014\u2014\u95ea\u7535\uff0c\u4f60\u83b7\u5f97\u4ec0\u4e48\u4e86\uff1f",
    "\u201c\u59ae\u7279No.249\u201d\uff0c\u8fd9\u662f\u5979\u7684\u6b63\u5f0f\u540d\u79f0\u3002",
    "\u50cf\u5979\u8fd9\u6837\u7684\uff0c\u8d77\u7801\u8fd8\u6709249\u4e2a\uff1f",
    "\u2026\u2026248\u2026\u2026",
    "\u6211\u2014\u2014\u6211\u5f53\u7136\u77e5\u9053\u662f248\uff01\u6211\u8bf4\u9519\u4e86\u2014\u2014\u6ca1\u9519\uff0c\u6211\u8bf4\u9519\u4e86\uff01\u54fc\uff0c\u6ca1\u60f3\u5230\u554a\u7eb3\u7f8e\u897f\u4e1d\u4f60\u5c45\u7136\u8fd9\u4e2a\u65f6\u5019\u597d\u597d\u8bf4\u8bdd\u4f60\u8981\u662f\u4ee5\u540e\u90fd\u8fd9\u6837\u8fd8\u7528\u6211\u6bcf\u5929\u90fd\u8ddf\u7740\u4f60\uff1f\uff01",
    "\u2026\u2026\u7075\u654f\u7684\u624b\u6307\uff0c\u62e8\u52a8\u5149\u6ed1\u7684\u7434\u5f26\u2026\u2026\u63cf\u6479\u51fa\u6109\u60a6\u7684\u5f62\u72b6\u2026\u2026",
    "\u4f60\u5fcd\u4e0d\u4f4f\u2014\u2014\u4f60\u5fcd\u4e0d\u4f4f\u95ed\u5634\u554a\u4f60\u597d\u597d\u542c\u961f\u957f\u8bf4\u8bdd\u554a\uff1f\uff01\u8fd9\u65f6\u5019\u5192\u51fa\u6765\u4f60\u662f\u6545\u610f\u7684\u5427\uff1f\uff01",
    "\uff08\u5bc7\u5c14\u8299\u6b63\u8981\u5f00\u53e3\u963b\u6b62\uff0c\u4f46\u53c8\u5f88\u5feb\u63a7\u5236\u4f4f\u4e86\u81ea\u5df1\u3002\uff09",
    "\uff08\u5979\u7ad9\u5728\u539f\u5730\uff0c\u795e\u60c5\u770b\u8d77\u6765\u6709\u4e9b\u843d\u5bde\u3002\uff09",
    "\uff08\u6211\u6309\u4e86\u6309\u5934\u3002\uff09",
    "\u505c\u4e00\u4e0b\uff0c\u7eb3\u7f8e\u897f\u4e1d\uff0c\u514b\u7f57\u4e3d\u79d1\uff0c\u5148\u542c\u95ea\u7535\u8bf4\u5b8c\u3002",
    "\u2026\u2026\u54fc\u3002",
    "\uff08\u6211\u5411\u95ea\u7535\u70b9\u70b9\u5934\u3002\uff09",
    "\u4e0e\u6211\u4eec\u8fd9\u4e9b\u4eba\u5f62\u76f8\u6bd4\uff0c\u5979\u8eab\u4f53\u7684\u4e00\u90e8\u5206\u4ecd\u7136\u5c5e\u4e8e\u4eba\u7c7b\uff0c\u673a\u80fd\u4e27\u5931\u7684\u90e8\u5206\u88ab\u673a\u68b0\u66ff\u6362\u3002\u5979\u7684\u8eab\u4f53\u5185\u88ab\u690d\u5165\u4e86\u7eb3\u7c73\u673a\u68b0\uff0c\u8fd9\u4e9b\u7eb3\u7c73\u673a\u68b0\u63a7\u5236\u7740\u5979\u7684\u601d\u8003\u548c\u884c\u52a8\u3002\u5979\u9700\u8981\u5b9a\u671f\u63a5\u53d7\u6765\u81ea\u7279\u5b9a\u4fe1\u53f7\u7684\u547d\u4ee4\uff0c\u5e76\u4ee5\u6b64\u51b3\u5b9a\u81ea\u8eab\u7684\u884c\u52a8\u8f68\u8ff9\u3002",
    "\u2026\u2026\u54ea\u91cc\u7684\u4fe1\u53f7\uff1f",
    "\u7279\u5b9a\u4fe1\u53f7\u6765\u6e90\u4e0d\u660e\u3001\u901a\u8baf\u5185\u5bb9\u52a0\u5bc6\u89c4\u5219\u590d\u6742\uff0c\u5927\u90e8\u5206\u65e0\u6cd5\u89e3\u6790\u3002\u6709\u9650\u7684\u53ef\u89e3\u6790\u5185\u5bb9\u4e2d\uff0c\u6211\u6355\u6349\u5230\u4e86\u51e0\u4e2a\u5173\u952e\u8bcd\uff0c\u201c\u4f0a\u9ed8\u5c14\u201d\u3001\u201c\u51c0\u5316\u4eea\u5f0f\u201d\u3001\u201c\u53ec\u96c6\u201d\uff0c\u5f53\u7136\u8fd8\u6709\u201c\u5e15\u62c9\u8482\u65af\u201d\u3002",
    "\u8fd9\u662f\u6211\u6240\u83b7\u5f97\u7684\u5168\u90e8\u4fe1\u606f\uff0c\u4ee5\u4e0b\u662f\u6211\u7684\u63a8\u6d4b\u3002",
    "\uff08\u4ece\u603b\u63a7\u53f0\u524d\u5fae\u5fae\u4fa7\u8fc7\u8eab\uff0c\u95ea\u7535\u7684\u795e\u8272\u6df9\u6ca1\u5728\u4ece\u843d\u5730\u7a97\u5916\u6295\u5c04\u800c\u6765\u7684\u5149\u7ebf\u4e2d\u3002\uff09",
    "\u6211\u4eec\u9047\u5230\u7684\u8fd9\u4e9b\u4eba\uff0c\u5305\u62ec\u201c\u59ae\u7279\u201d\u548c\u201c\u5f92\u4ece\u201d\uff0c\u90fd\u5c5e\u4e8e\u5e15\u62c9\u8482\u65af\u3002",
    "\u55ef\uff0c\u8fd9\u4e0e\u6211\u4e4b\u524d\u7684\u731c\u6d4b\u76f8\u7b26\u3002\u4ee5\u76ee\u524d\u7684\u4fe1\u606f\u63a8\u6d4b\uff0c\u4e5f\u5df2\u7ecf\u80fd\u591f\u786e\u8ba4\u3002",
    "\u800c\u5c5e\u4e8e\u201c\u5e15\u62c9\u8482\u65af\u201d\u7684\u201c\u59ae\u7279\u201d\u4f53\u5185\u7684\u7eb3\u7c73\u673a\u68b0\u5e94\u7528\u4e86\u4e00\u5957\u4e0e\u6211\u4eec\u4e0d\u540c\u7684\u7cfb\u7edf\uff0c\u57fa\u4e8e\u4e0d\u540c\u7684\u5e95\u5c42\u67b6\u6784\uff0c\u6211\u65e0\u6cd5\u987a\u5229\u63a5\u5165\u5979\u6295\u5c04\u5728\u4e8c\u7ea7\u5e73\u5c42\u5185\u7684\u5fc3\u667a\u6620\u5c04\uff0c\u4e5f\u56e0\u6b64\u9020\u6210\u4e86\u4e8c\u7ea7\u5e73\u5c42\u4e2d\u51fa\u73b0\u7684\u5f02\u5e38\u56fe\u50cf\u6270\u52a8\u3002\u5979\u6240\u63a5\u5165\u7684\u4e8c\u7ea7\u5e73\u5c42\u2014\u2014",
    "\u95ea\u7535\uff1f",
    "\u2026\u2026\u4e0e\u6211\u4eec\u76f8\u4f3c\uff0c\u8fd9\u662f\u6211\u80fd\u591f\u5165\u4fb5\u5e76\u83b7\u5f97\u90e8\u5206\u4fe1\u606f\u7684\u539f\u56e0\u3002",
    "\u8fd9\u4e00\u70b9\u4f60\u4e4b\u524d\u5df2\u7ecf\u8bf4\u660e\u8fc7\u3002\u95ea\u7535\uff0c\u662f\u201c\u59ae\u7279\u201d\u6240\u4f7f\u7528\u7684\u4e8c\u7ea7\u5e73\u5c42\u6709\u5f02\u5e38\uff1f",
    "\u6ca1\u6709\u5728\u76ee\u524d\u83b7\u5f97\u7684\u4fe1\u606f\u4e2d\u53d1\u73b0\u5f02\u5e38\u3002\u6b64\u5916\uff0c\u4f9d\u636e\u53ef\u89e3\u6790\u5185\u5bb9\u63a8\u6d4b\uff0c\u5979\u6240\u5728\u7684\u805a\u843d\uff0c\u6d3b\u52a8\u76ee\u7684\u4e0e\u201c\u4f0a\u9ed8\u5c14\u201d\u548c\u201c\u51c0\u5316\u4eea\u5f0f\u201d\u76f8\u5173\u3002",
    "\u2026\u2026\u4e94\u5929\u540e\u3002",
    "\u6211\u5728\u805a\u843d\u91cc\u542c\u5230\u5979\u548c\u5f92\u4ece\u7684\u5bf9\u8bdd\uff0c\u5979\u9700\u8981\u5f92\u4ece\u4eec\u5e26\u6765\u66f4\u591a\u7684\u4eba\uff0c\u8d8a\u591a\u8d8a\u597d\u3002\u4ece\u4eca\u5929\u7b97\u8d77\u7684\u4e94\u5929\u540e\uff0c\u6240\u6709\u4eba\u5c31\u8981\u8ddf\u7740\u5979\u4e00\u8d77\u53bb\u4e00\u4e2a\u5730\u65b9\uff0c\u53bb\u53c2\u52a0\u201c\u51c0\u5316\u4eea\u5f0f\u201d\u3002",
    "\u90a3\u4e2a\u5730\u65b9\u79bb\u201c\u4f0a\u9ed8\u5c14\u201d\u66f4\u8fd1\uff0c\u79bb\u201c\u4f0a\u9ed8\u5c14\u201d\u8d8a\u8fd1\uff0c\u901a\u8fc7\u201c\u51c0\u5316\u4eea\u5f0f\u201d\u7684\u53ef\u80fd\u6027\u5c31\u8d8a\u5927\u3002\u5750\u6807\u6211\u4e0d\u77e5\u9053\u3002",
    "\u201c\u4f0a\u9ed8\u5c14\u201d\u2026\u2026\u542c\u8d77\u6765\u662f\u6307\u4e00\u4e2a\u7279\u5b9a\u4e2a\u4f53\uff1f\u201c\u51c0\u5316\u4eea\u5f0f\u201d\u662f\uff1f",
    "\u6211\u4e0d\u77e5\u9053\u2026\u2026\u6211\u672c\u6765\u89c9\u5f97\u90a3\u4e9b\u53ea\u662f\u59ae\u7279\u7528\u6765\u7b3c\u7edc\u66f4\u591a\u4eba\u7684\u5fc3\u7406\u5b89\u6170\u2014\u2014\u4f60\u8bf4\u4ed6\u4eec\u662f\u201c\u5e15\u62c9\u8482\u65af\u201d\uff0c\u201c\u5e15\u62c9\u8482\u65af\u201d\u5230\u5e95\u662f\u4ec0\u4e48\u2014\u2014",
    "\u6307\u6325\u5b98\uff1f",
    "\uff08\u5bc7\u5c14\u8299\u770b\u7740\u6211\uff0c\u4f3c\u4e4e\u6709\u4e9b\u671f\u5f85\u3002\uff09",
    "\u2026\u2026\u5e15\u62c9\u8482\u65af\u3002",
    "\u66fe\u88ab\u79f0\u4e3a\u201c\u767d\u6559\u201d\uff0c\u5b83\u7684\u5b9e\u9645\u638c\u63a7\u8005\u4ee5\u4eba\u7c7b\u4e3a\u539f\u6599\u5236\u9020\u51fa\u4e86\u5927\u91cf\u534a\u4eba\u534a\u673a\u68b0\u7684\u201c\u6d85\u6258\u201d\u3002\u5dee\u4e0d\u591a\u5341\u5e74\u524d\uff0c\u65b0\u82cf\u8054\u4e0e\u7f57\u8054\u7b7e\u8ba2\u534f\u8bae\u7684\u90a3\u6bb5\u65f6\u671f\uff0c\u5e15\u62c9\u8482\u65af\u5728\u5404\u4e2a\u57ce\u5e02\u5f15\u53d1\u4e86\u5927\u89c4\u6a21\u9a9a\u4e71\uff0c\u6211\u66fe\u7ecf\u6240\u5728\u7684\u683c\u91cc\u82ac\u4e5f\u88ab\u5377\u5165\u5176\u4e2d\u3002",
    "\u5e15\u62c9\u8482\u65af\u6b7c\u706d\u884c\u52a8\u6211\u6709\u6240\u53c2\u4e0e\uff0c\u5728\u6211\u7684\u8bb0\u5fc6\u91cc\uff0c\u5e15\u62c9\u8482\u65af\u5df2\u7ecf\u88ab\u5b8c\u5168\u6d88\u706d\u3002\u867d\u7136\u6211\u6b64\u524d\u5df2\u7ecf\u6709\u4e86\u9884\u671f\u2026\u2026\u5f88\u9057\u61be\uff0c\u6700\u5dee\u7684\u60c5\u51b5\u3002\u5e15\u62c9\u8482\u65af\u786e\u5b9e\u91cd\u65b0\u51fa\u73b0\u4e86\u3002",
    "\u5e15\u62c9\u8482\u65af\u7684\u201c\u6d85\u6258\u201d\u4e0d\u5177\u5907\u81ea\u4e3b\u601d\u8003\u7684\u80fd\u529b\uff0c\u4e5f\u6ca1\u6709\u6027\u522b\u3002\u6211\u4eec\u901a\u5e38\u4ee5\u201c\u5b83\u201d\u6765\u79f0\u547c\u5b83\u4eec\u3002",
    "\u5b83\u4eec\u7684\u884c\u4e3a\u6a21\u5f0f\u548c\u884c\u52a8\u76ee\u7684\u90fd\u88ab\u9884\u5148\u5b9a\u5236\u2026\u2026\u9664\u4e86\u90a3\u51e0\u4e2a\u7279\u6b8a\u7684\u4e2a\u4f53\u2026\u2026\u4f46\u5f53\u5e74\u7684\u5e15\u62c9\u8482\u65af\u5e76\u6ca1\u6709\u5728\u201c\u6d85\u6258\u201d\u4eec\u8eab\u4f53\u5185\u690d\u5165\u7eb3\u7c73\u673a\u68b0\u2026\u2026\u5982\u679c\u201c\u59ae\u7279\u201d\u5c5e\u4e8e\u5e15\u62c9\u8482\u65af\uff0c\u5341\u5e74\u8fc7\u53bb\uff0c\u5e15\u62c9\u8482\u65af\u4e00\u5b9a\u6709\u4ec0\u4e48\u6211\u4e0d\u77e5\u9053\u7684\u53d1\u5c55\u2026\u2026\u4e0d\u540c\u7684\u7cfb\u7edf\u2026\u2026\u4e09\u8fdb\u5236\u2026\u2026\u4e0d\uff0c\u8fd9\u4e0d\u53ef\u80fd\u2026\u2026",
    "\u2026\u2026\u65e2\u5b9a\u7684\u89c4\u5f8b\uff0c\u81ea\u6d77\u9762\u800c\u6765\u2026\u2026",
    "\u8fd1\u536b\u2014\u2014\u4f5c\u6218\u5c0f\u961f\uff1f\u90a3\u662f\u4ec0\u4e48\u4e1c\u897f\uff1f",
    "\u2026\u2026\uff01",
    "\u514b\u7f57\u4e3d\u79d1\uff0c\u4f60\u53bb\u2014\u2014\u4f60\u51fa\u53bb\u7684\u65f6\u5019\uff0c\u7eb3\u7f8e\u897f\u4e1d\u8bf4\u8fc7\u51e0\u53e5\u8bdd\uff0c\u6211\u4eec\u90fd\u6ca1\u542c\u61c2\u3002\u8fd1\u536b\u4f5c\u6218\u5c0f\u961f\u2014\u2014\u7eb3\u7f8e\u897f\u4e1d\uff0c\u4f60\u77e5\u9053\u4ec0\u4e48\uff1f",
    "\u2026\u2026\u7e41\u6742\u7684\u5143\u7d20\uff0c\u5f2f\u66f2\u4e0e\u7f20\u7ed5\u2026\u2026",
    "\u5979\u8bf4\u8fd9\u662f\u5979\u552f\u4e00\u80fd\u786e\u5b9a\u7684\u3002",
    "\u90a3\u6211\u6362\u4e2a\u95ee\u9898\uff0c\u7eb3\u7f8e\u897f\u4e1d\u2026\u2026\u4e0d\u53ea\u662f\u89c1\u8fc7\uff0c\u4e5f\u4e0d\u53ea\u662f\u719f\u6089\uff0c\u4f60\uff0c\u4ee5\u524d\u5c5e\u4e8e\u5e15\u62c9\u8482\u65af\uff1f",
    "\u2026\u2026\u65e0\u8fb9\u7684\u98ce\u66b4\uff0c\u5012\u6620\u5728\u65e0\u5c3d\u7684\u6d77\u9762\u2026\u2026",
    "\u5979\u4e0d\u77e5\u9053\u3002\u8fde\u201c\u5e15\u62c9\u8482\u65af\u201d\u8fd9\u4e2a\u540d\u5b57\u90fd\u662f\u542c\u4f60\u8bf4\u624d\u77e5\u9053\u7684\u3002",
    "\uff08\u2026\u2026\u8fd9\u4e2a\u7b54\u6848\u5b8c\u5168\u5728\u6211\u7684\u9884\u6599\u4e4b\u5916\u3002\uff09",
    "\u8fd1\u536b\u4f5c\u6218\u5c0f\u961f\u53c8\u662f\u600e\u4e48\u56de\u4e8b\uff1f",
    "\u2026\u2026\u9ed1\u5f71\u7d2f\u79ef\uff0c\u85c9\u7531\u5f3a\u70c8\u7684\u5149\u8292\u95ea\u70c1\u2026\u2026\u6d41\u6ea2\u7684\u7247\u6bb5\u2026\u2026",
    "\u5c31\u662f\u8bb0\u5728\u5979\u5fc3\u667a\u91cc\u7684\u6240\u5c5e\u4fe1\u606f\uff0c\u8001\u662f\u548c\u4f60\u63d0\u7684\u90a3\u4e2a\u5706\u89c4\u56fe\u6848\u5728\u4e00\u8d77\u51fa\u73b0\u3002\u522b\u7684\u5979\u4e0d\u77e5\u9053\u3002",
    "\u771f\u7684\u662f\u8fd9\u4e2a\u610f\u601d\uff1f\u514b\u7f57\u4e3d\u79d1\uff0c\u4f60\u6ca1\u6709\u7ffb\u8bd1\u9519\uff1f",
    "\u54c8\uff1f\u4f60\u4e0d\u4fe1\u6211\u7684\u7ffb\u8bd1\uff1f\u90a3\u4f60\u95ee\u7eb3\u7f8e\u897f\u4e1d\u554a\uff1f\uff01",
    "\uff08\u7eb3\u7f8e\u897f\u4e1d\u5e94\u58f0\u70b9\u4e86\u70b9\u5934\u3002\uff09",
    "\u2026\u2026\u597d\u5427\u3002\u600e\u4e48\uff0c\u5bc7\u5c14\u8299\u5148\u8bf4\u4e86\u4e0d\u77e5\u9053\uff0c\u7eb3\u7f8e\u897f\u4e1d\u4f60\u4e5f\u8ddf\u7740\u4e0d\u77e5\u9053\uff0c\u4e0b\u4e00\u4e2a\u662f\u8c01\uff0c\u95ea\u7535\u5417\uff1f",
    "\u2026\u2026",
    "\u2026\u2026\u6211\u771f\u7684\u4e0d\u77e5\u9053\u3002",
    "\uff08\u95ea\u7535\u4ecd\u7136\u5b89\u9759\u5730\u770b\u7740\u6211\uff0c\u5b89\u9759\u5230\u751a\u81f3\u6709\u4e9b\u51b7\u6de1\u3002\uff09",
    "\u597d\u597d\u597d\uff0c\u6ca1\u4e8b\u3002\u90a3\u4e48\uff0c\u6700\u540e\u4e00\u4e2a\u95ee\u9898\uff0c\u201c\u6d88\u5931\u7684\u79d1\u672a\u5854\u201d\u3002",
    "\u5bc7\u5c14\u8299\uff0c\u73b0\u5728\u52a0\u4e0a\u7eb3\u7f8e\u897f\u4e1d\uff0c\u5982\u679c\u4f60\u4eec\u77e5\u9053\u4efb\u4f55\u548c\u201c\u79d1\u672a\u5854\u201d\u6709\u5173\u7684\u4fe1\u606f\uff0c\u6211\u5e0c\u671b\u4f60\u4eec\u80fd\u591f\u544a\u8bc9\u6211\u3002",
    "\uff08\u88ab\u538b\u5728\u5899\u4e0a\u7684\u59ae\u7279\u4e0d\u4e3a\u4eba\u5bdf\u89c9\u5730\u52a8\u5f39\u4e86\u4e00\u4e0b\u3002\uff09",
    "\u8fd9\u4e2a\u79f0\u547c\uff0c\u6211\u5728\u201c\u805a\u843d\u201d\u7684\u65f6\u5019\u542c\u5230\u8fc7\u51e0\u6b21\u3002\u6211\u53ea\u77e5\u9053\u201c\u79d1\u672a\u5854\u201d\u5bf9\u4ed6\u4eec\u2014\u2014\u6216\u8005\u8bf4\uff0c\u662f\u5bf9\u201c\u4f0a\u9ed8\u5c14\u201d\u5f88\u91cd\u8981\u3002",
    "\u2026\u2026\u6563\u5931\u5728\u7530\u91ce\u7684\u5fae\u98ce\uff0c\u5f92\u7136\u547c\u5524\u2026\u2026",
    "\u5979\u60f3\u4e0d\u5230\u2014\u2014\u6307\u6325\u5b98\uff0c\u79d1\u2026\u2026\u79d1\u672a\u5854\u600e\u4e48\u4e86\uff1f",
    "\u2026\u2026\u6ca1\u4ec0\u4e48\u3002\u6211\u5728\u60f3\u2026\u2026",
    "\uff08\u867d\u7136\u7b54\u6848\u4e0e\u6211\u7684\u9884\u671f\u76f8\u7b26\uff0c\u4f46\u6211\u8fd8\u662f\u6709\u4e9b\u5931\u671b\u3002\uff09",
    "\u6211\u539f\u672c\u4ee5\u4e3a\u6293\u4f4f\u59ae\u7279\uff0c\u6700\u4f4e\u9650\u5ea6\u4e5f\u80fd\u83b7\u5f97\u4e00\u4e9b\u5e15\u62c9\u8482\u65af\u7684\u4fe1\u606f\u2026\u2026\u73b0\u5728\u786e\u5b9e\u83b7\u5f97\u4e86\u4e00\u4e9b\uff0c\u4f46\u5927\u90e8\u5206\u53ea\u80fd\u662f\u731c\u6d4b\uff0c\u201c\u79d1\u672a\u5854\u201d\u2026\u2026",
    "\u201c\u4e94\u5929\u540e\u7684\u51c0\u5316\u4eea\u5f0f\u201d\uff0c\u6307\u6325\u5b98\u3002\u5982\u679c\u7ee7\u7eed\u8ffd\u67e5\uff0c\u6211\u4eec\u80fd\u591f\u83b7\u5f97\u66f4\u591a\u7ebf\u7d22\u3002",
    "\u2026\u2026\u55ef\u3002\u95ea\u7535\uff0c\u6211\u4eec\u73b0\u5728\u8ddd\u79bb\u4e4b\u524d\u7684\u201c\u805a\u843d\u201d\u591a\u8fdc\uff1f",
    "\u4f9d\u7167\u76ee\u524d\u901f\u5ea6\uff0c\u9700\u8981\u884c\u9a76\u5927\u7ea65\u5c0f\u65f6\u3002",
    "\u4e0d\u7b97\u592a\u8fdc\u3002\u8ba1\u5212\u53d8\u66f4\uff0c\u95ea\u7535\uff0c\u901a\u77e5\u7f8e\u73b2\u7acb\u523b\u6765\u6307\u6325\u5ba4\uff0c\u4fe1\u53f7\u5c4f\u853d\u88c5\u7f6e\u7528\u4e0d\u4e0a\u4e86\u3002",
    "\u2014\u2014\u8c01\uff1f\uff01",
    "\uff08\u9664\u4e86\u9700\u8981\u63a7\u5236\u4f4f\u59ae\u7279\u7684\u514b\u7f57\u4e3d\u79d1\uff0c\u4eba\u5f62\u4eec\u7acb\u523b\u8f6c\u5411\u6307\u6325\u5ba4\u5165\u53e3\u7684\u65b9\u5411\uff0c\u5305\u62ec\u5bc7\u5c14\u8299\u5728\u5185\uff0c\u6240\u6709\u4eba\u5f62\u90fd\u4e3e\u8d77\u4e86\u67aa\u3002\uff09",
    "\u5c31\u8bf4\u4e0d\u8981\u8d70\u592a\u5feb\u5440\u2026\u2026\u5443\u3001\u6307\u6325\u5b98\u548c\u961f\u957f\u5979\u4eec\u4e0d\u77e5\u9053\u5728\u8c08\u4ec0\u4e48\uff0c\u6211\u4eec\u6162\u4e00\u70b9\u2014\u2014\u554a\uff0c\u7ef4\u666e\u857e\uff0c\u4f60\u770b\uff0c\u8fd9\u5c31\u649e\u4e0a\u4e86\u5440\u2026\u2026",
    "\u5bf9\u4e0d\u8d77\u4e86\u5566\u2014\u2014\u53ef\u662f\u7ef4\u666e\u857e\u597d\u5f00\u5fc3\u5440\uff01\u6d77\u4f26\u5a1c\u9001\u7ed9\u7ef4\u666e\u857e\u4e00\u4e2a\u5c0f\u5c0f\u7684\u73a9\u5076\uff01\u867d\u7136\u4e0d\u77e5\u9053\u662f\u4ec0\u4e48\u5566\uff0c\u4f46\u662f\u8d85\u7ea7\u53ef\u7231\uff01\u7ef4\u666e\u857e\u2606\u8d85\u559c\u6b22\uff01",
    "\u53ef\u7231\u3001\u8981\u642d\u914d\uff01",
    "\u662f\u5427\u662f\u5427\uff0c\u642d\u914d\u62ab\u8428\u5148\u751f\u8d85\u7ea7\u5408\u9002\u7684\uff01",
    "\u7ef4\u666e\u857e\uff01\u4e0d\u8981\u626d\u5934\u8981\u770b\u8def\uff01",
    "\u6ca1\u5173\u7cfb\u7684\u5566\u2014\u2014\u554a\u554a\u554a\u8981\u6389\u4e86\u8981\u6389\u4e86\u2014\u2014\uff01",
    "\u4e0d\u8d70\uff01",
    "\u7ef4\u666e\u857e\uff01"
  ],
  [
    "\uff08\u89c1\u4e0d\u662f\u654c\u88ad\uff0c\u7eb3\u7f8e\u897f\u4e1d\u548c\u95ea\u7535\u6536\u8d77\u4e86\u67aa\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u8fdf\u4e00\u6b65\uff0c\u5c06\u67aa\u6536\u4e86\u56de\u53bb\u3002\uff09",
    "\u2026\u2026\u5979\u4eec\u5728\u5e72\u4ec0\u4e48\uff1f",
    "\u600e\u4e48\u8fd9\u4e48\u5feb\u2026\u2026\u514b\u7f57\u4e3d\u79d1\uff0c\u6321\u4f4f\u59ae\u7279\u3002",
    "\u6321\u4f4f\uff1f\u8fd9\u6837\uff1f",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u80cc\u5bf9\u95e8\u53e3\uff0c\u4fa7\u4e86\u4fa7\u8eab\u3002\uff09",
    "\uff08\u4f34\u968f\u626d\u66f2\u7684\u58f0\u97f3\uff0c\u7565\u5fae\u53d8\u5f62\u7684\u6ed1\u95e8\u5f00\u542f\uff0c\u6454\u5012\u5728\u95e8\u5916\u7684\u4e09\u4eba\u5931\u53bb\u652f\u6491\uff0c\u5012\u5728\u95e8\u53e3\uff0c\u5404\u7c7b\u88c5\u6709\u96f6\u4ef6\u7684\u7269\u8d44\u7bb1\u6563\u843d\u4e00\u5730\u3002\uff09",
    "\u6307\u3001\u6307\u6325\u5b98\u2014\u2014\u554a\uff01",
    "\u7ef4\u666e\u857e\u3001\u91cd\uff01",
    "\u4e0d\u80fd\u8bf4\u5076\u50cf\u91cd\u7684\u5566\uff01\u545c\u545c\u545c\u597d\u4e22\u4eba\u2014\u2014\u5927\u5bb6\u53ef\u4e0d\u53ef\u4ee5\u5fd8\u8bb0\u5440\uff01",
    "\u2026\u2026\u4f60\u4eec\u8fd9\u662f\uff1f",
    "\uff08\u6211\u5411\u524d\u8d70\u4e86\u51e0\u6b65\uff0c\u6321\u5728\u4e86\u6d77\u4f26\u5a1c\u548c\u514b\u7f57\u4e3d\u79d1\u4e2d\u95f4\u3002\uff09",
    "\u6211\u4eec\u5728\u56de\u6765\u7684\u8def\u4e0a\u78b0\u5230\u4e86\u6d77\u4f26\u5a1c\uff0c\u6d77\u4f26\u5a1c\u9001\u4e86\u7ef4\u666e\u857e\u4e00\u4e2a\u73a9\u5076\uff0c\u7ef4\u666e\u857e\u5f88\u9ad8\u5174\u2026\u2026\u5c31\u649e\u5230\u6276\u624b\u6454\u5012\u4e86\u3002",
    "\u4e0d\u662f\u6454\u5012\u5566\u2014\u2014\u662f\u5728\u7ec3\u4e60\u7279\u6b8a\u7684\u821e\u6b65\u54e6\uff01",
    "\u53ef\u662f\u6211\u4eec\u73b0\u5728\u6b63\u4e00\u8d77\u5012\u5728\u5730\u4e0a\u5440\u2026\u2026\u554a\u5bf9\u5bf9\u5bf9\uff0c\u96f6\u4ef6\uff01",
    "\uff08\u7f8e\u73b2\u8bd5\u56fe\u7ad9\u8d77\u6765\u53bb\u68c0\u67e5\u88c5\u6709\u96f6\u4ef6\u7684\u7269\u8d44\u7bb1\uff0c\u7ef4\u666e\u857e\u89c1\u72b6\u62ac\u8d77\u624b\u60f3\u8981\u5e2e\u5fd9\uff0c\u4f46\u624b\u8fd8\u6ca1\u4f38\u5f00\uff0c\u5c31\u518d\u6b21\u626f\u5230\u4e86\u6d77\u4f26\u5a1c\u7684\u5934\u53d1\u3002\uff09",
    "\u75db\uff01\u62fd\u5934\u53d1\uff01",
    "\u554a\u554a\u554a\u5bf9\u4e0d\u8d77\u5440\u2014\u2014\u7ef4\u666e\u857e\u4e0d\u662f\u6545\u610f\u7684\u5566\uff01\u54e6\u54e6\uff01\u5bc7\u5c14\u8299\u2606\u8d85\u7ea7\u611f\u8c22\uff01",
    "\uff08\u5bc7\u5c14\u8299\u5c06\u7ef4\u666e\u857e\u62c9\u4e86\u8d77\u6765\u3002\uff09",
    "\uff08\u7b49\u7ef4\u666e\u857e\u7ad9\u7a33\uff0c\u5bc7\u5c14\u8299\u53c8\u5411\u7f8e\u73b2\u548c\u6d77\u4f26\u5a1c\u4f38\u51fa\u624b\u3002\uff09",
    "\u2026\u2026\u6765\u3002",
