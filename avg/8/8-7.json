[
  [
    "\uff08\u514b\u7f57\u4e3d\u79d1\u5c06\u80e1\u4e71\u653b\u51fb\u7684\u6d77\u4f26\u5a1c\u6253\u6655\uff0c\u625b\u5728\u80a9\u4e0a\u3002\uff09",
    "\uff08\u56e0\u4e3a\u4e3b\u4eba\u610f\u8bc6\u7684\u4e27\u5931\uff0c\u6d77\u4f26\u5a1c\u7684\u4e09\u4e2a\u7403\u65e0\u7cbe\u6253\u91c7\u5730\u98d8\u5728\u514b\u7f57\u4e3d\u79d1\u5468\u56f4\uff0c\u514b\u7f57\u4e3d\u79d1\u62cd\u4e86\u62cd\u5979\u7684\u8138\u3002\uff09",
    "\u5582\uff0c\u5582\uff0c\u4f60\u8fd8\u597d\u5427\uff0c\u6d77\u4f26\u5a1c\uff1f",
    "\u5c0f\u514b\u7f57\u592a\u7c97\u66b4\u4e86\u5440\uff01\u7ef4\u666e\u857e\u3001\u770b\u5230\u4e86\u54e6\uff01\u201c\u7830\u201d\u5730\u4e00\u4e0b\u6253\u4e86\u6d77\u4f26\u5a1c\u2014\u2014\u542c\u8d77\u6765\u5c31\u597d\u75db\u597d\u75db\uff01\u90fd\u8bf4\u4e86\u8981\u5bf9\u5973\u5b69\u5b50\u6e29\u67d4\u4e00\u70b9\u7684\u5440\uff01",
    "\u5979\u5728\u80e1\u4e71\u653b\u51fb\u4f60\u6ca1\u770b\u5230\u5417\uff1f\uff01\u8981\u4e0d\u662f\u5979\u8fd9\u4e9b\u5f92\u4ece\u80fd\u6765\u653b\u51fb\u6211\u4eec\uff1f\u6211\u628a\u5979\u6253\u6655\u89e3\u51b3\u95ee\u9898\u6700\u5feb\u4f60\u77e5\u4e0d\u77e5\u9053\u800c\u4e14\u6211\u4e5f\u8bf4\u4e86\u4e0d\u8981\u8fd9\u4e48\u53eb\u6211\u7684\u5427\uff1f\uff01",
    "\u53ef\u662f\u521a\u624d\u5c0f\u514b\u7f57\u5c31\u7b54\u5e94\u4e86\u8036\u2014\u2014\u597d\u5e05\u90a3\u91cc\uff01",
    "\u4e0d\u8981\u8f6c\u79fb\u8bdd\u9898\u554a\uff1f\uff01",
    "\u7ef4\u666e\u857e\uff0c\u53d1\u9001\u201c49\u201d\uff01\u5404\u4f5c\u6218\u5355\u4f4d\uff0c0016\u540e\u64a4\u9000\uff01",
    "\uff08\u5341\u51e0\u7c73\u4e4b\u5916\uff0c\u5bc7\u5c14\u8299\u51b7\u9759\u5730\u62ff\u51fa\u4e00\u4e2a\u5f39\u5939\u6362\u4e86\u4e0a\u53bb\uff0c\u968f\u540e\u538b\u4f4e\u8eab\u4f53\uff0c\u8eb2\u8fc7\u4e86\u4e00\u4e2a\u5f92\u4ece\u7684\u653b\u51fb\uff0c\u53c8\u987a\u52bf\u4f7f\u7528\u4fa7\u8e22\u51fb\u9000\u4e86\u53e6\u4e00\u4e2a\u5f92\u4ece\u3002)",
    "\uff08\u5979\u76ee\u5149\u707c\u707c\u5730\u76ef\u7740\u81ea\u5df1\u7684\u6b63\u524d\u65b9\uff0c\u5c31\u5728\u4e0d\u8fdc\u5904\uff0c\u8d6b\u745e\u5c24\u53d7\u5230\u653b\u51fb\uff0c\u5df2\u7ecf\u534a\u8dea\u5728\u4e86\u5730\u4e0a\uff0c\u53d1\u4e0d\u51fa\u4efb\u4f55\u58f0\u97f3\u3002\uff09",
    "3\u3002",
    "\u7eb3\u7f8e\u897f\u4e1d\uff0c\u63a9\u62a4\u5bc7\u5c14\u8299\uff01",
    "\u2026\u2026\u9759\u9ed8\u65e0\u58f0\u2026\u2026\u4e8e\u5e7f\u9614\u7684\u6d77\u6d0b\u4e2d\u6e38\u5f0b\u2026\u2026",
    "\uff08\u7eb3\u7f8e\u897f\u4e1d\u85c9\u7531\u7ef4\u666e\u857e\u7559\u51fa\u7684\u7a7a\u6321\uff0c\u7784\u51c6\u4e86\u5bc7\u5c14\u8299\u65c1\u8fb9\u7684\u4e00\u4e2a\u5f92\u4ece\u3002\uff09",
    "\uff08\u67aa\u58f0\u5728\u8eab\u8fb9\u54cd\u8d77\uff0c\u5bc7\u5c14\u8299\u77a5\u4e86\u4e00\u773c\u3002\uff09",
    "2\u3002",
    "\uff08\u8eab\u4fa7\u7684\u5f92\u4ece\u4ef0\u9762\u5012\u4e0b\uff0c\u5bc7\u5c14\u8299\u987a\u52bf\u65cb\u8f6c\u4e86\u534a\u5708\uff0c\u5de6\u624b\u626f\u8fc7\u4ed6\u624b\u4e2d\u62ff\u7740\u7684\u5f29\u7bad\uff0c\u8fd1\u8ddd\u79bb\u7784\u51c6\u4e86\u9762\u524d\u7684\u4e00\u4e2a\u5f92\u4ece\u3002\uff09",
    "1\u3002",
    "\uff08\u5f92\u4ece\u4e2d\u7bad\u5012\u4e0b\uff0c\u5979\u4e0e\u8d6b\u745e\u5c24\u4e4b\u95f4\u7684\u76f4\u7ebf\u4e0a\uff0c\u518d\u4e5f\u6ca1\u6709\u4efb\u4f55\u4eba\u963b\u9694\u3002\uff09",
    "\uff08\u5bc7\u5c14\u8299\u51e0\u6b65\u51b2\u8fc7\u53bb\uff0c\u5c06\u67aa\u53e3\u62b5\u5728\u8d6b\u745e\u5c24\u7684\u5934\u4e0a\u3002\uff09",
    "\u7b2c\u5341\u4e00\u533b\u62a4\u9632\u5316\u8425\u7b2c\u4e09\u533b\u7597\u961f\uff0c\u4e3a\u4ec0\u4e48\u8981\u53bb\u88ad\u51fb\u4ed6\u4eec\uff0c\u8bf4\uff01",
    "\u54c8\u2026\u2026\u54c8\u2026\u2026",
    "\u4e3a\u51c0\u5316\u4e4b\u4eba\u732e\u4e0a\u795d\u798f\u3002",
    "\uff08\u6d85\u6258\u4f38\u51fa\u624b\u3002\uff09",
    "\uff08\u5b83\u6240\u6307\u7684\u65b9\u5411\uff0c\u6709\u4e00\u4e2a\u5904\u4e8e\u5149\u56e2\u4e2d\u7684\u5f92\u4ece\uff0c\u90a3\u4e2a\u5f92\u4ece\u7684\u8eab\u9ad8\u76f8\u8f83\u5176\u4ed6\u4eba\u77ee\u4e86\u8bb8\u591a\u3002\uff09",
    "\uff08\u95ea\u7535\u76ef\u7740\u5149\u56e2\uff0c\u611f\u53d7\u5230\u4e86\u602a\u5f02\u7684\u719f\u6089\u611f\u3002\uff09",
    "\u2026\u2026\u90a3\u662f\u2026\u2026\uff1f",
    "\uff08\u4e0d\u8fdc\u5904\uff0c\u5bc7\u5c14\u8299\u6293\u7d27\u4e86\u8d6b\u745e\u5c24\u7684\u9886\u53e3\u3002\uff09",
    "\u522b\u7ed9\u6211\u88c5\u50bb\uff01\u4f60\u4eec\u74e6\u826f\u683c\u5e2e\u5f53\u65f6\u6839\u672c\u5c31\u4e0d\u5728\u9644\u8fd1\u6d3b\u52a8\u2014\u2014\u4e3a\u4ec0\u4e48\u53bb\u88ad\u51fb\u4ed6\u4eec\uff0c\u4e3a\u4ec0\u4e48\u6740\u4e86\u4ed6\u4eec\u2014\u2014\u4e3a\u4ec0\u4e48\u2014\u2014\u6740\u4e86\u5b89\u83f2\u5a05\u2014\u2014\u4f60\u8bf4\u2014\u2014\u4f60\u8bf4\u554a\uff01",
    "\u54c8\u2026\u2026",
    "\u6211\u4eec\u5c06\u540c\u884c\u3002\u60e9\u7f5a\u964d\u4e34\uff0c\u8bf7\u4e3a\u6211\u4eec\u6446\u8131\u4e00\u5207\u9669\u6076\u3002",
    "\uff08\u5149\u56e2\u6563\u53bb\uff0c\u77ee\u5c0f\u7684\u5f92\u4ece\u542c\u4ece\u6d85\u6258\u7684\u547d\u4ee4\uff0c\u8f6c\u5411\u4e86\u95ea\u7535\u51e0\u4eba\u6240\u5728\u7684\u65b9\u5411\u3002\uff09",
    "\uff08\u515c\u5e3d\u4e0b\uff0c\u662f\u534a\u5f20\u770b\u8d77\u6765\u683c\u5916\u773c\u719f\u7684\u8138\u3002\uff09",
    "\u516d\u5206\u4eea\uff1f\uff01",
    "\u4f60\u7b11\u4ec0\u4e48\u2014\u2014\uff01\u4f60\u7b11\u4ec0\u4e48\u4f60\u7b11\u4ec0\u4e48\u4f60\u7b11\u4ec0\u4e48\u2014\u2014\u6211\u6740\u4e86\u4f60\u3001\u6211\u6740\u4e86\u4f60\u2014\u2014\u6211\u8981\u2014\u2014\u6740\u4e86\u4f60\uff01",
    "\u5bc7\u5c14\u8299\uff0c\u64a4\u9000\uff01",
    "\u6211\u2014\u2014\u6740\u4e86\u4f60\uff01"
  ]
