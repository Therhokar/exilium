[
  [
    "\u5373\u5c06\u5b8c\u6210\u57fa\u5730\u8f66\u81ea\u68c0\u7a0b\u5e8f\u3002\u5411\u529b\u5f15\u64ce\u5c06\u57280002\u540e\u542f\u52a8\u3002",
    "\u5e73\u3001\u5e73\u3001\u5e73\u2014\u2014\u9ad8\uff01|\u9ad8\u9ad8\u7684\u3001\u6709\u597d\u591a\uff1f",
    "\u8fd9\u91cc\u5df2\u7ecf\u975e\u5e38\u9760\u8fd1\u7ea2\u533a\uff0c\u5730\u8868\u7ed3\u6784\u4e0e\u9ec4\u533a\u4e0d\u540c\u3002",
    "\uff08\u6d77\u4f26\u5a1c\u8db4\u5728\u843d\u5730\u7a97\u524d\uff0c\u770b\u8d77\u6765\u975e\u5e38\u5174\u594b\u3002\u5728\u5979\u4e0d\u8fdc\u5904\uff0c\u95ea\u7535\u7ad9\u5728\u603b\u63a7\u53f0\u524d\uff0c\u6b63\u4f4e\u5934\u64cd\u4f5c\u3002\uff09",
    "\uff08\u6211\u7ad9\u5728\u95ea\u7535\u8eab\u540e\u4e0d\u8fdc\uff0c\u6765\u56de\u89c2\u5bdf\u7740\u4e24\u4eba\u3002\uff09",
    "\u7ea2\u533a\u3001\u7ea2\u7ea2\u7684\uff01",
    "\u201c\u7ea2\u533a\u201d\u53ea\u662f\u4e2a\u79f0\u547c\u2014\u2014\u867d\u7136\u6211\u4e5f\u4e0d\u6e05\u695a\u4e3a\u4ec0\u4e48\u4e00\u5f00\u59cb\u8fd9\u79cd\u9ad8\u6c61\u67d3\u533a\u4f1a\u88ab\u79f0\u4e3a\u201c\u7ea2\u533a\u201d\uff0c\u5927\u6982\u662f\u8b66\u793a\u7684\u610f\u601d\u3002",
    "\u54e6\u54e6\uff01\u7ea2\u7ea2\u7684\u3001\u6ee1\u6ee1\u7684\u3001\u662f\u79ef\u5206\uff01",
    "\u79ef\u5206\uff1f\u4f60\u8fd9\u662f\u6700\u8fd1\u548c\u7f8e\u73b2\u4e00\u8d77\u73a9\u6e38\u620f\u4e86\u5417\uff0c\u6d77\u4f26\u5a1c\uff1f",
    "\uff08\u6d77\u4f26\u5a1c\u7acb\u523b\u8f6c\u5934\uff0c\u53cc\u624b\u5728\u8eab\u524d\u5212\u4e86\u4e00\u4e2a\u8d85\u5927\u7684\u5706\u3002\uff09",
    "\u5bc7\u5c14\u8299\u3001\u4e00\u8d77\uff01\u7f8e\u73b2\u3001\u4e0d\u7ed3\u675f\u3001\u8d62\u597d\u591a\uff01",
    "\u2026\u2026\u7f8e\u73b2\u60f3\u7528\u73a9\u6e38\u620f\u6765\u529d\u5bc7\u5c14\u8299\uff1f\u5bc7\u5c14\u8299\u6ca1\u6709\u518d\u6b21\u79bb\u5f00\u57fa\u5730\u8f66\u53ef\u771f\u662f\u4e2a\u5947\u8ff9\u2026\u2026",
    "\u5514\u5514\uff1f",
    "\uff08\u6d77\u4f26\u5a1c\u6b6a\u7740\u5934\uff0c\u8868\u60c5\u56f0\u60d1\u3002\uff09",
    "\uff08\u827e\u83ab\u53f7\u6301\u7eed\u8fd0\u884c\uff0c\u843d\u5730\u7a97\u5916\u7684\u666f\u8272\u4e5f\u5728\u6301\u7eed\u53d8\u5316\uff0c\u6d77\u4f26\u5a1c\u5f88\u5feb\u5c31\u91cd\u65b0\u628a\u6ce8\u610f\u529b\u653e\u5728\u4e86\u89c2\u5bdf\u666f\u8272\u4e0a\u3002\uff09",
    "\u2026\u2026\u5012\u8ba1\u65f6\uff0c\u4e09\u3001\u4e8c\u3001\u4e00\uff0c\u57fa\u5730\u8f66\u81ea\u68c0\u7ed3\u675f\uff0c\u57fa\u5730\u8f66\u6240\u6709\u53d7\u635f\u6a21\u5757\u786e\u8ba4\u4fee\u590d\u5b8c\u6210\u3002\u5411\u529b\u5f15\u64ce\u8fd0\u884c\u6b63\u5e38\u3002",
    "\u6307\u6325\u5ba4\u5185\u574d\u584c\u8f90\u5c04\u5df2\u6e05\u9664\uff0c\u5f53\u524d\u574d\u584c\u8f90\u5c04\u6d53\u5ea6\u68c0\u6d4b\u2026\u2026\u6700\u65b0\u7ed3\u679c\u8fd4\u56de\uff0c\u786e\u8ba4\u4e3a0\uff0c\u6307\u6325\u5b98\u3002",
    "\u55ef\u3002\u867d\u7136\u4e5f\u53ea\u6709\u96f6\u70b9\u96f6\u51e0\uff0c\u6e05\u9664\u4e00\u4e0b\u8fd8\u662f\u597d\u4e9b\u3002",
    "\u5bc7\u5c14\u8299\u5df2\u7ecf\u5206\u6790\u8fc7\u4e86\u5730\u9762\u4e0a\u6b8b\u7559\u7684\u6697\u8910\u8272\u6db2\u4f53\u6210\u5206\uff0c\u5224\u65ad\u4e3a\u8840\u6db2\uff0c\u6765\u6e90\u5c5e\u4e8e\u4eba\u7c7b\u3002",
    "\u55ef\u3002\u4e4b\u524d\u786e\u5b9e\u4e5f\u731c\u5230\u4e86\u201c\u59ae\u7279\u201d\u4f1a\u548c\u201c\u6d85\u6258\u201d\u4e00\u6837\uff0c\u662f\u534a\u4eba\u534a\u673a\u68b0\u7684\u4eba\u5f62\uff0c\u4f46\u201c\u59ae\u7279\u201d\u8eab\u4e0a\u5c45\u7136\u4f1a\u51fa\u73b0\u574d\u584c\u7ed3\u6676\u2026\u2026\uff1f",
    "\u201c\u6d85\u6258\u201d\u8eab\u4e0a\u6ca1\u6709\u574d\u584c\u7ed3\u6676\u5417\uff0c\u6307\u6325\u5b98\uff1f",
    "\uff08\u2026\u2026\u53cd\u5e94\u901f\u5ea6\u3001\u8868\u8fbe\u65b9\u5f0f\u90fd\u4e0e\u6b64\u524d\u5e76\u65e0\u4e8c\u81f4\uff0c\u90a3\u4e4b\u524d\u2026\u2026\uff09",
    "\u2026\u2026\u4ee5\u6211\u5f53\u65f6\u6240\u9047\u5230\u7684\u6d85\u6258\u3001\u6240\u638c\u63e1\u7684\u60c5\u62a5\u6765\u770b\uff0c\u6ca1\u6709\u3002",
    "\u90a3\u4e9b\u201c\u6d85\u6258\u201d\u5c31\u7b97\u5728\u5b8c\u5168\u65e0\u9632\u62a4\u7684\u6761\u4ef6\u4e0b\u5904\u4e8e\u5f3a\u574d\u584c\u8f90\u5c04\u73af\u5883\u4e2d\uff0c\u8eab\u4f53\u5185\u4e5f\u4e0d\u4f1a\u2014\u2014\u6216\u8005\u8bf4\u5728\u77ed\u671f\u5185\u4e0d\u4f1a\u4ea7\u751fE.I.L.D.\u75c5\u53d8\uff0c\u66f4\u4e0d\u4f1a\u51fa\u73b0\u957f\u51fa\u574d\u584c\u7ed3\u6676\u8fd9\u79cd\u80fd\u591f\u88ab\u89c6\u4e3a\u5904\u4e8eE.L.I.D.\u665a\u671f\u7684\u75c7\u72b6\u3002",
    "\u53ef\u80fd\u6027\u6709\u4e24\u79cd\uff0c\u5e15\u62c9\u8482\u65af\u5728\u91cd\u7ec4\u65f6\u4ea7\u751f\u4e86\u6280\u672f\u9000\u6b65\uff0c\u201c\u59ae\u7279\u201d\u548c\u201c\u6d85\u6258\u201d\uff0c\u90fd\u6ca1\u6709\u6cbf\u7528\u6b64\u524d\u7684\u6280\u672f\u3002",
    "\u6216\u8005\uff0c\u8fc7\u5f80\u7684\u7ecf\u9a8c\u5df2\u7ecf\u65e0\u6cd5\u9002\u7528\u4e8e\u91cd\u7ec4\u540e\u7684\u5e15\u62c9\u8482\u65af\uff0c\u800c\u51fa\u4e8e\u4e00\u4e9b\u539f\u56e0\uff0c\u4f5c\u4e3a\u65b0\u4e2a\u4f53\u7684\u201c\u59ae\u7279\u201d\u5e94\u7528\u4e86\u65b0\u7684\u6280\u672f\uff1f",
    "\u4f46\u4ece\u5404\u4e2a\u65b9\u9762\u6765\u770b\uff0c\u201c\u59ae\u7279\u201d\u90fd\u548c\u6211\u4e4b\u524d\u89c1\u8fc7\u7684\u90a3\u4e9b\u201c\u6d85\u6258\u201d\u76f8\u5dee\u5f88\u8fdc\u3002",
    "\uff08\u6211\u5f88\u5feb\u5426\u5b9a\u4e86\u5176\u4e2d\u4e00\u4e2a\u731c\u6d4b\u3002\uff09",
    "\uff08\u6d77\u4f26\u5a1c\u88ab\u666f\u8272\u5438\u5f15\uff0c\u770b\u5f97\u975e\u5e38\u8ba4\u771f\uff0c\u6307\u6325\u5ba4\u91cc\u5b89\u9759\u4e86\u4e0b\u6765\uff0c\u53ea\u80fd\u542c\u5230\u827e\u83ab\u53f7\u5728\u884c\u9a76\u4e2d\u53d1\u51fa\u7684\u89c4\u5f8b\u6027\u566a\u97f3\u3002\uff09",
    "\uff08\u8fd9\u6837\u6709\u89c4\u5f8b\u7684\u566a\u97f3\u975e\u5e38\u9002\u5408\u601d\u8003\uff0c\u6211\u5f00\u59cb\u68b3\u7406\u81ea\u5df1\u7684\u601d\u8def\u3002\uff09",
    "\u4e0d\u8fc7\uff0c\u6211\u8fd8\u662f\u66f4\u503e\u5411\u4e8e\u540e\u8005\u3002|\u5982\u679c\u5e26\u9886\u5e15\u62c9\u8482\u65af\u7684\u4eba\u8fd8\u662f\u5a01\u5ec9\uff0c\u6211\u4e0d\u8ba4\u4e3a\u201c\u6280\u672f\u9000\u6b65\u201d\u7684\u53d1\u751f\u5bf9\u8c61\u4f1a\u662f\u5e15\u62c9\u8482\u65af\u3002",
    "\u5a01\u5ec9\u2026\u2026\uff1f",
    "\u6211\u5728\u683c\u91cc\u82ac\u6700\u540e\u4e00\u4e2a\u4efb\u52a1\u7684\u4efb\u52a1\u5bf9\u8c61\u3002\u5a01\u5ec9\u3002",
    "\uff08\u8fd9\u4e2a\u540d\u5b57\u5e26\u8d77\u4e86\u4e00\u4e9b\u4e0d\u592a\u597d\u7684\u56de\u5fc6\uff0c\u6211\u7684\u8138\u8272\u5e94\u8be5\u4e0d\u592a\u597d\u770b\u3002\uff09",
    "\u4ece\u9ec4\u533a\u7684\u5c45\u4f4f\u70b9\u91cc\u5438\u6536\u4eba\u7fa4\u3001\u4f7f\u7528\u201c\u5f92\u4ece\u201d\u8fd9\u4e2a\u540d\u79f0\u3001\u610f\u4e49\u4e0d\u660e\u7684\u201c\u51c0\u5316\u4eea\u5f0f\u201d\u3001\u201c\u6d77\u4f26\u5a1c\u201d\u3001\u6d77\u4f26\u5a1c\u63d0\u5230\u7684\u201c\u6ce2\u6797\u5361\u201d\u2026\u2026\u4ee5\u53ca\u8fd8\u4e0d\u77e5\u9053\u662f\u4e0d\u662f\u5c5e\u4e8e\u65b0\u578b\u6d85\u6258\u7684\u201c\u59ae\u7279\u201d\u548c\u201c\u4f0a\u9ed8\u5c14\u201d\u2026\u2026",
    "\u5982\u679c\u5a01\u5ec9\u7684\u76ee\u7684\u8fd8\u662f\u5236\u9020\u201c\u5b8c\u5168\u514d\u75ab\u4f53\u201d\uff0c\u5728\u4ed6\u7684\u5e26\u9886\u4e0b\uff0c\u7528\u4e86\u5341\u5e74\u91cd\u65b0\u53d1\u5c55\u8d77\u6765\u7684\u5e15\u62c9\u8482\u65af\uff0c\u4e00\u5b9a\u6bd4\u4e4b\u524d\u66f4\u52a0\u96be\u5bf9\u4ed8\u3002",
    "\u5514\uff1f\u5514\u5514\uff1f\u6d77\u4f26\u5a1c\u3001\u542c\u5230\u4e86\uff1f",
    "\uff08\u542c\u5230\u81ea\u5df1\u7684\u540d\u5b57\uff0c\u6d77\u4f26\u5a1c\u56de\u5934\u770b\u6211\u3002\uff09",
    "\u6ca1\u4ec0\u4e48\u3002",
    "\u6307\u6325\u5b98\u3001\u8bf4\u574f\u8bdd\u3001\u662f\u574f\u4eba\uff01",
    "\u2026\u2026\u4e0d\u53ea\u662f\u5404\u79cd\u5404\u6837\u7684\u77e5\u8bc6\uff0c\u4f60\u7684\u8868\u8fbe\u80fd\u529b\u662f\u4e0d\u662f\u4e5f\u63d0\u5347\u4e86\u4e00\u4e9b\uff0c\u6d77\u4f26\u5a1c\uff1f",
    "\u54cd\u4e86\u3001\u6709\u5708\u5708\uff01",
    "\uff08\u603b\u63a7\u53f0\u7684\u5c4f\u5e55\u4e0a\u51fa\u73b0\u65b0\u63d0\u793a\uff0c\u6d77\u4f26\u5a1c\u7acb\u523b\u8dd1\u8fc7\u53bb\uff0c\u60f3\u8981\u89e6\u6478\u5c4f\u5e55\u3002\uff09",
    "\u5708\u5708\u3001\u6709\u597d\u591a\uff01\u4e0d\u52a8\u4e86\uff1f",
    "\u8fd9\u4e2a\u4e0d\u80fd\u52a8\uff0c\u6d77\u4f26\u5a1c\u3002",
    "\u5514\u2026\u2026",
    "\uff08\u6d77\u4f26\u5a1c\u6323\u624e\u4e86\u4e00\u4e0b\uff0c\u6ca1\u80fd\u6323\u8131\u95ea\u7535\uff0c\u53ea\u80fd\u60bb\u60bb\u5730\u6536\u56de\u4e86\u81ea\u5df1\u7684\u624b\u3002\uff09",
    "\u6307\u6325\u5b98\uff0c\u7f8e\u73b2\u7ed9\u59ae\u7279\u52a0\u88c5\u7684\u5b9a\u4f4d\u56de\u4f20\u88c5\u7f6e\u663e\u793a\uff0c\u59ae\u7279\u5df2\u7ecf\u505c\u7559\u5728\u540c\u4e00\u4f4d\u7f6e\u8d85\u8fc732\u5206\u949f\uff0c\u8d85\u51fa\u8fc7\u5f80\u4e24\u65e5\u5185\u76d1\u6d4b\u83b7\u5f97\u7684\u6700\u5927\u505c\u7559\u5206\u949f\u6570\u3002",
    "\u8d85\u51fa\u767e\u5206\u6bd4\uff1f",
    "185%\u3002",
    "\u5230\u8fbe200%\u540e\u8bf7\u6c42\u5b9a\u4f4d\uff0c\u8bbe\u7f6e\u4e3a\u76ee\u6807\u4f4d\u7f6e\u3002",
    "\u4e86\u89e3\u3002",
    "\uff08\u6211\u770b\u4e86\u4e00\u773c\u843d\u5730\u7a97\u5916\u7684\u5730\u5f62\u3002\uff09",
    "\u76ee\u6807\u4f4d\u7f6e\u53ef\u80fd\u975e\u5e38\u63a5\u8fd1\u7ea2\u533a\uff0c\u95ea\u7535\uff0c\u901a\u77e5\u7f8e\u73b2\u6309\u7167III\u7ea7\u9632\u62a4\u8981\u6c42\u6574\u5907\uff0c\u5bf9\u8c61\u5305\u62ec\u4eba\u5f62\u5c0f\u961f\u5168\u4f53\u6210\u5458\u2014\u2014\u786e\u8ba4\u76ee\u6807\u4f4d\u7f6e\u540e\u4f60\u5c31\u53bb\u627e\u7f8e\u73b2\u3002",
    "III\u7ea7\u9632\u62a4\u8981\u6c42\u91cc\u7f3a\u5c11\u7684\u574d\u584c\u8f90\u5c04\u63a2\u6d4b\u4eea\u4e4b\u524d\u5df2\u7ecf\u901a\u77e5\u4f69\u59ae\u6cfd\u9001\u8d27\uff0c\u786e\u8ba4\u5230\u8d27\u540e\u8bf7\u7f8e\u73b2\u6309\u7167\u4f60\u4eec\u5404\u81ea\u7684\u9632\u62a4\u60c5\u51b5\u8bbe\u7f6e\u9884\u8b66\u4e0a\u9650\uff0c\u6218\u6597\u4e2d\u6ce8\u610f\u88c5\u7f6e\u663e\u793a\u6570\u503c\uff0c\u5230\u8fbe\u5371\u9669\u8303\u56f4\u540e\u7acb\u523b\u64a4\u9000\u3002",
    "\u64a4\u9000\uff1f",
    "\u6307\u6325\u5b98\uff0c\u8bbe\u7f6e\u4e0a\u9650\u540e\uff0c\u574d\u584c\u8f90\u5c04\u63a2\u6d4b\u4eea\u4f9d\u636e\u4e0a\u9650\u6240\u81ea\u52a8\u8ba1\u7b97\u51fa\u7684\u5371\u9669\u8303\u56f4\u533a\u95f4\u8f83\u5927\u3002\u8fdb\u5165\u5371\u9669\u8303\u56f4\u540e\u5373\u64a4\u9000\uff0c\u53ef\u80fd\u4f1a\u9519\u5931\u63a2\u67e5\u673a\u4f1a\u3002\u6211\u8ba4\u4e3a\u6211\u4eec\u9700\u8981\u5c3d\u53ef\u80fd\u83b7\u53d6\u4e0e\u5e15\u62c9\u8482\u65af\u6709\u5173\u7684\u4fe1\u606f\uff0c\u6307\u6325\u5b98\u3002",
    "\uff08\u95ea\u7535\u76f4\u89c6\u7740\u6211\uff0c\u8868\u60c5\u975e\u5e38\u575a\u51b3\u3002\uff09",
    "\u4e3a\u4e86\u7ed3\u675f\u4f60\u6ca1\u6709\u5b8c\u6210\u7684\u4efb\u52a1\uff0c\u4e0b\u8fbe\u64a4\u9000\u547d\u4ee4\u7684\u65f6\u673a\uff0c\u6211\u5e0c\u671b\u80fd\u591f\u4ea4\u7ed9\u6211\uff0c\u6307\u6325\u5b98\u3002",
    "\uff08\u2026\u2026\u8fd9\u4e2a\u8868\u60c5\uff0c\u6211\u975e\u5e38\u719f\u6089\u3002\uff09",
    "\uff08\u5dee\u4e0d\u591a\u5341\u5e74\u524d\uff0c\u5728\u4e00\u5904\u4f4d\u4e8e\u9ec4\u533a\u7684\u5e9f\u589f\u91cc\uff0c\u6211\u518d\u6b21\u9047\u5230\u4e86\u95ea\u7535\u3002\u4ece\u90a3\u65f6\u8d77\uff0c\u95ea\u7535\u5c31\u6709\u4e86\u4e00\u4e9b\u6539\u53d8\u3002\uff09",
    "\uff08\u4eba\u5f62\u7684\u5fc3\u667a\u53d7\u635f\uff0c\u56e0\u800c\u9020\u6210\u8bb0\u5fc6\u7f3a\u5931\uff0c\u5e76\u56e0\u540e\u5929\u518d\u6b21\u7684\u8bb0\u5fc6\u79ef\u7d2f\u800c\u53d1\u751f\u6027\u683c\u6539\u53d8\uff0c\u8fd9\u672c\u6765\u5c31\u662f\u4e00\u4ef6\u5f88\u6b63\u5e38\u7684\u4e8b\u3002\uff09",
    "\uff08\u5341\u5e74\u91cc\uff0c\u6211\u4e5f\u6709\u60f3\u8fc7\u5e2e\u95ea\u7535\u627e\u4e00\u627e\u5979\u7684\u5fc3\u667a\u5907\u4efd\u2014\u2014\u5728\u683c\u91cc\u82ac\u65f6\u671f\uff0c\u7559\u5728\u683c\u91cc\u82ac\u6570\u636e\u5e93\u4e2d\uff0c\u95ea\u7535\u7684\u5fc3\u667a\u5b58\u6863\uff0c\u4f46\u4e00\u76f4\u6ca1\u80fd\u627e\u5230\u5408\u9002\u7684\u673a\u4f1a\u3002\uff09",
    "\uff08\u800c\u6211\u51e0\u6b21\u4ea7\u751f\u8fd9\u79cd\u60f3\u6cd5\u2014\u2014\u90fd\u662f\u56e0\u4e3a\u95ea\u7535\u9732\u51fa\u4e86\u8fd9\u79cd\u8868\u60c5\u3002\uff09",
    "\uff08\u91cd\u9022\u540e\u7684\u95ea\u7535\uff0c\u83ab\u540d\u5176\u5999\u5730\u4f1a\u5bf9\u4e00\u4e9b\u4e8b\u60c5\u975e\u5e38\u6267\u7740\uff0c\u6267\u8457\u5230\u4e0d\u50cf\u662f\u4e4b\u524d\u6211\u6240\u8ba4\u8bc6\u7684\u90a3\u4e2a\u95ea\u7535\u3002\uff09",
    "\uff08\u4f46\u8fd9\u6b21\u7a81\u7136\u7684\u575a\u6301\uff0c\u662f\u56e0\u4e3a\u6211\u521a\u624d\u63d0\u5230\u201c\u5a01\u5ec9\u201d\u65f6\u7684\u8138\u8272\uff0c\u8fd8\u662f\u2026\u2026\uff09",
    "\u2026\u2026\u597d\u3002",
    "\uff08\u4e0e\u8fc7\u5f80\u4e00\u6837\uff0c\u6211\u9009\u62e9\u8ba9\u6b65\u3002\uff09",
    "\uff08\u5bf9\u7a97\u5916\u7684\u666f\u8272\u5f00\u59cb\u611f\u5230\u538c\u70e6\uff0c\u53c8\u4e0d\u80fd\u7a81\u7834\u95ea\u7535\u7684\u963b\u62e6\uff0c\u6d77\u4f26\u5a1c\u62d6\u7740\u811a\u6b65\u4ece\u6211\u8eab\u8fb9\u8d70\u8fc7\uff0c\u626f\u4e86\u626f\u6211\u7684\u8863\u670d\u3002\uff09",
    "\u56f0\u2026\u2026\u8981\u7761\u2026\u2026",
    "\u8981\u56de\u4f11\u606f\u5ba4\u5417\uff1f\u4f11\u606f\u533a\u7684\u6905\u5b50\u4f11\u606f\u4e00\u4e0b\u8fd8\u53ef\u4ee5\uff0c\u7761\u8d77\u6765\u4e0d\u4f1a\u5f88\u8212\u670d\u3002",
    "\u4e0d\uff01\u5728\u8fd9\u91cc\u2026\u2026\u8fd9\u91cc\u7761\uff01",
    "\uff08\u867d\u7136\u5df2\u7ecf\u56f0\u5230\u5728\u52aa\u529b\u7741\u7740\u773c\u775b\uff0c\u6d77\u4f26\u5a1c\u4ecd\u7136\u8868\u73b0\u51fa\u4e86\u575a\u51b3\u7684\u53cd\u5bf9\u3002\uff09",
    "\uff08\u2026\u2026\u5c31\u548c\u521a\u624d\u7684\u95ea\u7535\u4e00\u6837\u3002\uff09",
    "\u2026\u2026\u6bef\u5b50\u5728\u6905\u5b50\u5de6\u8fb9\u7b2c\u4e8c\u4e2a\u67dc\u5b50\u91cc\u3002",
    "\u5348\u5b89\u2026\u2026",
    "\uff08\u6d77\u4f26\u5a1c\u8d70\u5411\u4e86\u4f11\u606f\u533a\u3002\uff09",
    "\uff08\u6211\u5411\u95ea\u7535\u91cd\u590d\u4e86\u4e00\u6b21\u6211\u7684\u56de\u7b54\u3002\uff09",
    "\u5224\u65ad\u5e76\u9009\u62e9\u64a4\u9000\uff0c\u8fd9\u4e2a\u6743\u5229\u53ef\u4ee5\u4ea4\u7ed9\u4f60\u3002\u4f46\u6211\u8fd8\u9700\u8981\u786e\u8ba4\u4e00\u4e2a\u60c5\u51b5\u3002",
    "\u662f\uff1f",
    "\u5173\u4e8e\u4f60\u8eab\u4e0a\u53d1\u751f\u7684\u4e8b\u60c5\u2026\u2026\u6709\u4efb\u4f55\u8fdb\u5c55\u5417\uff0c\u95ea\u7535\uff1f",
    "\u6ca1\u6709\u4efb\u4f55\u80fd\u88ab\u79f0\u4e3a\u201c\u8fdb\u5c55\u201d\u7684\u53d1\u73b0\u3002",
    "\uff08\u95ea\u7535\u7684\u56de\u7b54\u5e72\u8106\u800c\u679c\u65ad\uff0c\u6ca1\u6709\u4e1d\u6beb\u72b9\u8c6b\u3002\uff09",
    "\uff08\u2026\u2026\u4f9d\u636e\u8fd9\u6837\u7684\u56de\u7b54\uff0c\u6211\u65e0\u6cd5\u505a\u51fa\u4efb\u4f55\u63a8\u65ad\u3002\uff09",
    "\u2026\u2026\u597d\u5427\u3002\u6709\u8fdb\u5c55\u544a\u8bc9\u6211\u3002",
    "\u597d\u3002",
    "\u53e6\u5916\uff0c\u6ce8\u610f\u7eb3\u7f8e\u897f\u4e1d\u3002",
    "\u867d\u7136\u6000\u7591\u961f\u91cc\u7684\u6210\u5458\u4e0d\u662f\u6211\u7684\u98ce\u683c\u2026\u2026\u5982\u679c\u7eb3\u7f8e\u897f\u4e1d\u4ee5\u524d\u5c5e\u4e8e\u5e15\u62c9\u8482\u65af\u7684\u8fd1\u536b\u4f5c\u6218\u5c0f\u961f\uff0c\u4e0d\u7ba1\u662f\u51fa\u4e8e\u81ea\u613f\u8fd8\u662f\u88ab\u8feb\uff0c\u5979\u6765\u5230\u6211\u4eec\u57fa\u5730\u8f66\uff0c\u53ef\u80fd\u6709\u522b\u7684\u76ee\u7684\u3002",
    "\u4f46\u4ece\u52a0\u5165\u57fa\u5730\u8f66\u5230\u76ee\u524d\u4e3a\u6b62\uff0c\u7eb3\u7f8e\u897f\u4e1d\u5e76\u6ca1\u6709\u505a\u51fa\u4efb\u4f55\u503c\u5f97\u88ab\u6000\u7591\u7684\u884c\u4e3a\u3002\u8fd9\u662f\u57fa\u4e8e\u4f60\u5bf9\u5e15\u62c9\u8482\u65af\u7684\u4e86\u89e3\u6240\u505a\u51fa\u7684\u5224\u65ad\u5417\uff0c\u6307\u6325\u5b98\uff1f",
    "\u2026\u2026\u7b97\u662f\u5427\u3002",
    "\u7eb3\u7f8e\u897f\u4e1d\u5728\u8bed\u8a00\u98ce\u683c\u65b9\u9762\u4e5f\u7b97\u662f\u6709\u4e9b\u53ef\u7591\uff1f\u603b\u800c\u8a00\u4e4b\uff0c\u4e8b\u5173\u5e15\u62c9\u8482\u65af\uff0c\u8c28\u614e\u4e00\u4e9b\u603b\u6ca1\u9519\u3002",
    "\u4e86\u89e3\u3002"
  ]
]