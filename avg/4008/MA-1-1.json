[
  [
    "\uff08CHE-02\u536b\u661f\u57ce\uff0c\u4e00\u4e2a\u9633\u5149\u660e\u5a9a\u7684\u4e0b\u5348\u2014\u2014\uff09",
    "\uff08\u767d\u65e5\u7684\u536b\u661f\u57ce\u4e2d\u4eba\u4e0d\u7b97\u591a\uff0c\u590f\u514b\u91cc\u548c\u6851\u6735\u83b1\u5e0c\u5404\u63a8\u7740\u4e00\u4e2a\u63a8\u8f66\u8d70\u5728\u8def\u4e0a\u3002\uff09",
    "\uff08\u63a8\u8f66\u4e0a\u7684\u7bb1\u5b50\u5c0f\u5c71\u4e00\u6837\u5792\u7740\uff0c\u590f\u514b\u91cc\u6c89\u6d78\u5728\u8d2d\u7269\u7684\u6109\u60a6\u4e2d\uff0c\u5979\u5634\u91cc\u54fc\u7740\u6b4c\uff0c\u6b65\u4f10\u4e5f\u8e66\u8e66\u8df3\u8df3\u8d77\u6765\u3002\uff09",
    "\u2026\u2026\u706f\u5149~\u8ba1\u7b97\u7684\u89d2\u5ea6\u2665\u821e\u53f0~\u5347\u8d77\u7d2b\u8272\u70df\u96fe\uff01|\u4e0d\u5c0f\u5fc3\u7684\u8bdd~\u5c31\u4f1a\u8dcc\u5165\u89c6\u89c9\u7684\u9a97\u672f\u2014\u2014\u545c\u54e6~\u2665",
    "\u662f\u4e3a\u5e97\u5e86\u8868\u6f14\u51c6\u5907\u7684\u6b4c\uff1f",
    "\u6ca1\u9519\uff01\u4e3a\u4e86\u914d\u5408\u9b54\u672f\u8868\u6f14\uff0c\u6211\u4e13\u95e8\u9009\u62e9\u4e86\u9b54\u672f\u4e3b\u9898\u7684\u6b4c\u54e6\uff0c\u600e\u4e48\u6837\u600e\u4e48\u6837\uff1f",
    "\u6211\u4e0d\u592a\u4e86\u89e3\u97f3\u4e50\uff0c\u4f46\u542c\u4e0a\u53bb\u5f88\u6b22\u5feb\u3002",
    "\u563b\u563b~\u8fd9\u53ef\u662f\u6211\u7cbe\u5fc3\u6311\u9009\u7684\u66f2\u76ee\uff01\u914d\u5408\u534e\u4e3d\u7684\u9e3d\u5b50\u9b54\u672f\uff0c\u7b49\u5230\u6b4c\u66f2\u6700\u9ad8\u6f6e\u7684\u65f6\u5019\uff0c\u518d\u8ba9\u70df\u706b\u5347\u7a7a\u2014\u2014\u201c\u7830\u201d\u7684\u4e00\u4e0b\uff01\u5927\u5bb6\u4e00\u5b9a\u90fd\u4f1a\u6c89\u9189\u5176\u4e2d\u7684\uff01",
    "\u539f\u6765\u5982\u6b64\uff0c\u6240\u4ee5\u60a8\u4e70\u4e86\u70df\u706b\u548c\u6d3b\u9e3d\u3002",
    "\u554a\u5bf9\u4e86\uff0c\u6851\u6735\u83b1\u5e0c\uff0c\u6211\u8fd8\u6ca1\u6709\u9053\u8c22\u5462~\u8c22\u8c22\u6851\u6735\u83b1\u5e0c\u4eca\u5929\u5e2e\u6211\u91c7\u8d2d~\u8981\u4e0d\u7136\u7684\u8bdd\uff0c\u590f\u514b\u91cc\u8981\u8dd1\u597d\u51e0\u8d9f\u624d\u80fd\u642c\u5b8c\u8fd9\u4e9b\u4e1c\u897f\u5462\u2665",
    "\u6ca1\u5173\u7cfb\uff0c\u5e97\u957f\u5631\u5490\u4e86\u6211\u597d\u51e0\u6b21\u8981\u597d\u597d\u534f\u52a9\u60a8\u5b8c\u6210\u5e97\u5e86\u8868\u6f14\u7684\u51c6\u5907\u3002",
    "\u554a\u2014\u2014\u8bf4\u5230\u8fd9\u4e2a\uff0c\u6211\u8fd8\u6709\u4e00\u4ef6\u4e8b\u60f3\u8bf7\u60a8\u5e2e\u5fd9\u54e6~",
    "\u8bf7\u8bb2\u3002",
    "\uff08\u590f\u514b\u91cc\u731b\u5730\u4e00\u63a8\u624b\u4e2d\u63a8\u7740\u7684\u5c0f\u63a8\u8f66\uff0c\u53cc\u624b\u677e\u5f00\u63e1\u628a\uff0c\u539f\u5730\u65cb\u8f6c\u4e00\u5708\u505a\u51fa\u9080\u8bf7\u7684\u52a8\u4f5c\u3002\uff09",
    "\uff08\u773c\u770b\u63a8\u8f66\u4e0a\u5806\u6210\u5c0f\u5c71\u7684\u7bb1\u5b50\u5373\u5c06\u5d29\u584c\uff0c\u6851\u6735\u83b1\u5e0c\u8d76\u7d27\u4f38\u624b\u7a33\u4f4f\u3002\uff09",
    "\u6851\u6735\u83b1\u5e0c\u5c0f\u59d0\uff0c\u6211\u80fd\u6709\u5e78\u9080\u8bf7\u60a8\u6765\u505a\u6211\u7684\u4f34\u6f14\u52a9\u624b\u5417\uff1f",
    "\u52a9\u624b\uff1f",
    "\u5c31\u662f\u5e2e\u6211\u8868\u6f14\u4e00\u4e9b\u9b54\u672f\u5566\uff0c\u653e\u653e\u9e3d\u5b50\u3001\u4e3e\u4e00\u4e0b\u706b\u5708\u4ec0\u4e48\u7684\uff0c\u5f88\u7b80\u5355\u7684\uff01",
    "\u706b\u5708\u6050\u6015\u4e0d\u592a\u5b89\u5168\u2014\u2014",
    "\u6240\u4ee5\u4e3a\u4e86\u5927\u5bb6\u7684\u5b89\u5168\uff0c\u53ea\u80fd\u627e\u53ef\u9760\u7684\u6851\u6735\u83b1\u5e0c\u5e2e\u5fd9\u5566~",
    "\u6c42\u6c42\u60a8\u561b~\u771f\u7684\u5f88\u7b80\u5355\uff0c\u800c\u4e14\u8981\u662f\u6ca1\u6709\u52a9\u624b\u7684\u8bdd\uff0c\u5c31\u6ca1\u529e\u6cd5\u6f14\u51fa\u5566\uff0c\u90a3\u6837\u5e97\u957f\u548c\u590f\u514b\u91cc\u90fd\u4f1a\u4f24\u5fc3\u7684\u2026\u2026\u597d\u4e0d\u597d\uff1f",
    "\u597d\uff0c\u6211\u77e5\u9053\u4e86\u3002",
    "\u563b\u563b\uff0c\u8c22\u8c22\uff01\u6211\u5c31\u77e5\u9053\uff0c\u6851\u6735\u83b1\u5e0c\u867d\u7136\u770b\u4e0a\u53bb\u6709\u70b9\u51b7\u51b0\u51b0\uff0c\u4f46\u4e00\u5b9a\u4f1a\u540c\u610f\u5e2e\u5fd9\u7684~",
    "\u55ef\u2026\u2026\u548c\u67d0\u4e2a\u767d\u5403\u767d\u559d\u8fd8\u4e0d\u80af\u5e2e\u5fd9\u7684\u5927\u5c0f\u59d0\u4e0d\u4e00\u6837\u5462\u3002",
    "\u767d\u5403\u767d\u559d\u2026\u2026\u662f\u8bf4\u739b\u7eee\u6735\u5c0f\u59d0\uff1f",
    "\u4ec0\u4e48\u4ec0\u4e48\uff0c\u539f\u6765\u6851\u6735\u83b1\u5e0c\u4e5f\u89c9\u5f97\u90a3\u5bb6\u4f19\u662f\u767d\u5403\u767d\u559d\u5440~\uff1f",
    "\u6ca1\u6709\uff0c\u53ea\u662f\u63a8\u6d4b\u3002\u76ee\u524d\uff0c\u6765\u5e97\u7528\u9910\u4e0d\u7528\u4ed8\u6b3e\u7684\u4e5f\u5c31\u53ea\u6709\u5979\u4e86\u3002|\u60a8\u5bf9\u5979\u7684\u6001\u5ea6\u4f3c\u4e4e\u5f88\u5c16\u9510\uff0c\u53d1\u751f\u4ec0\u4e48\u4e8b\u4e86\uff1f",
    "\u4e5f\u6ca1\u4ec0\u4e48\u5566\uff0c\u5c31\u662f\u5e97\u5e86\u7684\u65f6\u95f4\u4e0d\u662f\u521a\u597d\u548c\u72c2\u6b22\u8282\u5f88\u8fd1\u5417\uff1f\u8bf4\u5230\u5207\u5c14\u5361\u745f\u72c2\u6b22\u8282\uff0c\u4e00\u5b9a\u8981\u6709\u73a9\u5076\u5409\u7965\u7269\u7684\u5bf9\u5427\uff0c\u53ef\u662f\u6211\u6c42\u5979\u597d\u4e45\uff0c\u5927\u5c0f\u59d0\u90fd\u4e0d\u613f\u610f\u5e2e\u5fd9\u7a7f\u4e0a\u73a9\u5076\u670d\u6765\u5e2e\u6211\u62db\u63fd\u5ba2\u4eba\u6b38~",
    "\u5979\u662f\u4e0d\u592a\u559c\u6b22\u5f15\u4eba\u6ce8\u76ee\u3002",
    "\u53ef\u662f\u7a7f\u7740\u73a9\u5076\u670d\u53c8\u4e0d\u7528\u9732\u8138\uff0c\u6ca1\u4eba\u4f1a\u77e5\u9053\u91cc\u9762\u662f\u5979\u561b\u2026\u2026\u800c\u4e14\u800c\u4e14\uff0c\u5979\u8fd8\u8bf4\u590f\u514b\u91cc\u7684\u4e3b\u610f\u5f88\u65e0\u804a\uff01\u4e0d\u8fc7\u5206\u5417\uff1f",
    "\u2026\u2026\u6211\u60f3\uff0c\u8fd9\u53ea\u662f\u739b\u7eee\u6735\u5c0f\u59d0\u4e00\u8d2f\u7684\u8868\u8fbe\u98ce\u683c\u3002",
    "\u771f\u662f\u7684\uff0c\u600e\u4e48\u8fde\u6851\u6735\u83b1\u5e0c\u4e5f\u7ad9\u5728\u5979\u90a3\u8fb9\uff0c\u96be\u9053\u662f\u590f\u514b\u91cc\u4e0d\u591f\u53ef\u7231\u5417\uff1f",
    "\u4e0d\u662f\u8fd9\u6837\u2026\u2026",
    "\u6bcf\u4eba\u7684\u6c9f\u901a\u65b9\u5f0f\u4e0d\u540c\uff0c\u6211\u89c9\u5f97\uff0c\u60a8\u6709\u4e9b\u8fc7\u4e8e\u9488\u5bf9\u2014\u2014",
    "\u54c7\u2014\u2014\u8fd9\u4e2a\uff0c\u597d\u53ef\u7231\uff01",
    "\u2026\u2026",
    "\uff08\u590f\u514b\u91cc\u6ce8\u610f\u5230\u8def\u8fb9\u6a71\u7a97\u4e2d\u5c55\u793a\u7684\u5546\u54c1\uff0c\u53c8\u4e22\u4e0b\u5c0f\u63a8\u8f66\uff0c\u8e66\u8e66\u8df3\u8df3\u5730\u8dd1\u5230\u6a71\u7a97\u524d\uff0c\u53cc\u624b\u5708\u7740\u773c\u7736\u671d\u91cc\u9762\u5f20\u671b\u3002\uff09",
    "\uff08\u88ab\u6253\u65ad\u8bdd\u5934\u7684\u6851\u6735\u83b1\u5e0c\u4e0d\u5f97\u4e0d\u4e00\u4eba\u62c9\u7740\u4e24\u53f0\u63a8\u8f66\u8ddf\u4e0a\u5979\u3002\uff09",
    "\u6851\u6735\u83b1\u5e0c\uff0c\u6211\u4eec\u4e70\u4e0b\u5b83\u597d\u4e0d\u597d\uff1f",
    "\u2026\u2026\u73a9\u5076\u670d\uff1f",
    "\u5bf9\uff01\u8fd9\u4e2a\u6bdb\u8338\u8338\u7684\u8d28\u611f\u770b\u4e0a\u53bb\u50cf\u771f\u7684\u732b\u54aa\u4e00\u6837\uff0c\u5ba2\u4eba\u4eec\u4e00\u5b9a\u4e5f\u4f1a\u559c\u6b22\u5b83\u7684~",
    "\u53ef\u662f\u60a8\u5df2\u7ecf\u6709\u4e00\u5957\u718a\u7684\u4e86\u3002",
    "\u591a\u4e00\u4e2a\u5409\u7965\u7269\u7684\u8bdd\u6c14\u6c1b\u4e5f\u4f1a\u66f4\u70ed\u70c8\u5427\uff1f\u8fd9\u53ef\u662f\u96be\u5f97\u7684\u5e97\u5e86plus\u72c2\u6b22\u8282\u54e6\uff01",
    "\u8c01\u6765\u7a7f\u5462\uff1f",
    "\u8c01\u2026\u2026\u6211\u4f1a\u641e\u5b9a\u7684\u5566\uff0c\u5927\u4e0d\u4e86\u5c31\u8ba9\u52a9\u624b\u5c0f\u59d0\u7a7f\u561b\uff0c\u73a9\u5076\u52a9\u624b\u4e5f\u5f88\u53ef\u7231\u5462~",
    "\u52a9\u624b\u5c0f\u59d0\uff0c\u662f\u8bf4\u6211\u5417\uff1f|\u867d\u7136\u6211\u53ef\u4ee5\u7a7f\uff0c\u4f46\u7ed3\u5408\u6f14\u51fa\u8ba1\u5212\u548c\u76ee\u524d\u7684\u9884\u7b97\u60c5\u51b5\uff0c\u6211\u8ba4\u4e3a\u4e0d\u5e94\u8d2d\u4e70\u8ba1\u5212\u5916\u7684\u7269\u54c1\u3002",
    "\u5509\u2014\u2014\u867d\u7136\u6253\u76f4\u7403\u662f\u6851\u6735\u83b1\u5e0c\u7684\u4f18\u70b9\u5566\uff0c\u4f46\u590f\u514b\u91cc\u8fd8\u662f\u4f1a\u89c9\u5f97\u96be\u8fc7\u5462\u2026\u2026\u800c\u4e14\u4e5f\u4e0d\u80af\u6539\u53e3\uff0c\u90fd\u4e00\u76f4\u8fd9\u4e48\u5ba2\u6c14\u5730\u53eb\u590f\u514b\u91cc\uff0c\u662f\u4e0d\u662f\u4e0d\u559c\u6b22\u590f\u514b\u91cc\u5440\u2026\u2026",
    "\u62b1\u6b49\uff0c\u6211\u7684\u610f\u601d\u662f\u718a\u5df2\u7ecf\u8db3\u591f\u53ef\u7231\u2014\u2014",
    "\u8db4\u4e0b\uff01",
    "\u6b38\uff1f",
    "\uff08\u590f\u514b\u91cc\u8e72\u4e0b\u8eab\uff0c\u4e00\u4e2a\u5c0f\u578b\u5b89\u4fdd\u673a\u5668\u4eba\u64e6\u7740\u5979\u7684\u5934\u9876\u98de\u4e86\u8fc7\u6765\uff0c\u7838\u788e\u4e86\u5979\u8eab\u540e\u7684\u73bb\u7483\u6a71\u7a97\u2014\u2014\u649e\u51fb\u70b9\u6b63\u597d\u662f\u521a\u521a\u590f\u514b\u91cc\u7684\u8111\u888b\u6240\u5728\u7684\u4f4d\u7f6e\u3002\uff09",
    "\uff08\u590f\u514b\u91cc\u63a2\u51fa\u8111\u888b\uff0c\u671d\u7834\u788e\u7684\u6a71\u7a97\u91cc\u770b\u53bb\u3002\u5979\u521a\u521a\u770b\u4e2d\u7684\u732b\u54aa\u73a9\u5076\u670d\u8179\u90e8\u7834\u4e86\u4e00\u4e2a\u5927\u6d1e\uff0c\u5b89\u4fdd\u673a\u5668\u4eba\u7684\u6b8b\u9ab8\u6b63\u60b2\u60e8\u5730\u5728\u6d1e\u91cc\u95ea\u7740\u7535\u706b\u82b1\u3002\uff09",
    "\u732b\u54aa\u2014\u2014\u54ce\u5440\uff0c\u53d8\u6210\u4e07\u5723\u8282\u732b\u54aa\u4e86\uff01|\u4ec0\u4e48\u554a\uff01\u4eca\u5929\u7684\u5929\u6c14\u662f\u5c40\u90e8\u964d\u5b89\u4fdd\u673a\u5668\u4eba\u5417\uff1f",
    "\u8fd9\u91cc\u9690\u853d\uff01",
    "\uff08\u590f\u514b\u91cc\u88ab\u6851\u6735\u83b1\u5e0c\u62fd\u7740\u8eb2\u5230\u88c5\u6ee1\u7bb1\u5b50\u7684\u5c0f\u63a8\u8f66\u540e\u9762\uff0c\u5c0f\u5fc3\u7ffc\u7ffc\u5730\u4ece\u7f1d\u9699\u4e2d\u5f20\u671b\uff0c\u53ea\u89c1\u51e0\u53f0\u673a\u68b0\u5728\u8857\u9053\u4e0a\u6a2a\u51b2\u76f4\u649e\uff0c\u4e0d\u65ad\u5c06\u8eab\u8fb9\u7684\u4e1c\u897f\u6254\u5411\u8ffd\u7740\u5b83\u4eec\u7684\u5c0f\u578b\u5b89\u4fdd\u673a\u5668\u4eba\u3002\uff09",
    "\u90a3\u4e2a\u673a\u68b0\u2014\u2014\u90a3\u5bb6\u4f19\u624b\u91cc\u62ff\u7740\u7684\u662f\u7ea2\u7eff\u706f\uff1f\uff01",
    "\u54c7\uff0c\u90a3\u5bb6\u4f19\u628a\u7ea2\u7eff\u706f\u6254\u51fa\u53bb\u4e86\uff01|\u521a\u624d\u7684\u5b89\u4fdd\u673a\u5668\u4eba\u4e00\u5b9a\u4e5f\u662f\u5b83\u6254\u7684\u5427\uff01\u600e\u4e48\u4e71\u6254\u4e1c\u897f\u5440\uff0c\u7838\u5230\u590f\u514b\u91cc\u600e\u4e48\u529e\u5566\uff01",
    "\u770b\u4e0a\u53bb\u50cf\u662f\u5931\u63a7\u4e86\uff0c\u5148\u89c2\u5bdf\u4e00\u4e0b\u60c5\u51b5\u3002",
    "\u55ef\u2026\u2026\u60c5\u51b5\u5c31\u662f\u90a3\u4e9b\u5b89\u4fdd\u673a\u5668\u4eba\u8fde\u9760\u8fd1\u90fd\u505a\u4e0d\u5230\uff0c\u5b8c\u5168\u6ca1\u6709\u7528\u561b\uff01|\u554a\u2014\u2014\u5b83\u4eec\u8fc7\u6765\u4e86\uff01",
    "\u5b83\u4eec\u7684\u76ee\u6807\u662f\u5bf9\u9762\u7684\u8d27\u8f66\uff01\u5c0f\u5fc3\uff01",
    "\uff08\u5931\u63a7\u673a\u68b0\u51b2\u5411\u5bf9\u9762\u6b63\u5728\u88c5\u8d27\u7684\u5c0f\u8d27\u8f66\uff0c\u4e27\u5fc3\u75c5\u72c2\u5730\u6293\u8d77\u8d27\u8f66\u91cc\u7684\u7bb1\u5b50\u5411\u56db\u5468\u4e22\u53bb\u3002\uff09",
    "\u8d81\u73b0\u5728\uff0c\u6211\u4eec\u5feb\u79bb\u5f00\u8fd9\u91cc\u3002",
    "\u7b49\u7b49\uff01\u6851\u6735\u83b1\u5e0c\uff01\u90a3\u8fb9\u3001\u90a3\u4e2a\u4eba\u2014\u2014",
    "\u2014\u2014\uff01",
    "\uff08\u8d27\u8f66\u4e0a\u5306\u5fd9\u8df3\u4e0b\u6765\u4e00\u4e2a\u50cf\u662f\u53f8\u673a\u7684\u4eba\uff0c\u6b63\u624b\u5fd9\u811a\u4e71\u5730\u8bd5\u56fe\u6361\u8d77\u5730\u4e0a\u6563\u843d\u7684\u7bb1\u5b50\u3002\uff09",
