[
  [
    "\u5e97\u957f\uff0c\u4eca\u665a\u4e91\u5f69\u5f88\u539a\uff0c\u6ca1\u4ec0\u4e48\u6708\u5149\uff0c\u662f\u9002\u5408\u6f5c\u5165\u7684\u65e5\u5b50\u54e6\u3002",
    "\u55ef\u2026\u2026\u9644\u8fd1\u533a\u57df\u7684\u76d1\u63a7\u5e94\u8be5\u90fd\u5931\u6548\u4e86\uff0c\u4f46\u5374\u6709\u4e0d\u5c11\u5b89\u4fdd\u673a\u5668\u4eba\uff0c\u53ef\u80fd\u8fd8\u5728\u6301\u7eed\u641c\u5bfb\u901a\u7f09\u72af\u5427\u3002|\u590f\u514b\u91cc\uff0c\u5c0f\u5fc3\u4e00\u70b9\uff0c\u522b\u88ab\u53d1\u73b0\u4e86\u54e6\u3002",
    "\u771f\u662f\u7684\uff0c\u5e97\u957f\u600e\u4e48\u4e5f\u5bf9\u590f\u514b\u91cc\u6ca1\u4fe1\u5fc3\u5440\uff0c\u8fc7\u6765\u8fd9\u4e00\u8def\u4e0a\u4e0d\u662f\u4e5f\u6ca1\u51fa\u4ec0\u4e48\u4e8b\u561b~",
    "\u90a3\u4e5f\u662f\u56e0\u4e3a\u6709\u590f\u514b\u91cc\u7684\u5316\u5986\u63a9\u62a4\u3002\u5728\u4ee5\u7535\u5b50\u76d1\u63a7\u4e3a\u4e3b\u7684\u73af\u5883\u91cc\uff0c\u5316\u5986\u53ef\u6d3e\u4e0d\u4e0a\u4ec0\u4e48\u7528\u573a\u4e86\uff0c\u8981\u66f4\u52a0\u8c28\u614e\u624d\u884c\u3002",
    "\u54d4\u54d4\uff01",
    "\uff08\u5c0f\u54d4\u7f13\u7f13\u65cb\u8f6c\u7740\u81ea\u5df1\u7684\u5934\u90e8\uff0c\u4f69\u6234\u5728\u5b83\u8116\u5b50\u4e0a\u7684\u5047\u72ee\u5b50\u9b03\u6bdb\u50cf\u5411\u65e5\u8475\u4e00\u6837\u65cb\u8f6c\u5c55\u5f00\u3002\uff09",
    "\uff08\u65c1\u8fb9\u7684\u590f\u514b\u91cc\u6478\u6478\u5b83\u7684\u5934\uff0c\u5728\u6625\u7530\u773c\u524d\u62cd\u4e86\u62cd\u624b\u3002\uff09",
    "\u563b\u563b\uff0c\u5e97\u957f\uff0c\u590f\u514b\u91cc\u65e9\u5c31\u51c6\u5907\u597d\u5bf9\u7b56\u4e86\u54df\u2665",
    "\u5bf9\u7b56\uff1f",
    "\u9535\u9535\u2665\u6765\u6362\u4e0a\u8fd9\u4e2a\u5427~",
    "\u8fd9\u5757\u5e03\u662f\u2026\u2026\u53ef\u4ee5\u7a7f\u7684\u5417\uff1f",
    "\u4ec0\u4e48\u5e03\u554a\uff0c\u5e97\u957f\u597d\u8fc7\u5206\uff01\u8fd9\u53ef\u662f\u590f\u514b\u91cc\u7cbe\u5fc3\u51c6\u5907\u7684\u6f14\u51fa\u670d\u88c5\u3002|\u597d\u5566\uff01\u5148\u6362\u4e0a\u518d\u8bf4\u561b\uff0c\u4e00\u5b9a\u5f88\u5408\u9002\u5e97\u957f\u7684~\u2665",
    "\uff08\u590f\u514b\u91cc\u628a\u624b\u91cc\u7684\u8863\u7269\u5f80\u6625\u7530\u6000\u91cc\u4e00\u585e\uff0c\u8f6c\u773c\u95f4\uff0c\u5c31\u6362\u597d\u4e86\u81ea\u5df1\u7684\u8863\u670d\u3002\uff09",
    "\uff08\u90a3\u662f\u4e00\u5957\u53ef\u4ee5\u76f4\u63a5\u5957\u5728\u5df2\u7ecf\u7a7f\u7740\u7684\u8863\u670d\u5916\u4fa7\u7684\u8863\u88d9\uff0c\u989c\u8272\u7eaf\u9ed1\u3001\u9644\u5e26\u6597\u7bf7\uff0c\u5728\u5468\u56f4\u6697\u8272\u7684\u73af\u5883\u91cc\u975e\u5e38\u5177\u6709\u9690\u853d\u6027\u3002\uff09",
    "\u5e97\u957f\u600e\u4e48\u8fd8\u4e0d\u7a7f\uff1f\u5feb\u70b9\u5566\uff0c\u8981\u4e0d\u8981\u590f\u514b\u91cc\u6765\u5e2e\u5fd9~\uff1f",
    "\u2026\u2026\u6211\u8fd8\u662f\u7b97\u4e86\u5427\uff1f\u6211\u4e0d\u4e60\u60ef\u8fd9\u79cd\u6b3e\u5f0f\u2026\u2026",
    "\u6211\u77e5\u9053\u4e86\uff0c\u5e97\u957f\u5b8c\u5168\u4e0d\u660e\u767d\u8fd9\u5957\u8863\u670d\u7684\u5389\u5bb3\u4e4b\u5904\u5462\uff0c\u90a3\u4e48\u590f\u514b\u91cc\u5c31\u6765\u9686\u91cd\u4ecb\u7ecd\u4e00\u4e0b\u2665|\u8fd9\u662f\u4e3a\u4e86\u5728\u6362\u573a\u65f6\u8fc5\u901f\u66f4\u6362\u88c5\u626e\u800c\u51c6\u5907\u7684\u7279\u6b8a\u6f14\u51fa\u670d\u54e6~",
    "\u55ef\uff0c\u821e\u53f0\u670d\u9970\u8fd9\u4e00\u70b9\u5f88\u660e\u663e\u4e86\u3002",
    "\u5e97\u957f\u60a8\u770b\uff0c\u5b83\u8868\u9762\u4f7f\u7528\u7684\u662f\u5f31\u53cd\u5c04\u6750\u6599\uff0c\u80fd\u591f\u5f88\u597d\u5730\u9690\u85cf\u5728\u8fd9\u79cd\u660f\u6697\u7684\u73af\u5883\u91cc\u3002\u987a\u4fbf\u4e00\u63d0\uff0c\u53ea\u8981\u4e00\u62c9\u80f8\u524d\u7684\u7f0e\u5e26\uff0c\u5c31\u4f1a\u53d8\u6210\u4e00\u6761\u95ea\u4eae\u7684\u88d9\u5b50\u5462\uff01",
    "\u54e6\u2014\u2014",
    "\u563b\u563b\uff0c\u8fd8\u4e0d\u6b62\u8fd9\u6837\u54e6\u3002",
    "\u8170\u90e8\u8fd9\u91cc\u8fd8\u6709\u4e00\u4e2a\u5fae\u578b\u6295\u5f71\u88c5\u7f6e\uff0c\u53ea\u8981\u542f\u52a8\u8fd9\u4e2a\uff0c\u5c31\u80fd\u628a\u5468\u56f4\u7684\u73af\u5883\u6295\u5f71\u5230\u8863\u670d\u4e0a\uff0c\u8fd9\u6837\u518d\u8c03\u4f4e\u7d20\u4f53\u6e29\u5ea6\uff0c\u4e0d\u7ba1\u662f\u4eba\u773c\u8fd8\u662f\u7ea2\u5916\u7ebf\u6210\u50cf\u4eea\uff0c\u90fd\u4e0d\u4f1a\u53d1\u73b0\u6211\u4eec\u5566\uff01",
    "\u4f60\u51c6\u5907\u5f97\u2026\u2026\u5f88\u5145\u5206\u5462\u3002",
    "\u563b\u563b\uff0c\u5982\u679c\u4e0d\u5145\u5206\uff0c\u5e97\u957f\u80af\u5b9a\u8981\u627e\u51fa\u4e00\u5806\u7406\u7531\u4e0d\u7a7f\u7684\u561b\u2665\u590f\u514b\u91cc\uff0c\u4e00\u5b9a\u8981\u7ed9\u739b\u7eee\u6735\u2014\u2014\u554a\u4e0d\u5bf9\uff0c\u4e00\u5b9a\u8981\u505a\u597d\u4e00\u5207\u51c6\u5907\u2665",
    "\u6211\u597d\u50cf\u542c\u5230\u4e86\u739b\u7eee\u6735\uff1f",
    "\u5e97\u957f\u542c\u9519\u5566\uff0c\u590f\u514b\u91cc\u8bf4\u7684\u662f\uff0c\u4e00\u5b9a\u8981\u505a\u597d\u4e00\u5207\u51c6\u5907\u54e6~\u2665|\u60a8\u5feb\u770b\uff0c\u79bb\u8fdc\u4e00\u70b9\u7684\u8bdd\uff0c\u5e97\u957f\u662f\u4e0d\u662f\u4e5f\u5b8c\u5168\u770b\u4e0d\u5230\u6211\u5566\uff1f",
    "\u54d4\u54d4\uff01",
    "\u54c8\u54c8\uff0c\u5c0f\u54d4\u4e5f\u540c\u610f\u6211\u7684\u60f3\u6cd5\u5bf9\u5427\uff1f\u771f\u662f\u4e56\u72d7\u72d7~|\u5bf9\u4e86\uff01\u5e97\u957f\uff0c\u51fa\u53d1\u4e4b\u524d\uff0c\u6851\u6735\u83b1\u5e0c\u4e5f\u7b54\u5e94\u590f\u514b\u91cc\uff0c\u4f1a\u7a7f\u540c\u6837\u7684\u2014\u2014",
    "\u5c0f\u5fc3\u540e\u9762\uff01",
    "\u6b38\uff1f\uff01",
    "\uff08\u590f\u514b\u91cc\u8e09\u8e09\u8dc4\u8dc4\u5730\u505c\u4f4f\u811a\u6b65\uff0c\u5c0f\u5fc3\u7ffc\u7ffc\u5730\u56de\u5934\uff0c\u773c\u524d\u6b63\u662f\u4e00\u53f0\u5de1\u903b\u673a\u68b0\u3002\u95ea\u7740\u7ea2\u5149\u7684\u673a\u68b0\u773c\u626b\u4e86\u8fc7\u6765\u3002\uff09",
    "\uff08\u590f\u514b\u91cc\u8d76\u7d27\u6342\u4f4f\u81ea\u5df1\u7684\u5634\uff0c\u5c0f\u54d4\u4e5f\u505c\u6b62\u6492\u6b22\uff0c\u4e24\u4eba\u540c\u65f6\u8f7b\u8f7b\u5730\u3001\u6162\u6162\u5730\u5411\u540e\u9000\u53bb\u3002\uff09",
    "\u5b83\u5e94\u8be5\u6ca1\u53d1\u73b0\u6211\u5427\u2026\u2026\u5e97\u957f\uff0c\u8fd9\u5957\u8863\u670d\u679c\u7136\u6709\u7528\uff01",
    "\u4f60\u65c1\u8fb9\u8fd8\u6709\u4e00\u53f0\uff01\u7ad9\u5728\u90a3\u91cc\u522b\u52a8\uff01",
    "\u4ec0\u4e48\u2014\u2014\u545c\u54c7\uff01",
    "\u54d4\u54d4\u54d4\u2014\u2014\uff01",
    "\u7b49\u7b49\u3001\u5c0f\u54d4\u522b\u2014\u2014",
    "\uff08\u7a81\u7136\u610f\u8bc6\u5230\u81ea\u5df1\u7684\u60ca\u53eb\u89e6\u53d1\u4e86\u5c0f\u54d4\u7684\u653b\u51fb\u6a21\u5f0f\uff0c\u590f\u514b\u91cc\u60f3\u8981\u963b\u6b62\u5b83\uff0c\u5374\u5df2\u7ecf\u6765\u4e0d\u53ca\u4e86\u3002\uff09",
    "\uff08\u5c0f\u54d4\u8df3\u5230\u4e86\u590f\u514b\u91cc\u8eab\u524d\uff0c\u4f38\u51fa\u70ae\u7ba1\u3002\uff09",
    "\u554a\u5440\u2026\u2026\u8fd9\u5b69\u5b50\u662f\u4e0d\u662f\u8fc7\u4e8e\u53ef\u9760\u4e86\uff1f",
    "\u54d4\u54d4\uff1f",
    "\u8fd9\u4e2a\u5de1\u903b\u673a\u68b0\uff0c\u6ca1\u6709\u6cbb\u5b89\u961f\u7684\u6807\u5fd7\u2026\u2026",
    "\u5e97\u957f\uff01\u5148\u522b\u7ba1\u90a3\u4e2a\u5566\uff0c\u5feb\u9000\u540e\uff0c\u53e6\u4e00\u4e2a\u8981\u653b\u51fb\u4e86\uff01"
  ]
]