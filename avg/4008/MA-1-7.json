    "\u76f4\u63a5\u901a\u8baf\u2014\u2014\u597d\u50cf\u4e5f\u4e0d\u884c\u5462~",
    "\u4ee5\u6211\u4eec\u76ee\u524d\u7684\u706b\u529b\uff0c\u5c31\u7b97\u5168\u529b\u653b\u51fb\u8fd9\u8258\u8239\uff0c\u4e5f\u5f88\u96be\u9020\u6210\u8db3\u4ee5\u60ca\u52a8\u6cbb\u5b89\u90e8\u95e8\u7684\u54cd\u58f0\uff0c\u7279\u522b\u662f\u5728\u5982\u6b64\u504f\u50fb\u7684\u5730\u65b9\u3002",
    "\u4e5f\u53ea\u80fd\u89c1\u673a\u884c\u4e8b\u4e86\uff0c\u6211\u4eec\u5148\u628a\u80fd\u505a\u7684\u505a\u597d\u3002|\u6851\u6735\u83b1\u5e0c\u2026\u2026\u4e0d\u80fd\u6218\u6597\uff0c\u4f46\u8eab\u624b\u6bd4\u8f83\u7075\u6d3b\uff0c\u4f60\u8d1f\u8d23\u5bfb\u627e\u63a7\u5236\u5ba4\u3002",
    "\u597d\uff0c\u627e\u5230\u63a7\u5236\u5ba4\u540e\uff0c\u6211\u6765\u9650\u5236\u654c\u65b9\u884c\u52a8\u3002",
    "\u90a3\uff0c\u5f53\u8bf1\u9975\u7684\u5de5\u4f5c\u5c31\u4ea4\u7ed9\u6211\u5427~",
    "\u8bf1\u9975\u8fd9\u4e2a\u4f4d\u7f6e\uff0c\u5bf9\u6ca1\u4ec0\u4e48\u6218\u6597\u7ecf\u9a8c\u7684\u4f60\u6765\u8bf4\u592a\u5371\u9669\u4e86\u2026\u2026",
    "\u563b\u563b\uff0c\u5e97\u957f\u53ef\u4ee5\u5728\u6697\u4e2d\u63a9\u62a4\u6211\u561b~\u5e97\u957f\u4ee5\u524d\u4e5f\u662f\u975e\u5e38\u5389\u5bb3\u7684\u7cbe\u82f1\u6218\u672f\u4eba\u5f62\uff0c\u5bf9\u5427\uff1f",
    "\u4e0d\u884c\uff0c\u590f\u514b\u91cc\u8fd9\u79cd\u6ca1\u6709\u7ecf\u9a8c\u7684\u4eba\u5f62\uff0c\u80af\u5b9a\u65e0\u6cd5\u80dc\u4efb\u8fd9\u4e2a\u4f4d\u7f6e\u3002\u6625\u7530\u79bb\u5f00\u6218\u573a\u5f88\u4e45\u4e86\uff0c\u5408\u7406\u8003\u8651\u4e5f\u4e0d\u80fd\u7b97\u4e00\u4e2a\u5b8c\u6574\u7684\u6218\u529b\u3002",
    "\u90a3\u739b\u7eee\u6735\u6765\u505a\u8bf1\u9975\u5417\uff1f\u6211\u6765\u534f\u52a9\u4f60\u3002",
    "\u4e0d\u884c\uff0c\u590f\u514b\u91cc\u6ca1\u6709\u5355\u4eba\u6267\u884c\u4efb\u52a1\u7684\u7ecf\u9a8c\uff0c\u4e0d\u80fd\u8ba9\u5979\u81ea\u5df1\u4e00\u7ec4\u3002",
    "\u8fd8\u662f\u6211\u4e00\u4e2a\u4eba\u6765\u505a\u8bf1\u9975\uff0c\u4f60\u4eec\u4e24\u4e2a\u4f5c\u4e3a\u7b2c\u4e8c\u7ec4\u6210\u5458\u597d\u4e86\uff0c\u5b89\u5168\u4e00\u4e9b\u3002\u590f\u514b\u91cc\u90a3\u4e9b\u7a00\u5947\u53e4\u602a\u7684\u70b9\u5b50\u4e5f\u80fd\u6d3e\u4e0a\u7528\u573a\u3002",
    "\u6211\u53cd\u5bf9\u3002|\u6211\u4e0d\u80fd\u628a\u4f60\u4e00\u4e2a\u4eba\u653e\u5728\u6700\u5371\u9669\u7684\u4f4d\u7f6e\u4e0a\u3002\u6211\u548c\u4f60\u4e00\u8d77\uff0c\u4e24\u4eba\u4e4b\u95f4\u8fd8\u53ef\u4ee5\u4e92\u76f8\u7167\u5e94\u3002",
    "\u8fd9\u3001\u8fd9\u7b97\u4ec0\u4e48\u7406\u7531\uff0c\u6211\u90fd\u8bf4\u4e0d\u7528\u4e86\uff0c\u4e24\u4e2a\u4eba\u53cd\u800c\u5f71\u54cd\u6211\u53d1\u6325\uff01|\u4f60\u8fd8\u662f\u770b\u7740\u70b9\u590f\u514b\u91cc\u522b\u8ba9\u5979\u2014\u2014",
    "\u590f\u514b\u91cc\u4e5f\u53cd\u5bf9\u54df\u2665|\u5077\u5077\u6478\u6478\u7684\u5de5\u4f5c\u739b\u7eee\u6735\u6765\u505a\u5c31\u597d\u4e86\u561b\uff0c\u590f\u514b\u91cc\u624d\u4e0d\u8981\u505a\u5462~",
    "\u2026\u2026\u4f60\u522b\u6dfb\u4e71\u4e86\u597d\u5417\uff1f",
    "\u8bf1\u9975\u8981\u627f\u62c5\u7684\u4e0d\u4ec5\u662f\u5438\u5f15\u706b\u529b\u7684\u4efb\u52a1\uff0c\u66f4\u8981\u517c\u987e\u5168\u573a\u7684\u5c40\u52bf\uff0c\u8ba9\u654c\u4eba\u4e0d\u4f1a\u53d1\u73b0\u5e76\u5e72\u6270\u53e6\u5916\u4e24\u7ec4\u7684\u5de5\u4f5c\uff0c\u7a0d\u6709\u95ea\u5931\u5c31\u4f1a\u5168\u76d8\u7686\u8f93\uff0c\u4f60\u505a\u5f97\u5230\u5417\uff1f",
    "\u54c8\uff0c\u96be\u9053\u5728\u739b\u7eee\u6735\u773c\u91cc\uff0c\u590f\u514b\u91cc\u5c31\u662f\u4e00\u4e2a\u4ec0\u4e48\u90fd\u505a\u4e0d\u5230\u3001\u53ea\u4f1a\u8ba8\u597d\u522b\u4eba\u7684\u6d0b\u5a03\u5a03\uff1f|\u522b\u5c0f\u77a7\u4eba\u4e86\uff0c\u5371\u9669\u7684\u4e8b\u590f\u514b\u91cc\u4e5f\u7ecf\u5386\u4e86\u4e0d\u5c11\u54e6\u3002",
    "\u2026\u2026\u73b0\u5728\u4e0d\u662f\u8bf4\u8fd9\u4e2a\u7684\u65f6\u5019\uff01",
    "\u590f\u514b\u91cc\u5c0f\u59d0\u3001\u739b\u7eee\u6735\u5c0f\u59d0\uff0c\u8bf7\u90fd\u5148\u51b7\u9759\u4e00\u70b9\u3002",
    "\u590f\u514b\u91cc\u73b0\u5728\u5f88\u51b7\u9759\u54e6\u3002|\u8bf4\u5230\u5e95\uff0c\u739b\u7eee\u6735\u5c31\u662f\u4e0d\u76f8\u4fe1\u590f\u514b\u91cc\u5427\uff1f",
    "\u6211\u6ca1\u6709\u2014\u2014",
    "\u5c31\u8fd9\u4e48\u4e0d\u613f\u610f\u627f\u8ba4\u590f\u514b\u91cc\u5417\uff1f\u660e\u660e\u521a\u624d\u8fd8\u662f\u9760\u590f\u514b\u91cc\u89e3\u51b3\u4e86\u5371\u673a\u5462\u3002",
    "\u2026\u2026",
    "\u554a\u2014\u2014\u6211\u5fcd\u8d85\u4e45\u4e86\uff0c\u739b\u7eee\u6735\u597d\u50cf\u89c9\u5f97\u53ea\u6709\u81ea\u5df1\u80fd\u6446\u5e73\u4e00\u5207\u5462\uff0c\u8fd9\u79cd\u81ea\u5c0a\u4e5f\u592a\u53ef\u7b11\u4e86\u5427\uff1f",
    "\u4e4b\u524d\u4e5f\u662f\uff0c\u660e\u660e\u5728\u65b9\u7cd6\u7684\u65f6\u5019\u5927\u5bb6\u5546\u91cf\u597d\u4e86\uff0c\u7b49\u5168\u90e8\u62b5\u8fbe\u540e\u518d\u5f00\u59cb\u52d8\u5bdf\uff0c\u739b\u7eee\u6735\u975e\u8981\u81ea\u5df1\u4e00\u4e2a\u4eba\u53bb\u3002\u73b0\u5728\u739b\u7eee\u6735\u7adf\u7136\u53c8\u60f3\u4e00\u4e2a\u4eba\u53bb\u5f53\u8bf1\u9975\u2026\u2026\u590f\u514b\u91cc\u5c31\u7b97\u518d\u6ca1\u6709\u6218\u6597\u7ecf\u9a8c\uff0c\u4e5f\u77e5\u9053\u6ca1\u6709\u4efb\u4f55\u63a5\u5e94\u3001\u5355\u72ec\u884c\u52a8\u7684\u8bf1\u9975\u5371\u9669\u6027\u9ad8\u5230\u79bb\u8c31\u5427\uff01",
    "\u8bf4\u7740\u7a0d\u6709\u95ea\u5931\u5c31\u4f1a\u5168\u76d8\u7686\u8f93\uff0c\u5230\u5e95\u662f\u8c01\u5728\u6a2a\u51b2\u76f4\u649e\u5440\uff01",
    "\u603b\u3001\u603b\u4f1a\u6709\u529e\u6cd5\u7684\uff0c\u4f60\u4eec\u548c\u6851\u6735\u83b1\u5e0c\u4efb\u610f\u4e00\u65b9\u5b8c\u6210\u4efb\u52a1\uff0c\u90fd\u53ef\u4ee5\u6765\u63a5\u5e94\u6211\u2026\u2026",
    "\u90a3\u6ca1\u5b8c\u6210\u5462\uff1f\u6ca1\u5b8c\u6210\u5c31\u53bb\u56de\u6536\u739b\u7eee\u6735\u90a3\u88ab\u6253\u70c2\u4e86\u7684\u7d20\u4f53\u5417\uff1f\uff01|\u603b\u662f\u4e00\u53e3\u56de\u7edd\u590f\u514b\u91cc\u7684\u60f3\u6cd5\u4e5f\u5c31\u7b97\u4e86\uff0c\u6bd5\u7adf\u590f\u514b\u91cc\u5728\u6218\u6597\u65b9\u9762\u662f\u5e2e\u4e0d\u4e0a\u4ec0\u4e48\u5fd9\uff0c\u4f46\u739b\u7eee\u6735\u73b0\u5728\u8fde\u5e97\u957f\u7684\u5173\u5fc3\u4e5f\u8981\u5047\u88c5\u770b\u4e0d\u5230\u5417\uff1f",
    "\u96be\u9053\u8bf4\uff0c\u739b\u7eee\u6735\u5c0f\u59d0\u5f88\u4eab\u53d7\u8fd9\u6837\u9ad8\u9ad8\u5728\u4e0a\u7684\u611f\u89c9\uff0c\u6240\u4ee5\u6839\u672c\u4e0d\u5728\u4e4e\u522b\u4eba\u7684\u60f3\u6cd5\uff1f",
    "\u6211\u6ca1\u6709\uff01\u4f60\u5728\u80e1\u8bf4\u4ec0\u4e48\u554a\uff1f\uff01",
    "\u90fd\u4e0d\u8981\u5435\u4e86\uff01",
    "\uff08\u6625\u7530\u5f80\u524d\u51e0\u6b65\uff0c\u7ad9\u5728\u4e86\u4e24\u4eba\u4e2d\u95f4\u3002\uff09",
    "\uff08\u770b\u5230\u6625\u7530\u96be\u5f97\u5f3a\u786c\u7684\u6837\u5b50\uff0c\u590f\u514b\u91cc\u762a\u7740\u5634\u6c89\u9ed8\u4e0b\u6765\uff0c\u739b\u7eee\u6735\u76b1\u7740\u7709\u79fb\u5f00\u76ee\u5149\uff0c\u90fd\u6ca1\u6709\u7ee7\u7eed\u4e89\u6267\u3002\uff09",
    "\u90fd\u51b7\u9759\u4e0b\u6765\u4e86\uff1f\u8010\u5fc3\u542c\u6211\u8bf4\u597d\u5417\uff1f",
    "\uff08\u6625\u7530\u8f7b\u8f7b\u5730\u53f9\u4e86\u4e00\u53e3\u6c14\uff0c\u4e0d\u514d\u60f3\u8d77\u4e86\u8fc7\u53bb\u5728\u683c\u91cc\u82ac\u7684\u65f6\u5019\u3002\uff09",
    "\uff08\u683c\u91cc\u82ac\u4e2d\u4eba\u5f62\u4f17\u591a\uff0c\u4e5f\u4e0d\u65f6\u4f1a\u6709\u8fd9\u6837\u7684\u6469\u64e6\u53d1\u751f\u3002\uff09",
    "\uff08\u90a3\u65f6\u5019\u7684\u4e89\u6267\uff0c\u5927\u90fd\u662f\u7531\u6307\u6325\u5b98\u4ece\u4e2d\u8c03\u89e3\uff0c\u5916\u51fa\u4f5c\u6218\u65f6\u7684\u5206\u914d\uff0c\u4e5f\u591a\u662f\u6307\u6325\u5b98\u628a\u63a7\u3002\uff09",
    "\uff08\u73b0\u5728\uff0c\u8fd9\u4efd\u804c\u8d23\u6765\u5230\u4e86\u81ea\u5df1\u8eab\u4e0a\u3002\uff09",
    "\u590f\u514b\u91cc\uff0c\u4f60\u8bef\u4f1a\u739b\u7eee\u6735\u4e86\uff0c\u5979\u53ea\u662f\u62c5\u5fc3\u5927\u5bb6\u7684\u5b89\u5168\uff0c\u8fd9\u4e2a\u201c\u5927\u5bb6\u201d\u91cc\uff0c\u4e5f\u5305\u62ec\u4f60\u3002|\u4f46\u662f\u739b\u7eee\u6735\uff0c\u590f\u514b\u91cc\u8bf4\u7684\u4e5f\u6709\u9053\u7406\uff0c\u6211\u4eec\u65e2\u7136\u6765\u4e864\u4e2a\u4eba\uff0c\u5c31\u8981\u57fa\u4e8e\u5f53\u524d\u6210\u5458\uff0c\u9009\u62e9\u6700\u4f18\u89e3\u53bb\u5b8c\u6210\u4efb\u52a1\u3002",
    "\u6240\u4ee5\uff0c\u6211\u548c\u739b\u7eee\u6735\u4e00\u7ec4\uff0c\u8d1f\u8d23\u652f\u63f4\uff0c\u4fdd\u8bc1\u5438\u5f15\u5168\u90e8\u706b\u529b\uff0c\u6851\u6735\u83b1\u5e0c\u524d\u5f80\u63a7\u5236\u5ba4\uff0c\u590f\u514b\u91cc\u5148\u5bfb\u627e\u9690\u853d\u5904\u5f85\u673a\uff0c\u7b49\u6211\u4eec\u5c06\u654c\u4eba\u5f15\u8d70\u540e\u518d\u4f3a\u673a\u5236\u9020\u5927\u52a8\u9759\uff0c\u5f15\u8d77\u6cbb\u5b89\u90e8\u95e8\u6ce8\u610f\u3002",
    "\u2026\u2026",
    "\u4e0d\u8bf4\u8bdd\u6211\u5c31\u5f53\u4f60\u7b54\u5e94\u4e86\u54e6\uff0c\u590f\u514b\u91cc\u5462\uff1f",
    "\u2026\u2026\u5e97\u957f\u7684\u8bdd\uff0c\u590f\u514b\u91cc\u5f53\u7136\u4f1a\u542c\u5566\u2665",
    "\u62b1\u6b49\u54e6\uff0c\u590f\u514b\u91cc\u521a\u624d\u592a\u7740\u6025\u4e86\u5462\uff0c\u628a\u6c14\u6c1b\u90fd\u641e\u50f5\u5566\u2026\u2026\u8c01\u8ba9\u739b\u7eee\u6735\u8fd9\u4e2a\u4e0d\u884c\u90a3\u4e2a\u4e0d\u8ba9\u7684\u2026\u2026\u660e\u660e\u4eba\u5bb6\u60f3\u597d\u597d\u627f\u62c5\u8d77\u8d23\u4efb\u7684\u545c\u545c\u545c\u2026\u2026",
    "\u54d4\u54d4\u54d4\u2026\u2026",
    "\u55ef\uff0c\u8fd8\u6709\u5c0f\u54d4\u966a\u7740\u590f\u514b\u91cc\u5462\u3002",
    "\u54d4\u2014\u2014\u6ecb\u2014\u2014",
    "\u5c0f\u54d4\u7684\u58f0\u97f3\u600e\u4e48\u53d8\u5f97\u8fd9\u4e48\u96be\u542c\u4e86\uff1f",
    "\u4e0d\u662f\u5c0f\u54d4\u7684\u58f0\u97f3\uff01",
    "\u6b38\uff1f",
    "\uff08\u623f\u9876\u4e0a\u7684\u6269\u97f3\u5668\u4f20\u51fa\u5de8\u5927\u7684\u566a\u97f3\uff0c\u523a\u8033\u7684\u58f0\u6d6a\u8ba9\u91d1\u5c5e\u7684\u5899\u58c1\u4f3c\u4e4e\u4e5f\u55e1\u9e23\u8d77\u6765\uff0c\u51e0\u4eba\u90fd\u62ac\u624b\u6342\u4f4f\u4e86\u8033\u6735\u3002\uff09",
    "\u5582\u5582\u5582\u8fd9\u662f\u600e\u4e48\u56de\u4e8b\uff1f\u521a\u624d\u662f\u4e00\u4e2a\u8fd9\u4f1a\u6709\u56db\u4e2a\u4e86\uff1f",
    "\u2026\u2026\u8c01\uff1f",
    "\u662f\u4f5b\u5c14\u5361\u4ec0\uff01",
    "\u628a\u6240\u6709\u673a\u68b0\u5168\u90e8\u542f\u52a8\uff01\u6240\u6709\u4eba\uff0c\u5230\u5e95\u5c42\u4ed3\u5e93\u96c6\u5408\uff01",
    "\u5567\uff0c\u66f4\u9ebb\u70e6\u4e86\u2026\u2026",
    "\u600e\u3001\u600e\u4e48\u6709\u8fd9\u4e48\u591a\u673a\u68b0\u554a\uff1f\uff01",
    "\u53ea\u80fd\u8fce\u6218\u4e86\u2026\u2026\u53ef\u4ee5\u5417\uff1f",
    "\u5f53\u7136\u3002\u5f39\u836f\u65e9\u5c31\u8865\u5145\u597d\u4e86\u3002",
    "\u591a\u4e8f\u4e86\u590f\u514b\u91cc\uff0c\u5bf9\u5427~\uff1f"
  ]
]