[
  [
    "\uff08\u4eca\u5929\u7684CHE-02\u4e0e\u5f80\u65e5\u4e0d\u540c\uff0c\u4e3a\u4e86\u5728\u7d27\u6025\u7ba1\u5236\u524d\u91c7\u8d2d\u597d\u7269\u54c1\uff0c\u4e34\u8fd1\u508d\u665a\u65f6\u5206\uff0c\u8857\u9053\u4e0a\u5c31\u6709\u4e86\u4e0d\u5c11\u884c\u4eba\u3002\uff09",
    "\uff08\u8282\u65e5\u5c06\u8fd1\uff0c\u4e00\u4e9b\u5e97\u94fa\u5df2\u7ecf\u5f00\u59cb\u88c5\u626e\u4e86\u8d77\u6765\uff0c\u5728\u5e97\u95e8\u53e3\uff0c\u4e5f\u51fa\u73b0\u4e86\u4e00\u4e9b\u7a7f\u7740\u73a9\u5076\u670d\u7684\u4eba\u3002\uff09",
    "\uff08\u8857\u8fb9\u4e00\u89d2\uff0c\u6709\u4e00\u4e2a\u7a7f\u7740\u5c0f\u718a\u73a9\u5076\u670d\u7684\u3002\u90a3\u91cc\u9762\u7684\u4eba\u4f3c\u4e4e\u8fd8\u4e0d\u592a\u4e60\u60ef\u8fd9\u5957\u8863\u670d\uff0c\u540c\u624b\u540c\u811a\u5730\u8d70\u5728\u8def\u4e0a\u2014\u2014\uff09",
    "\u2026\u2026\uff01",
    "\uff08\u201c\u549a\u201d\u7684\u4e00\u58f0\uff0c\u5c0f\u718a\u505c\u4e0b\u4e86\u811a\u6b65\uff0c\u50f5\u786c\u5730\u4f4e\u5934\u770b\u5411\u81ea\u5df1\u7684\u811a\u8fb9\u3002\uff09",
    "\u2014\u2014\u545c\uff01\u597d\u75db\u554a\u2026\u2026",
    "\uff08\u649e\u5728\u718a\u817f\u4e0a\u7684\u5c0f\u5b69\u6454\u4e86\u4e2a\u5c41\u80a1\u58a9\u3002\u5c0f\u5b69\u62ac\u8d77\u5934\uff0c\u9762\u524d\u7684\u73a9\u5076\u718a\u50f5\u786c\u5730\u4f38\u51fa\u624b\u2014\u2014\uff09",
    "\u545c\u2026\u2026\u718a\u718a\u2026\u2026\u597d\u53ef\u6015\u2026\u2026\u545c\u54c7\u2014\u2014\uff01",
    "\u554a\uff0c\u771f\u662f\u7684\u2026\u2026\u6211\u4e0d\u662f\u8bf4\u4e86\u8981\u8d76\u7d27\u53bb\u91c7\u8d2d\u7269\u8d44\u4e0d\u8981\u4e71\u8dd1\u5417\uff1f|\u5b9e\u5728\u5bf9\u4e0d\u8d77\uff0c\u7ed9\u60a8\u6dfb\u9ebb\u70e6\u4e86\u3002",
    "\uff08\u9762\u5bf9\u8868\u8fbe\u6b49\u610f\u7684\u6bcd\u4eb2\uff0c\u5c0f\u718a\u5982\u4e34\u5927\u654c\u5730\u5f80\u540e\u9000\u4e86\u51e0\u6b65\uff0c\u53cc\u624b\u5728\u80f8\u524d\u5938\u5f20\u5730\u6325\u821e\u4e86\u51e0\u4e0b\uff0c\u731b\u5730\u8f6c\u8fc7\u8eab\u8dd1\u8d70\u4e86\u3002\uff09",
    "\u545c\u54c7\u2014\u2014\u574f\u718a\u718a\u8dd1\u6389\u4e86\u2014\u2014\u5988\u5988\u2014\u2014",
    "\u8fd9\u91cc\u2026\u2026\u65b9\u5411\u6b63\u786e\u2026\u2026|\u554a\u2014\u2014\u6211\u4e3a\u4ec0\u4e48\u8981\u7a7f\u8fd9\u7834\u4e1c\u897f\u554a\uff01",
    "\uff08\u534e\u5e1d\u6e2f\u9644\u8fd1\u7684\u67d0\u6761\u8857\u9053\u4e0a\u2014\u2014\uff09",
    "\uff08\u5c0f\u718a\u5728\u4e00\u5904\u884c\u4eba\u7a00\u5c11\u7684\u5efa\u7b51\u7269\u9644\u8fd1\u505c\u4e86\u4e0b\u6765\uff0c\u73a9\u5076\u670d\u91cc\u7684\u4eba\u9760\u5728\u5899\u89d2\uff0c\u4e00\u628a\u6458\u6389\u4e86\u81ea\u5df1\u7684\u5934\u5957\u3002\uff09",
    "\u771f\u662f\u70e6\u6b7b\u4e86\uff01",
    "\u6b38\u2014\u2014\u53ef\u662f\u739b\u7eee\u6735\u7a7f\u4e0a\u540e\uff0c\u660e\u660e\u53d8\u5f97\u53d7\u5c0f\u5b69\u5b50\u6b22\u8fce\u4e86\uff1f",
    "\u90a3\u53eb\u6b22\u8fce\u5417\uff1f\uff01\u4ed6\u90fd\u54ed\u4e86\u597d\u5427\u2026\u2026\u800c\u4e14\u3001\u6211\u3001\u6211\u53c8\u4e0d\u9700\u8981\u53d7\u5c0f\u5b69\u5b50\u6b22\u8fce\uff01",
    "\u8863\u670d\u8fd8\u662f\u8981\u7a7f\u597d\u624d\u884c\u5566\uff0c\u739b\u7eee\u6735\u5feb\u628a\u5934\u5957\u6234\u4e0a\uff0c\u5feb\u6234\u4e0a\u561b~",
    "\u5567\uff0c\u8fd9\u4e1c\u897f\u53c8\u6ca1\u7528\u53c8\u5f71\u54cd\u89c6\u91ce\uff01\u5f53\u521d\u5c31\u4e0d\u8be5\u7b54\u5e94\u7a7f\uff0c\u8822\u6b7b\u4e86\uff01",
    "\u771f\u662f\u7684\uff0c\u8981\u4e0d\u662f\u718a\u5148\u751f\u5e2e\u5fd9\u6321\u4f4f\u8138\uff0c\u7b28\u624b\u7b28\u811a\u7684\u739b\u7eee\u6735\u65e9\u5c31\u88ab\u76d1\u63a7\u62cd\u5f97\u4e00\u6e05\u4e8c\u695a\u4e86\uff0c\u73b0\u5728\u5374\u8bf4\u4eba\u5bb6\u6ca1\u7528\uff0c\u771f\u662f\u65e0\u60c5\u554a\u545c\u545c\u545c~",
    "\u590f\u514b\u91cc\uff0c\u4e0d\u8981\u95f9\u4e86\u3002|\u739b\u7eee\u6735\uff0c\u4f60\u90a3\u8fb9\u60c5\u51b5\u600e\u4e48\u6837\uff1f",
    "\u2026\u2026\u54fc\uff0c\u5df2\u7ecf\u62b5\u8fbe\u5b89\u5168\u63a7\u5236\u5ba4\u4e86\u3002\u6b63\u5728\u89e3\u9501\uff0c\u9a6c\u4e0a\u5c31\u80fd\u6253\u5f00\u3002",
    "\u597d\uff0c\u4f60\u6ce8\u610f\u5b89\u5168\uff0c\u63a7\u5236\u5ba4\u5185\u53ef\u80fd\u6709\u76d1\u63a7\u6216\u5b89\u5168\u8b66\u62a5\u88c5\u7f6e\u2026\u2026",
    "\u6211\u77e5\u9053\u6211\u77e5\u9053\uff0c\u522b\u62c5\u5fc3\u4e86\u3002",
    "\u597d\u4e86\uff0c\u95e8\u5f00\u4e86\uff0c\u7b49\u6211\u6d88\u606f\u3002",
    "\u55ef\u3002",
    "\uff08\u8c28\u614e\u5730\u786e\u8ba4\u4e86\u4e00\u4e0b\u5468\u56f4\u7684\u73af\u5883\uff0c\u739b\u7eee\u6735\u548c\u624b\u4e2d\u7684\u5c0f\u718a\u5934\u5957\u5bf9\u89c6\u4e86\u4e24\u79d2\uff0c\u8fd8\u662f\u4e0d\u60c5\u4e0d\u613f\u5730\u5c06\u5934\u5957\u6234\u4e0a\u3002\uff09",
    "\uff08\u968f\u540e\uff0c\u5979\u8d70\u5165\u4e86\u5b89\u5168\u63a7\u5236\u5ba4\u3002\uff09"
  ],
  [
    "\uff08\u660f\u6697\u7684\u611f\u5e94\u706f\u4eae\u8d77\u6765\uff0c\u739b\u7eee\u6735\u8270\u96be\u5730\u901a\u8fc7\u5934\u5957\u4e0a\u7684\u773c\u775b\u6d1e\u5411\u5916\u5f20\u671b\uff0c\u4e00\u773c\u770b\u5230\u5899\u4e0a\u7684\u6e05\u6d01\u8bb0\u5f55\u2014\u2014\uff09",
    "\uff08\u8ddd\u79bb\u4e0a\u4e00\u6b21\u6709\u4eba\u6765\u6253\u626b\uff0c\u5df2\u7ecf\u662f\u5c06\u8fd1\u4e24\u5e74\u524d\u4e86\u3002\uff09",
    "\u4e24\u5e74\u6ca1\u4eba\u8fdb\u6765\u8fc7\u4e86\u554a\u2026\u2026\u6211\u770b\u770b\uff0c\u8fd9\u4e2a\u662f\u63a7\u5236\u53f0\uff0c\u76d1\u63a7\u7684\u6743\u9650\u5728\u2026\u2026\u5567\uff0c\u8fd9\u50bb\u5934\u5957\uff0c\u773c\u775b\u5c31\u4e0d\u80fd\u5f00\u5927\u4e00\u70b9\u5417\uff01",
    "\u627e\u5230\u4e86\uff0c\u63a5\u4e0b\u6765\u2026\u2026\u54fc\uff0c\u83b1\u5a05\u7684\u9ed1\u5ba2\u7a0b\u5e8f\u6700\u597d\u6709\u7528\u3002",
    "\u54ce\uff1f\u600e\u4e48\u8fd8\u9700\u8981\u8f85\u52a9\u64cd\u4f5c\u7684\u2026\u2026\u554a\uff0c\u771f\u662f\u9ebb\u70e6\u6b7b\u4e86\uff01\u5feb\u70b9\u628a\u8fd9\u4e00\u5207\u7ed3\u675f\u5427\uff01"
  ]
]