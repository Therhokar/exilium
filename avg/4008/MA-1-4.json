[
  [
    "\uff08\u6210\u529f\u53d6\u5f97\u76d1\u63a7\u7684\u63a7\u5236\u6743\u9650\u540e\uff0c\u739b\u7eee\u6735\u677e\u4e86\u53e3\u6c14\u3002\uff09",
    "\uff08\u5979\u4e00\u8fb9\u63a5\u901a\u6625\u7530\u7684\u8054\u7edc\uff0c\u4e00\u8fb9\u8bd5\u56fe\u628a\u81ea\u5df1\u4ece\u73a9\u5076\u670d\u91cc\u5265\u51fa\u6765\u3002\uff09",
    "\u6625\u7530\uff0c\u6211\u5df2\u7ecf\u5207\u65ad\u6e2f\u53e3\u533a\u76d1\u63a7\u548c\u6cbb\u5b89\u90e8\u95e8\u670d\u52a1\u5668\u4e4b\u95f4\u7684\u8054\u7cfb\u4e86\u3002",
    "\u8f9b\u82e6\u4e86\uff0c\u6ca1\u53d1\u751f\u4ec0\u4e48\u610f\u5916\u5427\uff1f",
    "\u6ca1\u4ec0\u4e48\u3002\u54fc\uff0c\u90a3\u4e2a\u83b1\u5a05\u63d0\u4f9b\u7684\u9ed1\u5ba2\u7a0b\u5e8f\u6ca1\u4ec0\u4e48\u7528\u554a\uff0c\u7adf\u7136\u8fd8\u9700\u8981\u6211\u4ece\u4e8c\u7ea7\u5e73\u5c42\u8f85\u52a9\u653b\u51fb\u3002",
    "\u55ef\u2026\u2026\u53ef\u80fd\u662f\u56e0\u4e3a\u6e2f\u53e3\u533a\u7684\u8bbe\u5907\uff0c\u666e\u904d\u6bd4\u8f83\u8001\u65e7\uff0c\u4f1a\u6709\u4e0d\u517c\u5bb9\u7684\u95ee\u9898\u5427\u3002",
    "\u8c01\u77e5\u9053\u5462\u3002|\u6211\u6b63\u5728\u7528\u9884\u8bbe\u753b\u9762\u8986\u76d6\u6e2f\u53e3\u533a\u76d1\u63a7\uff0c\u5efa\u8bae\u4f60\u4eec45\u5206\u949f\u540e\u51fa\u53d1\u3002",
    "\u4e86\u89e3\u3002|\u590f\u514b\u91cc\uff0c\u9ebb\u70e6\u4f60\u505a\u4e00\u4e0b\u6700\u7ec8\u68c0\u67e5\uff0c\u4e0d\u8981\u6709\u758f\u6f0f\u54e6\u3002",
    "\u597d\u54e6~\u4e0d\u8fc7\u90fd\u662f\u590f\u514b\u91cc\u51c6\u5907\u7684\u4e1c\u897f\uff0c\u5f88\u96be\u60f3\u8c61\u4f1a\u6709\u758f\u6f0f\u5462\u2665",
    "\u5c31\u662f\u8fd9\u6837\u624d\u4ee4\u4eba\u62c5\u5fc3\u5427\uff0c\u4f60\u53ef\u522b\u7a7f\u5f97\u82b1\u91cc\u80e1\u54e8\u51fa\u6765\uff0c\u542c\u5230\u6ca1\uff1f",
    "\u739b\u7eee\u6735\u8fd8\u662f\u5148\u62c5\u00b7\u5fc3\u00b7\u81ea\u00b7\u5df1\u5427\uff0c\u90a3\u4e2a\u5c0f\u718a\u7684\u773c\u775b\u5f00\u5f97\u6709\u70b9\u5c0f\u8036\uff0c\u4f46\u739b\u7eee\u6735\u7684\u8bdd\u80af\u5b9a\u6ca1\u95ee\u9898\u5427\uff0c\u563b\u563b\uff01",
    "\u6211\u5df2\u7ecf\u8131\u6389\u4e86\uff01\u4f60\u662f\u6545\u610f\u7ed9\u6211\u627e\u9ebb\u70e6\u7684\uff1f\uff01",
    "\u624d\u6ca1\u6709~\u90a3\u5957\u8863\u670d\u53c8\u4e0d\u662f\u590f\u514b\u91cc\u505a\u7684\u2665",
    "\u6700\u8fd1\u7684\u76d1\u63a7\u8bb0\u5f55\uff0c\u6709\u4ec0\u4e48\u5f02\u5e38\u5417\uff1f",
    "\u6ca1\u6709\uff0c\u53ea\u770b\u5230\u6cbb\u5b89\u961f\u7684\u641c\u67e5\u673a\u5668\u4eba\u7684\u786e\u6765\u8fc7\u8fd9\u91cc\uff0c\u4f46\u4f3c\u4e4e\u6ca1\u6709\u53d1\u73b0\u4ec0\u4e48\uff0c\u5f88\u5feb\u5c31\u8fd4\u56de\u4e86\u3002",
    "\u679c\u7136\u2026\u2026",
    "\u6211\u53bb\u627e\u4e00\u4e2a\u5408\u9002\u7684\u4ed3\u5e93\u653e\u7f6e\u519b\u7528\u6b66\u5668\u3002|\u786e\u5b9a\u540e\u540c\u6b65\u5750\u6807\u3002",
    "\u597d\uff0c\u4f60\u6ce8\u610f\u5b89\u5168\u3002",
    "\u77e5\u9053\u4e86\uff0c\u4f60\u4e5f\u592a\u62c5\u5fc3\u8fc7\u5ea6\u4e86\u5427\u3002",
    "\u563b\u563b\uff0c\u867d\u7136\u739b\u7eee\u6735\u8fd9\u6837\u8bf4\uff0c\u4f46\u73b0\u5728\u80af\u5b9a\u5728\u68c0\u67e5\u76d1\u63a7\u662f\u4e0d\u662f\u5df2\u7ecf\u88ab\u5b8c\u5168\u8986\u76d6\u4e86\u5427\uff1f",
    "\u8fd9\u3001\u8fd9\u53eb\u8c28\u614e\uff01\u83dc\u9e1f\u5c31\u7ed9\u6211\u597d\u597d\u5b66\u7740\u70b9\uff01|\u597d\u4e86\u522b\u5e72\u6270\u6211\uff01",
    "\u597d\u2026\u2026\u8fd9\u6837\u7684\u8bdd\uff0c\u5c31\u7b97\u590f\u514b\u91cc\u90a3\u5bb6\u4f19\u5728\u8fd9\u91cc\u5f00\u6f14\u5531\u4f1a\uff0c\u6cbb\u5b89\u90e8\u95e8\u4e5f\u4e0d\u4f1a\u5bdf\u89c9\u3002",
    "\uff08\u786e\u8ba4\u5b8c\u6bd5\uff0c\u739b\u7eee\u6735\u8e72\u4e86\u4e0b\u6765\uff0c\u4ece\u5730\u4e0a\u7684\u5c0f\u718a\u73a9\u5076\u670d\u809a\u5b50\u91cc\u6478\u51fa\u81ea\u5df1\u7684\u7434\u7bb1\u3002\uff09",
    "\uff08\u72b9\u8c6b\u4e86\u4e24\u79d2\uff0c\u5979\u8fd8\u662f\u62b1\u8d77\u4e86\u5c0f\u718a\u73a9\u5076\u670d\uff0c\u79bb\u5f00\u4e86\u63a7\u5236\u5ba4\u3002\uff09"
  ],
  [
    "\u2026\u2026\u5bf9\u4e8e\u5df2\u7ecf\u5e9f\u5f03\u7684\u533a\u57df\u6765\u8bf4\uff0c\u5de1\u903b\u673a\u68b0\u592a\u591a\u4e86\u2026\u2026\u578b\u53f7\u5012\u662f\u5f88\u666e\u901a\u3002",
    "\u591c\u89c6\u4eea\u6ca1\u6709\u53d1\u73b0\u53ef\u7591\u70ed\u6e90\uff0c\u5c31\u8fd9\u91cc\u5427\u3002|\u55ef\u2026\u2026\u88c5\u8f7d\u9ed1\u5ba2\u7a0b\u5e8f\u7684\u82af\u7247\u2026\u2026",
    "\uff08\u628a\u82af\u7247\u63d2\u5165\u63a5\u53e3\u540e\uff0c\u9ed1\u5ba2\u7a0b\u5e8f\u8fc5\u901f\u5f00\u59cb\u8fd0\u8f6c\u3002\uff09",
    "\uff08\u8d81\u8fd9\u671f\u95f4\uff0c\u739b\u7eee\u6735\u628a\u7d2f\u8d58\u7684\u73a9\u5076\u670d\u5f80\u4e0a\u62ce\u4e86\u62ce\u3002\uff09",
    "\uff08\u4e00\u4e2a\u4eba\u884c\u52a8\u56fa\u7136\u81ea\u7531\u5f88\u591a\uff0c\u4f46\u8fd9\u79cd\u65f6\u5019\uff0c\u8fde\u4e2a\u642d\u628a\u624b\u7684\u4eba\u90fd\u6ca1\u6709\u3002\uff09",
    "\uff08\u8981\u662f\u90a3\u5bb6\u4f19\u5728\u5c31\u597d\u4e86\u2026\u2026\u8fd9\u5957\u73a9\u5076\u670d\u5c3a\u5bf8\u591f\u5927\uff0c\u90a3\u5bb6\u4f19\u5e94\u8be5\u4e5f\u80fd\u7a7f\u5f97\u4e0a\u5427\uff1f\uff09",
    "\uff08\u4e0d\u8fc7\u90a3\u5bb6\u4f19\u7b28\u624b\u7b28\u811a\u7684\uff0c\u8eab\u4e0a\u5957\u7740\u8fd9\u79cd\u4e1c\u897f\uff0c\u884c\u52a8\u80af\u5b9a\u4e0d\u4f1a\u50cf\u81ea\u5df1\u90a3\u4e48\u7075\u6d3b\u3002\uff09",
    "\uff08\u60f3\u5230\u90a3\u4e2a\u4eba\u7a7f\u4e0a\u73a9\u5076\u670d\u540e\u4e0d\u4f1a\u8d70\u8def\u7684\u6837\u5b50\uff0c\u739b\u7eee\u6735\u4e0d\u81ea\u89c9\u5730\u9732\u51fa\u4e86\u4e00\u70b9\u7b11\u610f\u3002\uff09",
    "\uff08\u4f46\u4e0b\u4e00\u79d2\u2014\u2014\uff09",
    "\u2026\u2026\u65e0\u6cd5\u89e3\u6790\uff1f\u600e\u4e48\u56de\u4e8b\uff1f|\u8fd9\u7535\u5b50\u9501\u662f\u591a\u5c11\u5e74\u524d\u7684\u8001\u53e4\u8463\u554a\uff1f\u8fd9\u4e2a\u7248\u672c\u53f7\u2026\u2026\u771f\u662f\u7684\uff0c\u8fd9\u91cc\u7684\u4eba\u90fd\u4e0d\u77e5\u9053\u8bbe\u7f6e\u81ea\u52a8\u5347\u7ea7\u5417\uff01",
    "\u2026\u2026\u53ea\u80fd\u624b\u52a8\u8fde\u63a5\u670d\u52a1\u5668\uff0c\u4ece\u4e8c\u7ea7\u5e73\u5c42\u60f3\u60f3\u529e\u6cd5\u4e86\u3002|\u54fc\uff0c\u8fd9\u79cd\u7834\u70c2\u7cfb\u7edf\uff0c\u9632\u5fa1\u60f3\u5fc5\u4e5f\u5f88\u5f31\u5427\u3002"
  ],
  [
    "\u2026\u2026\u786e\u8ba4\u65e0\u5f02\u5e38\u3002",
    "\u6625\u7530\uff0c\u4ed3\u5e93\u7684\u5750\u6807\u53d1\u9001\u7ed9\u4f60\u4e86\u3002",
    "\u6536\u5230\u3002",
    "\u4f60\u4eec\u628a\u4e1c\u897f\u5b89\u5168\u9001\u5230\u6307\u5b9a\u5750\u6807\u7684\u4ed3\u5e93\u91cc\u5c31\u884c\u3002\u8bb0\u5f97\u6309\u7167\u6211\u53d1\u51fa\u7684\u8def\u7ebf\u524d\u8fdb\uff0c\u80fd\u591f\u907f\u5f00\u5927\u90e8\u5206\u76d1\u63a7\u3002|\u987a\u4fbf\u95ee\u4e00\u53e5\uff0c\u4f60\u4eec\u6253\u626e\u6b63\u5e38\u5427\uff1f",
    "\u662f\u662f\u662f\uff0c\u590f\u514b\u91cc\u548c\u5e97\u957f\u90fd\u5316\u5986\u6210\u4e86\u6b63\u5e38\u7684\u4eba\u7c7b\uff0c\u6b63\u5728\u7a7f\u7740\u6b63\u5e38\u7684\u8863\u670d\uff0c\u5728\u6b63\u5e38\u5730\u905b\u72d7\u54e6\u3002",
    "\u905b\u72d7\uff1f\u4ec0\u4e48\u72d7\uff1f",
    "\u201c\u5c0f\u54d4\u201d\uff0c\u739b\u7eee\u6735\u89c1\u8fc7\u7684\u3002",
    "\u90a3\u4e2a\u808c\u8089\u5175\u8681\uff1f\u4f60\u4eec\u628a\u5b83\u542f\u52a8\u4e86\uff1f\uff01",
    "\u5c0f\u54d4\u4e5f\u662f\u8fd9\u6279\u519b\u7528\u6b66\u5668\u7684\u4e00\u90e8\u5206\u554a\uff0c\u5f53\u7136\u4e5f\u8981\u5e26\u8fc7\u53bb\uff0c\u800c\u4e14\u590f\u514b\u91cc\u6709\u597d\u597d\u628a\u5b83\u6253\u626e\u6210\u5ba0\u7269\u5175\u8681\u7684\u6837\u5b50\u54e6\uff0c\u72ee\u5b50\u8033\u6735\u548c\u9b03\u6bdb\u914d\u9970\u6d3e\u4e0a\u7528\u573a\u4e86\u5462\u2665",
    "\u739b\u7eee\u6735\uff0c\u592a\u6ca1\u60f3\u8c61\u529b\u5566~",
    "\u60f3\u8c61\u529b\u5c31\u662f\u7275\u7740\u4e00\u4e2a\u6d51\u8eab\u70ae\u7ba1\u7684\u5175\u5668\u5728\u8857\u4e0a\u8d70\uff1f\uff01\u554a\u2014\u2014\u65e9\u77e5\u9053\u5c31\u5e94\u8be5\u6211\u81ea\u5df1\u591a\u8dd1\u4e24\u8d9f\u7684\u2026\u2026",
    "\u4f60\u653e\u5fc3\uff0c\u6211\u5df2\u7ecf\u5173\u95ed\u4e86\u5b83\u7684\u81ea\u52a8\u7d22\u654c\u548c\u653b\u51fb\u5f00\u5173\uff0c\u6ca1\u6709\u53e3\u4ee4\uff0c\u5b83\u4e0d\u4f1a\u4e3b\u52a8\u5f00\u706b\u3002",
    "\u739b\u7eee\u6735\u5c31\u662f\u4e0d\u76f8\u4fe1\u590f\u514b\u91cc\u561b\u3002|\u800c\u4e14\uff0c\u4e07\u4e00\u9047\u5230\u4ec0\u4e48\u9ebb\u70e6\uff0c\u5c0f\u54d4\u4e5f\u53ef\u4ee5\u5e2e\u5fd9\uff0c\u8bf4\u4e0d\u5b9a\u6bd4\u739b\u7eee\u6735\u8fd8\u8981\u53ef\u9760\u5462~",
    "\u2026\u2026\u54fc\uff0c\u53cd\u6b63\u5982\u679c\u51fa\u4ec0\u4e48\u610f\u5916\u522b\u6307\u671b\u6211\u53bb\u6551\u4f60\u4eec\u3002|\u5230\u4e86\u5c31\u5728\u8fd9\u95f4\u5b89\u5168\u7684\u4ed3\u5e93\u91cc\u7b49\u6211\uff0c\u522b\u81ea\u5df1\u4e71\u8dd1\uff0c\u6211\u8981\u5148\u53bb\u9644\u8fd1\u7684\u4ed3\u5e93\u68c0\u67e5\u4e00\u4e0b\u3002",
    "\u6b38\uff1f|\u8fd8\u662f\u7b49\u6211\u4eec\u5230\u4e86\u518d\u2014\u2014",
    "\u6211\u81ea\u5df1\u6ca1\u95ee\u9898\u7684\uff0c\u5148\u8fd9\u6837\u3002",
    "\u597d\u3002\u90a3\u4e48\uff0c\u6700\u53ef\u7591\u7684\u533a\u57df\u662f\u2026\u2026"
  ],
  [
    "\u5509\u2026\u2026",
    "\u4ec0\u4e48\u561b\uff0c\u7adf\u7136\u4e0d\u7b49\u590f\u514b\u91cc\u5c31\u81ea\u5df1\u884c\u52a8\u4e86\uff0c\u6211\u4eec\u4e4b\u524d\u4e0d\u662f\u8bf4\u597d\u4e86\u8981\u4e00\u8d77\u5417\uff1f",
    "\uff08\u6625\u7530\u548c\u590f\u514b\u91cc\u63a8\u7740\u5c0f\u63a8\u8f66\u8d70\u5728\u591c\u665a\u7a0d\u663e\u70ed\u95f9\u7684\u536b\u661f\u57ce\u8857\u5934\u3002\uff09",
    "\uff08\u5c0f\u63a8\u8f66\u4e0a\u6574\u9f50\u7801\u7740\u4e00\u4e9b\u7bb1\u5b50\uff0c\u770b\u4e0a\u53bb\u548c\u51fa\u95e8\u91c7\u8d2d\u7684\u5c45\u6c11\u6ca1\u6709\u4efb\u4f55\u533a\u522b\u3002\uff09",
    "\u739b\u7eee\u6735\u66f4\u4e60\u60ef\u4e8e\u5355\u72ec\u6267\u884c\u4efb\u52a1\uff0c\u90a3\u4e00\u5e26\u7684\u8b66\u5907\u529b\u91cf\u677e\u61c8\uff0c\u5979\u81ea\u5df1\u5e94\u8be5\u4e5f\u6ca1\u95ee\u9898\u2026\u2026\u5c31\u968f\u5979\u53bb\u5427\u3002",
    "\uff08\u867d\u7136\u5bf9\u739b\u7eee\u6735\u6709\u6240\u62c5\u5fe7\uff0c\u4f46\u6625\u7530\u4e5f\u5b8c\u5168\u660e\u767d\u739b\u7eee\u6735\u7684\u5fc3\u60c5\u3002\uff09",
    "\uff08\u56e0\u65e0\u6cd5\u548c\u601d\u5ff5\u7684\u4eba\u89c1\u9762\u800c\u7126\u707c\u4e0d\u5b89\uff0c\u5728\u6f2b\u957f\u7684\u65f6\u5149\u4e2d\uff0c\u9010\u6e10\u4e60\u60ef\u4e8e\u72ec\u6765\u72ec\u5f80\uff0c\u4e0d\u613f\u610f\u4f9d\u9760\u4ed6\u4eba\u3002\uff09",
    "\uff08\u8fd9\u4e00\u70b9\uff0c\u81ea\u5df1\u4e5f\u662f\u4e00\u6837\u7684\u3002\uff09",
    "\uff08\u597d\u5728\u65b9\u7cd6\u5496\u5561\u9986\u8d70\u4e0a\u6b63\u8f68\u540e\uff0c\u5df2\u7ecf\u901a\u8fc7\u4e00\u4e9b\u6e20\u9053\uff0c\u627e\u5230\u4e86\u548c\u827e\u83ab\u53f7\u95f4\u63a5\u4ea4\u6d41\u7684\u529e\u6cd5\u3002\uff09",
    "\uff08\u4e4b\u540e\u53ea\u8981\u7ef4\u62a4\u597d\u53cc\u65b9\u7684\u6c9f\u901a\uff0c\u5496\u5561\u9986\u7684\u6210\u5458\uff0c\u60f3\u5fc5\u5f88\u5feb\u5c31\u80fd\u4e0e\u6307\u6325\u5b98\u76f8\u89c1\u4e86\u3002\uff09",
    "\u54fc\uff0c\u5e97\u957f\u603b\u8fd9\u4e48\u60ef\u7740\u5979\uff0c\u8fdf\u65e9\u4f1a\u78b0\u4e0a\u5371\u9669\u60c5\u51b5\u7684\u54e6\u3002",
