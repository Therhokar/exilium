    "\u600e\u4e48\u4e86\uff1f\u8fd9\u4e48\u7740\u6025\u3002",
    "\u55ef\u2026\u2026\u5176\u5b9e\uff0c\u6709\u4e00\u4e2a\u5728\u57fa\u8f85\u5468\u8fb9\u536b\u661f\u57ce\u6d3b\u52a8\u7684\u901a\u7f09\u72af\uff0c\u6700\u8fd1\u53ef\u80fd\u53bb\u4e86\u5207\u5c14\u5361\u745f\u3002\u5f7c\u5f97\u514b\u00b7\u4f5b\u5c14\u5361\u4ec0\uff0c\u4e3b\u8981\u4ece\u4e8b\u975e\u6cd5\u83b7\u53d6\u519b\u7528\u6b66\u5668\u5e76\u8fdb\u884c\u6539\u9020\u548c\u51fa\u552e\u7b49\u72af\u7f6a\u6d3b\u52a8\u3002",
    "\u6539\u9020\u519b\u7528\u6b66\u5668\u2026\u2026\uff01",
    "\u6b38\u2014\u2014\uff01",
    "\u5c0f\u5fc3\u676f\u5b50\uff01",
    "\u600e\u4e48\uff1f",
    "\u2026\u2026\u6ca1\u4ec0\u4e48\uff0c\u4f60\u8bf4\u3002",
    "\u55ef\u3002\u4f5b\u5c14\u5361\u4ec0\u5df2\u6302\u5728\u901a\u7f09\u540d\u5355\u4e0a\u534a\u5e74\u591a\u4e86\uff0c\u4f46\u6cbb\u5b89\u90e8\u95e8\u4e00\u76f4\u6ca1\u80fd\u6293\u5230\u4ed6\u3002",
    "\u4f5b\u5c14\u5361\u4ec0\u2026\u2026\u770b\u6765\u975e\u5e38\u72e1\u733e\u5462\u3002",
    "\u6ca1\u9519~\u4ed6\u66f4\u662f\u5229\u7528\u81ea\u5df1\u591a\u6b21\u6210\u529f\u9003\u8fc7\u8ffd\u6355\u7684\u7ecf\u5386\u4e3a\u81ea\u5df1\u505a\u5ba3\u4f20\uff0c\u540d\u58f0\u5df2\u7ecf\u4ece\u57fa\u8f85\u5468\u8fb9\u6269\u5927\u5230\u5207\u5c14\u5361\u745f\u9644\u8fd1\u4e86\u5462\u3002",
    "\u6700\u8fd1\u6709\u60c5\u62a5\u8868\u660e\uff0c\u4ed6\u7684\u6700\u65b0\u4ea4\u6613\u5bf9\u8c61\u4f4d\u4e8e\u5207\u5c14\u5361\u745f\u7684\u536b\u661f\u57ce\uff0c\u800c\u4ed6\u672c\u4eba\u4e5f\u5f88\u53ef\u80fd\u8ddf\u968f\u8d27\u7269\u4e00\u540c\u524d\u5f80\u4e86\u3002",
    "\u539f\u6765\u5982\u6b64\u2026\u2026\u5176\u5b9e\uff0c\u521a\u521aCHE-02\u536b\u661f\u57ce\u53d1\u5e03\u4e86\u7d27\u6025\u7ba1\u5236\u63aa\u65bd\uff0c\u6211\u60f3\u53ef\u80fd\u548c\u8fd9\u4f4d\u5148\u751f\u7684\u5230\u6765\u6709\u5173\u3002",
    "\u55ef\uff0c\u8fd9\u4e48\u770b\u6765\uff0cCHE-02\u7684\u6cbb\u5b89\u90e8\u95e8\u5e94\u8be5\u5df2\u7ecf\u53d1\u73b0\u4ec0\u4e48\u7ebf\u7d22\u4e86\uff0c\u4ed6\u5e94\u8be5\u5c31\u5728CHE-02\u3002",
    "\u4f46\u662f\uff0c\u83b1\u5a05\u4f60\u4e3a\u4ec0\u4e48\u5173\u5fc3\u8fd9\u4ef6\u4e8b\uff1f",
    "\u563f\u563f\u2026\u2026\u524d\u4e0d\u4e45\u6211\u4eec\u4ece\u975e\u6cd5P.M.C.\u5904\u7f34\u83b7\u4e86\u4e00\u6279\u519b\u7528\u6b66\u5668\uff0c\u4f46\u5728\u8fd0\u9001\u9014\u4e2d\u5c31\u5931\u7a83\u4e86\uff0c\u6211\u4eec\u5f97\u5230\u7ebf\u7d22\uff0c\u4f5b\u5c14\u5361\u4ec0\u5373\u5c06\u51fa\u552e\u7684\u8fd9\u6279\u519b\u7528\u6b66\u5668\uff0c\u5c31\u662f\u6211\u4eec\u4e22\u5931\u7684\u90a3\u6279\u3002",
    "\u55ef\uff0c\u6240\u4ee5\u4f60\u4eec\u975e\u519b\u4e8b\u529b\u91cf\u7ba1\u7406\u5c40\u4e5f\u62c5\u8d1f\u4e86\u5bfb\u56de\u519b\u7528\u6b66\u5668\u7684\u8d23\u4efb\u3002",
    "\u7b54\u5bf9\u5566~\u800c\u4e14\u4e0d\u5de7\u7684\u662f\uff0c\u76ee\u524d\u975e\u519b\u4e8b\u529b\u91cf\u7ba1\u7406\u5c40\u6ca1\u6709\u53ef\u4f9b\u6d3e\u9063\u7684\u4eba\u624b\u524d\u5f80\u5207\u5c14\u5361\u745f\u5904\u7406\u8fd9\u4ef6\u4e8b\u2026\u2026",
    "\u597d\u7684\u3002\u5728CHE-02\uff0c\u6211\u80fd\u591f\u505a\u7684\u4e8b\u60c5\u4e5f\u591a\u4e00\u4e9b\u3002",
    "\u8c22\u8c22\uff0c\u6625\u7530\u8fd8\u662f\u4e00\u6837\u53ef\u9760\u6b38~|\u4f46\u662f\uff0c\u518d\u600e\u4e48\u8bf4\uff0c\u5bf9\u65b9\u4e5f\u662f\u62e5\u6709\u5f3a\u706b\u529b\u7684\u901a\u7f09\u72af\uff0c\u6625\u7530\u4f60\u4eec\u5728\u884c\u52a8\u65f6\u8fd8\u662f\u4f18\u5148\u4fdd\u8bc1\u81ea\u8eab\u5b89\u5168\uff0c\u5f53\u7136\u8fd8\u6709\u201c\u65b9\u7cd6\u201d\u3002",
    "\u597d\uff0c\u4f60\u653e\u5fc3\u3002|\u8fd9\u53ef\u662f\u2014\u2014\u6765\u81ea\u83b1\u5a05\u7684\u6c42\u52a9\uff0c\u76f8\u5f53\u7a00\u6709\u5462\uff0c\u6211\u4f1a\u59a5\u5584\u5904\u7406\u7684\u3002",
    "\u90a3\u6211\u5c31\u7b49\u4f60\u7684\u597d\u6d88\u606f\u3002",
    "\u5e97\u957f\uff0c\u83b1\u5a05\u6709\u4ec0\u4e48\u4e8b\u5440\uff1f\u6211\u542c\u5230\u4e86\u201c\u6539\u9020\u519b\u7528\u6b66\u5668\u201d\uff0c\u4f1a\u4e0d\u4f1a\u548c\u90a3\u53ea\u8d85\u7ea7\u5175\u8681\u6709\u5173\u5440\uff1f\u4ec0\u4e48\u4eba\u5e72\u7684\uff1f\u5371\u4e0d\u5371\u9669\uff1f\u6211\u4eec\u8981\u505a\u4ec0\u4e48\uff1f",
    "\uff08\u770b\u5230\u6625\u7530\u6302\u65ad\u901a\u8baf\uff0c\u590f\u514b\u91cc\u7acb\u523b\u51d1\u5230\u5979\u8eab\u8fb9\uff0c\u62c9\u7740\u5979\u7684\u624b\u8098\u8fde\u73e0\u70ae\u5730\u95ee\u51fa\u4e00\u4e32\u95ee\u9898\u3002\uff09",
    "\uff08\u6625\u7530\u8f7b\u8f7b\u6478\u4e86\u6478\u590f\u514b\u91cc\u7684\u5934\u3002\uff09",
    "\u590f\u514b\u91cc\uff0c\u4e0d\u7528\u611f\u5230\u6b49\u759a\uff0c\u8fd9\u4e0d\u662f\u4f60\u7684\u95ee\u9898\u3002",
    "\u90a3\u2014\u2014\u90a3\u590f\u514b\u91cc\u6ca1\u6709\u4ec0\u4e48\u80fd\u5e2e\u5fd9\u7684\u5417\uff1f",
    "\u5148\u8bb2\u4e00\u4e0b\u60c5\u51b5\u3002\u83b1\u5a05\u8bf4\uff0c\u6709\u4e00\u4e2a\u540d\u53eb\u4f5b\u5c14\u5361\u4ec0\u7684\u901a\u7f09\u72af\u9003\u5230\u8fd9\u8fb9\u6765\u4e86\uff0c\u901a\u7f09\u539f\u56e0\u662f\u975e\u6cd5\u6539\u9020\u548c\u5012\u5356\u519b\u7528\u6b66\u5668\uff0c\u83b1\u5a05\u8bf7\u6211\u4eec\u5e2e\u5fd9\u534f\u52a9\u6293\u6355\u4ed6\u3002",
    "\u679c\u7136\uff0c\u6211\u4eec\u624b\u91cc\u7684\u90a3\u4e9b\u4e1c\u897f\u2026\u2026",
    "\u55ef\uff0c\u5f88\u6709\u53ef\u80fd\u5c31\u662f\u4ed6\u7684\u3002",
    "\u65e2\u7136\u662f\u901a\u7f09\u72af\u2026\u2026\u90a3\u6211\u4eec\u53ea\u8981\u628a\u90a3\u4e9b\u519b\u7528\u6b66\u5668\u5168\u90e8\u4ea4\u7ed9\u6cbb\u5b89\u90e8\u95e8\u5c31\u53ef\u4ee5\u4e86\u5427\uff1f\u8fd9\u6837\u65e2\u6446\u8131\u4e86\u70eb\u624b\u5c71\u828b\uff0c\u4e5f\u5e2e\u83b1\u5a05\u6293\u6355\u901a\u7f09\u72af\u63d0\u4f9b\u4e86\u7ebf\u7d22\uff0c\u563b\u563b\uff0c\u4e00\u4e3e\u4e24\u5f97\u561b\uff01",
    "\u8fd9\u6050\u6015\u2026\u2026",
    "\u54fc\uff0c\u4f60\u4ee5\u4e3a\u6cbb\u5b89\u90e8\u95e8\u4f1a\u653e\u8fc7\u4f60\uff1f",
    "\u5514\uff1f\u8fd9\u662f\u4ec0\u4e48\u610f\u601d\uff1f",
    "\u5982\u679c\u662f\u4f60\uff0c\u4e00\u4e2a\u4eba\u5f62\u62b1\u7740\u4e00\u5927\u5806\u519b\u7528\u6b66\u5668\u8bf4\u201c\u8fd9\u4e9b\u9a6c\u8def\u4e0a\u6361\u5230\u7684\u519b\u7528\u6b66\u5668\u5c31\u662f\u4f60\u4eec\u8981\u627e\u7684\u901a\u7f09\u72af\u7684\uff0c\u62ff\u53bb\u5427\u8fd9\u5c31\u662f\u7ebf\u7d22\u201d\uff0c\u4f60\u4f1a\u76f8\u4fe1\u8fd9\u4e2a\u4eba\u5f62\u548c\u901a\u7f09\u72af\u5b8c\u5168\u6ca1\u5173\u7cfb\u5417\uff1f",
    "\u8fd9\u662f\u4e8b\u5b9e\u554a\uff0c\u6211\u53c8\u6ca1\u6709\u8bf4\u8c0e\u3002",
    "\u590f\u514b\u91cc\uff0c\u6709\u65f6\u5019\uff0c\u4e3a\u4e86\u544a\u8bc9\u5927\u5bb6\u771f\u76f8\uff0c\u662f\u8981\u4ed8\u51fa\u4e00\u4e9b\u4ee3\u4ef7\u7684\u3002",
    "\u4ee3\u4ef7\uff1f\u4e3a\u4ec0\u4e48\uff1f",
    "\u4ece\u6cbb\u5b89\u90e8\u95e8\u7684\u89d2\u5ea6\u6765\u8bf4\uff0c\u7a81\u7136\u51fa\u73b0\u7684\u7ebf\u7d22\u7684\u786e\u5f88\u53ef\u7591\uff0c\u751a\u81f3\u6709\u53ef\u80fd\u662f\u9677\u9631\uff0c\u4ed6\u4eec\u5fc5\u7136\u4f1a\u5bf9\u6b64\u8fdb\u884c\u8c03\u67e5\u3002",
    "\u5bf9\u8c61\u662f\u4eba\u5f62\u7684\u8bdd\uff0c\u5e94\u8be5\u4f1a\u91c7\u53d6\u76f4\u63a5\u8bfb\u53d6\u5fc3\u667a\u6570\u636e\u7684\u65b9\u5f0f\u5427\u3002",
    "\u2026\u2026\u8bfb\u53d6\u5fc3\u667a\uff01",
    "\u55ef\u3002|\u5f88\u4e0d\u5de7\uff0c\u6211\u624b\u4e2d\u6709\u592a\u591a\u6765\u81ea\u7070\u8272\u5730\u5e26\u7684\u4fe1\u606f\u548c\u8d44\u6e90\uff0c\u662f\u4e0d\u80fd\u968f\u4fbf\u4ea4\u51fa\u5fc3\u667a\u6570\u636e\u7684\u3002",
    "\u662f\u554a\uff0c\u771f\u88ab\u53d1\u73b0\u7684\u8bdd\uff0c\u6050\u6015\u79c1\u85cf\u519b\u7528\u6b66\u5668\u90fd\u7b97\u662f\u5c0f\u4e8b\u3002",
    "\u6709\u3001\u6709\u8fd9\u4e48\u4e25\u91cd\u5417\uff1f",
    "\u4f60\u4ee5\u4e3a\u5462\uff1f|\u5230\u65f6\u5019\u65b9\u7cd6\u5173\u95e8\u4e0d\u8bf4\uff0c\u6625\u7530\u548c\u6211\u4e5f\u4f1a\u88ab\u5e26\u8d70\u5427\uff1f\u800c\u4e14\u2014\u2014",
    "\u739b\u7eee\u6735\uff01",
    "\uff08\u6625\u7530\u63d0\u9ad8\u97f3\u91cf\uff0c\u7528\u773c\u5149\u5411\u739b\u7eee\u6735\u793a\u610f\u4e86\u4e00\u4e0b\u65c1\u8fb9\u53d8\u4e86\u8138\u8272\u7684\u590f\u514b\u91cc\u3002\uff09",
    "\u2026\u2026\u6211\u660e\u767d\u4e86\uff0c\u662f\u2026\u2026\u6211\u60f3\u5f97\u592a\u5929\u771f\u4e86\u3002",
    "\u5e26\u4e86\u9ebb\u70e6\u7684\u5bb6\u4f19\u56de\u6765\u4e0d\u8bf4\u2026\u2026\u8fd8\u60f3\u5f53\u7136\u5730\u5dee\u70b9\u5bb3\u4e86\u6625\u7530\u5e97\u957f\u2026\u2026\u5bf9\u4e0d\u8d77\u2026\u2026\u6211\u4e0d\u77e5\u9053\u4f1a\u8fd9\u6837\uff0c\u4e5f\u6ca1\u6709\u591a\u60f3\u3002",
    "\u5443\u2026\u2026\u73b0\u3001\u73b0\u5728\u4e0d\u662f\u8fd8\u6ca1\u66b4\u9732\u5417\u2026\u2026\u4f60\u63d0\u524d\u81ea\u8d23\u8d77\u6765\u662f\u5e72\u4ec0\u4e48\u2026\u2026",
    "\u6211\u5c31\u662f\u3001\u5e0c\u671b\u65b9\u7cd6\u80fd\u5c3d\u5feb\u6446\u8131\u8fd9\u51e0\u7bb1\u9ebb\u70e6\u4e1c\u897f\uff0c\u4e0d\u8981\u5f71\u54cd\u5230\u5e97\u5e86\u2026\u2026\u6709\u70b9\u7740\u6025\uff0c\u5bf9\u4e0d\u8d77\u2026\u2026",
    "\u5582\u2026\u2026",
    "\u590f\u514b\u91cc\u5c0f\u59d0\uff0c\u6211\u77e5\u9053\u60a8\u7684\u672c\u610f\u662f\u4e3a\u65b9\u7cd6\u7740\u60f3\u7684\uff0c\u522b\u7740\u6025\uff0c\u6211\u4eec\u53ef\u4ee5\u4e00\u8d77\u60f3\u529e\u6cd5\u3002",
    "\u5bf9\uff0c\u522b\u62c5\u5fc3\uff0c\u6211\u4eec\u4e00\u5b9a\u4f1a\u60f3\u51fa\u89e3\u51b3\u529e\u6cd5\u7684\u3002",
    "\u2026\u2026\u771f\u7684\u4f1a\u6709\u89e3\u51b3\u529e\u6cd5\u5417\uff1f",
    "\u5f53\u7136\uff0c\u4f60\u4e0d\u76f8\u4fe1\u5e97\u957f\u5417\uff1f|\u5076\u50cf\u5982\u679c\u54ed\u8d77\u6765\u7684\u8bdd\uff0c\u5927\u5bb6\u4e5f\u4f1a\u6cae\u4e27\u7684\u54e6\u3002",
    "\u6ca1\u6709\u2026\u2026\u6211\u6ca1\u6709\u54ed\uff0c\u5b8c\u7f8e\u7684\u5076\u50cf\u624d\u4e0d\u4f1a\u968f\u4fbf\u54ed\u2026\u2026\u662f\u5e97\u957f\u7684\u89c6\u89c9\u6a21\u5757\u574f\u6389\u4e86\u5566\uff01",
    "\u54e6\uff0c\u662f\u5417\uff1f\u4f60\u7684\u773c\u7ebf\u8fd8\u6709\u776b\u6bdb\u818f\u53ef\u90fd\u7cca\u6210\u4e00\u56e2\u4e86\u3002",
    "\u4e0d\u53ef\u80fd\uff01\u6211\u7528\u7684\u53ef\u662f\u4e13\u4e1a\u7684\u9632\u6c34\u5316\u5986\u54c1\uff01|\u6211\u7684\u955c\u5b50\u2026\u2026\u5514\uff0c\u955c\u5b50\u5462\u2026\u2026",
    "\u90a3\u5c31\u662f\u7684\u786e\u54ed\u4e86\uff1f",
    "\u6ca1\u6709\uff01\u6ca1\u6709\u54ed\uff01\u6211\u7684\u5986\u5bb9\u597d\u597d\u7684\uff0c\u739b\u7eee\u6735\u662f\u3001\u662f\u6ca1\u6709\u770b\u5230\u5417\uff1f",
    "\uff08\u590f\u514b\u91cc\u867d\u7136\u4e49\u6b63\u8f9e\u4e25\u5730\u5426\u5b9a\u4e86\u739b\u7eee\u6735\uff0c\u8fd8\u662f\u4e0d\u653e\u5fc3\u5730\u8f6c\u8fc7\u8eab\uff0c\u7528\u5c0f\u62c7\u6307\u8e6d\u6389\u4e86\u773c\u7736\u4e2d\u7684\u6cea\u6c34\uff0c\u987a\u4fbf\u7167\u7740\u73bb\u7483\u7a97\u4e0a\u7684\u5012\u5f71\u6574\u7406\u4e86\u4e00\u4e0b\u81ea\u5df1\u5934\u9876\u7684\u8774\u8776\u7ed3\u3002\uff09",
    "\u54fc\uff0c\u8bf4\u56de\u6b63\u9898\u5427\uff0c\u65b9\u7cd6\u7684\u4eba\u7684\u786e\u4e0d\u9002\u5408\u51fa\u9762\u4e0a\u4ea4\u519b\u7528\u6b66\u5668\uff0c\u6240\u4ee5\uff0c\u53ea\u8981\u4e0d\u8ba9\u4ed6\u4eec\u77e5\u9053\u662f\u65b9\u7cd6\u7684\u4eba\u5c31\u53ef\u4ee5\u4e86\u5427\uff1f",
    "\u2026\u2026\u60a8\u7684\u610f\u601d\u662f\uff0c\u5728\u4e0d\u88ab\u53d1\u73b0\u7684\u60c5\u51b5\u4e0b\uff1f",
    "\u6ca1\u9519\uff0c\u8fd9\u4ef6\u4e8b\u5c31\u4ea4\u7ed9\u6211\u5427\u3002",
    "\u4e0d\u884c\u3002",
    "\u4e3a\u4ec0\u4e48\u4e0d\u884c\uff1f\u8fd9\u91cc\u9664\u4e86\u6211\u8fd8\u6709\u8c01\u80fd\u8eb2\u8fc7\u4ece\u8fd9\u91cc\u5230\u6cbb\u5b89\u90e8\u95e8\u5927\u697c\u95e8\u53e3\u6240\u6709\u7684\u76d1\u63a7\u6444\u50cf\u5934\uff1f",
    "\u8fd9\u98ce\u9669\u592a\u5927\u4e86\uff0c\u4f60\u4e5f\u4e0d\u80fd\u4fdd\u8bc1\u81ea\u5df1\u53ef\u4ee5\u8eb2\u8fc7\u6240\u6709\u76d1\u63a7\u5427\uff1f\u800c\u4e14\u4f60\u53c8\u60f3\u628a\u4e8b\u60c5\u63fd\u5230\u81ea\u5df1\u8eab\u4e0a\uff1f",
    "\u6709\u4ec0\u4e48\u5173\u7cfb\uff0c\u6211\u4e00\u4e2a\u4eba\u884c\u52a8\u8fd8\u66f4\u65b9\u4fbf\uff0c\u5c31\u7b97\u88ab\u53d1\u73b0\uff0c\u5927\u4e0d\u4e86\u8dd1\u51fa\u53bb\u907f\u907f\u98ce\u5934\u3002",
    "\u7b49\u4e00\u4e0b\u7b49\u4e00\u4e0b\uff0c\u590f\u514b\u91cc\u4e5f\u505a\u5f97\u5230\u54e6\u2665",
    "\u54c8\uff1f\u4f60\u54ea\u6709\u6f5c\u5165\u7684\u7ecf\u9a8c\uff01",
    "\u54ce\u5440\uff0c\u739b\u7eee\u6735\u5fd8\u8bb0\u4e86\u5417\uff1f\u867d\u7136\u590f\u514b\u91cc\u662f\u4e0d\u61c2\u6f5c\u5165\u5566\uff0c\u4f46\u4f5c\u4e3a\u9876\u7ea7\u5076\u50cf\uff0c\u590f\u514b\u91cc\u53ef\u662f\u5f88\u64c5\u957f\u7528\u5316\u5986\u6765\u4f2a\u88c5\u81ea\u5df1\u7684\u54e6~",
    "\u627f\u62c5\u5c5e\u4e8e\u81ea\u5df1\u7684\u8d23\u4efb\uff0c\u662f\u4f18\u79c0\u5076\u50cf\u7684\u5fc5\u5907\u54c1\u5fb7\u2665",
    "\u597d\u2014\u2014\u6682\u505c\uff01\u867d\u7136\u5f88\u611f\u8c22\u4f60\u4eec\u6709\u8fd9\u4efd\u5fc3\uff0c\u4f46\u5e97\u957f\u53ef\u662f\u6211\u54e6\uff1f",
    "\u2026\u2026\u6211\u53c8\u4e0d\u662f\u4f60\u7684\u5458\u5de5\u3002",
    "\u6392\u9664\u98ce\u9669\u592a\u9ad8\u7684\u65b9\u6cd5\uff0c\u5f53\u52a1\u4e4b\u6025\u662f\u5148\u628a\u8fd9\u4e9b\u519b\u7528\u6b66\u5668\u8f6c\u79fb\u51fa\u53bb\uff0c\u5426\u5219\u88ab\u4eba\u53d1\u73b0\u5e97\u91cc\u6709\u5927\u91cf\u8fdd\u7981\u54c1\uff0c\u4e0d\u7ba1\u600e\u4e48\u505a\u90fd\u6d17\u4e0d\u6e05\u4e86\u3002",
    "\u90a3\u5c31\u4ea4\u7ed9\u6211\uff0c\u6211\u4eca\u665a\u5c31\u8ba9\u5b83\u4eec\u201c\u556a\u2014\u2014\u201d\u5730\u5168\u90e8\u6d88\u5931\u2665",
    "\u4f60\u8981\u628a\u5b83\u4eec\u8f6c\u79fb\u8d70\uff1f\u4f60\u77e5\u9053\u8981\u8f6c\u79fb\u5230\u54ea\u91cc\uff1f",
    "\u54ce\u5440\uff0c\u8fd9\u4e2a\u65f6\u5019\u5c31\u8be5\u7ecf\u9a8c\u4e30\u5bcc\u7684\u739b\u7eee\u6735\u5c0f\u59d0\u51fa\u573a\u4e86\u561b\uff01\u739b\u7eee\u6735\u6765\u9009\u5b9a\u5730\u65b9\uff0c\u590f\u514b\u91cc\u8fd0\u8fc7\u53bb~\u4fdd\u8bc1\u795e\u4e0d\u77e5\u9b3c\u4e0d\u89c9\u3002",
    "\u9009\u5b9a\u5730\u70b9\u540e\u6211\u81ea\u5df1\u53bb\u4e0d\u5c31\u597d\u4e86\uff0c\u641e\u90a3\u4e48\u9ebb\u70e6\u505a\u4ec0\u4e48\u3002",
    "\u8fd9\u4e48\u591a\u4e1c\u897f\uff0c\u739b\u7eee\u6735\u81ea\u5df1\u8981\u4ece\u72c2\u6b22\u8282\u642c\u5230\u65b0\u5e74\u5566\uff01\u800c\u4e14\u2026\u2026",
    "\u2026\u2026\u800c\u4e14\u590f\u514b\u91cc\u624d\u4e0d\u662f\u4ec0\u4e48\u9ebb\u70e6\u3002",
    "\u4f46\u662f\uff0c\u5982\u679c\u5c06\u519b\u7528\u6b66\u5668\u8f6c\u79fb\u51fa\u53bb\u7684\u8bdd\uff0c\u6211\u4eec\u53ef\u80fd\u4e5f\u4f1a\u5931\u53bb\u4e0e\u4e4b\u76f8\u5173\u7684\u7ebf\u7d22\uff0c\u8fd9\u6837\u83b1\u5a05\u5c0f\u59d0\u62dc\u6258\u7684\u4e8b\u60c5\u5c31\u5f88\u96be\u63a8\u8fdb\u4e86\u3002",
    "\u55ef\u2026\u2026\u4f46\u6211\u89c9\u5f97\uff0c\u5982\u679c\u4f5b\u5c14\u5361\u4ec0\u662f\u8981\u6765\u505a\u4ea4\u6613\u7684\uff0c\u5c31\u4e00\u5b9a\u643a\u5e26\u4e86\u66f4\u5927\u91cf\u7684\u8d27\u7269\uff0c\u4e0d\u4f1a\u4ec5\u9650\u4e8e\u51c6\u5907\u95f4\u91cc\u7684\u90a3\u4e9b\u3002",
    "\u6240\u4ee5\u5e97\u957f\u60a8\u7684\u610f\u601d\u662f\uff0c\u6211\u4eec\u8981\u8bd5\u7740\u627e\u51fa\u4ed6\u7684\u85cf\u8d27\u5730\u70b9\uff1f",
    "\u6ca1\u9519\uff0c\u4ed6\u4eec\u4e00\u5b9a\u8fd8\u6709\u5927\u6279\u8d27\u7269\u5b58\u653e\u5728CHE-02\u5185\u7684\u67d0\u4e2a\u5730\u65b9\u3002",
    "\u5927\u5bb6\u5728\u8f6c\u79fb\u624b\u4e0a\u7684\u519b\u7528\u6b66\u5668\u65f6\uff0c\u4e5f\u53ef\u4ee5\u6ce8\u610f\u4e00\u4e0b\u6709\u6ca1\u6709\u53ef\u80fd\u88ab\u4f5b\u5c14\u5361\u4ec0\u7528\u4f5c\u4ed3\u5e93\u7684\u5730\u65b9\uff0c\u6211\u60f3\u8fd9\u4e00\u5b9a\u662f\u83b1\u5a05\u9700\u8981\u7684\u4fe1\u606f\u3002",
    "\u90a3\u4e48\u8f6c\u79fb\u5230\u54ea\u91cc\u597d\u5462\uff0c\u739b\u7eee\u6735\u5c0f\u59d0\u6709\u5efa\u8bae\u5417\uff1f",
    "\u2026\u2026\u534e\u5e1d\u6e2f\u3002",
    "\u534e\u5e1d\u6e2f\u2026\u2026\u90a3\u4e2a\u5e9f\u5f03\u6e2f\u53e3\uff1f",
    "\u5bf9\uff0c\u6d32\u9645\u5217\u8f66\u5f00\u901a\u540e\uff0c\u534e\u5e1d\u6e2f\u5c31\u56e0\u4e3a\u65e0\u4eba\u4f7f\u7528\u8352\u5e9f\u4e86\uff0c\u628a\u519b\u7528\u6b66\u5668\u8f6c\u79fb\u5230\u90a3\u91cc\u76f8\u5bf9\u5bb9\u6613\u4e00\u4e9b\u3002",
    "\u800c\u4e14\u2026\u2026CHE-02\u7684\u6cb3\u9053\u4e0e\u57fa\u8f85\u9644\u8fd1\u591a\u4e2a\u536b\u661f\u57ce\u76f8\u901a\uff0c\u90a3\u91cc\u4e5f\u6709\u5927\u91cf\u7684\u95f2\u7f6e\u4ed3\u5e93\u548c\u914d\u5957\u8fd0\u8f93\u8bbe\u65bd\uff0c\u5f88\u6709\u53ef\u80fd\u4f1a\u6210\u4e3a\u4f5b\u5c14\u5361\u4ec0\u7684\u76ee\u6807\u3002",
    "\u786e\u5b9e\uff0c\u5982\u679c\u643a\u5e26\u5927\u5b97\u8d27\u7269\uff0c\u4f7f\u7528\u6c34\u8fd0\u662f\u5408\u7406\u7684\u60f3\u6cd5\u2014\u2014\u6bd5\u7adf\u4ed6\u4eec\u4e0d\u80fd\u5927\u6447\u5927\u6446\u5730\u642d\u4e58\u6d32\u9645\u5217\u8f66\u3002",
    "\u5514\u2026\u2026\u4f46\u662f\uff0c\u6cbb\u5b89\u90e8\u95e8\u5e94\u8be5\u4e5f\u60f3\u5f97\u5230\u8fd9\u4e00\u70b9\u54e6\uff1f",
    "\u55ef\uff0c\u5e94\u8be5\u5df2\u7ecf\u505a\u8fc7\u6392\u67e5\u4e86\u5427\u2026\u2026\u4e0d\u8fc7\uff0c\u5bf9\u901a\u7f09\u72af\u6765\u8bf4\uff0c\u521a\u6392\u67e5\u8fc7\u7684\u5730\u65b9\u53cd\u800c\u662f\u66f4\u52a0\u5b89\u5168\u7684\u706f\u4e0b\u9ed1\u533a\u57df\u5462\u3002",
    "\u6240\u4ee5\uff0c\u6211\u4eec\u8981\u53bb\u6e2f\u53e3\uff0c\u7ed9\u90a3\u4f4d\u901a\u7f09\u72af\u5148\u751f\u4e00\u4e2a\u60ca\u559c\uff1f",
    "\u4e0d\uff0c\u8fd8\u4e0d\u80fd\u786e\u5b9a\u5bf9\u65b9\u4e00\u5b9a\u5728\u6e2f\u53e3\u3002|\u6211\u4eec\u7684\u7b2c\u4e00\u76ee\u6807\u4ecd\u7136\u662f\u8f6c\u79fb\u5e97\u5185\u7684\u519b\u7528\u6b66\u5668\uff0c\u4f46\u53ef\u4ee5\u987a\u4fbf\u63a2\u67e5\u4e00\u4e0b\u3002",
    "\u55ef\uff0c\u5148\u628a\u4e1c\u897f\u8f6c\u79fb\u5230\u6e2f\u53e3\u7684\u5e9f\u5f03\u4ed3\u5e93\uff0c\u8fd9\u6837\u6211\u4eec\u7684\u7b56\u7565\u7a7a\u95f4\u4e5f\u66f4\u5927\u3002\u4e4b\u540e\u518d\u89c6\u60c5\u51b5\uff0c\u60f3\u529e\u6cd5\u8ba9\u8b66\u5bdf\u6ce8\u610f\u5230\u8fd9\u6279\u519b\u7528\u6b66\u5668\uff0c\u751a\u81f3\u4e5f\u53ef\u4ee5\u4ee5\u6b64\u4e3a\u9677\u9631\u5f15\u8bf1\u90a3\u4e2a\u4f5b\u5c14\u5361\u4ec0\u2026\u2026",
    "\u563b\u563b\uff0c\u739b\u7eee\u6735\u5bf9\u5f85\u5e97\u957f\u7684\u6001\u5ea6\u5c31\u4e0d\u4e00\u6837\u5462~",
    "\u6211\u3001\u6211\u5bf9\u6240\u6709\u4eba\u90fd\u662f\u4e00\u6837\u7684\uff01",
    "\u4f46\u662f\u590f\u514b\u91cc\u559c\u6b22\u9677\u9631\u8fd9\u4e2a\u4e3b\u610f\u54e6\uff0c\u6240\u4ee5\u8d5e\u6210~\u2665|\u79d8\u5bc6\u884c\u52a8\u7684\u8bdd\u8981\u9690\u85cf\u8eab\u4efd\u5427\uff1f\u90a3\u590f\u514b\u91cc\u7684\u5316\u5986\u672f\u5c31\u8981\u51fa\u573a\u5566~\u5feb\u8fc7\u6765\uff01",
    "\u522b\u3001\u522b\u62c9\u6211\uff01|\u4ec0\u4e48\u5316\u5986\u672f\uff0c\u6211\u624d\u4e0d\u9700\u8981\u554a\uff01"
  ]
]