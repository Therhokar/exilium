[
  [
    "\u653b\u51fb\u6307\u4ee4\u89e3\u9664\u4e86\uff0c\u6625\u7530\uff01",
    "\u55ef\u2026\u2026\u6211\u6b63\u8bd5\u7740\u53cd\u5411\u8ffd\u8e2a\uff0c\u83b7\u53d6\u5b83\u7684\u63a7\u5236\u6743\u9650\u3002",
    "\u54d4\u54d4\u2014\u2014\u54d4\u2026\u2026\u54d4\u2026\u2026",
    "\u563b\u563b\uff0c\u521a\u521a\u4e0d\u662f\u5f88\u5a01\u98ce\u5417\uff1f|\u8fd8\u662f\u73b0\u5728\u8fd9\u79cd\u5782\u5934\u4e27\u6c14\u7684\u6837\u5b50\u66f4\u53ef\u7231\u5462\u2665",
    "\u54d4\u2014\u2014\uff01",
    "\uff08\u590f\u514b\u91cc\u8f6c\u7740\u5708\u8df3\u5230\u521a\u521a\u8fd8\u5f20\u7259\u821e\u722a\u7684\u53ef\u7591\u8d85\u7ea7\u5175\u8681\u9762\u524d\uff0c\u6572\u4e86\u6572\u5b83\u7684\u8111\u888b\uff0c\u7ed3\u679c\u88ab\u5b83\u7a81\u7136\u518d\u6b21\u7ad6\u8d77\u7684\u67aa\u7ba1\u5413\u5f97\u539f\u5730\u8df3\u8d77\u3002\uff09",
    "\u545c\u54c7\u2014\u2014\u8fd9\u3001\u8fd9\u8fd9\u662f\u5e72\u561b\uff1f",
    "\u54d4\u54d4\u54d4\uff0c\u54d4\u54d4\uff01",
    "\u600e\u4e48\u56de\u4e8b\uff1f",
    "\u6211\u6b63\u5728\u67e5\u627e\u6743\u9650\u4fee\u6539\u8bb0\u5f55\u2026\u2026\u597d\u50cf\u662f\u628a\u590f\u514b\u91cc\u521a\u624d\u7684\u8bdd\u767b\u5f55\u4e3a\u653b\u51fb\u53e3\u4ee4\u4e86\u5462\u3002",
    "\u54ea\u53e5\u8bdd\uff1f",
    "\u201c\u545c\u54c7\u2014\u2014\u201d\u8fd9\u53e5\u54e6\u3002",
    "\u2026\u2026\u4f60\u4eec\u5728\u5f00\u73a9\u7b11\u5417\uff1f",
    "\u6211\u4e5f\u4e0d\u77e5\u9053\u4f1a\u8fd9\u6837\u561b\u2014\u2014",
    "\u7b97\u4e86\u8fd9\u4e0d\u662f\u91cd\u70b9\u3002\u8fd9\u5bb6\u4f19\u5f88\u5371\u9669\uff0c\u6625\u7530\u4f60\u6700\u597d\u8fd8\u662f\u628a\u5b83\u5173\u6389\u5427\uff01",
    "\u55ef\u3002",
    "\u54d4\u2014\u2014",
    "\uff08\u8d85\u7ea7\u5175\u8681\u8eab\u4e0a\u7684\u6b66\u5668\u5168\u90e8\u6536\u56de\u4e86\u4f53\u5185\uff0c\u6307\u793a\u706f\u4e5f\u7184\u706d\u4e86\uff0c\u5b89\u9759\u5730\u8e72\u5728\u5730\u4e0a\uff0c\u770b\u4e0a\u53bb\u975e\u5e38\u65e0\u5bb3\uff0c\u751a\u81f3\u6709\u4e9b\u53ef\u7231\u3002\uff09",
    "\uff08\u590f\u514b\u91cc\u5374\u4f9d\u7136\u7ad9\u5728\u7a0d\u8fdc\u5904\uff0c\u7591\u795e\u7591\u9b3c\u5730\u770b\u7740\u5b83\u3002\uff09",
    "\u5514\u2026\u2026\u5b83\u4e0d\u4f1a\u518d\u7a81\u7136\u201c\u54d4\u2014\u2014\u201d\u4e86\u5427\uff1f",
    "\u4e0d\u4f1a\u4e86\u3002|\u90a3\u4e48\uff0c\u590f\u514b\u91cc\uff0c\u73b0\u5728\u53ef\u4ee5\u597d\u597d\u89e3\u91ca\u4e00\u4e0b\u4e86\u5417\uff1f",
    "\u5c31\u3001\u5c31\u5c31\u662f\u2026\u2026\u590f\u514b\u91cc\u53ea\u662f\u6309\u7167\u5e97\u957f\u7684\u5429\u5490\uff0c\u4e56\u4e56\u5730\u5230\u8fd9\u91cc\u51c6\u5907\u6574\u7406\u4e1c\u897f\uff0c\u7ed3\u679c\u4e00\u6253\u5f00\u95e8\uff0c\u5c31\u53d1\u73b0\u5b83\u7ad9\u5728\u90a3\u8fb9\u4e00\u526f\u8981\u548c\u50a8\u85cf\u67dc\u6253\u4e00\u67b6\u7684\u6837\u5b50\u5462\u3002",
    "\u90a3\u91cc\u8fb9\u90fd\u662f\u5496\u5561\u9986\u7684\u5b9d\u8d1d\uff0c\u590f\u514b\u91cc\u80af\u5b9a\u4e0d\u80fd\u5141\u8bb8\u5b83\u4e71\u6765\u5566\uff0c\u7ed3\u679c\u5c31\u2026\u2026\u5c31\u50cf\u5e97\u957f\u8d76\u8fc7\u6765\u65f6\u7684\u6837\u5b50\u4e86\u3002",
    "\u55ef\u2026\u2026\u4f60\u771f\u7684\u6ca1\u6709\u8bef\u89e6\u4ec0\u4e48\uff1f",
    "\u771f\u7684\u6ca1\u6709\u5566\uff01\u5e97\u957f\u8fd9\u4e48\u8bf4\u590f\u514b\u91cc\u4f1a\u96be\u8fc7\u7684\uff01",
    "\u4f60\u4eec\u8fc7\u6765\u770b\u3002|\u8fd9\u4e2a\u7bb1\u5b50\u4e0a\u6709\u4e2a\u6d1e\u3002",
    "\u5e94\u8be5\uff0c\u5c31\u662f\u4ece\u8fd9\u4e2a\u7bb1\u5b50\u94bb\u51fa\u6765\u7684\u5427\u3002",
    "\u54ce\uff1f\u600e\u4e48\u56de\u4e8b\uff0c\u96be\u9053\u5b83\u81ea\u5df1\u542f\u52a8\u4e86\u5417\uff1f\u8fd8\u7834\u574f\u4e86\u7bb1\u5b50\uff1f\u597d\u53ef\u6015\u54e6\u3002",
    "\u4f60\u8fd8\u597d\u610f\u601d\u8bf4\uff0c\u8fd9\u4e9b\u4e0d\u90fd\u662f\u4f60\u4e70\u56de\u6765\u7684\u4e1c\u897f\u5417\uff1f|\u8fde\u8d85\u7ea7\u5175\u8681\u90fd\u6562\u4e70\uff0c\u4f60\u6709\u8ba4\u8d2d\u8d44\u8d28\u5417\uff1f\u8ba9\u522b\u4eba\u77e5\u9053\u7684\u8bdd\uff0c\u6cbb\u5b89\u961f\u9a6c\u4e0a\u5c31\u4f1a\u8d76\u6765\u67e5\u5c01\u65b9\u7cd6\uff01",
    "\u6240\u4ee5\u8bf4\u2014\u2014\u6211\u4e70\u7684\u662f\u4e8c\u624b\u7684\u5ba0\u7269\u5175\u8681\u5566\uff01",
    "\u6253\u7b97\u7528\u6765\u8868\u6f14\u94bb\u706b\u5708\u7684\uff0c\u8eab\u6750\u5c0f\u5de7\u8fd8\u6709\u8774\u8776\u7ed3\uff0c\u4e0d\u662f\u8fd9\u79cd\u53ef\u4ee5\u62cd\u808c\u8089\u7535\u5f71\u7684\u4e1c\u897f\uff01\u8fd9\u4e48\u4e0d\u53ef\u7231\u590f\u514b\u91cc\u624d\u4e0d\u559c\u6b22\u5462\uff01",
    "\u94bb\u706b\u5708\u2026\u2026\uff1f",
    "\u662f\u554a\uff0c\u662f\u4e00\u53ea\u67e0\u6aac\u9ec4\u8272\u7684\u5ba0\u7269\u5175\u8681\u54e6\uff0c\u6211\u8fd8\u7ed9\u5b83\u4e70\u4e86\u642d\u914d\u7684\u72ee\u5b50\u8033\u6735\u548c\u9b03\u6bdb\u914d\u9970\u5462\uff01",
    "\u4f60\u786e\u5b9a\u5417\uff1f\u771f\u4e0d\u662f\u4e70\u4e86\u592a\u591a\u6ca1\u7528\u7684\u4e1c\u897f\u7ed3\u679c\u5f04\u9519\u2014\u2014",
    "\u4ec0\u4e48\u53eb\u6ca1\u7528\u7684\u4e1c\u897f\uff1f\u6211\u770b\u8fd9\u91cc\u6700\u6ca1\u7528\u7684\u2014\u2014",
    "\uff08\u6625\u7530\u4e00\u628a\u62c9\u4f4f\u590f\u514b\u91cc\uff0c\u963b\u6b62\u5979\u7ee7\u7eed\u8bf4\u4e0b\u53bb\u3002\uff09",
    "\u54c8\u2014\u2014\uff1f\uff01",
    "\u5e97\u957f\uff0c\u6682\u65f6\u6ca1\u6709\u5ba2\u4eba\u4e86\uff0c\u8fd9\u91cc\u8fd8\u597d\u5417\uff1f",
    "\u6851\u6735\u83b1\u5e0c\uff01\u6765\u5f97\u6b63\u597d\uff0c\u5feb\u544a\u8bc9\u5979\u4eec\uff0c\u6211\u4eec\u4e70\u7684\u5175\u8681\u6839\u672c\u5c31\u4e0d\u662f\u8fd9\u53ea\u561b\uff01",
    "\u8d85\u7ea7\u5175\u8681\u2026\u2026\u600e\u4e48\u56de\u4e8b\uff1f",
    "\u770b\u6837\u5b50\uff0c\u5b83\u662f\u88ab\u4f60\u4eec\u8fde\u540c\u5e97\u5e86\u7269\u8d44\u4e00\u8d77\u8fd0\u56de\u6765\u7684\u3002",
    "\u4e0d\uff0c\u6211\u4eec\u6240\u8d2d\u4e70\u7684\uff0c\u4e0d\u5e94\u8be5\u662f\u8fd9\u53ea\u3002",
    "\u6211\u4eec\u5728\u5e97\u5185\u9a8c\u8fc7\u8d27\uff0c\u7bb1\u5b50\u91cc\u7684\u5175\u8681\u4e0d\u662f\u5b83\uff0c\u662f\u4e00\u53ea\u6f06\u6210\u67e0\u6aac\u9ec4\u8272\u7684\u5ba0\u7269\u5175\u8681\u3002\u8fd9\u4e2a\u7bb1\u5b50\u4e5f\u4e0d\u662f\u6211\u4eec\u4ece\u5e97\u91cc\u62ff\u5230\u7684\uff0c\u6211\u80fd\u80af\u5b9a\u8fd9\u4e00\u70b9\u3002",
    "\u770b\u5427\uff0c\u6211\u65e9\u5c31\u8bf4\u4e86\u561b\u3002",
    "\u8fd9\u2026\u2026\u5230\u5e95\u662f\u600e\u4e48\u56de\u4e8b\uff1f",
    "\u2026\u2026\u4e8b\u6545\u3002\u4e8b\u6545\u91cc\uff0c\u7bb1\u5b50\u88ab\u5f04\u9519\u4e86\uff1f",
    "\u55ef\uff1f|\u2026\u2026\u554a\uff0c\u539f\u6765\u662f\u8fd9\u6837\uff01\u662f\u90a3\u4e2a\u4eba\u7684\u7bb1\u5b50\uff01",
    "\u4ec0\u4e48\u4e8b\u6545\uff1f\u4ec0\u4e48\u4eba\uff1f\u4f60\u4eec\u8bf4\u6e05\u695a\u70b9\u554a\u3002",
    "\u6211\u4eec\u5728\u56de\u6765\u7684\u8def\u4e0a\uff0c\u88ab\u5377\u5165\u4e86P.M.C.\u673a\u68b0\u5f15\u53d1\u7684\u9a9a\u4e71\u3002\u5f53\u65f6\u9644\u8fd1\u6709\u4e00\u8f86\u6b63\u5728\u88c5\u8d27\u7684\u8d27\u8f66\u3002\u8d27\u8f66\u91cc\u7684\u7bb1\u5b50\u548c\u6211\u4eec\u7684\u5916\u89c2\u4e0a\u5dee\u522b\u4e0d\u5927\uff0c\u4e5f\u8bb8\u662f\u5728\u6df7\u4e71\u4e2d\u88ab\u62ff\u9519\u4e86\u3002",
    "\u5bf9\uff0c\u5f53\u65f6\u56e0\u4e3a\u6025\u7740\u79bb\u5f00\uff0c\u4e5f\u6ca1\u6709\u6253\u5f00\u68c0\u67e5\u2026\u2026",
    "\u6211\u5bdf\u89c9\u5230\u91cd\u91cf\u4f3c\u4e4e\u6709\u5dee\u5f02\uff0c\u4f46\u65f6\u95f4\u7d27\u6025\uff0c\u6ca1\u6709\u505a\u8fdb\u4e00\u6b65\u7684\u786e\u8ba4\u3002|\u62b1\u6b49\uff0c\u8fd9\u4ef6\u4e8b\u4e0a\u6211\u6709\u4e0d\u53ef\u63a8\u5378\u7684\u8d23\u4efb\u3002",
    "\u6b38\uff1f\u600e\u4e48\u5c31\u90fd\u63fd\u5230\u81ea\u5df1\u8eab\u4e0a\u5566\uff1f\u4e0d\u5bf9\u4e0d\u5bf9\uff0c\u5f53\u65f6\u8981\u7559\u4e0b\u591a\u7ba1\u95f2\u4e8b\u7684\uff0c\u662f\u590f\u514b\u91cc\uff01",
    "\u60c5\u51b5\u6211\u4e86\u89e3\u4e86\u3002|\u8fd9\u4ef6\u4e8b\u4e0d\u662f\u4f60\u4eec\u4e24\u4eba\u7684\u9519\uff0c\u4e3a\u4e86\u6551\u4eba\u7559\u5728\u73b0\u573a\u5e76\u6ca1\u6709\u95ee\u9898\u2026\u2026",
    "\u6211\u8bf4\uff0c\u90a3\u4e9b\u4e8b\u600e\u6837\u90fd\u597d\u5427\u3002",
    "\u6211\u770b\u8fd8\u662f\u5148\u628a\u7bb1\u5b50\u90fd\u6253\u5f00\u68c0\u67e5\u4e00\u904d\uff0c\u8bf4\u4e0d\u5b9a\u8fd8\u6709\u522b\u7684\u60ca\u559c\u3002",
    "\u55ef\uff0c\u5927\u5bb6\u4e00\u8d77\u52a8\u624b\u5427\u3002",
    "\u8fd9\u3001\u600e\u4e48\u56de\u4e8b\u5566\u2014\u2014",
    "\u6f14\u51fa\u670d\u5012\u662f\u90fd\u8fd8\u5728\uff0c\u4f46\u662f\u9b54\u672f\u9053\u5177\u5168\u90fd\u2026\u2026",
    "\u8fd9\u79cd\u89c4\u683c\u7684\uff0c\u662f\u519b\u7528\u5236\u5f0f\u7684\u6b66\u5668\u5427\uff1f|\u590f\u514b\u91cc\uff0c\u4f60\u8be5\u4e0d\u4f1a\u662f\u62ff\u8fd9\u4e9b\u4e1c\u897f\u55b7\u5f69\u5e26\u5427\uff1f",
    "\u2026\u2026\u739b\u7eee\u6735\u7684\u7b11\u8bdd\uff0c\u771f\u70c2\u5462\u3002",
    "\u6211\u6ca1\u5f00\u73a9\u7b11\uff01",
    "\uff08\u6625\u7530\u4e0d\u8d5e\u540c\u5730\u770b\u4e86\u590f\u514b\u91cc\u4e00\u773c\uff0c\u53c8\u62cd\u4e86\u62cd\u739b\u7eee\u6735\u7684\u80a9\u8180\u3002\uff09",
    "\u884c\u5427\uff0c\u90a3\u5c31\u6765\u6574\u7406\u4e00\u4e0b\u72b6\u51b5\u3002",
    "\u6309\u7167\u73b0\u6709\u60c5\u51b5\u63a8\u6d4b\uff0c\u6709\u4eba\u5728\u8fd0\u9001\u8fd9\u6279\u519b\u7528\u6b66\u5668\u7684\u65f6\u5019\u53d1\u751f\u4e86\u610f\u5916\uff0c\u548c\u590f\u514b\u91cc\u7684\u9b54\u672f\u9053\u5177\u5f04\u6df7\uff0c\u4e92\u76f8\u62ff\u9519\u4e86\u3002\u73b0\u5728\u8fd9\u4e9b\u6b66\u5668\u5728\u8fd9\u91cc\uff0c\u90a3\u4e48\u90a3\u4eba\u624b\u91cc\u7684\u662f\uff1f",
    "\u2026\u2026\u662f\u9e3d\u5b50\u3001\u6c14\u7403\u8fd8\u6709\u70df\u82b1\u2026\u2026\u5509\uff0c\u90a3\u4e9b\u9e3d\u5b50\u8fd8\u6709\u70df\u82b1\u597d\u8d35\u7684\u5566\u2026\u2026",
    "\u91cd\u70b9\u662f\u8fd9\u4e2a\uff1f\uff01",
    "\u2026\u2026",
    "\u6851\u6735\u83b1\u5e0c\uff0c\u600e\u4e48\u4e86\uff1f",
    "\u8896\u6263\u4e0d\u89c1\u4e86\uff0c\u53ef\u80fd\u662f\u6389\u8fdb\u4e86\u54ea\u4e2a\u7bb1\u5b50\u2026\u2026",
    "\u662f\u90a3\u4e2a\u5427\u3002\u53cd\u5149\u90a3\u4e2a\u3002",
    "\u8c22\u8c22\u2026\u2026\u55ef\uff1f",
    "\u8fd9\u662f\u2026\u2026\u80f8\u9488\uff1f",
    "\u8fd9\u4e2a\u5f62\u72b6\u662f\u72fc\u5934\u5417\u2026\u2026\u6ca1\u89c1\u8fc7\u5462\u3002",
    "\u8ba9\u6211\u770b\u770b\uff1f",
    "\u5514\u2026\u2026\u4e0d\u662f\u4efb\u4f55\u65f6\u5c1a\u54c1\u724c\u7684\u4ea7\u54c1\u3002",
    "\u53ef\u80fd\u662f\u6240\u6709\u8005\u6807\u8bb0\uff0c\u4fdd\u9669\u8d77\u89c1\uff0c\u5148\u653e\u56de\u53bb\u3002",
    "\u4f46\u7bb1\u5b50\u91cc\u521a\u597d\u6709\u6807\u8bb0\uff1f\u8fd9\u7b80\u76f4\u5c31\u50cf\u547d\u8fd0\u4e00\u6837\u5462~\u8fd9\u6837\u6211\u4eec\u5c31\u53ef\u4ee5\u8bd5\u7740\u627e\u5230\u4ed6\u4eec\uff0c\u7136\u540e\u628a\u4e1c\u897f\u9001\u56de\u53bb\u5427\uff1f",
    "\u4e5f\u4e0d\u662f\u6545\u610f\u62ff\u9519\u7684\uff0c\u53ea\u8981\u597d\u597d\u89e3\u91ca\u4e00\u4e0b\uff0c\u4ed6\u4eec\u5e94\u8be5\u4f1a\u539f\u8c05\u53ef\u7231\u7684\u590f\u514b\u91cc\u5462\u2665",
    "\u6ca1\u90a3\u4e48\u7b80\u5355\u3002\u8fd9\u4e9b\u6b66\u5668\uff0c\u770b\u8d77\u6765\u591a\u5c11\u90fd\u7ecf\u8fc7\u6539\u88c5\uff0c\u7279\u522b\u662f\u90a3\u4e2a\u8d85\u7ea7\u5175\u8681\u3002",
