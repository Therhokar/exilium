[
  [
    "\uff08\u9ec4\u533a\uff0c\u67d0\u5904\u5de5\u5382\u5185\u2014\u2014\uff09",
    "\u62d2\u7edd\u52a0\u5165\uff0c\u7942\u5c06\u60e9\u7f5a\u964d\u4e0b\u2014\u2014\u662f\u5979\u4eec\uff01",
    "\uff08\u968f\u7740\u4ed6\u7684\u547c\u558a\uff0c\u539f\u672c\u53ea\u662f\u8c03\u8f6c\u89c6\u7ebf\u7684\u957f\u888d\u4eba\u7fa4\u7eb7\u7eb7\u5411\u4e09\u4eba\u6d8c\u6765\u3002\uff09",
    "\uff08\u539f\u672c\u4eba\u5458\u5bc6\u5ea6\u5c31\u975e\u5e38\u5927\u7684\u5ba4\u5185\u4e00\u65f6\u95f4\u4eba\u6f6e\u6d8c\u52a8\u3002\u5373\u5c06\u88ab\u4eba\u6f6e\u6df9\u6ca1\u7684\u4e09\u4eba\u88ab\u8feb\u6324\u5728\u4e86\u4e00\u8d77\u3002\uff09",
    "\u7b49\u3001\u7b49\u4e00\u4e0b\u2014\u2014\u8d6b\u745e\u5c24\uff0c\u8d6b\u745e\u5c24\u5728\u54ea\u91cc\u2014\u2014",
    "\u554a\u3001\u4e0d\u8981\u6324\u561b\uff0c\u7ef4\u666e\u857e\u7684\u53d1\u578b\u90fd\u88ab\u5f04\u4e71\u4e86\u2014\u2014",
    "\u73b0\u5728\u662f\u5728\u4e4e\u4f60\u53d1\u578b\u7684\u65f6\u5019\uff1f\uff01\u8d76\u7d27\u6321\u4f4f\u4ed6\u4eec\u6211\u4eec\u64a4\u554a\uff01",
    "\u7ef4\u666e\u857e\u5df2\u7ecf\u52aa\u529b\u4e86\uff01\u53ef\u5927\u5bb6\u90fd\u597d\u70ed\u60c5\uff0c\u7ef4\u666e\u857e\u56de\u5e94\u4e0d\u8fc7\u6765\u4e86\u5440\uff01",
    "\u5582\u5bc7\u5c14\u8299\uff0c\u4f60\u7ad9\u8fc7\u6765\uff0c\u79bb\u8fd9\u4e48\u8fdc\u88ab\u51b2\u6563\u4e86\u6211\u53ef\u4e0d\u7ba1\u4f60\uff01",
    "\u8d6b\u745e\u5c24\uff0c\u5728\u54ea\u91cc\uff0c\u4e0d\u89c1\u4e86\u2014\u2014\u6211\u660e\u660e\u2014\u2014\u6211\u4e4b\u524d\u660e\u660e\u5728\u8fd9\u91cc\u597d\u597d\u7684\uff0c\u8fd8\u5dee\u4e00\u70b9\u2014\u2014\u8fd8\u5dee\u4e00\u70b9\u6211\u5c31\u80fd\u7ed9\u5b89\u83f2\u5a05\u62a5\u4ec7\uff01",
    "\u4ec0\u4e48\uff0c\u5b89\u83f2\u5a05\u53c8\u662f\u8c01\uff1f",
    "\u5514\uff0c\u7ef4\u666e\u857e\u77e5\u9053\u5566\uff0c\u662f\u5bc7\u5c14\u8299\u8d85\u7ea7\u91cd\u8981\u7684\u89c2\u4f17\uff0c\u5bf9\u4e0d\u5bf9\uff1f",
    "\u89c2\u4f17\uff1f\uff01\u4f60\u4eec\u2014\u2014\u4f60\u4eec\u4ec0\u4e48\u90fd\u4e0d\u77e5\u9053\uff01\u4f60\u4eec\u4e3a\u4ec0\u4e48\u8fc7\u6765\uff01\u4f60\u4eec\u2014\u2014\u4e3a\u4ec0\u4e48\u8981\u8fc7\u6765\uff01",
    "\u54c8\uff1f\uff01\u6211\u4eec\u4e0d\u8fc7\u6765\u7b49\u4f60\u88ab\u62c6\u6210\u96f6\u4ef6\u6254\u5230\u56de\u6536\u7ad9\u5c31\u665a\u4e86\u597d\u5427\uff1f\uff01|\u54e6\u2014\u2014\u4f60\u8fde\u5fc3\u667a\u5907\u4efd\u90fd\u5220\u4e86\u96be\u4e0d\u6210\u6211\u4eec\u8fd8\u8981\u53bb\u627e\u6697\u5546\u518d\u53bb\u62fc\u4e00\u4e2a\u4f60\u7684\u5fc3\u667a\u51fa\u6765\uff1f\uff01\u8fde\u7eed\u52a0\u73ed\u5c31\u7b97\u662f\u7f8e\u73b2\u4e5f\u4f1a\u53d7\u4e0d\u4e86\u7684\u554a\uff1f\uff01",
    "\u4ec0\u4e48\u52a0\u73ed\uff0c\u4ec0\u4e48\u62fc\u6211\u7684\u5fc3\u667a\u2014\u2014\u4f60\u4eec\u4e0d\u7528\u6765\u5047\u60fa\u60fa\u5730\u627e\u6211\uff0c\u4e0d\u8981\u88c5\u5f97\u6211\u4eec\u5f88\u719f\uff01|\u6211\u4ee5\u524d\u2014\u2014\u6211\u4ee5\u524d\u90fd\u662f\u88c5\u7684\uff01\u6211\u6839\u672c\u5c31\u4e0d\u662f\u4f60\u4ee5\u4e3a\u7684\u90a3\u79cd\u4eba\u5f62\uff01\u4f60\u4eec\u770b\u4e0d\u51fa\u6765\u5417\uff1f\uff01",
    "\u4f60\u88c5\u4e0d\u88c5\u8ddf\u6211\u6709\u4ec0\u4e48\u5173\u7cfb\uff1f",
    "\u90a3\u4f60\u7ba1\u4ec0\u4e48\u95f2\u4e8b\uff1f\uff01\u4f60\u4eec\u8fd9\u6837\u2014\u2014\u4f60\u4eec\u8fd9\u6837\uff0c\u8fdf\u65e9\u8981\u6b7b\u5728\u6c61\u67d3\u533a\u7684\u4f60\u4eec\u77e5\u4e0d\u77e5\u9053\uff1f\uff01",
    "\u54fc\uff0c\u4f60\u60f3\u6e05\u695a\uff0c\u5f53\u521d\u662f\u8c01\u5728\u6c61\u67d3\u533a\u8fc7\u4e0d\u4e0b\u53bb\u4e86\u8eb2\u6211\u4eec\u7269\u8d44\u7bb1\u91cc\u5077\u5077\u6478\u6478\u6df7\u4e0a\u57fa\u5730\u8f66\uff1f\u8fd8\u6211\u7ba1\u95f2\u4e8b\uff1f|\u6211\u4eec\u6b7b\u5728\u6c61\u67d3\u533a\uff1f\u6211\u770b\u4f60\u662f\u4e0d\u6e05\u695a\u6211\u6709\u591a\u5389\u5bb3\u5427\uff1f",
    "\u505c\u4e00\u4e0b\uff01\u5927\u5bb6\u90fd\u662f\u4e00\u8d77\u6f14\u51fa\u7684\u540c\u4f34\uff0c\u5435\u67b6\u662f\u4e0d\u597d\u7684\u2014\u2014|\u554a\uff0c\u89c2\u4f17\u4eec\u597d\u70ed\u60c5\uff0c\u7ef4\u666e\u857e\u8981\u6321\u4e0d\u4f4f\u4e86\u5566\u2014\u2014",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u5e94\u58f0\u51b2\u8fc7\u53bb\u6d88\u706d\u4e86\u51e0\u4e2a\u6293\u4f4f\u7ef4\u666e\u857e\u624b\u81c2\u7684\u957f\u888d\u4eba\uff0c\u5c06\u7ef4\u666e\u857e\u4ece\u4eba\u7fa4\u4e2d\u62c9\u4e86\u51fa\u6765\u3002\uff09",
    "\u90fd\u8bf4\u4e86\u522b\u79bb\u6211\u592a\u8fdc\uff0c\u5bc7\u5c14\u8299\uff0c\u4f60\u4e5f\u8fc7\u6765\uff01",
    "\u6211\u2014\u2014",
    "\u961f\u957f\u548c\u6307\u6325\u5b98\u662f\u4e0d\u662f\u5feb\u8981\u5230\u4e86\u5440\uff1f",
    "\u770b\u65f6\u95f4\u5dee\u4e0d\u591a\u4e86\u5427\u3002\u5567\uff0c\u8fd9\u600e\u4e48\u641e\u7684\uff0c\u901a\u8baf\u8fd8\u7528\u4e0d\u4e86\uff1f|\u5582\uff0c\u5bc7\u5c14\u8299\uff0c\u7ef4\u666e\u857e\uff0c\u8ddf\u7d27\u6211\uff0c\u6211\u4eec\u5148\u5230\u5165\u53e3\u90a3\u8fb9\u3002",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u51fb\u8d25\u4e86\u773c\u524d\u7684\u51e0\u4e2a\u7a7f\u7740\u957f\u888d\u7684\u4eba\uff0c\u77ed\u6682\u6e05\u51fa\u4e00\u6761\u901a\u8def\uff0c\u5411\u8eab\u540e\u504f\u4e86\u504f\u5934\u3002\uff09",
    "\u961f\u5f62\u2606\u53d8\u66f4\u5f00\u59cb\uff01\u7ef4\u666e\u857e\u3001\u9a6c\u4e0a\u5c31\u5230\uff01",
    "\u6211\u4e0d\u8d70\u3002",
    "\u4f60\u53c8\u95f9\u4ec0\u4e48\u522b\u626d\uff1f\uff01",
    "\u961f\u957f\u2014\u2014\u961f\u957f\u548c\u6307\u6325\u5b98\u6839\u672c\u5c31\u4e0d\u4f1a\u6765\uff01",
    "\u54c8\uff1f\uff01",
    "\u5475\uff0c\u8fd9\u4e48\u5389\u5bb3\u7684\u514b\u7f57\u4e3d\u79d1\u60f3\u4e0d\u5230\u5417\uff1f\uff01",
    "\u8fd9\u91cc\u6709\u8fd9\u4e48\u591a\u4eba\uff0c\u628a\u4f60\u4eec\u548c\u6211\u5e26\u51fa\u53bb\uff0c\u4f1a\u82b1\u8d39\u597d\u591a\u8865\u7ed9\u548c\u65f6\u95f4\u2014\u2014\u5fc3\u667a\u5907\u4efd\u3001\u4eba\u5f62\u6743\u9650\u5f52\u5c5e\uff0c\u90fd\u662f\u6211\u81ea\u5df1\u5220\u6389\u7684\uff01|\u57fa\u5730\u8f66\u4e0a\u8fd8\u6709\u5907\u7528\u7d20\u4f53\uff0c\u53ea\u8981\u2014\u2014\u53ea\u8981\u7528\u5fc3\u667a\u5907\u4efd\u91cd\u542f\u4f60\u548c\u7ef4\u666e\u857e\uff0c\u4f60\u4eec\u5c31\u5f97\u6551\u4e86\u2014\u2014\u6307\u6325\u5b98\u8fc7\u6765\uff0c\u662f\u7279\u610f\u6765\u9001\u6b7b\u5417\uff1f\uff01",
    "\u5601\uff0c\u8bf4\u4f60\u95f9\u522b\u626d\u4f60\u8fd8\u771f\u5c31\u95f9\u4e0a\u4e86\uff1f\u6211\u8bf4\u5bc7\u5c14\u8299\uff0c\u4e0d\u7136\u6211\u4eec\u6253\u4e2a\u8d4c\u3002",
    "\uff08\u9762\u524d\u7684\u901a\u8def\u5f88\u5feb\u88ab\u5982\u6f6e\u6c34\u822c\u6d8c\u6765\u7684\u957f\u888d\u4eba\u7fa4\u5835\u4e0a\u3002\uff09",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u538b\u4f4e\u4e86\u4e0a\u534a\u8eab\uff0c\u5200\u950b\u671d\u5916\uff0c\u5fae\u5fae\u772f\u8d77\u4e86\u773c\u775b\u3002\uff09",
    "\u8d4c\u4ec0\u4e48\uff1f\uff01",
    "\u961f\u957f\u4e00\u5b9a\u4f1a\u8fc7\u6765\uff0c\u54e6\uff0c\u5f53\u7136\u8fd8\u6709\u90a3\u4e2a\u788d\u4e8b\u7684\u7eb3\u7f8e\u897f\u4e1d\u3002|\u6307\u6325\u5b98\u672c\u4eba\u5012\u662f\u4e0d\u4f1a\u8fc7\u6765\u2014\u2014\u4e0d\u8fc7\u961f\u957f\u5979\u4eec\u4f1a\u8fc7\u6765\uff0c\u4e00\u5b9a\u662f\u6307\u6325\u5b98\u7684\u547d\u4ee4\u3002",
    "\u5bc7\u5c14\u8299\uff0c\u5982\u679c\u770b\u5230\u961f\u957f\uff0c\u4f60\u5c31\u8ddf\u6211\u56de\u53bb\u3002\u8d4c\u4e0d\u8d4c\uff1f",
    "\u6211\u4e0d\u8ddf\u4f60\u8d4c\uff01\u4e0b\u8fd9\u79cd\u6ca1\u6709\u610f\u4e49\u7684\u8d4c\u6ce8\u6709\u4ec0\u4e48\u610f\u4e49\uff1f\uff01\u6211\u8981\u7559\u5728\u8fd9\u91cc\u2014\u2014\u6211\u4e00\u5b9a\u8981\u7559\u5728\u8fd9\u91cc\uff01",
    "\u5bc7\u5c14\u8299\u662f\u8bf4\uff0c\u4e00\u5b9a\u4f1a\u770b\u5230\u961f\u957f\u7684\u561b\uff1f",
    "\u54fc\uff0c\u770b\u5427\uff0c\u6211\u5c31\u8bf4\uff0c\u4f60\u8fd9\u662f\u5728\u95f9\u522b\u626d\u3002",
    "\u5bc7\u5c14\u8299\uff0c\u6211\u4eec\u5df2\u7ecf\u88ab\u8fd9\u4e9b\u4eba\u5f53\u505a\u654c\u4eba\uff0c\u800c\u548c\u6211\u4eec\u5728\u4e00\u8d77\u7684\u4f60\uff0c\u4e0d\u53ef\u80fd\u518d\u7559\u5728\u8fd9\u91cc\u3002",
    "\u4e0d\u884c\uff0c\u6211\u4e0d\u80fd\u2026\u2026\uff01",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u518d\u6b21\u5229\u843d\u5730\u89e3\u51b3\u6389\u9762\u524d\u7684\u51e0\u4e2a\u7a7f\u7740\u957f\u888d\u7684\u4eba\u540e\uff0c\u56de\u5934\u770b\u5411\u5bc7\u5c14\u8299\uff0c\u9732\u51fa\u4e0d\u8010\u70e6\u7684\u8868\u60c5\u3002\uff09",
    "\u884c\u4e86\u884c\u4e86\uff0c\u95f9\u522b\u626d\u56de\u5934\u53bb\u627e\u961f\u957f\u2014\u2014",
    "\uff08\u51fa\u73b0\u5728\u5bc7\u5c14\u8299\u8eab\u540e\u7684\uff0c\u662f\u51e0\u4e2a\u624b\u6301\u4e0d\u540c\u6b66\u5668\u7684\u4eba\uff0c\u4e0b\u4e00\u523b\uff0c\u90a3\u4e9b\u6b66\u5668\u5c31\u8981\u547d\u4e2d\u5bc7\u5c14\u8299\u3002\uff09",
    "\u5bc7\u5c14\u8299\uff0c\u4f60\u540e\u9762\uff01"
  ]
]