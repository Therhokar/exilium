    "\uff08\u6211\u9010\u6e10\u51b7\u9759\u4e0b\u6765\uff0c\u5f00\u59cb\u5c1d\u8bd5\u7740\u56de\u5fc6\u3002\uff09",
    "\uff08\u95ea\u7535\u5e76\u6ca1\u6709\u4e4b\u524d\u7684\u5fc3\u667a\u8bb0\u5f55\uff0c\u5982\u679c\u53ea\u8bf4\u6211\u7684\u8bb0\u5fc6\u2026\u2026\uff09",
    "\u6211\u5728\u683c\u91cc\u82ac\u65f6\u671f\u6240\u89c1\u5230\u7684\u201c\u5e15\u62c9\u8482\u65af\u201d\u6210\u5458\uff0c\u8eab\u4f53\u4f1a\u6709\u4e00\u90e8\u5206\u662f\u673a\u68b0\uff0c\u4f53\u8868\u4e5f\u4e0d\u4f1a\u51fa\u73b0\u574d\u584c\u7ed3\u6676\u2026\u2026|\u800c\u4e14\uff0c\u89c1\u5230\u5176\u4ed6\u4eba\uff0c\u4e5f\u4e0d\u4f1a\u4e3b\u52a8\u8981\u6c42\u5176\u4ed6\u4eba\u52a0\u5165\u3002\u4ece\u8fd9\u4e2a\u65b9\u9762\u6765\u770b\uff0c\u4ed6\u4eec\u786e\u5b9e\u548c\u6211\u8bb0\u5fc6\u91cc\u7684\u201c\u5e15\u62c9\u8482\u65af\u201d\u4e0d\u592a\u4e00\u6837\u3002",
    "\u4e00\u6837\u7684\u5730\u65b9\u2026\u2026\u662f\u4e0d\u5bb3\u6015\u574d\u584c\u8f90\u5c04\uff1f\u8fd8\u6709\u4f7f\u7528\u201c\u53d7\u6d17\u8005\u201d\u8fd9\u4e2a\u975e\u5e38\u5c11\u89c1\u7684\u79f0\u547c\uff1f",
    "\u201c\u4f0a\u9ed8\u5c14\u201d\uff0c\u4ed6\u4eec\u53cd\u590d\u5f3a\u8c03\u7684\u8fd9\u4e2a\u540d\u79f0\u51fa\u73b0\u8fc7\u5417\uff1f",
    "\u6ca1\u6709\u2026\u2026\u6216\u8005\u8bf4\uff0c\u6211\u4e0d\u77e5\u9053\u3002\u5f53\u65f6\u6211\u6240\u638c\u63e1\u7684\u8d44\u6599\u4e2d\uff0c\u6ca1\u6709\u770b\u5230\u7c7b\u4f3c\u7684\u540d\u79f0\u3002",
    "\u201c\u4e0d\u5bb3\u6015\u574d\u584c\u8f90\u5c04\u201d\uff0c\u5982\u679c\u8fd9\u662f\u4ed6\u4eec\u7684\u7279\u70b9\uff0c\u4e4b\u524d\u90a3\u4e9b\u4eba\u9047\u5230\u6211\u4eec\u8fd9\u4e9b\u6ca1\u6709\u7a7f\u7740\u9632\u62a4\u670d\u7684\u4eba\u5f62\u4e4b\u540e\uff0c\u4f1a\u9080\u8bf7\u6211\u4eec\u52a0\u5165\u4e5f\u5c31\u4e0d\u5947\u602a\u4e86\u3002",
    "\u4f46\u4ed6\u4eec\u7684\u8eab\u4e0a\u5e76\u6ca1\u6709\u51fa\u73b0\u7c7b\u4f3c\u5e15\u62c9\u8482\u65af\u7684\u6807\u5fd7\uff0c\u4ed6\u4eec\u662f\u4e0d\u662f\u5e15\u62c9\u8482\u65af\uff0c\u6211\u8fd8\u4e0d\u80fd\u786e\u5b9a\u3002",
    "\u6d77\u4f26\u5a1c\u7684\u7bb1\u5b50\u4e0a\u4e5f\u6ca1\u6709\u7c7b\u4f3c\u7684\u6807\u8bb0\u3002",
    "\u2026\u2026\u786e\u5b9e\u3002",
    "\u5982\u679c\u5bc7\u5c14\u8299\u79bb\u5f00\u57fa\u5730\u8f66\u540e\u9047\u5230\u4ed6\u4eec\uff0c\u5f88\u53ef\u80fd\u4f1a\u63a5\u53d7\u4ed6\u4eec\u9080\u8bf7\uff0c\u4e0e\u4ed6\u4eec\u4e00\u540c\u884c\u52a8\u3002",
    "\u76ee\u524d\u6765\u770b\uff0c\u786e\u5b9e\u6709\u5f88\u5927\u53ef\u80fd\u3002",
    "\uff08\u95ea\u7535\u4ece\u603b\u63a7\u53f0\u524d\u79bb\u5f00\uff0c\u8d70\u5230\u6211\u8eab\u8fb9\u3002\uff09",
    "\u6ca1\u5173\u7cfb\u7684\uff0c\u6307\u6325\u5b98\u3002\u987a\u7740\u8fd9\u4e2a\u7ebf\u7d22\u8ffd\u67e5\u4e0b\u53bb\uff0c\u5982\u679c\u4f60\u5728\u683c\u91cc\u82ac\u6700\u540e\u7684\u4efb\u52a1\u6ca1\u6709\u5b8c\u6210\u2014\u2014",
    "\uff08\u95ea\u7535\u7684\u624b\u8f7b\u8f7b\u5730\u653e\u5728\u4e86\u6211\u7684\u80a9\u8180\u4e0a\u3002\uff09",
    "\u8fd9\u4e00\u6b21\uff0c\u6211\u4eec\u4e00\u5b9a\u4f1a\u7ed3\u675f\u6389\u5b83\u3002",
    "\u2026\u2026\u597d\u3002",
    "\u5b9a\u65f6\u8054\u7edc\uff1f",
    "\u4e0d\uff0c\u514b\u7f57\u4e3d\u79d1\u5979\u4eec\u4f20\u56de\u4e86\u753b\u9762\u3002",
    "\u64ad\u653e\u3002",
    "\uff08\u7ecf\u7531\u95ea\u7535\u7684\u64cd\u4f5c\uff0c\u514b\u7f57\u4e3d\u79d1\u56de\u4f20\u7684\u753b\u9762\u663e\u793a\u5728\u4e86\u827e\u83ab\u53f7\u7684\u6295\u5f71\u5c4f\u4e0a\u3002\uff09",
    "\uff08\u753b\u9762\u4e2d\uff0c\u514b\u7f57\u4e3d\u79d1\u548c\u7ef4\u666e\u857e\u8ddf\u7740\u8eab\u7a7f\u957f\u888d\u7684\u4eba\u7fa4\u8fdb\u5165\u4e86\u4e00\u4e2a\u770b\u8d77\u6765\u975e\u5e38\u666e\u901a\u7684\u5c45\u4f4f\u70b9\u3002\uff09",
    "\uff08\u5c45\u4f4f\u70b9\u5185\u4e00\u7247\u5e73\u548c\uff0c\u4ece\u5916\u89c2\u770b\u4e0a\u53bb\uff0c\u548c\u8428\u73c8\u6240\u5728\u7684\u5c45\u4f4f\u70b9\u5e76\u6ca1\u6709\u5f88\u5927\u7684\u533a\u522b\u3002\uff09",
    "\uff08\u552f\u4e00\u4e0d\u540c\u7684\u662f\uff0c\u6563\u5e03\u5728\u5c45\u4f4f\u70b9\u5404\u5904\u7684\u5c45\u6c11\u90fd\u7a7f\u7740\u4e0e\u6211\u4eec\u4e4b\u524d\u89c1\u8fc7\u7684\u76f8\u540c\u957f\u888d\uff0c\u6b63\u5728\u5fd9\u788c\u7740\u6539\u5efa\u4e00\u4e9b\u5efa\u7b51\u3002\uff09",
    "\u5582\uff0c\u6211\u8bf4\uff01",
    "\u4ed6\u4eec\u5570\u5570\u55e6\u55e6\u5435\u4e86\u4e00\u8def\uff0c\u6211\u4eec\u4ec0\u4e48\u65f6\u5019\u624d\u80fd\u56de\u53bb\u554a\u6307\u6325\u5b98\u3002",
    "\u7ef4\u666e\u857e\u90fd\u6709\u597d\u597d\u56de\u7b54\u4ed6\u4eec\u54e6\uff01\u54cd\u5e94\u89c2\u4f17\u7684\u547c\u58f0\uff0c\u4e5f\u662f\u5076\u50cf\u7684\u5de5\u4f5c\u2606",
    "\u4f60\u90a3\u53eb\u56de\u7b54\uff1f\uff01\u4f60\u4eec\u90a3\u6839\u672c\u4e0d\u662f\u5bf9\u8bdd\u554a\uff01\u800c\u4e14\u6700\u5435\u7684\u4eba\u5c31\u662f\u4f60\u5427\uff01",
    "\u7ef4\u3001\u7ef4\u666e\u857e\u624d\u4e0d\u5435\uff01\u90a3\u662f\u2026\u2026\u90a3\u662f\u2026\u2026",
    "\u522b\u8bf4\u8bdd\uff0c\u6709\u4eba\u8fc7\u6765\u4e86\u3002",
    "\uff08\u753b\u9762\u4e0a\u8fce\u9762\u51fa\u73b0\u4e00\u961f\u7a7f\u7740\u540c\u7c7b\u957f\u888d\u7684\u4eba\u7fa4\u3002\uff09",
    "\uff08\u4ed6\u4eec\u8eab\u4e0a\u660e\u663e\u6709\u4f24\uff0c\u5374\u6ca1\u6709\u7a7f\u7740\u4efb\u4f55\u80fd\u591f\u62b5\u6321\u574d\u584c\u8f90\u5c04\u7684\u8bbe\u5907\u3002\u4ed6\u4eec\u7684\u8138\u4e0a\uff0c\u662f\u5982\u51fa\u4e00\u8f99\u7684\u3001\u5728\u6c61\u67d3\u533a\u4f4f\u6c11\u4e2d\u975e\u5e38\u5c11\u89c1\u7684\u72c2\u70ed\u3002\uff09",
    "\u6211\u4eec\u7684\u540c\u4f34\uff0c\u6b22\u8fce\u52a0\u5165\u3002\u63a5\u53d7\u6d17\u793c\uff0c\u6c38\u6052\u7684\u5f52\u53bb\u4e4b\u6240\uff0c\u6211\u4eec\u5c06\u5171\u540c\u524d\u5f80\u3002\u5b89\u5b81\uff0c\u4e0e\u6211\u4eec\u540c\u5728\u3002",
    "\u5b89\u5e15\u5229\u5854\u3002",
    "\u786e\u8ba4\u5750\u6807\u3002",
    "\u53d1\u56de\u53bb\u4e86\u3002\u6211\u4eec\u63a5\u4e0b\u6765\u5e72\u4ec0\u4e48\uff0c\u6307\u6325\u5b98\uff1f",
    "\u786e\u8ba4\u5bc7\u5c14\u8299\u662f\u4e0d\u662f\u5728\u8fd9\u91cc\uff0c\u4ee5\u53ca\u2014\u2014",
    "\u6709\u654c\u65b9\u5355\u4f4d\u63a5\u8fd1\uff0c\u5f71\u50cf\u6bd4\u5bf9\u2014\u2014\u662f\u4e4b\u524d\u90a3\u4e9b\u4eba\u7fa4\uff01",
    "\u6446\u8131\u4ed6\u4eec\uff0c\u6211\u4eec\u53bb\u5750\u6807\u70b9\u9644\u8fd1\u3002|\u514b\u7f57\u4e3d\u79d1\uff0c\u5c3d\u53ef\u80fd\u6536\u96c6\u4fe1\u606f\uff01"
  ]
]