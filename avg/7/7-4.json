[
  [
    "\u654c\u65b9\u5355\u4f4d\u786e\u8ba4\u6e05\u9664\u5b8c\u6bd5\u3002\u95ea\u7535\u5c0f\u961f\uff0c\u56de\u5f52\u57fa\u5730\u8f66\u3002",
    "\u5165\u53e3\u5904\u5f85\u673a\u3002\u7f8e\u73b2\uff0c\u8428\u73c8\u90a3\u8fb9\u6709\u56de\u590d\u5417\uff1f",
    "\u6211\u4e00\u76f4\u5728\u6ce8\u610f\uff0c\u8fd8\u6ca1\u6709\u6536\u5230\u65b0\u4fe1\u606f\u2026\u2026\u5bc7\u5c14\u8299\uff0c\u5230\u5e95\u53bb\u54ea\u91cc\u4e86\u5440\u2026\u2026\u4f1a\u4e0d\u4f1a\u9047\u5230\u5371\u9669\u2026\u2026",
    "\u6ca1\u6709\u51c6\u786e\u5750\u6807\uff0c\u53ea\u6709\u65b9\u5411\uff0c\u6211\u4eec\u5c31\u4e00\u4e2a\u4e00\u4e2a\u5730\u70b9\u53bb\u627e\u3002\u522b\u62c5\u5fc3\u3002|\u7f8e\u73b2\uff0c\u6062\u590d\u884c\u9a76\u901f\u5ea6\u3002",
    "\u597d\u3001\u597d\u7684\uff01\u884c\u9a76\u901f\u5ea6\u5c06\u57280005\u5185\u6062\u590d\u81f390KM/H\uff01",
    "\u54e6\u54e6\u54e6\u3001\u52a0\u901f\u4e86\uff01",
    "\uff08\u4e00\u76f4\u9ecf\u5728\u6211\u8eab\u8fb9\u7684\u6d77\u4f26\u5a1c\u5174\u594b\u5730\u9ad8\u4e3e\u53cc\u624b\uff0c\u7ad9\u4e86\u8d77\u6765\u3002\uff09",
    "\u5750\u597d\u3002",
    "\u5514\u2014\u2014\u5c0f\u77f3\u5934\u2014\u2014",
    "\uff08\u770b\u5230\u6211\u7ec8\u4e8e\u5bf9\u5979\u7684\u52a8\u4f5c\u505a\u51fa\u4e86\u53cd\u5e94\uff0c\u6d77\u4f26\u5a1c\u7acb\u523b\u4e56\u4e56\u5750\u597d\uff0c\u5145\u6ee1\u671f\u5f85\u5730\u76ef\u7740\u6211\u3002\uff09",
    "\u6211\u4eec\u53bb\u627e\u5bc7\u5c14\u8299\u3002",
    "\u54e6\u2014\u2014",
    "\uff08\u6d77\u4f26\u5a1c\u5931\u671b\u5730\u4f4e\u4e0b\u5934\u3002\uff09",
    "\uff08\u770b\u6211\u5e76\u6ca1\u6709\u548c\u5f80\u5e38\u4e00\u6837\u5b89\u6170\u5979\uff0c\u6d77\u4f26\u5a1c\u6241\u4e86\u6241\u5634\uff0c\u4e0d\u60c5\u613f\u5730\u5c06\u89c6\u7ebf\u653e\u5230\u843d\u5730\u7a97\u4e0a\u3002\uff09",
    "\uff08\u52a0\u901f\u884c\u8fdb\u5f00\u59cb\uff0c\u7a97\u5916\u7684\u666f\u8272\u5f00\u59cb\u98de\u5feb\u5730\u5411\u540e\u79fb\u52a8\u3002\uff09",
    "\u6307\u6325\u5b98\uff0c\u5411\u529b\u5f15\u64ce\u867d\u7136\u4e4b\u524d\u6709\u6574\u4f53\u68c0\u4fee\u8fc7\uff0c\u4f46\u8fd8\u662f\u4e0d\u80fd\u7ef4\u6301\u957f\u65f6\u95f4\u9ad8\u529f\u7387\u8fd0\u8f6c\u2026\u2026|\u4e00\u5c0f\u65f6\u540e\uff0c\u884c\u9a76\u901f\u5ea6\u9700\u8981\u964d\u4f4e\u81f330KM/H\uff0c\u5e76\u5f00\u59cb\u6267\u884c\u65f6\u957f\u4e00\u5c0f\u65f6\u7684\u5e94\u6025\u964d\u6e29\u7a0b\u5e8f\uff01",
    "\u55ef\uff0c\u6301\u7eed\u76d1\u63a7\u5f15\u64ce\u72b6\u51b5\u3002|\u95ea\u7535\uff0c\u6709\u6ca1\u6709\u53d1\u73b0\u5bc7\u5c14\u8299\u7684\u76f8\u5173\u7ebf\u7d22\uff1f",
    "\u9014\u7ecf\u7684\u4e24\u4e2a\u5c45\u4f4f\u70b9\u90fd\u5904\u4e8e\u5e9f\u5f03\u72b6\u6001\uff0c\u6ca1\u6709\u53d1\u73b0\u5904\u4e8e\u6d3b\u52a8\u72b6\u6001\u7684\u4e2a\u4f53\u3002",
    "\u2026\u2026\u632f\u7fc5\u800c\u98de\u7684\u9e1f\u7fa4\uff0c\u6620\u4e8e\u6e56\u9762\u7684\u5012\u5f71\u2026\u2026\u788e\u88c2\u2026\u2026",
    "\u2014\u2014\u7eb3\u7f8e\u897f\u4e1d\u8bf4\uff0c\u521a\u624d\u90a3\u7fa4\u751f\u9ab8\u6709\u95ee\u9898\uff01",
    "\u8d85\u6709\u95ee\u9898\u7684\uff01\u7ef4\u666e\u857e\u6362\u4e86\u597d\u51e0\u9996\u6b4c\uff0c\u53ef\u5b83\u4eec\u6bcf\u4e00\u9996\u90fd\u6ca1\u6709\u597d\u597d\u542c\uff01",
    "\u554a\u554a\u6211\u8bf4\u5462\uff0c\u4e71\u4e03\u516b\u7cdf\u7684\u58f0\u97f3\u4e00\u5927\u5806\uff0c\u6211\u90fd\u4ee5\u4e3a\u662f\u751f\u9ab8\u53d8\u5f02\u4e86\u2014\u2014\u7ef4\u666e\u857e\uff0c\u6218\u6597\u7684\u65f6\u5019\u5b89\u9759\u70b9\u3002\u8ddf\u5bc7\u5c14\u8299\u5dee\u4e0d\u591a\u9ad8\u600e\u4e48\u6bd4\u5bc7\u5c14\u8299\u8fd8\u5435\u3002",
    "\u4e71\u3001\u4e71\u4e03\u516b\u7cdf\u2026\u2026",
    "\u90a3\u660e\u660e\u662f\u7ef4\u666e\u857e\u7ec3\u4e60\u4e86\u5f88\u4e45\u7684\u6b4c\uff0c\u514b\u7f57\u4e3d\u79d1\uff0c\u8981\u66f4\u8ba4\u771f\u4e00\u70b9\u542c\u5440\uff01",
    "\u91cd\u70b9\u4e0d\u5bf9\u5427\uff1f\uff01",
    "\u6218\u6597\u4e5f\u8981\u66f4\u8ba4\u771f\u4e00\u70b9\uff0c\u7ef4\u666e\u857e\u3002|\u53e6\u5916\uff0c\u6307\u6325\u5b98\uff0c\u6211\u7684\u611f\u53d7\u4e0e\u7eb3\u7f8e\u897f\u4e1d\u76f8\u540c\uff0c\u521a\u624d\u6218\u6597\u4e2d\uff0c\u6211\u4eec\u6240\u906d\u9047\u7684\u751f\u9ab8\u786e\u5b9e\u6709\u4e9b\u5f02\u5e38\u3002",
    "\u65b0\u7684\u4e9a\u79cd\u3001\u65b0\u7684\u653b\u51fb\u65b9\u5f0f\uff0c\u8fd8\u662f\uff1f",
    "\u5916\u5f62\u548c\u653b\u51fb\u65b9\u5f0f\u6ca1\u6709\u4e0d\u540c\u3002",
    "\u4e0d\u2026\u2026\u8fd9\u79cd\u5f62\u5bb9\u5e76\u4e0d\u8c28\u614e\u2026\u2026\u662f\u2026\u2026\u4f53\u8c8c\u4e0a\u7684\u5f02\u5e38\uff1f",
    "\u2026\u2026\u5b58\u5728\u4e8e\u9ad8\u5927\u7684\u58c1\u5792\u4e2d\uff0c\u6084\u7136\u53d8\u5316\u7684\u79cd\u5b50\u2026\u2026",
    "\u66f4\u591a\u7684\u2026\u2026\u7ed3\u6676\u2026\u2026\u5316\uff1f\u7ed3\u6676\u5316\u7684\u5dee\u5f02\uff1f\u7eb3\u7f8e\u897f\u4e1d\uff0c\u4f60\u662f\u8fd9\u4e2a\u610f\u601d\u5417\uff1f",
    "\uff08\u901a\u8baf\u4e2d\uff0c\u7eb3\u7f8e\u897f\u4e1d\u70b9\u4e86\u70b9\u5934\u3002\uff09",
    "\u786e\u5b9e\u3002",
    "\u751f\u7269\u5728\u627f\u53d7\u8fc7\u91cf\u574d\u584c\u8f90\u5c04\u3001\u611f\u67d3E.L.I.D.\u540e\uff0c\u5982\u679c\u4e0d\u4f7f\u7528\u6297E.L.I.D.\u75ab\u82d7\u6e05\u9664\u4f53\u5185\u7684\u574d\u584c\u7c92\u5b50\uff0c\u5e76\u8f85\u4ee5\u533b\u7597\u624b\u6bb5\u6cbb\u7597\uff0c\u8eab\u4f53\u72b6\u51b5\u4f1a\u9010\u6e10\u6076\u5316\uff0c\u4ee5\u4f53\u8868\u51fa\u73b0\u574d\u584c\u7ed3\u6676\u4e3a\u6807\u5fd7\uff0c\u751f\u7269\u5c06\u6700\u7ec8\u6210\u4e3a\u751f\u9ab8\u3002",
    "\u901a\u5e38\u7684\u7ed3\u6676\u5316\u4f1a\u4ece\u751f\u7269\u5185\u90e8\u5f00\u59cb\uff0c\u4f53\u8868\u51fa\u73b0\u7684\u574d\u584c\u7ed3\u6676\u8d8a\u591a\uff0c\u751f\u9ab8\u7684\u7ed3\u6676\u5316\u7a0b\u5ea6\u8d8a\u9ad8\uff0c\u5bf9\u5e94\u6218\u6597\u529b\u4e5f\u5c31\u8d8a\u5f3a\u3002",
    "\u8fd9\u4e48\u8bf4\u8d77\u6765\uff0c\u521a\u624d\u90a3\u6279\u751f\u9ab8\uff0c\u5916\u9762\u957f\u7684\u7ed3\u6676\u770b\u7740\u771f\u6ca1\u4e4b\u524d\u5e72\u6389\u7684\u90a3\u4e9b\u591a\u3002|\u4e4b\u524d\u6211\u4eec\u603b\u770b\u5230\u7684\u90a3\u4e2a\u53eb\u4ec0\u4e48\u2026\u2026\u6376\u3001\u6376\u5730\u87fe\u870d\uff1f",
    "\u2026\u2026B.R.I.E.F.\u7684\u6ce8\u518c\u540d\u79f0\u662f\u9524\u5ca9\u87fe\u870d\uff0c\u514b\u7f57\u4e3d\u79d1\u3002",
    "\u54e6\u597d\uff0c\u4ee5\u524d\u770b\u5230\u7684\u90a3\u4e2a\u87fe\u870d\u817f\u90a3\u91cc\u5168\u662f\u7ed3\u6676\u4e0d\u80fd\u52a8\u3002\u4eca\u5929\u7684\u8fd9\u51e0\u4e2a\uff0c\u8fd8\u80fd\u8df3\u51e0\u6b65\u2014\u2014\u54e6\uff0c\u5b83\u540d\u5b57\u662f\u4e0d\u662f\u5c31\u662f\u8bf4\u5b83\u8df3\u8d77\u6765\u7684\u65f6\u5019\u53ea\u80fd\u62cd\u5730\u9762\u554a\uff1f",
    "\u662f\u87fe\u870d\u4e4b\u821e\uff01\u7ef4\u666e\u857e\u4e5f\u60f3\u5b66\uff01\u514b\u7f57\u4e3d\u79d1\uff0c\u6a21\u4eff\u52a8\u7269\u7684\u821e\u8e48\u597d\u96be\u7684\u2014\u2014\u4f60\u8981\u662f\u8bb0\u5f97\uff0c\u6559\u6559\u7ef4\u666e\u857e\uff0c\u597d\u4e0d\u597d\uff1f",
    "\u54c8\uff1f\uff01",
    "\u597d\u4e86\u597d\u4e86\uff0c\u6211\u4eec\u76ee\u524d\u6240\u5728\u7684\u4f4d\u7f6e\u8d8a\u6765\u8d8a\u9760\u8fd1\u7ea2\u533a\uff0c\u751f\u9ab8\u4e5f\u4f1a\u6709\u4e00\u5b9a\u7a0b\u5ea6\u7684\u53d8\u5f02\u3002|\u5e94\u8be5\u4e0d\u662f\u4ec0\u4e48\u5927\u95ee\u9898\uff0c\u6301\u7eed\u5173\u6ce8\u60c5\u51b5\u3002",
    "\u6307\u6325\u5b98\uff0c\u524d\u9762\u6709\u4e00\u5904\u5c0f\u578b\u57ce\u5e02\u5e9f\u589f\uff01",
    "\u7ed5\u884c\u3002",
    "\u5443\uff0c\u90a3\u4e2a\u2026\u2026\u6307\u6325\u5b98\uff0c\u8f66\u8f7d\u76d1\u6d4b\u5668\u56de\u4f20\u7684\u70ed\u6210\u50cf\u4fe1\u606f\u663e\u793a\uff0c\u5e9f\u589f\u5185\u6709\u5927\u9762\u79ef\u7684\u533a\u57df\u88ab\u6807\u6ce8\u4e86\u8f83\u9ad8\u6e29\u5ea6\uff0c\u53ef\u80fd\u6709\u4eba\u4f4f\u5728\u8fd9\u91cc\u2026\u2026|\u4f46\u6211\u6bd4\u5bf9\u4e86\u5730\u5f62\u60c5\u62a5\uff0c\u6ca1\u6709\u770b\u5230\u8fd9\u91cc\u662f\u4e2a\u5c45\u4f4f\u70b9\u2026\u2026",
    "\u95ea\u7535\uff0c\u53bb\u6253\u63a2\u4e00\u4e0b\u3002\u6ce8\u610f\u56de\u4f20\u5f71\u50cf\u3002",
    "\u4e86\u89e3\u3002"
  ]
]