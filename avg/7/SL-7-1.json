[
  [
    "\uff08\u591c\u665a\uff0c\u6211\u4eec\u79bb\u5f00\u5730\u4e0b\u6697\u8857\uff0c\u6cbf\u7740\u4e00\u6761\u5c1a\u4e14\u8fd8\u80fd\u591f\u884c\u9a76\u7684\u516c\u8def\uff0c\u5411\u7740\u8428\u73c8\u6240\u5728\u7684\u5c45\u4f4f\u70b9\u524d\u8fdb\u3002\uff09",
    "\uff08\u6cbf\u7740\u8428\u73c8\u6240\u6307\u7684\u8def\uff0c\u827e\u83ab\u53f7\u8d76\u5728\u574d\u584c\u98ce\u66b4\u52a0\u5267\u524d\u79bb\u5f00\u4e86\u98ce\u66b4\u7684\u5f71\u54cd\u8303\u56f4\u3002\uff09",
    "\u54e6\u2014\u2014\u54e6\uff0c\u4f60\u8fd9\u4e2a\u5927\u5bb6\u4f19\u8fd8\u633a\u7075\u6d3b\u3002\u521a\u624d\u574d\u584c\u98ce\u66b4\u8d77\u6765\u5f97\u90a3\u4e48\u5feb\uff0c\u6211\u90fd\u4ee5\u4e3a\u8981\u8eb2\u4e0d\u8fc7\u53bb\u4e86\u3002",
    "\u53ea\u8981\u4e0d\u5728\u574d\u584c\u98ce\u66b4\u4e2d\u505c\u7559\u592a\u957f\u65f6\u95f4\uff0c\u57fa\u5730\u8f66\u5c31\u80fd\u591f\u4fdd\u8bc1\u6211\u4eec\u7684\u5b89\u5168\u3002",
    "\u8bf4\u771f\u7684\uff0c\u4f60\u771f\u4e0d\u6253\u7b97\u5356\u4e86\u5b83\uff1f",
    "\u5356\u4e86\u5b83\uff0c\u53bb\u7eff\u533a\uff1f",
    "\u4e70\u4e2a\u5c0f\u4e00\u70b9\u7684\uff0c\u4e0d\u662f\u4e5f\u80fd\u7528\uff1f",
    "\u591a\u51fa\u6765\u7684\u94b1\uff0c\u53bb\u4e0d\u4e86\u7eff\u533a\uff0c\u8fd8\u80fd\u627e\u4e2a\u5c45\u4f4f\u70b9\u91cc\u7684\u5efa\u7b51\u6539\u9020\u4e00\u4e0b\uff0c\u5954\u6ce2\u4e45\u4e86\u8fd8\u80fd\u6709\u4e2a\u77ed\u671f\u5b89\u987f\u7684\u5730\u65b9\u3002\u6211\u4eec\u7684\u5c45\u4f4f\u70b9\u5c31\u5f88\u6b22\u8fce\u50cf\u4f60\u8fd9\u6837\u7684\u2014\u2014",
    "\u8d22\u4e3b\uff1f",
    "\u6211\u53ef\u6ca1\u8fd9\u4e48\u8bf4\u3002",
    "\u4f60\u4eec\u7684\u5c45\u4f4f\u70b9\uff0c\u662f\u4e0d\u662f\u5feb\u5230\u4e86\uff1f",
    "\u5bf9\uff0c\u524d\u9762\u90a3\u5806\u77f3\u5934\u7684\u4f4d\u7f6e\u53f3\u8f6c\uff0c\u9a6c\u4e0a\u5c31\u5230\u4e86\u3002",
    "\u53f3\u8f6c\uff0c\u95ea\u7535\u3002",
    "\u4e86\u89e3\u3002",
    "\uff08\u968f\u7740\u827e\u83ab\u53f7\u7684\u8f6c\u5411\uff0c\u4e00\u5904\u7531\u5e9f\u5f03\u5efa\u7b51\u56f4\u8d77\u6765\u7684\u534a\u5730\u4e0b\u5f0f\u8857\u533a\u9010\u6e10\u51fa\u73b0\u5728\u6211\u4eec\u89c6\u91ce\u524d\u65b9\u3002\uff09",
    "\u5443\uff0c\u90a3\u4e2a\u2026\u2026\u7ef4\u666e\u857e\u90a3\u91cc\u6ca1\u95ee\u9898\u5427\uff1f",
    "\u4eba\u5f62\u6743\u9650\u5df2\u7ecf\u8f6c\u79fb\uff0c\u7f8e\u73b2\u6b63\u5728\u5e2e\u5979\u68c0\u4fee\u7d20\u4f53\u3002\u68c0\u4fee\u5b8c\u6210\u540e\uff0c\u4f1a\u5e26\u5979\u719f\u6089\u4e00\u4e0b\u57fa\u5730\u8f66\u3002",
    "\u201c\u7f8e\u73b2\u201d\uff0c\u5c31\u662f\u4f60\u4fe1\u8d56\u7684\u673a\u68b0\u5e08\uff1f",
    "\u55ef\u3002",
    "\u4e0d\u53ea\u662f\u673a\u68b0\u5e08\u5427\u3002\u770b\u6837\u5b50\uff0c\u8fd8\u627f\u62c5\u4e86\u4e0d\u5c11\u5176\u5b83\u5de5\u4f5c\uff1f",
    "\u55ef\uff0c\u5979\u8fd8\u662f\u6211\u975e\u5e38\u5f97\u529b\u7684\u540e\u52e4\u5b98\u2026\u2026|\u55ef\u2026\u2026\u5982\u679c\u53ef\u4ee5\uff0c\u6211\u5e0c\u671b\u80fd\u5728\u4f60\u8fd9\u91cc\u505c\u7559\u4e00\u6bb5\u65f6\u95f4\u3002",
    "\u975e\u5e38\u6b22\u8fce\u3002\u539f\u56e0\u662f\uff1f",
    "\u6211\u4eec\u63a5\u4e0b\u6765\u6709\u4e00\u8d9f\u957f\u9014\u65c5\u884c\uff0c\u5728\u6b64\u4e4b\u524d\uff0c\u6211\u5e0c\u671b\u6211\u7684\u673a\u68b0\u5e08\u517c\u540e\u52e4\u5b98\u80fd\u591f\u597d\u597d\u4f11\u606f\u4e00\u4e0b\u3002|\u65e2\u7136\u662f\u5c45\u4f4f\u70b9\uff0c\u6709\u6ca1\u6709\u4ec0\u4e48\u63a8\u8350\u7684\u6e38\u4e50\u9879\u76ee\uff1f",
    "\u6211\u4eec\u8fd9\u91cc\u53ef\u4e0d\u662f\u4ec0\u4e48\u5927\u578b\u7684\u5c45\u4f4f\u70b9\u3002",
    "\u6e38\u4e50\u9879\u76ee\u2026\u2026\u55ef\uff0c\u5c45\u4f4f\u70b9\u897f\u5317\u65b9\u5411\u6709\u4e2a\u4e0d\u7b97\u77ee\u7684\u5c71\u4e18\uff0c\u5b69\u5b50\u4eec\u5076\u5c14\u4f1a\u53bb\u90a3\u91cc\u6ed1\u6c99\u3002|\u6216\u8005\uff0c\u4f60\u90a3\u4f4d\u53eb\u201c\u7f8e\u73b2\u201d\u7684\u673a\u68b0\u5e08\u517c\u526f\u5b98\u4f1a\u66f4\u559c\u6b22\u53bb\u5730\u4e0b\u6e29\u5ba4\u5e2e\u5fd9\u91c7\u6458\u6c34\u679c\u2026\u2026\u548c\u7ef4\u4fee\u95e8\u7a97\u6253\u626b\u8857\u9053\u76f8\u6bd4\uff0c\u5e94\u8be5\u4e5f\u7b97\u662f\u6e38\u4e50\u9879\u76ee\uff1f",
    "\u2026\u2026\u8fd9\u53ef\u7b97\u4e0d\u4e0a\u662f\u6e38\u4e50\u9879\u76ee\u5427\u3002|\u4f60\u4eec\u8fd9\u91cc\u6709\u6c34\u679c\uff1f\u4ef7\u683c\u662f\uff1f",
    "\u5982\u679c\u662f\u4f60\u4e70\uff0c\u4f1a\u7ed9\u4f60\u53cb\u60c5\u6298\u6263\uff0c\u6bd4\u5176\u4ed6\u6697\u5546\u90a3\u91cc\u4fbf\u5b9c\u4e0d\u5c11\u3002\u7a0d\u540e\u6211\u53bb\u786e\u8ba4\u4e00\u4e0b\u3002",
    "\u597d\u7684\u3002\u91c7\u6458\u5c31\u4e0d\u7528\u4e86\uff0c\u9ebb\u70e6\u4f60\u786e\u8ba4\u4e0b\u4ef7\u683c\u3002",
    "\u4e0d\u9ebb\u70e6\u4e0d\u9ebb\u70e6\u3002\u6211\u4eec\u79cd\u4e86\u4e5f\u662f\u8981\u5356\u51fa\u53bb\uff0c\u8fd9\u4e0b\u7701\u4e86\u8fd0\u8f93\u7684\u635f\u8017\uff0c\u5bf9\u5927\u5bb6\u90fd\u662f\u597d\u4e8b\u3002|\u5728\u6c61\u67d3\u533a\u8d2d\u4e70\u6c34\u679c\uff0c\u53ef\u771f\u662f\u4e00\u7b14\u4e0d\u5c0f\u7684\u5f00\u9500\u3002\u63d0\u524d\u611f\u8c22\u60e0\u987e\u4e86\uff0c\u6307\u6325\u5b98\u3002",
    "\u5e0c\u671b\u4f60\u7684\u611f\u8c22\u80fd\u591f\u4f53\u73b0\u5728\u6298\u6263\u4e0a\u3002\u5c45\u4f4f\u70b9\uff0c\u5c31\u662f\u8fd9\u91cc\uff1f",
    "\u54e6\u5bf9\u5bf9\u3002\u8fd9\u4e48\u5feb\u5c31\u5230\u4e86\u554a\u2026\u2026\u6307\u6325\u5b98\uff0c\u6211\u4eec\u4e00\u8d77\u4e0b\u53bb\uff1f",
    "\u57fa\u5730\u8f66\u8fd8\u6709\u4e00\u4e9b\u4e8b\u9879\u9700\u8981\u5904\u7406\uff0c\u51e0\u5206\u949f\uff0c\u6211\u7a0d\u540e\u5c31\u5230\u3002",
    "\u884c\uff0c\u6211\u5728\u2026\u2026\u7b97\u4e86\uff0c\u5c45\u4f4f\u70b9\u8def\u7ebf\u590d\u6742\uff0c\u6211\u5728\u57fa\u5730\u8f66\u5165\u53e3\u7b49\u4f60\u3002",
    "\u95ea\u7535\uff0c\u7eb3\u7f8e\u897f\u4e1d\u548c\u514b\u7f57\u4e3d\u79d1\u5462\uff1f",
    "\u6b63\u5728\u4f11\u606f\u5ba4\u3002",
    "\u8bf7\u5979\u4eec\u9001\u8428\u73c8\u53bb\u57fa\u5730\u8f66\u5165\u53e3\u3002|\u8428\u73c8\uff0c\u8bf7\u7a0d\u7b49\u3002",
    "\u8c22\u4e86\uff0c\u4f60\u8fd9\u4e2a\u57fa\u5730\u8f66\u91cc\u9762\u592a\u590d\u6742\uff0c\u8981\u9760\u6211\u81ea\u5df1\uff0c\u53ef\u771f\u662f\u627e\u4e0d\u5230\u3002|\u961f\u957f\u5c0f\u59d0\uff0c\u5979\u4eec\u8981\u591a\u4e45\uff1f",
    "\u4e09\u5206\u949f\u5185\u3002",
    "\u6211\u53bb\u6307\u6325\u5ba4\u95e8\u53e3\u7b49\u5979\u4eec\u3002\u6307\u6325\u5b98\uff0c\u4e00\u4f1a\u89c1\u3002",
    "\u597d\u3002",
    "\u7a81\u7136\u63d0\u51fa\u8d2d\u4e70\u6c34\u679c\u4ee5\u53ca\u77ed\u671f\u505c\u7559\uff0c\u6307\u6325\u5b98\uff0c\u8fd9\u662f\u8981\u505a\u4ec0\u4e48\uff1f",
    "\u4e4b\u524d\u7b54\u5e94\u7f8e\u73b2\u7684\uff0c\u8ba9\u7f8e\u73b2\u4f11\u606f\u4e00\u4e0b\u2026\u2026\u7136\u540e\uff0c\u6b22\u8fce\u4f1a\u3002|\u95ea\u7535\uff0c\u9ebb\u70e6\u4f60\u548c\u7f8e\u73b2\u7b79\u5907\u4e00\u4e0b\u5bc7\u5c14\u8299\u548c\u7ef4\u666e\u857e\u7684\u6b22\u8fce\u4f1a\u2026\u2026\u554a\uff0c\u5bf9\uff0c\u8fd8\u6709\u6d77\u4f26\u5a1c\u3002",
    "\u5bc7\u5c14\u8299\u662f\u4e0d\u662f\u559c\u6b22\u5de7\u514b\u529b\uff1f\u7ef4\u666e\u857e\u559c\u6b22\u4ec0\u4e48\u8fd8\u4e0d\u6e05\u695a\u2026\u2026\u4ed3\u5e93\u91cc\u5e94\u8be5\u6709\u4e9b\u50a8\u5907\uff0c\u7b49\u4e0b\u518d\u4e70\u70b9\u6c34\u679c\uff0c\u5e94\u8be5\u8fd8\u591f\u7528\uff1f",
    "\u4eba\u5f62\u8fdb\u98df\u540e\u7684\u9488\u5bf9\u6027\u5904\u7406\u9700\u8981\u82b1\u8d39\u4e0d\u5c11\u65f6\u95f4\u3002|\u4f9d\u7167\u5f80\u5e38\u6b22\u8fce\u4f1a\u7684\u7b79\u5907\u60c5\u51b5\uff0c\u51c6\u8bb8\u5bc7\u5c14\u8299\u548c\u7ef4\u666e\u857e\u63d0\u51fa\u4e00\u5b9a\u989d\u5ea6\u7684\u91c7\u8d2d\u7533\u8bf7\uff0c\u5e76\u989d\u5916\u7ed9\u6d77\u4f26\u5a1c\u51c6\u5907\u4e00\u4e9b\u751c\u54c1\uff0c\u4e0d\u662f\u66f4\u52a0\u5408\u9002\u5417\uff0c\u6307\u6325\u5b98\uff1f",
    "\u2026\u2026\u4e0a\u6b21\u6211\u4eec\u827e\u83ab\u53f7\u4e0a\u8fd9\u4e48\u591a\u6210\u5458\uff0c\u662f\u4ec0\u4e48\u65f6\u5019\u4e86\uff1f",
    "22\u4e2a\u6708\u524d\uff1f\u6b64\u540e\uff0c\u4eba\u5458\u6301\u7eed\u7f29\u51cf\uff0c\u6700\u7ec8\u57fa\u5730\u8f66\u4ee5\u6211\u4e0e\u7eb3\u7f8e\u897f\u4e1d\u7684\u4e24\u4eba\u6218\u6597\u7f16\u5236\u4ece2073\u5e745\u67083\u65e5\u8fd0\u4f5c\u81f32074\u5e741\u67084\u65e5\uff0c\u540c\u5e744\u670814\u65e5\uff0c\u514b\u7f57\u4e3d\u79d1\u6b63\u5f0f\u52a0\u5165\u3002",
    "\u6211\u4e0d\u662f\u5728\u95ee\u8fd9\u4e2a\uff0c\u95ea\u7535\u3002\u5bc7\u5c14\u8299\u7684\u60c5\u7eea\u4e0d\u592a\u5bf9\u3002",
    "\u2026\u2026\u55ef\u3002",
    "\u867d\u7136\u4e0d\u77e5\u9053\u5bc7\u5c14\u8299\u5177\u4f53\u7ecf\u5386\u4e86\u4ec0\u4e48\uff0c\u4f46\u4ee5\u5979\u5bf9\u7b2c\u5341\u4e00\u533b\u62a4\u9632\u5316\u8425\u4ee5\u53ca\u74e6\u826f\u683c\u5e2e\u7684\u6267\u8457\uff0c\u6211\u5927\u81f4\u4e5f\u80fd\u731c\u5230\u2026\u2026",
    "\u7b2c\u5341\u4e00\u533b\u62a4\u9632\u5316\u8425\u906d\u5230\u4e86\u74e6\u826f\u683c\u5e2e\u88ad\u51fb\u3002\u7f57\u8054\u7f16\u5236\u7684\u4eba\u5f62\u53ea\u6709\u7f16\u53f7\uff0c\u7ed9\u4e86\u5bc7\u5c14\u8299\u201c\u5bc7\u5c14\u8299\u201d\u8fd9\u4e2a\u540d\u5b57\u7684\u4eba\uff0c\u5728\u88ad\u51fb\u4e2d\u6b7b\u5728\u74e6\u826f\u683c\u5e2e\u624b\u91cc\u3002",
    "\uff08\u95ea\u7535\u7565\u5fae\u79fb\u5f00\u4e86\u89c6\u7ebf\u3002\uff09",
    "\u4f5c\u4e3a\u6218\u672f\u4eba\u5f62\uff0c\u5bc7\u5c14\u8299\u9700\u8981\u66f4\u52a0\u5173\u6ce8\u5f53\u524d\u7684\u6218\u6597\u5c40\u9762\u53ca\u654c\u6211\u529b\u91cf\u5bf9\u6bd4\uff0c\u8fd9\u662f\u6211\u4f5c\u4e3a\u961f\u957f\u505a\u51fa\u7684\u5224\u65ad\u3002",
    "\u5bc7\u5c14\u8299\u8fd8\u9700\u8981\u65f6\u95f4\uff0c\u95ea\u7535\u3002",
    "\u4eba\u5f62\u548c\u4eba\u7c7b\u4e0d\u540c\uff0c\u5916\u8868\u5e74\u9f84\u5e76\u4e0d\u80fd\u4f5c\u4e3a\u5fc3\u7406\u5e74\u9f84\u7684\u53c2\u8003\u3002|\u4e0d\u7ba1\u88ab\u9884\u8bbe\u4e86\u4ec0\u4e48\u6837\u7684\u5fc3\u667a\uff0c\u7ec8\u7a76\u9700\u8981\u5728\u65f6\u95f4\u7684\u6d41\u901d\u4e2d\u6210\u957f\u3002\u7a81\u7136\u9047\u5230\u4e4b\u524d\u6ca1\u7ecf\u5386\u8fc7\u7684\u4e8b\u60c5\uff0c\u53ea\u8981\u6ca1\u9020\u6210\u592a\u4e25\u91cd\u7684\u540e\u679c\uff0c\u7a0d\u5fae\u4efb\u6027\u4e00\u70b9\uff0c\u4e5f\u4e0d\u662f\u4ec0\u4e48\u5927\u95ee\u9898\u3002",
    "\u533b\u7597\u4eba\u5f62\u662f\u56e2\u961f\u6301\u7eed\u4f5c\u6218\u7684\u4fdd\u969c\uff0c\u5bc7\u5c14\u8299\u66f4\u5e94\u8be5\u2014\u2014",
    "\u548c\u4e4b\u524d\u7684\u6210\u5458\u4eec\u6bd4\uff0c\u5bc7\u5c14\u8299\u4eca\u5929\u7684\u884c\u4e3a\u4e5f\u6ca1\u6709\u591a\u8fc7\u5206\u3002\u5f53\u65f6\u4f60\u90fd\u6ca1\u4ec0\u4e48\u7279\u522b\u53cd\u5e94\uff0c\u4eca\u5929\u8fd9\u662f\u600e\u4e48\u4e86\uff1f\u96be\u9053\u662f\u56e0\u4e3a\u6211\u4eec\u827e\u83ab\u53f7\u597d\u4e45\u6ca1\u6709\u8fd9\u4e48\u591a\u6210\u5458\u4e86\uff0c\u4e0d\u592a\u4e60\u60ef\uff1f",
    "\u2026\u2026\u4e5f\u8bb8\u662f\u5427\u3002",
    "\u90a3\u5c31\u7528\u4eba\u7c7b\u7684\u65b9\u5f0f\u9002\u5e94\u548c\u653e\u677e\u4e00\u4e0b\u3002|\u827e\u83ab\u53f7\u7684\u5b89\u5371\u548c\u6b22\u8fce\u4f1a\u7684\u524d\u671f\u51c6\u5907\u5c31\u4ea4\u7ed9\u514b\u7f57\u4e3d\u79d1\u548c\u7eb3\u7f8e\u897f\u4e1d\uff0c\u5e26\u4e0a\u4f5c\u4e3a\u4e3b\u89d2\u7684\u7ef4\u666e\u857e\u548c\u5bc7\u5c14\u8299\uff0c\u6211\u4eec\u53bb\u5c45\u4f4f\u70b9\u770b\u770b\uff1f",
    "\u7b49\u6211\u4eec\u8c08\u597d\u4e86\u4ef7\u683c\uff0c\u7f8e\u73b2\u90a3\u8fb9\u7684\u62df\u6001\u56fe\u8c31\u88c5\u8f7d\u8bbe\u5907\u8c03\u8bd5\u5de5\u4f5c\u4e5f\u5dee\u4e0d\u591a\u80fd\u7ed3\u675f\u4e86\u3002|\u4e00\u8d77\u4e70\u70b9\u6c34\u679c\uff0c\u4e00\u8d77\u56de\u57fa\u5730\u8f66\uff0c\u4e00\u8d77\u628a\u6b22\u8fce\u4f1a\u7684\u5269\u4f59\u5de5\u4f5c\u505a\u5b8c\uff0c\u4e00\u8d77\u70ed\u70ed\u95f9\u95f9\u5e86\u795d\u4e00\u4e0b\u987a\u4fbf\u7ed9\u5927\u5bb6\u4ecb\u7ecd\u65b0\u6210\u5458\uff0c\u518d\u4e00\u8d77\u53bb\u9001\u6d77\u4f26\u5a1c\uff1f",
    "\u2014\u2014\u5f53\u7136\uff0c\u5bc7\u5c14\u8299\u4e0d\u5728\u8f66\u4e0a\u7684\u65f6\u5019\uff0c\u6d77\u4f26\u5a1c\u5c31\u53ea\u80fd\u62dc\u6258\u514b\u7f57\u4e3d\u79d1\u4e86\uff0c\u5e0c\u671b\u5979\u80fd\u591f\u575a\u6301\u4f4f\u3002",
    "\u2026\u2026",
    "\uff08\u4e5f\u8bb8\u662f\u60f3\u8d77\u6765\u4ee5\u524d\u7ecf\u5386\u8fc7\u7684\u68d8\u624b\u4e8b\u4ef6\uff0c\u4e5f\u8bb8\u662f\u6211\u7684\u8bdd\u8d77\u4e86\u4f5c\u7528\uff0c\u95ea\u7535\u7565\u663e\u7a81\u5140\u5730\u5e73\u9759\u4e0b\u6765\u3002\uff09",
    "\uff08\u5979\u7ef4\u6301\u7ad9\u7acb\u7684\u59ff\u52bf\uff0c\u5b89\u9759\u5730\u505c\u5728\u539f\u5730\u3002\uff09",
    "\u4e45\u8fdd\u7684\u82f9\u679c\u6d3e\uff0c\u5b89\u6392\u4e00\u4e0b\u5417\uff0c\u6211\u7684\u961f\u957f\uff1f",
    "\u2026\u2026\u5bf9\u4e0d\u8d77\uff0c\u6307\u6325\u5b98\uff0c\u8bf7\u91cd\u590d\u4e00\u904d\uff1f",
    "\u5443\uff0c\u5982\u679c\u82f9\u679c\u6d3e\u4e0d\u884c\uff0c\u676f\u5b50\u86cb\u7cd5\u4e5f\u53ef\u4ee5\u2026\u2026\u5f53\u7136\u8c46\u5b50\u6c64\u4e5f\u6ca1\u95ee\u9898\uff1f",
    "\u597d\u7684\u3002",
    "\u90a3\u5c31\u51fa\u53d1\u5427\u3002"
  ],
  [
    "\uff08\u5341\u4e94\u5206\u949f\u540e\u2014\u2014\uff09",
    "\uff08\u79bb\u5f00\u827e\u83ab\u53f7\uff0c\u8428\u73c8\u5e26\u7740\u6211\u3001\u95ea\u7535\u3001\u7ef4\u666e\u857e\u4e00\u540c\u8d70\u5411\u4e0d\u8fdc\u5904\u7684\u534a\u5730\u4e0b\u5f0f\u8857\u533a\uff0c\u5bc7\u5c14\u8299\u8ddf\u5728\u6211\u4eec\u540e\u9762\u3002\uff09",
    "\uff08\u4e24\u4fa7\u51cc\u4e71\u7684\u5efa\u7b51\u7ec4\u6210\u5c4f\u969c\uff0c\u5c06\u8857\u533a\u4e0e\u5916\u56f4\u7684\u9ec4\u6c99\u9694\u5f00\u3002\u5185\u90e8\u9053\u8def\u9519\u7efc\u590d\u6742\uff0c\u6211\u4eec\u4e24\u4fa7\u9010\u6e10\u51fa\u73b0\u4eba\u6d41\u3002\uff09",
    "\uff08\u867d\u7136\u5df2\u7ecf\u5230\u4e86\u665a\u4e0a\uff0c\u8fd9\u4e2a\u5c45\u4f4f\u70b9\u5185\u5374\u5b8c\u5168\u6ca1\u6709\u5b89\u9759\u4e0b\u6765\u7684\u8ff9\u8c61\u3002\uff09",
    "\u6211\u6765\u5f97\u665a\uff0c\u4f4f\u7684\u5730\u65b9\u9760\u8fb9\uff0c\u8fd8\u9700\u8981\u8d70\u4e00\u6bb5\u8def\u3002|\u6e29\u5ba4\u90a3\u8fb9\u6211\u5728\u7b49\u4f60\u4eec\u7684\u65f6\u5019\u5df2\u7ecf\u8054\u7cfb\u8fc7\u4e86\uff0c\u50cf\u4f60\u8fd9\u6837\u7684\u5927\u5ba2\u6237\uff0c\u4ed6\u4eec\u975e\u5e38\u6b22\u8fce\u3002",
    "\u4ef7\u683c\u786e\u5b9a\u8fc7\u4e86\u5417\uff1f",
    "\u8fd9\u5f53\u7136\u51b3\u5b9a\u4e8e\u4f60\u8981\u4e70\u4ec0\u4e48\u3002|\u4e0d\u8fc7\u770b\u521a\u624d\u7684\u4e91\uff0c\u660e\u5929\u5e94\u8be5\u4f1a\u6709\u574d\u584c\u98ce\u66b4\uff0c\u5927\u5bb6\u90fd\u5728\u5fd9\u7740\u52a0\u56fa\u5efa\u7b51\uff0c\u6e29\u5ba4\u8fd9\u4f1a\u6ca1\u4eba\u3002\u7b49\u574d\u584c\u98ce\u66b4\u8fc7\u4e86\u518d\u53bb\u6bd4\u8f83\u597d\u3002",
    "\u574d\u584c\u98ce\u66b4\uff1f\u5c45\u4f4f\u70b9\u7684\u9009\u5740\u4e00\u822c\u4e0d\u662f\u4f1a\u9009\u62e9\u907f\u5f00\u574d\u584c\u98ce\u66b4\uff1f",
    "\u8fd8\u662f\u51c0\u5316\u5de5\u7a0b\u7684\u95ee\u9898\u3002|\u51c0\u5316\u533a\u6301\u7eed\u5411\u6c61\u67d3\u533a\u63a8\u8fdb\uff0c\u6c61\u67d3\u533a\u672c\u8eab\u7684\u72b6\u51b5\u4e5f\u5728\u6076\u5316\uff0c\u9002\u5408\u5efa\u8bbe\u6210\u5c45\u4f4f\u70b9\u7684\u5e9f\u589f\u8d8a\u6765\u8d8a\u5c11\u2026\u2026\u50cf\u6211\u4eec\u8fd9\u79cd\u8fdb\u4e0d\u4e86\u51c0\u5316\u533a\u7684\uff0c\u53ea\u80fd\u9009\u8fd9\u79cd\u4e5f\u7b97\u80fd\u4f4f\u7684\u5730\u65b9\u4e86\u3002",
    "\u574d\u584c\u98ce\u66b4\u7684\u51fa\u73b0\u4e0d\u7b97\u9891\u7e41\uff0c\u91cd\u70b9\u662f\u6ca1\u6709\u7f57\u8054\u90a3\u8fb9\u7684\u4eba\u8fc7\u6765\u2014\u2014\u542c\u8d77\u6765\u4e0d\u662f\u975e\u5e38\u4e0d\u9519\u5417\uff1f",
    "\u7f57\u8054\u90a3\u8fb9\u7684\u4eba\uff1f",
    "\u554a\u54c8\u2014\u2014\u751f\u6d3b\u5728\u6c61\u67d3\u533a\uff0c\u90fd\u81ea\u7531\u60ef\u4e86\uff0c\u8ba9\u7f57\u8054\u7684\u4eba\u6765\u7ba1\u4e0d\u60c5\u613f\u5427\uff1f",
    "\u7f57\u8054\u600e\u4e48\u4f1a\u7ba1\u6211\u4eec\u6b7b\u6d3b\uff1f\u662f\u56e0\u4e3a\u5dee\u4e0d\u591a\u534a\u5e74\u524d\uff0c\u4f4f\u5728\u9644\u8fd1\u4e00\u4e2a\u5c45\u4f4f\u70b9\u7684\u4eba\u9047\u5230\u4e86\u7f57\u8054\u7684\u4e00\u4e2a\u533b\u7597\u961f\uff0c\u574d\u584c\u98ce\u66b4\u6765\u7684\u65f6\u5019\u88ab\u5360\u4e86\u623f\u5b50\uff0c\u5168\u6b7b\u4e86\u3002|\u4ece\u90a3\u4ee5\u540e\uff0c\u6211\u4eec\u4f4f\u5728\u9644\u8fd1\u7684\u8fd9\u4e9b\u4eba\u9047\u5230\u548c\u7f57\u8054\u76f8\u5173\u7684\u90fd\u8981\u7ed5\u7740\u8d70\u3002",
    "\u2026\u2026\uff01",
    "\uff08\u5bc7\u5c14\u8299\u9aa4\u7136\u6fc0\u52a8\u8d77\u6765\u3002\uff09",
    "\uff08\u5979\u505c\u4e0b\u811a\u6b65\uff0c\u8eab\u4f53\u7d27\u7ef7\uff0c\u63e1\u7d27\u4e86\u53cc\u624b\uff0c\u8fd1\u4e4e\u4ec7\u6068\u5730\u76ef\u7740\u8428\u73c8\u3002\uff09",
    "\u5443\uff0c\u600e\u4e48\u4e86\uff1f",
    "\u5bc7\u5c14\u8299\uff0c\u751f\u6c14\u4e86\uff1f",
    "\u8981\u4e0d\u8981\u542c\u4e00\u542c\u7ef4\u666e\u857e\u7684\u65b0\u6b4c\u5440\uff1f\u4e3b\u9898\u662f\u5927\u6d77\u54e6\uff0c\u548c\u5bc7\u5c14\u8299\u7684\u5934\u53d1\u4e00\u6837\uff01|\u521a\u624d\u5728\u8def\u4e0a\u7ec3\u4e60\u4e86\u597d\u4e45\uff0c\u7ef4\u666e\u857e\u2606\u8d85\u52aa\u529b\u7684\uff01",
    "\u2026\u2026",
    "\u90a3\u7ef4\u666e\u857e\u5c31\u8981\u5f00\u59cb\u6f14\u51fa\u5566\uff0c\u4e3b\u4eba\u4e5f\u8981\u8ba4\u771f\u542c\uff01",
    "\u7b49\u7b49\uff0c\u4e3b\u4eba\uff1f\u4f60\u5728\u53eb\u6211\uff1f",
    "\uff08\u6211\u6b63\u6ce8\u610f\u7740\u5bc7\u5c14\u8299\u7684\u53cd\u5e94\uff0c\u7a81\u7136\u88ab\u7ef4\u666e\u857e\u53eb\u5230\uff0c\u4e00\u65f6\u6ca1\u6709\u53cd\u5e94\u8fc7\u6765\u3002\uff09",
    "\u4e3b\u4eba\u4e0d\u559c\u6b22\u8fd9\u4e2a\u79f0\u547c\u5417\uff1f\u8428\u73c8\u4e5f\u4e0d\u559c\u6b22\u2026\u2026\u90a3\u7ef4\u666e\u857e\u53eb\u4f60\u7684\u540d\u5b57\u597d\u4e0d\u597d\uff0c{}\uff1f",
    "\u5443\uff0c\u53eb\u6211\u6307\u6325\u5b98\u5c31\u884c\u4e86\u3002",
    "\u597d\u7684\u2606\u6307\u6325\u5b98\uff01\u7ef4\u666e\u857e\u8981\u5f00\u59cb\u4e86\uff01",
    "\u5514\u5514\u2014\u2014\u554a\uff01",
    "\uff08\u4e00\u65c1\u7684\u5c0f\u8def\u4e2d\u7a81\u7136\u7a9c\u51fa\u6765\u4e00\u4e2a\u8863\u7740\u6b8b\u7834\u3001\u5168\u8eab\u810f\u4e71\u3001\u515c\u5e3d\u906e\u4f4f\u534a\u5f20\u8138\u3001\u51e0\u4e4e\u770b\u4e0d\u51fa\u957f\u76f8\u7684\u4eba\u3002\uff09",
    "\uff08\u4f3c\u4e4e\u56e0\u4e3a\u5de6\u811a\u60a3\u6709\u67d0\u79cd\u75be\u75c5\uff0c\u4ed6\u5728\u884c\u8d70\u65f6\u8eab\u4f53\u5411\u5de6\u4fa7\u503e\u659c\uff0c\u5306\u5fd9\u95f4\u649e\u4e0a\u4e86\u7ef4\u666e\u857e\uff0c\u95ea\u7535\u4f38\u624b\u6276\u4e86\u4e00\u4e0b\u7ef4\u666e\u857e\u3002\uff09",
    "\u5c0f\u5fc3\u3002",
    "\u554a\uff0c\u8c22\u8c22\u961f\u957f\u2606",
    "\uff08\u7a81\u7136\u51fa\u73b0\u7684\u4eba\u5728\u649e\u4e0a\u7ef4\u666e\u857e\u540e\uff0c\u52a8\u4f5c\u50f5\u786c\u5730\u505c\u5728\u7ef4\u666e\u857e\u9762\u524d\uff0c\u5e76\u6ca1\u6709\u5c06\u5e94\u8be5\u662f\u9762\u90e8\u7684\u4f4d\u7f6e\u8f6c\u5411\u7ef4\u666e\u857e\u3002\uff09",
    "\uff08\u4ed6\u7ef4\u6301\u6b64\u524d\u7684\u671d\u5411\uff0c\u795e\u60c5\u6728\u8bb7\u3002\uff09",
    "\u4f0a\u9ed8\u5c14\uff0c\u4f0a\u9ed8\u5c14\u5927\u4eba\u6765\u4e86\u2026\u2026\u8981\u3001\u8981\u4f0a\u9ed8\u5c14\u2026\u2026",
    "\u662f\u8d6b\u745e\u5c24\uff01\u4eca\u5929\u8eab\u4f53\u597d\u4e00\u4e9b\u5566\uff0c\u80fd\u51fa\u95e8\u4e86\uff01",
    "\u5514\u2026\u2026\u8d6b\u745e\u5c24\u2026\u2026\u80fd\u51fa\u95e8\uff0c\u4f46\u662f\u4e0d\u5f00\u5fc3\uff1f",
    "\uff08\u7ef4\u666e\u857e\u8ba4\u771f\u67e5\u770b\u4e86\u4f30\u7b97\u51fa\u7684\u8d6b\u745e\u5c24\u7684\u60c5\u7eea\u6570\u503c\uff0c\u975e\u5e38\u4e25\u8083\u5730\u5411\u8d6b\u745e\u5c24\u63d0\u95ee\u3002\uff09",
    "\uff08\u8d6b\u745e\u5c24\u5e76\u6ca1\u6709\u56de\u7b54\u7ef4\u666e\u857e\u3002\uff09",
    "\u2026\u2026\u8f90\u5c04\u2026\u2026\u6069\u8d50\u2026\u2026\u4f0a\u9ed8\u5c14\u6765\u4e86\uff0c\u4f0a\u9ed8\u5c14\u5927\u4eba\u6765\u4e86\u2026\u2026\u6551\u8d4e\u2026\u2026\u8bf7\u6551\u8d4e\u6211\u2026\u2026",
    "\uff08\u8fd9\u4e2a\u540d\u4e3a\u201c\u8d6b\u745e\u5c24\u201d\u7684\u4eba\u770b\u8d77\u6765\u5b8c\u5168\u6ca1\u6709\u5bf9\u8428\u73c8\u548c\u7ef4\u666e\u857e\u505a\u51fa\u4efb\u4f55\u53cd\u5e94\u3002\uff09",
    "\uff08\u8428\u73c8\u6320\u4e86\u6320\u5934\uff0c\u4ece\u968f\u8eab\u643a\u5e26\u7684\u884c\u674e\u4e2d\u62ff\u51fa\u4e00\u4e2a\u8865\u7ed9\u5305\uff0c\u585e\u5230\u8d6b\u745e\u5c24\u6000\u4e2d\u3002\uff09",
    "\u8fd9\u4e2a\u7ed9\u4f60\uff0c\u91cc\u9762\u6709\u8001\u9f20\u5e72\u548c\u5e72\u51c0\u7684\u6c34\uff0c\u8db3\u591f\u4f60\u7528\u4e09\u5929\uff0c\u62ff\u597d\u4e86\u3002",
    "\uff08\u88ab\u52a8\u800c\u673a\u68b0\u5730\u63a5\u53d7\u8428\u73c8\u585e\u8fc7\u6765\u7684\u8865\u7ed9\u5305\uff0c\u8d6b\u745e\u5c24\u8f6c\u53d8\u4e86\u884c\u52a8\u65b9\u5411\uff0c\u5411\u7740\u5c45\u4f4f\u70b9\u7684\u5165\u53e3\u65b9\u5411\u79bb\u5f00\u3002\uff09",
    "\uff08\u76ee\u9001\u8d6b\u745e\u5c24\u8e09\u8dc4\u7740\u79bb\u53bb\uff0c\u6211\u5c1d\u8bd5\u8f6c\u79fb\u8bdd\u9898\u3002\uff09",
    "\u8fd9\u4e2a\u6837\u5b50\uff0c\u770b\u8d77\u6765\u662f\u56e0\u4e3aE.L.I.D.\u5427\uff1f",
    "\u55ef\uff0c\u6765\u6211\u4eec\u8fd9\u91cc\u5dee\u4e0d\u591a\u6709\u5c0f\u534a\u5e74\u3002\u521a\u5f00\u59cb\u8fd8\u662f\u8f7b\u75c7\uff0c\u62ff\u4e86\u4e00\u5927\u7b14\u94b1\u627e\u4eba\u4e70\u836f\u7269\uff0c\u53ef\u6700\u7ec8\u4e5f\u6ca1\u4ec0\u4e48\u7528\uff0c\u73b0\u5728\u90fd\u6709\u70b9\u610f\u8bc6\u4e0d\u6e05\u4e86\u3002",
