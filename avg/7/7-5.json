    "\uff08\u8d70\u5728\u961f\u5217\u6700\u540e\u7684\u7ef4\u666e\u857e\u5411\u6d77\u4f26\u5a1c\u6325\u4e86\u6325\u624b\u3002\uff09",
    "\uff08\u88ab\u201c\u7ef4\u666e\u857e\u201d\u8fd9\u4e2a\u540d\u5b57\u523a\u6fc0\u5230\uff0c\u6d77\u4f26\u5a1c\u7acb\u523b\u60ca\u6050\u5730\u7741\u5f00\u773c\uff0c\u5feb\u901f\u6447\u5934\u3002\uff09",
    "\u7ef4\u666e\u857e\uff01\u4e0d\u8fc7\u6765\uff01",
    "\u4e0d\u3001\u4e0d\u8fc7\u6765\u2014\u2014",
    "\u4e0d\u884c\uff01\u7ef4\u666e\u857e\uff0c\u632f\u4f5c\u8d77\u6765\u2014\u2014\u867d\u7136\u6ca1\u6709\u5c0f\u77f3\u5934\uff0c\u4f46\u662f\uff01\u5654\u5654~",
    "\u662f\u7ef4\u666e\u857e\u7684\u73cd\u85cf~\u9ec4\u8272\u7684\u8774\u8776\u7ed3\uff0c\u8d85\u597d\u770b\u7684\uff01|\u6d77\u4f26\u5a1c\u8981\u4e0d\u8981\u8bd5\u4e00\u8bd5\uff1f\u88c5\u9970\u5728\u4f60\u524d\u9762\u7684\u5934\u53d1\u4e0a\uff0c\u4e00\u5b9a\u975e\u5e38\u5408\u9002\uff01\u662f\u5076\u50cf\u7684\u80af\u5b9a\u54e6\uff01",
    "\u8774\u8776\u7ed3\uff1f",
    "\u597d\u770b\uff01\u559c\u6b22\uff01|\u5c0f\u77f3\u5934\u3001\u4e00\u8d77\u3001\u597d\u770b\uff01",
    "\u7ef4\u666e\u857e\u60f3\u4e86\u597d\u591a\u79cd\u88c5\u9970\u65b9\u6cd5\u54e6\uff01\u8981\u4e0d\u8981\u8bd5\u4e00\u8bd5\uff1f",
    "\u8bd5\uff01",
    "\uff08\u6d77\u4f26\u5a1c\u63a5\u8fc7\u4e86\u8774\u8776\u7ed3\uff0c\u5979\u62c9\u7740\u7ef4\u666e\u857e\u7684\u624b\uff0c\u4e24\u4eba\u4e00\u8d77\u8d70\u5230\u4f11\u606f\u533a\uff0c\u7ef4\u666e\u857e\u5f00\u59cb\u7528\u8774\u8776\u7ed3\u7ed9\u6d77\u4f26\u5a1c\u88c5\u9970\u5934\u53d1\u3002\uff09",
    "\u8981\u6c42\u6211\u4eec\u8fc7\u6765\uff0c\u662f\u56e0\u4e3a\u6d77\u4f26\u5a1c\u5417\uff0c\u6307\u6325\u5b98\uff1f",
    "\u2026\u2026\u53ea\u662f\u4e00\u90e8\u5206\u539f\u56e0\u3002\u521a\u624d\u5f71\u50cf\u91cc\u7684\u90a3\u4e9b\u4eba\uff0c\u4f60\u4eec\u6709\u4ec0\u4e48\u770b\u6cd5\uff1f",
    "\u90a3\u4e2a\u7ad9\u5728\u6700\u524d\u9762\u7684\u4eba\uff0c\u8eab\u4e0a\u6709\u574d\u584c\u7ed3\u6676\uff01\u4f46\u662f\u8fd8\u4f1a\u8bf4\u8bdd\u2026\u2026\u8fd9\u3001\u8fd9\u4e0d\u53ef\u80fd\u7684\u2026\u2026\u6211\u89c1\u8fc7\u8fd9\u6837\u7684\u4eba\uff0c\u8eab\u4f53\u6076\u5316\u5230\u8fd9\u79cd\u7a0b\u5ea6\uff0c\u4e00\u5b9a\u4f1a\u4e27\u5931\u7406\u667a\u7684\uff01",
    "\u8fd9\u4e0d\u6b63\u597d\u8ddf\u6211\u4eec\u4e4b\u524d\u6253\u8fc7\u7684\u90a3\u7fa4\u751f\u9ab8\u76f8\u53cd\uff1f\u7ed3\u6676\u5316\u7a0b\u5ea6\u4f4e\uff0c\u6bd4\u5176\u4ed6\u751f\u9ab8\u5f3a\uff1f",
    "\u54e6\u5bf9\uff0c\u9886\u5934\u90a3\u4e2a\u8bf4\u8bdd\u542c\u8d77\u6765\u8fd8\u771f\u50cf\u7eb3\u7f8e\u897f\u4e1d\u3002\u800c\u4e14\u6211\u4e00\u8fd9\u4e48\u8bf4\uff0c\u4ed6\u4eec\u5c31\u51b2\u6211\u4eec\u8fc7\u6765\u4e86\u2014\u2014|\u54fc\uff0c\u7eb3\u7f8e\u897f\u4e1d\uff0c\u96be\u9053\u4f60\u4ee5\u524d\u6b20\u4ed6\u4eec\u94b1\uff1f",
    "\u2026\u2026\u7a00\u8584\u7684\u5fae\u98ce\uff0c\u7a7f\u68ad\u4e8e\u6797\u95f4\u2026\u2026",
    "\u8c01\u5fc3\u667a\u6e29\u5ea6\u8fc7\u9ad8\u70e7\u4e86\u2014\u2014\u8981\u4e0d\u8981\u6211\u7ed9\u4f60\u6cfc\u70b9\u6c34\u964d\u964d\u6e29\u554a\uff1f\uff01",
    "\u2026\u2026\u5143\u7d20\u91cd\u6784\u2026\u2026\u6df7\u4e71\u7684\u96c6\u7ed3\u2026\u2026",
    "\u2014\u2014\u5567\uff0c\u7eb3\u7f8e\u897f\u4e1d\u4e4b\u524d\u89c1\u8fc7\u4ed6\u4eec\u3002",
    "\u89c1\u8fc7\u4ed6\u4eec\uff1f\u662f\u89c1\u8fc7\u8fd9\u4e9b\u4eba\uff0c\u8fd8\u662f\u89c1\u8fc7\u6709\u76f8\u4f3c\u7279\u8d28\u7684\u4eba\uff1f\u5728\u54ea\u91cc\u3001\u4eba\u7fa4\u89c4\u6a21\u3001\u5f53\u65f6\u60c5\u5883\uff1f",
    "\u2026\u2026\u8d2a\u5a6a\u7684\u589e\u503c\u8005\u2026\u2026\u7802\u783e\u5d29\u6bc1\u2026\u2026",
    "\u4e0d\u77e5\u9053\u3002",
    "\u522b\u770b\u6211\uff0c\u8ddf\u4e4b\u524d\u4e00\u6837\u3002\u6211\u4e0d\u77e5\u9053\u54ea\u4e2a\u624d\u662f\u6709\u6548\u4fe1\u606f\u2014\u2014\u4eba\u591a\u3001\u5728\u4e00\u8d77\u3001\u5f88\u591a\u5730\u65b9\u3001\u6700\u540e\u662f\u4e00\u4e2a\uff0c\u5c31\u8fd9\u4e9b\u3002",
    "\u6307\u6325\u5b98\uff0c\u6d77\u4f26\u5a1c\u8fd9\u4e48\u5bb3\u6015\uff0c\u662f\u4e0d\u662f\u4e5f\u50cf\u7eb3\u7f8e\u897f\u4e1d\u4e00\u6837\uff0c\u4ee5\u524d\u770b\u5230\u8fc7\u5f88\u50cf\u4ed6\u4eec\u7684\u4eba\uff1f|\u8fd9\u91cc\u5df2\u7ecf\u5f88\u9760\u8fd1\u7ea2\u533a\u4e86\uff0c\u4e0d\u7a7f\u9632\u62a4\u670d\u2026\u2026\u8fd9\u6837\u7684\u4eba\uff0c\u6211\u5728\u5916\u9762\u770b\u5230\u4e86\uff0c\u4e5f\u4f1a\u5f88\u5bb3\u6015\u7684\uff01",
    "\u2026\u2026\u5982\u679c\u662f\u8fd9\u6837\u2026\u2026",
    "\u6709\u3001\u6709\u597d\u591a\u4eba\u5411\u7740\u6211\u4eec\u8fc7\u6765\u4e86\uff01\u5f71\u50cf\u8bc6\u522b\u2026\u2026\u548c\u4e4b\u524d\u7684\u90a3\u4e9b\u4eba\u4e00\u6837\uff01",
    "\u2026\u2026\u8fce\u51fb\uff0c\u95ea\u7535\u3002\u53e6\u5916\u3002",
    "\u4ed6\u4eec\u8eab\u4e0a\u53ef\u80fd\u4f1a\u6709\u7279\u6b8a\u7684\u7b26\u53f7\uff0c\u6ce8\u610f\u89c2\u5bdf\u3002",
    "\u4e86\u89e3\u3002"
  ]
]