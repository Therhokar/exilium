    "\u6211\u4eec\u5c06\u5728\u5149\u8f89\u4e2d\u884c\u8d70\uff0c\u8bf7\u4e3a\u6211\u4eec\u51c0\u5316\u4e00\u5207\u82e6\u75db\uff0c\u8bf7\u5f15\u5bfc\u6211\u4eec\u8d70\u5411\u5171\u540c\u5531\u8bf5\u7684\u5f52\u53bb\u4e4b\u6240\u3002",
    "\u5b89\u5e15\u5229\u5854\u3002",
    "\u5bc7\u5c14\u8299\uff01",
    "\uff08\u4ee5\u957f\u888d\u4eba\u7fa4\u7684\u52a8\u4f5c\u4e3a\u63a9\u62a4\uff0c\u514b\u7f57\u4e3d\u79d1\u4e0e\u7ef4\u666e\u857e\u6084\u6084\u63a5\u8fd1\u4e86\u5bc7\u5c14\u8299\uff0c\u514b\u7f57\u4e3d\u79d1\u6293\u4f4f\u4e86\u5bc7\u5c14\u8299\u7684\u624b\u3002\uff09",
    "\u2026\u2026",
    "\u8d70\uff0c\u6211\u4eec\u5148\u53bb\u5165\u53e3\u90a3\u91cc\u3002\u4ed6\u4eec\u8fd9\u4e0d\u77e5\u9053\u5e72\u4ec0\u4e48\uff0c\u7b49\u4ed6\u4eec\u4e0d\u52a8\u4e86\u6211\u4eec\u5c31\u96be\u8d70\u4e86\uff01",
    "\uff08\u5bc7\u5c14\u8299\u88ab\u514b\u7f57\u4e3d\u79d1\u62fd\u7740\u8e09\u8dc4\u4e86\u4e00\u4e0b\uff0c\u4f46\u5979\u7528\u53e6\u4e00\u53ea\u624b\u63a8\u5f00\u4e86\u514b\u7f57\u4e3d\u79d1\u3002\uff09",
    "\u6211\u7559\u5728\u8fd9\u91cc\u3002",
    "\u5567\uff0c\u4f60\u95f9\u4ec0\u4e48\u522b\u626d\u554a\uff0c\u4e0d\u5c31\u662f\u90a3\u4e2a\u4ec0\u4e48\u8d6b\u2026\u2026\u8d6b\u4ec0\u4e48\uff0c\u6211\u5e2e\u4f60\u5e72\u6389\u4e0d\u5c31\u884c\u4e86\u3002\u5148\u8ddf\u6211\u4eec\u56de\u53bb\u3002",
    "\u5e72\u6389\uff0c\u5514\uff0c\u662f\u8981\u8ba9\u4ed6\u6210\u4e3a\u7ef4\u666e\u857e\u7684\u5fe0\u5b9e\u89c2\u4f17\u561b\uff1f\u7ef4\u666e\u857e\u4e5f\u80fd\u5e2e\u5fd9\u54e6\uff01",
    "\u2026\u2026\u65e0\u5c3d\u7684\u82e6\u75db\uff0c\u7f20\u7ed5\u800c\u6765\uff0c\u6d88\u5931\u7684\u79d1\u672a\u5854\u3002|\u7942\u4ee5\u82e6\u75db\u7684\u9762\u5b54\u793a\u6211\uff0c\u6124\u6012\u4e0e\u60b2\u4f24\u5316\u4e3a\u575a\u56fa\u7684\u5ca9\u77f3\uff0c\u622a\u65ad\u5f52\u53bb\u7684\u8def\u9014\u2026\u2026",
    "\u4f60\u4eec\u5e2e\u6211\u5e72\u6389\uff1f\u6211\u73b0\u5728\u8ddf\u4f60\u4e00\u8d77\u79bb\u5f00\uff0c\u4f60\u4eec\u7559\u4e0b\u6765\uff0c\u5e2e\u6211\u5e72\u6389\uff1f",
    "\u4e5f\u4e0d\u662f\u4e0d\u884c\u5566~\u8fd9\u91cc\u6709\u597d\u591a\u89c2\u4f17\uff0c\u5927\u5bb6\u7684\u60c5\u7eea\u90fd\u6709\u4e9b\u5947\u602a\uff0c\u7ef4\u666e\u857e\u3001\u8981\u5927\u5bb6\u5f00\u5fc3\u8d77\u6765\uff01",
    "\u600e\u4e48\u5f00\u5fc3\uff1f\u5531\u4e2a\u6b4c\u8df3\u4e2a\u821e\uff1f\u4f60\u8fc7\u53bb\u8bd5\u8bd5\uff1f\u770b\u770b\u4ed6\u4eec\u4f1a\u4e0d\u4f1a\u53cd\u8fc7\u6765\u628a\u4f60\u5e72\u6389\uff1f",
    "\u5514\uff0c\u610f\u601d\u662f\u8bf4\uff0c\u8fd9\u91cc\u7684\u89c2\u4f17\uff0c\u662f\u5f88\u559c\u6b22\u4e92\u52a8\u7684\u7c7b\u578b\u5417\uff1f",
    "\u2026\u2026\u4f60\u2026\u2026",
    "\u514b\u7f57\u4e3d\u79d1\uff0c\u5e26\u5979\u56de\u6765\u3002",
    "\u961f\u957f\u2014\u2014\u8fd9\u662f\u53c8\u8ba1\u7b97\u4e86\u8d44\u6e90\u6d88\u8017\u548c\u53ef\u80fd\u98ce\u9669\u4e4b\u540e\u505a\u51fa\u7684\u51b3\u5b9a\uff1f",
    "\u6267\u884c\u547d\u4ee4\uff0c\u514b\u7f57\u4e3d\u79d1\uff01",
    "\u554a\u554a\u6211\u4e0d\u7ba1\u4e86\u554a\uff01\u5bc7\u5c14\u8299\uff0c\u8fc7\u6765\u6211\u4eec\u8d70\uff01",
    "\u2026\u2026\u770b\u554a\uff0c\u5927\u5730\uff0c\u661f\u706b\u8513\u5ef6\u3002\u7942\u6b63\u5728\u6ce8\u89c6\u7740\u6211\u4eec\uff0c\u4ee5\u82e6\u75db\u7684\u76ee\u5149\uff0c\u6ce8\u89c6\u6211\u4eec\u5c06\u79d1\u672a\u5854\u5949\u4e0a\u3002",
    "\u7942\u6ce8\u89c6\u7740\u6211\u4eec\uff0c\u6211\u4eec\u5c06\u2014\u2014\u4f9d\u7167\u7942\u7684\u8bdd\u8bed\uff0c\u83b7\u5f97\u5b89\u5b81\uff01",
    "\u9075\u4ece\u7942\u7684\u542f\u793a\uff01",
    "\uff08\u56de\u4f20\u7684\u753b\u9762\u4e2d\uff0c\u8eab\u7a7f\u957f\u888d\u7684\u4eba\u7fa4\u9759\u6b62\u5728\u53cc\u624b\u4ea4\u53c9\u7684\u52a8\u4f5c\u3002\uff09",
    "\uff08\u4e00\u7247\u5b89\u9759\u91cc\uff0c\u514b\u7f57\u4e3d\u79d1\u7684\u58f0\u97f3\u663e\u5f97\u683c\u5916\u7a81\u51fa\u3002\uff09",
    "\uff08\u7ad9\u5728\u7b80\u964b\u53f0\u5b50\u4e0a\u7684\u4eba\u5f71\u8f7b\u5de7\u5730\u8f6c\u8fc7\u5934\uff0c\u5411\u53d1\u51fa\u58f0\u97f3\u7684\u514b\u7f57\u4e3d\u79d1\u65b9\u5411\u770b\u8fc7\u6765\u3002\uff09",
    "\uff08\u900f\u8fc7\u955c\u5934\uff0c\u6211\u4e0e\u9ed1\u8272\u4eba\u5f71\u77ed\u6682\u5730\u5bf9\u4e0a\u4e86\u89c6\u7ebf\u3002\uff09",
    "\u2026\u2026\u7cdf\u4e86\u3002\u64a4\uff01",
    "\uff08\u9ed1\u8272\u4eba\u5f71\u62ac\u8d77\u624b\u81c2\uff0c\u6307\u5411\u6b64\u524d\u89c6\u7ebf\u843d\u70b9\u7684\u65b9\u5411\u3002\uff09",
    "\uff08\u968f\u7740\u9ed1\u8272\u4eba\u5f71\u7684\u52a8\u4f5c\uff0c\u957f\u888d\u4eba\u7fa4\u6574\u9f50\u5212\u4e00\u5730\u8f6c\u8fc7\u4e86\u5934\u3002\uff09",
    "\u54c7\uff0c\u4e0d\u8981\u6fc0\u52a8\u5440\uff0c\u5927\u5bb6\u6162\u4e00\u70b9\u2014\u2014",
    "\uff08\u6df7\u4e71\u7684\u6742\u97f3\u4e2d\uff0c\u901a\u8baf\u65ad\u5f00\u3002\uff09",
    "\uff08\u65ad\u5f00\u7684\u524d\u4e00\u523b\uff0c\u6211\u6a21\u7cca\u542c\u5230\u6709\u4e2a\u4eba\u5728\u558a\u3002\uff09",
    "\u662f\u5979\u4eec\uff01"
  ]
]