    "\uff08\u8bf4\u8bdd\u7684\u4eba\u624b\u81c2\u62ac\u8d77\uff0c\u6307\u5411\u4e86\u4e00\u4e2a\u65b9\u5411\u3002\uff09",
    "\u4e3a\u4e86\u6c38\u6052\u7684\u5b89\u5b81\uff0c\u5b89\u5e15\u5229\u5854\u3002",
    "\uff08\u4ed6\u505c\u6b62\u4e86\u8bf4\u8bdd\u3002\u4e0d\u77e5\u4e3a\u4f55\uff0c\u73af\u7ed5\u5728\u8bf4\u8bdd\u4eba\u5468\u56f4\u7684\u957f\u888d\u4eba\u7fa4\u5f00\u59cb\u54ed\u6ce3\uff0c\u5e76\u5f00\u59cb\u5411\u8bf4\u8bdd\u4eba\u805a\u96c6\u3002\uff09",
    "\u4ed6\u4eec\u54ed\u4ec0\u4e48\uff1f\u8fd9\u79cd\u53cd\u5e94\uff0c\u600e\u4e48\u8fd8\u8ddf\u4e4b\u524d\u90a3\u5e2e\u4eba\u4e0d\u4e00\u6837\uff1f",
    "\u662f\u8981\u6392\u7ec3\u7279\u522b\u7684\u821e\u8e48\u5417\uff1f\u4ed6\u4eec\u7684\u961f\u5f62\uff0c\u597d\u5947\u602a\u2026\u2026",
    "\uff08\u7a7a\u5730\u4e0a\u7684\u4eba\u8d8a\u6765\u8d8a\u591a\uff0c\u4ed6\u4eec\u5f00\u59cb\u6709\u5e8f\u6392\u6210\u51e0\u4e2a\u961f\u5217\uff0c\u770b\u8d77\u6765\u51c6\u5907\u79fb\u52a8\u3002\uff09",
    "\uff08\u901a\u8baf\u53e6\u4e00\u4fa7\uff0c\u827e\u83ab\u53f7\u4e0a\u2014\u2014\uff09",
    "\uff08\u6211\u7ad9\u5728\u6295\u5f71\u5c4f\u524d\uff0c\u7d27\u76ef\u7740\u90a3\u4e0a\u9762\u663e\u793a\u7684\u56de\u4f20\u5f71\u50cf\u3002\uff09",
    "\u2026\u2026\u53d7\u6d17\u8005\u2026\u2026\u8fd9\u4e2a\u79f0\u547c\u2026\u2026\u5982\u679c\u6211\u6ca1\u731c\u9519\u2026\u2026\uff01",
    "\u4ed6\u4eec\u4eba\u592a\u591a\u4e86\uff0c\u4f60\u4eec\u5148\u56de\u6765\u8865\u5145\u5f39\u836f\u3002\u6211\u4eec\u5b88\u5728\u5916\u9762\uff0c\u8ddf\u7d27\u4ed6\u4eec\u3002",
    "\uff08\u7a7a\u5730\u5468\u56f4\u2014\u2014\uff09",
    "\u514b\u7f57\u4e3d\u79d1\uff01\u7ef4\u666e\u857e\uff01\u6709\u4eba\u5411\u4f60\u4eec\u8fc7\u53bb\u4e86\uff01",
    "\uff08\u53e6\u4e00\u4e2a\u7a7f\u7740\u76f8\u540c\u957f\u888d\u7684\u4eba\u7a81\u7136\u4ece\u4e00\u4fa7\u7684\u5c0f\u8def\u63a5\u8fd1\uff0c\u5728\u770b\u5230\u7ef4\u666e\u857e\u548c\u514b\u7f57\u4e3d\u79d1\u540e\uff0c\u8d70\u5230\u4e86\u5979\u4eec\u8eab\u8fb9\u3002\uff09",
    "\u5b89\u5e15\u5229\u5854\u3002",
    "\uff08\u5168\u795e\u8d2f\u6ce8\u89c2\u5bdf\u773c\u524d\u60c5\u5883\u7684\u514b\u7f57\u4e3d\u79d1\u548c\u7ef4\u666e\u857e\u6ca1\u6709\u53ca\u65f6\u54cd\u5e94\u95ea\u7535\u7684\u63d0\u9192\uff0c\u88ab\u8fd1\u5728\u54ab\u5c3a\u7684\u58f0\u97f3\u5413\u4e86\u4e00\u8df3\u3002\uff09",
    "\u554a\uff1f\uff01",
    "\uff08\u514b\u7f57\u4e3d\u79d1\u7684\u58f0\u97f3\u5728\u4e00\u7247\u54ed\u6ce3\u58f0\u4e2d\u5c24\u5176\u660e\u663e\u3002\uff09",
    "\uff08\u7a7a\u5730\u4e2d\u7684\u4eba\u7fa4\u95fb\u58f0\u770b\u5411\u514b\u7f57\u4e3d\u79d1\u6240\u5728\u65b9\u5411\uff0c\u54ed\u58f0\u6e10\u6e10\u6b62\u606f\uff0c\u4f46\u5e76\u6ca1\u6709\u5982\u540c\u6b64\u524d\u7684\u4eba\u7fa4\u4e00\u822c\u5411\u4e24\u4eba\u53d1\u52a8\u653b\u51fb\u3002\uff09",
    "\u7942\u5c06\u6551\u8d4e\uff0c\u7942\u5c06\u9009\u62e9\u3002\u53d7\u6d17\u8005\uff0c\u627f\u53d7\u8bd5\u70bc\uff0c\u52a0\u5165\u6211\u4eec\u3002",
    "\u5514\uff0c\u4f60\u4e5f\u8981\u6211\u4eec\u8ddf\u4f60\u8d70\u5417\uff1f",
    "\u2026\u2026",
    "\u52a0\u5165\u4ed6\u4eec\u3002",
    "\u7ef4\u666e\u857e\u548c\u514b\u7f57\u4e3d\u79d1\u5f39\u836f\u4e0d\u8db3\uff0c\u5982\u679c\u9700\u8981\u957f\u65f6\u95f4\u79bb\u5f00\u53ef\u80fd\u4f1a\u6709\u5371\u9669\uff0c\u6307\u6325\u5b98\uff01",
    "\u5148\u8ddf\u4e0a\u4ed6\u4eec\uff01\u4ed6\u4eec\u8fd9\u4e2a\u4eba\u6570\uff0c\u73b0\u5728\u5f00\u6218\u4e5f\u5f39\u836f\u4e0d\u8db3\uff01|\u8865\u7ed9\u53ef\u4ee5\u518d\u60f3\u529e\u6cd5\uff0c\u8ddf\u7d27\u4ed6\u4eec\uff0c\u6211\u51e0\u4e4e\u80fd\u786e\u5b9a\uff0c\u4ed6\u4eec\u2014\u2014\u4ed6\u4eec\u662f\uff01",
    "\u662f\u5e15\u62c9\u8482\u65af\uff01"
  ]
