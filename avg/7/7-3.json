[
  [
    "\u51c6\u5907\u5dee\u4e0d\u591a\u4e86\u5427\uff1f",
    "\u82f9\u679c\u6d3e\u3001\u5de7\u514b\u529b\u5e03\u6717\u5c3c\u5728\u70e4\u7bb1\u4e2d\u4fdd\u6e29\uff0c\u6a59\u5b50\u6c41\u5df2\u7ecf\u51b0\u9547\u5b8c\u6bd5\uff0c\u50a8\u5b58\u5728\u4f11\u606f\u5ba4\u8d70\u5eca\u5c3d\u5934\u7684\u51b0\u7bb1\u4e2d\u3002|\u4ed3\u5e93\u4e2d\u7684\u5f69\u7eb8\u5e93\u5b58\u6ca1\u6709\u8865\u5145\uff0c\u793c\u82b1\u4e5f\u6709\u4e9b\u53d7\u6f6e\uff0c\u5df2\u7ecf\u4e0d\u80fd\u4f7f\u7528\u3002\u4f5c\u4e3a\u66ff\u4ee3\uff0c\u514b\u7f57\u4e3d\u79d1\u627e\u5230\u4e86\u4e00\u4e9b\u70df\u96fe\u55b7\u5c04\u88c5\u7f6e\u3002",
    "\u2026\u2026\u4ec0\u4e48\uff1f",
    "\u5979\u8bf4\uff0c\u5979\u5df2\u7ecf\u6d4b\u8bd5\u8fc7\uff0c\u5c06\u91cc\u9762\u5177\u6709\u5f3a\u523a\u6fc0\u6027\u7684\u5316\u5b66\u6210\u5206\u66ff\u6362\u4e3a\u65e0\u6bd2\u67d3\u6599\uff0c\u5f00\u542f\u540e\u5c31\u80fd\u55b7\u51fa\u5404\u79cd\u989c\u8272\u7684\u70df\u96fe\uff0c\u62ff\u6765\u5f00\u6b22\u8fce\u4f1a\u6548\u679c\u548c\u793c\u82b1\u5dee\u4e0d\u591a\u3002",
    "\u2026\u2026",
    "\u884c\u4e86\uff0c\u5dee\u4e0d\u591a\u90fd\u51c6\u5907\u597d\u4e86\uff0c\u901a\u77e5\u5927\u5bb6\u6765\u6307\u6325\u5ba4\u5427\u3002|\u6a59\u5b50\u6c41\u8bf7\u7eb3\u7f8e\u897f\u4e1d\u5e26\u8fc7\u6765\uff0c\u82f9\u679c\u6d3e\u548c\u5de7\u514b\u529b\u5e03\u6717\u5c3c\u4ea4\u7ed9\u514b\u7f57\u4e3d\u79d1\uff0c\u70df\u96fe\u55b7\u5c04\u88c5\u7f6e\u2026\u2026\u5c31\u7b97\u4e86\u3002",
    "\u597d\u7684\u3002",
    "\u8bf4\u8d77\u6765\uff0c\u4e00\u76f4\u6ca1\u6536\u5230\u7f8e\u73b2\u7684\u62a5\u544a\uff0c\u62df\u6001\u56fe\u8c31\u88c5\u8f7d\u8bbe\u5907\u8c03\u8bd5\u5b8c\u6210\u4e86\u5417\uff1f",
    "\uff08\u603b\u63a7\u53f0\u524d\uff0c\u95ea\u7535\u9661\u7136\u4e25\u8083\u8d77\u6765\u3002\uff09",
    "\u2014\u2014\u6307\u6325\u5b98\uff0c\u5bc7\u5c14\u8299\u7684\u540d\u5b57\u4ece\u57fa\u5730\u8f66\u7684\u901a\u8baf\u4ee3\u7801\u8bb0\u5f55\u4e2d\u6d88\u5931\u4e86\u3002",
    "\u2026\u2026\uff01",
    "\uff08\u6211\u7acb\u523b\u62ff\u8d77\u901a\u8baf\u7ec8\u7aef\uff0c\u627e\u5230\u201c\u5bc7\u5c14\u8299\u201d\u7684\u540d\u5b57\uff0c\u5c1d\u8bd5\u547c\u53eb\u3002\uff09",
    "\uff08\u6ca1\u6709\u7b49\u5f88\u4e45\uff0c\u901a\u8baf\u5c31\u4e2d\u65ad\u4e86\u3002\uff09",
    "\u8fd9\u4e2a\u63d0\u793a\u97f3\uff0c\u901a\u8baf\u7ebf\u8def\u4e2d\u65ad\uff1f|\u95ea\u7535\uff0c\u67e5\u627e\u57fa\u5730\u8f66\u76d1\u63a7\u8bb0\u5f55\u3002",
    "\u6700\u540e\u7684\u8bb0\u5f55\u5904\u4e8e\u57fa\u5730\u8f66\u5165\u53e3\uff0c\u7ea6\u572819\u65f649\u5206\uff0c\u5979\u4e0e\u7ef4\u666e\u857e\u4e00\u540c\u56de\u5230\u57fa\u5730\u8f66\u3002|\u5728\u6b64\u4e4b\u540e\uff0c\u57fa\u5730\u8f66\u5185\u8bbe\u7f6e\u7684\u5404\u4e2a\u76d1\u63a7\u6444\u50cf\u5934\u6240\u8bb0\u5f55\u7684\u753b\u9762\u4e2d\uff0c\u90fd\u6ca1\u6709\u627e\u5230\u5bc7\u5c14\u8299\u3002",
    "\u2026\u2026\u539f\u6765\u5979\u56de\u57fa\u5730\u8f66\u662f\u8fd9\u4e2a\u6253\u7b97\u3002",
    "\u6307\u6325\u5b98\u3002",
    "\u867d\u7136\u8bbf\u95ee\u548c\u64cd\u4f5c\u4e0d\u9700\u8981\u989d\u5916\u5bc6\u94a5\u2026\u2026\u4f46\u57fa\u5730\u8f66\u603b\u63a7\u7cfb\u7edf\u4f7f\u7528\u4e86\u4e8c\u7ea7\u5bc6\u94a5\uff0c\u57fa\u5730\u8f66\u6240\u5c5e\u4eba\u5f62\u540d\u5f55\u4e5f\u5b58\u50a8\u5728\u52a0\u5bc6\u8def\u5f84\u4e0b\uff0c\u4e00\u4e2a\u665a\u4e0a\u7684\u65f6\u95f4\u5e76\u4e0d\u8db3\u4ee5\u5b8c\u6210\u7834\u89e3\u3002",
    "\u4f60\u662f\u6307\uff1f",
    "\u2026\u2026\u5bc7\u5c14\u8299\u786e\u5b9e\u6709\u4e9b\u4efb\u6027\uff0c\u4f46\u6211\u76f8\u4fe1\u5bc7\u5c14\u8299\u4e0d\u4f1a\u505a\u51fa\u5220\u9664\u6743\u9650\u5f52\u5c5e\u3001\u79bb\u5f00\u57fa\u5730\u8f66\u7684\u884c\u4e3a\u3002",
    "\u53ef\u80fd\u662f\u2026\u2026\u57fa\u5730\u8f66\u7cfb\u7edf\u88ab\u5165\u4fb5\uff0c\u5bc7\u5c14\u8299\u7684\u6743\u9650\u88ab\u5220\u9664\u2026\u2026\u5409\u62c9\u5c14\u96c6\u56e2\u4e4b\u524d\u4e5f\u505a\u8fc7\u7c7b\u4f3c\u7684\u4e8b\u60c5\u2026\u2026|\u5bc7\u5c14\u8299\u2026\u2026\u53ea\u662f\u51fa\u4e8e\u4e00\u4e9b\u539f\u56e0\uff0c\u77ed\u65f6\u95f4\u79bb\u5f00\u2026\u2026",
    "\u4f60\u80fd\u8fd9\u4e48\u8bf4\u6211\u5f88\u9ad8\u5174\u2026\u2026\u786e\u5b9e\u6709\u8fd9\u79cd\u53ef\u80fd\u3002",
    "\u95ea\u7535\uff0c\u5148\u8054\u7edc\u8428\u73c8\uff0c\u95ee\u4e00\u4e0b\u8d6b\u745e\u5c24\u6240\u53bb\u5c45\u4f4f\u70b9\u7684\u5177\u4f53\u5750\u6807\u3002",
    "\uff08\u95ea\u7535\u6c89\u9ed8\u4e86\u4e00\u4e0b\uff0c\u5f00\u59cb\u64cd\u4f5c\u603b\u63a7\u53f0\u3002\uff09",
    "\uff08\u6307\u6325\u5ba4\u5916\u4f20\u6765\u55a7\u95f9\u58f0\u3002\uff09",
    "\uff08\u6ed1\u95e8\u6253\u5f00\uff0c\u514b\u7f57\u4e3d\u79d1\u6293\u7740\u6d77\u4f26\u5a1c\u8d70\u4e86\u8fdb\u6765\uff0c\u540e\u9762\u8ddf\u7740\u7eb3\u7f8e\u897f\u4e1d\u3002\uff09",
    "\u4f60\u522b\u52a8\u2014\u2014\u4f60\u522b\u52a8\u884c\u4e0d\u884c\uff1f\uff01\u961f\u957f\u505a\u7684\u70b9\u5fc3\u90fd\u8981\u6389\u4e0b\u6765\u4e86\uff01",
    "\u70b9\u5fc3\uff01\u8981\u5403\uff01",
    "\u6025\u4ec0\u4e48\u4e00\u4f1a\u53c8\u4e0d\u662f\u6ca1\u4f60\u7684\u2014\u2014\u4f60\u522b\u62a2\u884c\u4e0d\u884c\uff1f\uff01",
    "\u2026\u2026\u6301\u7eed\u63a5\u8fd1\u7684\u82e6\u75db\u2026\u2026\u6e34\u5ff5\u7ec8\u7ed3\u2026\u2026",
    "\u54fc\uff0c\u4f60\u6765\u8bd5\u8bd5\uff1f\u4e5f\u5c31\u662f\u4f60\u624b\u91cc\u7684\u6a59\u5b50\u6c41\u5979\u89c9\u5f97\u592a\u9178\u4e86\u624d\u6ca1\u62a2\uff0c\u4e0d\u7136\u4f60\u4ee5\u4e3a\u73b0\u5728\u8fd8\u80fd\u5269\u4e0b\uff1f",
    "\u9178\uff01\u6a59\u5b50\u6c41\u3001\u4e0d\u559c\u6b22\uff01",
    "\uff08\u770b\u5230\u6211\u8868\u60c5\u51dd\u91cd\uff0c\u514b\u7f57\u4e3d\u79d1\u6709\u4e9b\u5947\u602a\u5730\u4e0a\u4e0b\u6253\u91cf\u4e86\u4e00\u4e0b\u6211\u3002\uff09",
    "\u600e\u4e48\u8fd9\u4e2a\u8868\u60c5\uff0c\u6307\u6325\u5b98\uff0c\u4f60\u6ca1\u5403\u9519\u4ec0\u4e48\u5427\uff1f",
    "\u521a\u624d\u8def\u4e0a\u770b\u5230\u7f8e\u73b2\u4e86\uff0c\u5979\u8fd8\u5728\u4ed3\u5e93\u4e0d\u77e5\u9053\u627e\u4ec0\u4e48\uff0c\u7ef4\u666e\u857e\u5c31\u5728\u4f11\u606f\u5ba4\uff0c\u6b63\u5728\u8fc7\u6765\u3002|\u5012\u662f\u6ca1\u770b\u5230\u5bc7\u5c14\u8299\uff0c\u4f60\u8981\u662f\u5403\u9519\u4e1c\u897f\u4e86\uff0c\u53eb\u5bc7\u5c14\u8299\u8fc7\u6765\u7ed9\u4f60\u770b\u770b\uff1f",
    "\u2026\u2026\u4ece\u6628\u665a\u5230\u73b0\u5728\uff0c\u4f60\u4eec\u770b\u5230\u5bc7\u5c14\u8299\u4e86\u5417\uff1f",
    "\u6ca1\u770b\u5230\u2014\u2014\u5979\u600e\u4e48\u4e86\uff1f",
    "\u2026\u2026\u65e0\u7455\u7684\u79e9\u5e8f\uff0c\u8fdc\u79bb\u2026\u2026\u5f15\u5411\u65e0\u8fb9\u754c\u7684\u571f\u5730\u2026\u2026",
    "\u5728\u54ea\u91cc\uff1f\u54ea\u4e2a\u533a\u57df\uff1f",
    "\u2026\u2026\u8d2f\u901a\u7684\u8def\u9014\uff0c\u5ef6\u4f38\u2026\u2026",
    "\u7eb3\u7f8e\u897f\u4e1d\u8bf4\u5979\u665a\u4e0a\u5728\u88c5\u914d\u5ba4\u5916\u770b\u5230\u4e86\u5bc7\u5c14\u8299\uff0c\u8bf4\u662f\u8981\u53bb\u7ed9\u6d77\u4f26\u5a1c\u627e\u5c0f\u77f3\u5934\uff0c\u5411\u7740\u57fa\u5730\u8f66\u7684\u5165\u53e3\u90a3\u8fb9\u8d70\u4e86\u2014\u2014\u54e6\u5bf9\uff0c\u5927\u69829\u70b9\u591a\uff0c\u5bc7\u5c14\u8299\u8fd8\u7a7f\u7740\u6218\u6597\u670d\u3002",
    "\u554a\u3001\u5c0f\u77f3\u5934\uff01",
    "\uff08\u88ab\u5173\u952e\u8bcd\u201c\u5c0f\u77f3\u5934\u201d\u6fc0\u8d77\u4e86\u8bb0\u5fc6\uff0c\u6d77\u4f26\u5a1c\u505c\u6b62\u4e86\u5c1d\u8bd5\u53bb\u62a2\u514b\u7f57\u4e3d\u79d1\u624b\u4e2d\u751c\u70b9\u7684\u4e3e\u52a8\u3002\uff09",
    "\u5c0f\u77f3\u5934\u3001\u6307\u6325\u5b98\u3001\u5c0f\u77f3\u5934\uff01",
    "\u2026\u2026\u6d77\u4f26\u5a1c\uff0c\u4f60\u89c1\u8fc7\u5bc7\u5c14\u8299\u5417\uff1f",
    "\u5bc7\u5c14\u8299\u3001\u89c1\u8fc7\uff01\u5c0f\u77f3\u5934\u3001\u6ca1\u6709\u3001\u6492\u8c0e\uff01",
    "\u8981\u5c0f\u77f3\u5934\u2014\u2014\u6307\u6325\u5b98\u2014\u2014\u8981\u5c0f\u77f3\u5934\u2014\u2014",
    "\uff08\u6d77\u4f26\u5a1c\u51b2\u5411\u6211\uff0c\u7d27\u7d27\u62fd\u4f4f\u6211\u7684\u8863\u670d\u4e0b\u6446\uff0c\u5c31\u8981\u8e72\u4e0b\u6765\u6253\u6eda\u3002\uff09",
    "\u4f60\u5148\u8d77\u6765\uff0c\u6d77\u4f26\u5a1c\u3002|\u4f60\u4ec0\u4e48\u65f6\u5019\u3001\u5728\u54ea\u91cc\u89c1\u5230\u5bc7\u5c14\u8299\u7684\uff1f",
    "\u60f3\u4e0d\u5230\u2014\u2014\u5c0f\u77f3\u5934\u2014\u2014\u5c0f\u77f3\u5934\u3001\u6ca1\u6709\u2014\u2014\u60f3\u4e0d\u5230\u2014\u2014",
    "\u2026\u2026",
    "\u4e3b\u2026\u2026\u554a\uff0c\u662f\u6307\u6325\u5b98\uff01|\u6307\u6325\u5b98\uff0c\u7ef4\u666e\u857e\u2606\u767b\u573a\u5566\uff0c\u6709\u4ec0\u4e48\u4e8b\u60c5\u9700\u8981\u7ef4\u666e\u857e\u5e2e\u5fd9\u561b\uff1f",
    "\uff08\u770b\u5230\u7ef4\u666e\u857e\uff0c\u6d77\u4f26\u5a1c\u7acb\u523b\u4ece\u5730\u4e0a\u7ad9\u4e86\u8d77\u6765\uff0c\u8eb2\u5230\u4e86\u6211\u8eab\u540e\u3002\uff09",
    "\u574f\u4eba\uff01\u574f\u4eba\uff01\u8ba8\u538c\uff01\u4e0d\u8981\u8fc7\u6765\uff01",
    "\u600e\u3001\u600e\u4e48\u4f1a\u88ab\u8ba8\u538c\u2014\u2014\u7ef4\u666e\u857e\u53ef\u3001\u53ef\u662f\u5076\u50cf\u554a\uff01",
    "\u2026\u2026\u8fd9\u53c8\u662f\u600e\u4e48\u4e86\uff1f",
    "\u54e6\uff0c\u6628\u5929\u665a\u4e0a\u7ef4\u666e\u857e\u6765\u5e2e\u6211\u7167\u987e\u6d77\u4f26\u5a1c\uff0c\u521a\u89c1\u9762\u5c31\u626f\u4e86\u6d77\u4f26\u5a1c\u7684\u5934\u53d1\u3002",
    "\u626f\u5934\u53d1\uff01\u597d\u75db\uff01",
    "\u6ca1\u6709\u626f\u5566\uff01\u5c31\u662f\u2026\u2026\u6d77\u4f26\u5a1c\u524d\u9762\u7684\u5934\u53d1\u597d\u50cf\u4e24\u4e2a\u7fc5\u8180\uff0c\u7ef4\u666e\u857e\u89c9\u5f97\u8d85\u7ea7\u53ef\u7231\u561b\uff01\u6ca1\u5fcd\u4f4f\u2026\u2026\u5bf9\u4e0d\u8d77\u561b\u2026\u2026|\u4f46\u7ef4\u666e\u857e\u771f\u7684\u662f\u5f88\u5c0f\u5fc3\u7684\u5440\uff01\u5c31\u8f7b\u8f7b\u5730\u6478\u4e86\u4e00\u4e0b\u2026\u2026",
    "\u554a\uff1f\u5f53\u65f6\u6d77\u4f26\u5a1c\u90fd\u5feb\u88ab\u4f60\u62fd\u5012\u4e86\uff0c\u8fd9\u53eb\u8f7b\u8f7b\u5730\uff1f",
    "\u4e0d\u4e0d\u4e0d\uff01\u7ef4\u666e\u857e\u3001\u6ca1\u6709\u529b\u6c14\u5f88\u5927\uff01",
    "\u6211\u662f\u5728\u8bf4\u4f60\u529b\u6c14\u5927\u5417\uff1f\uff01\u6709\u4e00\u4e2a\u7eb3\u7f8e\u897f\u4e1d\u4e5f\u5c31\u7b97\u4e86\u4f60\u80fd\u4e0d\u80fd\u597d\u597d\u542c\u4eba\u8bf4\u8bdd\u554a\uff1f\uff01",
    "\u2026\u2026\u6797\u95f4\uff0c\u9519\u843d\u751f\u957f\u7684\u679d\u53f6\u2026\u2026\u6295\u4e0b\u5e7d\u6697\u7684\u9634\u5f71\u2026\u2026",
    "\u4f60\uff1f\uff01",
    "\u505c\u4e0b\uff01",
    "\u5514\u3001\u5514\uff1f",
    "\u5404\u4f4d\uff0c\u5bc7\u5c14\u8299\u4e0d\u89c1\u4e86\u3002",
    "\u4e0d\u5c31\u662f\u51fa\u53bb\u4e00\u4e0b\u5417\uff0c\u5bc7\u5c14\u8299\u4e5f\u4e0d\u81f3\u4e8e\u627e\u4e0d\u5230\u8def\u5427\uff1f|\u54fc\uff0c\u627e\u4e0d\u5230\u8def\u8fd9\u79cd\u4e8b\uff0c\u6211\u770b\u53ea\u6709\u7eb3\u7f8e\u897f\u4e1d\u624d\u505a\u5f97\u51fa\u6765\u3002",
    "\u5bc7\u5c14\u8299\u7684\u6743\u9650\u5df2\u7ecf\u88ab\u5220\u9664\u3002\u5bc7\u5c14\u8299\u4e0d\u518d\u5f52\u5c5e\u4e8e\u201c\u827e\u83ab\u53f7\u201d\u3002",
    "\u5220\u9664\u3001\u6628\u5929\u3001\u597d\u591a\u8def\u3001\u5927\u5c4f\u5e55\uff01|\u4eae\u4eae\u7684\u3001\u5bc7\u5c14\u8299\u3001\u70b9\u5c4f\u5e55\uff01",
    "\u5bc7\u5c14\u8299\u2026\u2026\u201c\u6d88\u9664\u201d\uff1f\u4e0d\u8981\u2026\u2026\u4e0d\u8981\u6d88\u9664\u2026\u2026\u6307\u6325\u5b98\uff01",
    "\u597d\u591a\u8def\u2026\u2026\u4e2d\u67a2\u533a\uff1f\u5bc7\u5c14\u8299\u6628\u5929\u5728\u4e2d\u67a2\u533a\u4f7f\u7528\u4e86\u57fa\u5730\u8f66\u603b\u63a7\u7cfb\u7edf\uff1f\u7f8e\u73b2\u5462\uff1f\u7f8e\u73b2\u4e0d\u662f\u5728\u4e2d\u67a2\u533a\uff1f",
    "\u7f8e\u73b2\u3001\u6ca1\u6709\uff01",
    "\uff08\u95ea\u7535\u7acb\u523b\u8054\u7edc\u7f8e\u73b2\u3002\uff09",
    "\uff08\u8fd9\u4e00\u6b21\uff0c\u901a\u8baf\u5f88\u5feb\u88ab\u63a5\u4e86\u8d77\u6765\u3002\uff09",
    "\u961f\u957f\u2026\u2026\uff1f",
    "\u547c\u554a\u2014\u2014\u6211\u5c1d\u8bd5\u4e86\u597d\u591a\u65b9\u6cd5\uff0c\u62df\u6001\u56fe\u8c31\u88c5\u8f7d\u8bbe\u5907\u7684\u9650\u5236\u8fd8\u662f\u89e3\u51b3\u4e0d\u4e86\u2026\u2026|\u53ef\u80fd\u2026\u2026\u4e0d\u662f\u96f6\u4ef6\u7684\u95ee\u9898\u2026\u2026\u800c\u662f\u539f\u59cb\u6570\u636e\u2026\u2026",
    "\u4f60\u628a\u5bc7\u5c14\u8299\u81ea\u5df1\u7559\u5728\u4e2d\u67a2\u533a\u4e86\uff1f",
    "\u554a\u2026\u2026\u662f\u2026\u2026\u662f\u7684\u2026\u2026\uff1f",
    "\u57fa\u5730\u8f66\u603b\u63a7\u7cfb\u7edf\u5904\u4e8e\u5f00\u542f\u72b6\u6001\uff1f\u5904\u7406\u8fc7\u7ef4\u666e\u857e\u7684\u6743\u9650\u4e4b\u540e\uff0c\u6709\u6ca1\u6709\u6309\u7167\u89c4\u5b9a\u6e05\u9664\u8bbf\u95ee\u8bb0\u5f55\uff1f",
    "\u6211\uff0c\u6211\u6709\u70b9\u4e0d\u8bb0\u5f97\u4e86\u2026\u2026",
    "\u2026\u2026",
    "\u51fa\u4ec0\u4e48\u4e8b\u60c5\u4e86\u5417\uff1f\u662f\u2026\u2026\u662f\u5bc7\u5c14\u8299\u5f04\u574f\u4e86\u54ea\u91cc\uff1f\u90a3\u6211\u2026\u2026\u90a3\u6211\u8d76\u7d27\u53bb\u770b\u4e00\u4e0b\u2014\u2014",
    "\u5bc7\u5c14\u8299\u8d70\u4e86\u3002",
    "\u2026\u2026\u4ec0\u4e48\u2026\u2026\u8d70\u4e86\u2026\u2026",
    "\u7f8e\u73b2\uff0c\u62df\u6001\u56fe\u8c31\u5148\u653e\u5728\u4e00\u8fb9\uff0c\u4f60\u7acb\u523b\u5230\u6307\u6325\u5ba4\u6765\u3002",
    "\u5230\u5e95\u3001\u5230\u5e95\u51fa\u4ec0\u4e48\u4e8b\u4e86\u554a\u6307\u6325\u5b98\uff1f\u4ec0\u4e48\u8d70\u4e86\u2026\u2026\u662f\u3001\u96be\u9053\u662f\u5bc7\u5c14\u8299\u5979\u2026\u2026",
    "\u2026\u2026\u55ef\u3002",
    "\u201c\u827e\u83ab\u53f7\u201d\u7684\u901a\u8baf\u4ee3\u7801\u8bb0\u5f55\u4e2d\u6ca1\u6709\u627e\u5230\u5bc7\u5c14\u8299\u7684\u540d\u5b57\uff0c\u5979\u7684\u6743\u9650\u53ef\u80fd\u5df2\u7ecf\u4ece\u201c\u827e\u83ab\u53f7\u201d\u4e0a\u5220\u9664\u3002",
    "\u5220\u3001\u5220\u4e86\uff1f\uff01\u5bc7\u5c14\u8299\uff1f\uff01\u53ef\u8fd9\u6837\u2014\u2014\u5bc7\u5c14\u8299\u7684\u5fc3\u667a\u5907\u4efd\u4e5f\u90fd\u6ca1\u4e86\u5440\uff1f",
    "\u96be\u9053\u662f\u2026\u2026\u662f\u6628\u5929\u2026\u2026\u662f\u56e0\u4e3a\u6211\uff1f\u2026\u2026\u660e\u660e\u5bc7\u5c14\u8299\u6700\u8fd1\u90fd\u5f88\u4e0d\u5f00\u5fc3\u2026\u2026\u6211\u79bb\u5f00\u7684\u65f6\u5019\u8fd8\u5fd8\u4e86\u9501\u5b9a\u603b\u63a7\u7cfb\u7edf\u3001\u6e05\u9664\u8bb0\u5f55\u2026\u2026\u8fd8\u628a\u5979\u4e00\u4e2a\u4eba\u7559\u5728\u4e2d\u67a2\u533a\u2026\u2026|\u662f\u6211\u2026\u2026\u545c\u5443\u3001\u5bf9\u4e0d\u8d77\uff01\u6307\u6325\u5b98\uff0c\u5bf9\u4e0d\u8d77\uff0c\u662f\u3001\u662f\u6211\u592a\u9a6c\u864e\u4e86\uff0c\u662f\u6211\u2014\u2014",
    "\u6ca1\u4e8b\uff0c\u4f60\u5148\u8fc7\u6765\u3002\u95ea\u7535\uff0c\u548c\u8428\u73c8\u786e\u8ba4\u8fc7\u5750\u6807\u4e86\u5417\uff1f",
    "\u8fd8\u6ca1\u6709\u6536\u5230\u8428\u73c8\u7684\u56de\u590d\u3002",
    "\uff08\u6211\u5c3d\u529b\u7a33\u5b9a\u4e86\u4e00\u4e0b\u81ea\u5df1\u7684\u60c5\u7eea\u3002\uff09",
    "\u2026\u2026\u786e\u8ba4\u540e\uff0c\u7acb\u523b\u8bbe\u7f6e\u76ee\u6807\u70b9\uff0c\u5168\u901f\u884c\u8fdb\u3002|\u95ea\u7535\uff0c\u51c6\u5907\u4f5c\u6218\u3002"
  ]
]