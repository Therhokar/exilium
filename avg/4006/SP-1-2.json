[
  [
    "\u547c\uff0c\u603b\u7b97\u641e\u5b9a\u4e86~",
    "\u60c5\u62a5\u5df2\u7ecf\u5230\u624b\u4e86\uff0c\u83b1\u5a1c\uff0c\u90a3\u5bb6\u4f19\u53ea\u662f\u4e00\u4e2a\u4e2d\u95f4\u5546\u800c\u5df2\u3002",
    "\u53e3\u4f9b\u548c\u80fd\u6316\u5230\u7684\u8d44\u6599\u6211\u90fd\u53d1\u9001\u7ed9\u4f60\u4e86\uff0c\u5f53\u4e00\u4e2a\u5907\u4efd\uff0c\u5230\u65f6\u7ed9\u683c\u7433\u5427\u3002",
    "\u6709\u4ef7\u503c\u7684\u60c5\u62a5\u4e0d\u591a\uff1f",
    "\u55ef\u3002\u786e\u5b9e\u80fd\u4ece\u4fa7\u9762\u5370\u8bc1\u4e00\u4e9b\u4e8b\u5b9e\uff0c\u4e0d\u8fc7\u4e5f\u6ca1\u4ec0\u4e48\u65b0\u9c9c\u7684\u3002",
    "\u66f4\u7cbe\u51c6\u7684\u60c5\u62a5\u8fd8\u662f\u5f97\u9760\u683c\u7433\u90a3\u8fb9\u3002",
    "\u6211\u5c31\u8bf4\u683c\u7433\u59d0\u6ca1\u63d0\u8fc7\u7684\u60c5\u62a5\u591a\u534a\u662f\u4e0d\u51c6\u7684\u561b\u3002",
    "\u4f46\u6211\u8fd8\u662f\u60f3\u8bd5\u4e00\u8bd5\uff0c\u603b\u4e0d\u80fd\u6bcf\u6b21\u90fd\u4f9d\u8d56\u5979\u3002",
    "\u6240\u4ee5\u4f9d\u8d56\u6211\u5c31\u53ef\u4ee5\u54af\uff1f",
    "\u786c\u8981\u8bf4\u7684\u8bdd\u2026\u2026\u8fd9\u6b21\u4efb\u52a1\u7684\u6700\u4f73\u4eba\u9009\u5c31\u662f\u4f60\u3002",
    "\u2026\u2026\u554a\u266a",
    "\uff08\u83b1\u5a1c\u7b11\u4e86\u8d77\u6765\uff0c\u4e00\u526f\u5f88\u9a84\u50b2\u7684\u6837\u5b50\u3002\uff09",
    "\uff08\u6211\u5c06\u89c6\u7ebf\u8f6c\u5411\u4e86\u5468\u56f4\u7684\u673a\u68b0\u548c\u76d1\u63a7\u6444\u50cf\u5934\u3002\uff09",
    "\u90fd\u5904\u7406\u5b8c\u4e86\uff1f",
    "\u662f\u5440\uff0c\u76d1\u63a7\u91cc\u7684\u6570\u636e\u5df2\u7ecf\u5168\u90e8\u6e05\u7a7a\u4e86\u54e6~",
    "\u8fd9\u4e9b\u673a\u68b0\u5462\uff1f",
    "\u770b\u8d77\u6765\u2026\u2026\u4f60\u6ca1\u6709\u5b8c\u5168\u7834\u574f\u5b83\u4eec\u3002",
    "\u4e0d\u81ea\u89c9\u5c31\u2026\u2026\u4e0d\u8fc7\uff0c\u76f8\u5173\u7684\u8bb0\u5f55\u4e5f\u5df2\u7ecf\u6e05\u9664\u4e86\uff0c\u4e0d\u4f1a\u6709\u4ec0\u4e48\u5f71\u54cd~",
    "\u662f\u60f3\u8d77\u4f60\u7684\u8fc7\u53bb\u4e86\uff1f",
    "\u4e0d\u8981\u8bf4\u51fa\u6765\u561b\uff01",
    "\u5bf9\u4e86\uff0c\u90a3\u4e2a\u5bb6\u4f19\u600e\u4e48\u5904\u7406\uff1f",
    "\u8fd9\u79cd\u9ebb\u70e6\u7684\u4e8b\uff0c\u4e00\u4f1a\u8ba9\u6cbb\u5b89\u90e8\u95e8\u53bb\u6536\u62fe\u5427\u3002",
    "\u8bdd\u8bf4\u56de\u6765\uff0c\u6316\u60c5\u62a5\u8fd9\u79cd\u5c0f\u4e8b\u660e\u660e\u76f4\u63a5\u8ba9\u6211\u6765\u5c31\u597d\u4e86\u561b\uff0c\u4e3a\u4ec0\u4e48\u8fd8\u8981\u81ea\u5df1\u6d89\u9669\uff1f",
    "\u5982\u679c\u6211\u4e0d\u4eb2\u81ea\u505a\u4e9b\u4ec0\u4e48\uff0c\u603b\u89c9\u5f97\u5fc3\u91cc\u4e0d\u592a\u8e0f\u5b9e\u2026\u2026",
    "\u563f\u563f\uff0c\u90a3\u8fd9\u6b21\u6211\u7b97\u662f\u5e2e\u5230\u6307\u6325\u5b98\u4e86\u54e6\uff1f",
    "\u8bf4\u597d\u7684\u5956\u52b1\u8fd8\u6709\u8865\u507f\uff0c\u4e00\u70b9\u90fd\u4e0d\u80fd\u5c11\uff01",
    "\u4f60\u8981\u4ec0\u4e48\uff1f",
    "\u6211\u8981\u2014\u2014\u6211\u3001\u6211\u8bf4\u4e86\u554a\uff1f\u8bf4\u4e86\u4f60\u53ef\u4e0d\u80fd\u62d2\u7edd\u2026\u2026",
    "\u662f\u2026\u2026\uff1f",
    "\u8ba9\u6211\u4e5f\u53bb\u827e\u83ab\u53f7\uff01\u6211\u77e5\u9053\u53ef\u9732\u51ef\u5979\u4eec\u4e5f\u5728\u90a3\u8fb9\uff0c\u8ba9\u6211\u4eec\u4e45\u8fdd\u5730\u91cd\u65b0\u4e00\u8d77\u884c\u52a8\u5427\uff01",
    "\u4f46\u683c\u7433\u548c\u83b1\u5a05\u90a3\u8fb9\u8fd8\u6709\u9700\u8981\u4f60\u505a\u7684\u4e8b\u5427\uff1f",
    "\u8bdd\u662f\u8fd9\u4e48\u8bf4\u5566\uff0c\u6709\u9700\u8981\u5c31\u518d\u53bb\u5e2e\u683c\u7433\u59d0\u5c31\u597d\u4e86\u5440\uff01",
    "\u83b1\u5a05\u59d0\u7684\u8bdd\u2026\u2026\u7b49\u683c\u7433\u59d0\u90a3\u8fb9\u7684\u95ee\u9898\u89e3\u51b3\u540e\uff0c\u4e5f\u4f1a\u6765\u7684\uff01\u5e0c\u671b\u5979\u80fd\u548c\u683c\u7433\u59d0\u4e00\u8d77\uff01",
    "\u2026\u2026\u6211\u660e\u767d\u4e86\uff0c\u4ece\u4e00\u5f00\u59cb\u4f60\u7b54\u5e94\u5e2e\u6211\uff0c\u5c31\u662f\u6253\u7684\u8fd9\u4e2a\u4e3b\u610f\u5427\uff1f",
    "\u563f\u563f\uff0c\u6211\u5c31\u662f\u60f3\u548c404\u7684\u5bb6\u4eba\u4eec\u8fd8\u6709\u6307\u6325\u5b98\u56e2\u805a\u5440~",
    "\u201c\u5bb6\u4eba\u201d\u561b\uff0c\u5c31\u662f\u5e94\u8be5\u5f85\u5728\u4e00\u8d77\u5440\u3002\u6ca1\u9519\u5427\uff0c\u6307\u6325\u5b98\uff1f",
    "\u771f\u62ff\u4f60\u6ca1\u529e\u6cd5\uff0c\u8fd9\u6b21\u6211\u662f\u771f\u7684\u8981\u548c\u83b1\u5a05\u597d\u597d\u89e3\u91ca\u4e86\u2026\u2026",
    "\u8d70\u5427\uff0c\u5e0c\u671b\u8fd9\u6b21\u7ed5\u8def\u6ca1\u6709\u803d\u8bef\u548c\u683c\u7433\u7684\u7ea6\u5b9a\u3002",
    "\u4e0d\u4f1a\u7684\uff0c\u65f6\u95f4\u8fd9\u79cd\u4e8b\u6211\u53ef\u662f\u7b97\u5f97\u975e\u5e38\u7cbe\u51c6\u7684\uff01",
    "\u90a3\u6211\u4eec\u8d76\u7d27\u56de\u827e\u83ab\u53f7\u5427\uff0c\u6307\u6325\u5b98\uff01",
    "\u5929\u6709\u4e9b\u9ed1\u4e86\u5462\uff0c\u4e3a\u4e86\u9632\u6b62\u8d70\u6563\uff0c\u563f\u563f\u2014\u2014",
    "\uff08\u83b1\u5a1c\u4e3b\u52a8\u7275\u8d77\u6211\u7684\u624b\uff0c\u4e0e\u6211\u5e76\u80a9\u8d70\u8fdb\u4e86\u7b3c\u7f69\u7740\u536b\u661f\u57ce\u7684\u6708\u5149\u91cc\u3002\uff09",
    "\uff08\u5728\u90a3\u4e00\u6210\u4e0d\u53d8\u7684\u6708\u5149\u4e0b\uff0c\u6211\u4eec\u7684\u5f71\u5b50\u62d6\u5f97\u5f88\u957f\u3002\uff09",
    "\uff08\u65f6\u5149\u6216\u8bb8\u4f1a\u6539\u53d8\u5f88\u591a\u4e1c\u897f\u2014\u2014\uff09",
    "\uff08\u7136\u800c\u771f\u6b63\u91cd\u8981\u7684\u5bb6\u4eba\uff0c\u5374\u662f\u5728\u6d41\u901d\u7684\u65f6\u5149\u4e2d\uff0c\u6c38\u8fdc\u65e0\u53ef\u52a8\u6447\u7684\u5b58\u5728\u3002\uff09"
  ]
]