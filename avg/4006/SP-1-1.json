    "\u554a\uff0c\u8bf4\u6765\u8fd9\u4e00\u8def\u4e5f\u6ca1\u8fdb\u884c\u4ec0\u4e48\u8865\u7ed9\uff0c\u6e34\u4e86\u5427\uff1f",
    "\u4f60\u8fd9\u4e48\u4e00\u8bf4\u662f\u6709\u70b9\u3002",
    "\u55ef\uff0c\u6211\u8bb0\u5f97\u8fd9\u9644\u8fd1\u6709\u5bb6\u5c0f\u5e97\u6765\u7740\u2026\u2026",
    "\uff08\u83b1\u5a1c\u73af\u987e\u56db\u5468\uff0c\u62c9\u7740\u6211\u8d70\u5230\u8857\u8fb9\u7684\u4e00\u6247\u5c0f\u5c0f\u7684\u552e\u5356\u7a97\u53e3\u524d\u3002\uff09",
    "\u5e78\u597d\u8fd8\u5728\uff0c\u8fd9\u5bb6\u5e97\u6709\u4e0d\u5c11\u7279\u996e\uff0c\u6307\u6325\u5b98\u60f3\u559d\u4ec0\u4e48\uff1f",
    "\u90fd\u884c\uff0c\u80fd\u89e3\u6e34\u5c31\u884c\u3002",
    "\u611f\u89c9\u4f60\u5bf9\u8fd9\u91cc\u8fd8\u633a\u719f\u6089\u7684\u3002",
    "\u4ee5\u524d\u6267\u884c\u4efb\u52a1\u65f6\u5f85\u8fc7\u4e00\u6bb5\u65f6\u95f4~",
    "\u6240\u4ee5~\u5514\u2014\u2014\u7ed9\u6307\u6325\u5b98\u6765\u4e00\u74f6\u7535\u6c60\u9178\u6db2\u600e\u4e48\u6837\uff1f",
    "\u554a\uff1f",
    "\u563f\u563f~\u90a3\u5c31\u8fd9\u4e2a\u5566\uff01",
    "\u540d\u5b57\u542c\u7740\u5f88\u5413\u4eba\u5bf9\u5427\uff1f\u4f46\u5176\u5b9e\u5c31\u662f\u666e\u901a\u7684\u78b3\u9178\u996e\u6599~\u540d\u5b57\u90fd\u662f\u5671\u5934\u5566\u3002",
    "\uff08\u9648\u65e7\u4f46\u6574\u6d01\u7684\u7a97\u53e3\u4e0a\uff0c\u5f20\u8d34\u7740\u892a\u8272\u7684\u83dc\u5355\u3002\u5728\u53d1\u7535\u673a\u7684\u55e1\u55e1\u4f5c\u54cd\u4e2d\uff0c\u6ee1\u8138\u5199\u7740\u5751\u4eba\u7684\u8001\u677f\uff0c\u4ece\u51b0\u7bb1\u91cc\u62ff\u51fa\u4e86\u4e00\u74f6\u4e0e\u4ef7\u683c\u4e0d\u7b26\u7684\u78b3\u9178\u996e\u6599\u2026\u2026\uff09",
    "\uff08\u6211\u5c1d\u4e86\u5c1d\uff0c\u8001\u5b9e\u8bf4\uff0c\u8fd8\u4e0d\u9519\u3002\uff09",
    "\uff08\u8fd9\u5ea7\u7ecf\u8fc7\u4e86\u51c0\u5316\u5de5\u7a0b\u6539\u5efa\u7684\u536b\u661f\u57ce\uff0c\u548c\u5b83\u5f88\u591a\u5e74\u524d\u6f66\u5012\u7684\u6837\u5b50\u76f8\u6bd4\uff0c\u4e5f\u8fd8\u4e0d\u9519\u3002\uff09",
    "\u8fd9\u5ea7\u57ce\u5e02\u53d1\u5c55\u5f97\u8fd8\u4e0d\u9519\u554a\uff0c\u6211\u8fd8\u4ee5\u4e3a\u53ea\u6709\u50cf\u5965\u5fb7\u5345\u6d32\u90a3\u8fb9\u624d\u6709\u8fd9\u79cd\u7e41\u8363\u7684\u666f\u8c61\u3002",
    "\u6bd5\u7adf\u4e00\u5207\u4e5f\u5728\u6162\u6162\u53d8\u597d\u561b\u2026\u2026",
    "\u5c31\u8fd9\u6837\u4e0b\u53bb\uff0c\u4e00\u5b9a\u4f1a\u6709\u4e00\u5207\u90fd\u7ed3\u675f\u3001\u5927\u5bb6\u90fd\u5b89\u5fc3\u7684\u90a3\u4e00\u5929\uff0c\u8bf4\u4e0d\u5b9a\uff0c\u4ee5\u540e\u53ef\u4ee5\u548c\u5bb6\u4eba\u4eec\u4e00\u8d77\u5728\u54ea\u91cc\u5b9a\u5c45\u5462\uff1f",
    "\u54c8\u54c8\u2026\u2026\u5982\u679c\u771f\u80fd\u6709\u90a3\u4e00\u5929\u2026\u2026",
    "\uff08\u6211\u53f9\u4e86\u53e3\u6c14\u3002\uff09",
    "\u5e72\u561b\u8981\u53f9\u6c14\u5440\uff01\u6307\u6325\u5b98\u4e0d\u76f8\u4fe1\u6211\u8bf4\u7684\u8bdd\u5417\uff1f",
    "\u5514\u2026\u2026\u597d\u5427\uff0c\u6211\u662f\u6ca1\u6709\u83b1\u5a05\u59d0\u683c\u7433\u59d0\u9760\u8c31\u5566\uff0c\u4f46\u683c\u7433\u59d0\u4e5f\u662f\u8fd9\u4e48\u60f3\u7684\uff01\u6307\u6325\u5b98\u603b\u8981\u76f8\u4fe1\u683c\u7433\u59d0\u5427\uff01",
    "\u5979\u4e5f\u2026\u2026",
    "\u6ca1\u9519\u54e6\uff0c\u4e0d\u8981\u8bf4\u73b0\u5728\u4e86\uff0c\u5c31\u662f\u4f60\u4eec\u5728ODE-01\u518d\u6b21\u76f8\u9047\u3001\u5316\u89e3\u4e86\u8bef\u4f1a\u4e4b\u524d\uff0c\u5979\u5176\u5b9e\u90fd\u8bf4\u8fc7\u597d\u591a\u6b21\u7c7b\u4f3c\u7684\u8bdd\u54e6\u3002",
    "\u867d\u7136\u90a3\u4e2a\u65f6\u5019\u683c\u7433\u59d0\u603b\u559c\u6b22\u88c5\u4f5c\u751f\u4f60\u6c14\u7684\u6837\u5b50\uff0c\u4f46\u6211\u77e5\u9053\u5979\u4e00\u76f4\u5728\u6084\u6084\u5173\u6ce8\u7740\u4f60\u3001\u60f3\u8981\u5e2e\u4f60\u5462\uff01\u5927\u5bb6\u90fd\u662f\u5bb6\u4eba\u5440\uff01",
    "\u53ea\u662f\u5979\u592a\u5fd9\u4e86\u2026\u2026\u4e0d\u80fd\u50cf\u6211\u8fd9\u6837\u8bf4\u8dd1\u5c31\u8dd1\u8fc7\u6765\u5566\u3002",
    "\u2026\u2026\u2026\u2026",
    "\uff08\u770b\u6211\u6ca1\u6709\u56de\u590d\uff0c\u83b1\u5a1c\u4e5f\u5b89\u9759\u4e86\u4e0b\u6765\uff0c\u4e00\u8d2f\u4ee5\u6765\u7684\u53e4\u7075\u7cbe\u602a\u6d88\u5931\u4e86\u3002\uff09",
    "\uff08\u6211\u4eec\u5c31\u8fd9\u6837\u4fdd\u6301\u7740\u9ed8\u5951\uff0c\u6c89\u9ed8\u5730\u6ce8\u89c6\u7740\u773c\u524d\u7684\u8857\u9053\u3002\uff09",
    "\uff08\u4e00\u5982\u5176\u4ed6\u536b\u661f\u57ce\u4e00\u6837\uff0c\u8fd9\u91cc\u9648\u65e7\u3001\u5435\u95f9\uff0c\u5374\u5e76\u4e0d\u6df7\u4e71\u3002\uff09",
    "\u83b1\u5a05\u59d0\u66fe\u5728\u81ed\u6c34\u6c9f\u4e00\u6837\u7684\u8857\u9053\u4e0a\u6551\u4e86\u6211\uff0c\u90a3\u65f6\u7684\u6211\u8fd8\u662f\u83b1\u5a1c\uff0c\u540e\u6765\u6211\u8f6c\u53d8\u6210\u4e86UMP9\uff0c\u73b0\u5728\u6211\u53c8\u56de\u6765\u4e86\uff0c\u867d\u7136\u548c\u4ee5\u524d\u5f88\u4e0d\u4e00\u6837\u2026\u2026\u4f46\u6211\u53c8\u4f5c\u4e3a\u83b1\u5a1c\u6d3b\u5728\u4e86\u8fd9\u4e2a\u4e16\u754c\u4e0a\u3002",
    "UMP9\u7684\u5b58\u5728\u662f\u4e3a\u4e86404\u5c0f\u961f\u3002",
    "\u800c\u83b1\u5a1c\u2026\u2026\u662f\u4e3a\u4e86\u5bb6\u4eba\u624d\u5b58\u5728\u7684\u3002",
    "\u6211\u66fe\u5728\u6700\u9ed1\u6697\u7684\u65f6\u523b\u83b7\u5f97\u4e86\u62ef\u6551\u6211\u7684\u5bb6\u4eba\uff0c\u6211\u4e5f\u60f3\u2026\u2026\u8ddf\u4e0a\u5bb6\u4eba\u4eec\u7684\u811a\u6b65\uff0c\u80fd\u591f\u5e2e\u4e0a\u5927\u5bb6\u3002",
    "\u6307\u6325\u5b98\uff0c\u5927\u5bb6\u4e5f\u66fe\u7ecf\u88ab\u4f60\u62ef\u6551\uff0c\u73b0\u5728\uff0c\u4e5f\u968f\u65f6\u7b49\u7740\u4f60\u7684\u547d\u4ee4\u3002",
    "\u6211\u7684\u547d\u4ee4\u2026\u2026",
    "\u6ca1\u9519\uff0c\u201c\u6765\u4e00\u8d77\u6218\u6597\u5427\u201d\uff0c\u5927\u5bb6\u90fd\u4e00\u76f4\u5728\u7b49\u7740\u4f60\u8fd9\u4e48\u8bf4\u54e6~",
    "\uff08\u8fd8\u672a\u843d\u4e0b\u7684\u9633\u5149\uff0c\u7167\u8fc7\u5979\u7684\u5934\u53d1\uff0c\u5370\u8c61\u4e2d\u90a3\u773c\u4e0a\u7684\u4f24\u75a4\u4eff\u4f5b\u53c8\u6d6e\u73b0\u4e86\u51fa\u6765\u3002\uff09",
    "\uff08\u66fe\u7ecf\u542c\u5979\u8bf4\u8fc7\uff0c\u90a3\u9053\u4f24\u75a4\u662f\u5979\u81ea\u5df1\u5272\u7684\uff0c\u4e3a\u4e86\u8d34\u8fd1\u81ea\u5df1\u7684\u5bb6\u4eba\u3002\uff09",
    "\uff08\u73b0\u5728\u7684\u5979\uff0c\u4e0d\u9700\u8981\u8fd9\u9053\u4f24\u75a4\uff0c\u4e5f\u4e0e\u5bb6\u4eba\u4eec\u5efa\u7acb\u4e86\u65e0\u53ef\u64bc\u52a8\u7684\u8054\u7cfb\u3002\uff09",
    "\uff08\u5979\u7b11\u4e86\u7b11\uff0c\u5c31\u50cf\u4e00\u76f4\u4ee5\u6765\u7684\u90a3\u6837\uff0c\u5634\u5507\u7684\u5f62\u72b6\u4e0e\u732b\u54aa\u76f8\u4f3c\uff0c\u6709\u4e9b\u4fcf\u76ae\u3002\uff09",
    "\uff08\u4f46\u8fd9\u4e00\u523b\uff0c\u5979\u773c\u4e2d\u7684\u7eaf\u7cb9\u4e0e\u5e72\u51c0\uff0c\u8ba9\u5979\u770b\u8d77\u6765\u66f4\u50cf\u5929\u4f7f\u3002\uff09",
    "\u2026\u2026",
    "\uff08\u773c\u524d\u8fd9\u4e2a\u770b\u4f3c\u5b69\u5b50\u7684\u5c11\u5973\u4eba\u5f62\uff0c\u624d\u4e0d\u662f\u4e2a\u5b69\u5b50\u3002\uff09",
    "\u554a\uff0c\u5230\u5403\u996d\u65f6\u95f4\u5566\uff01",
    "\uff08\u4e0b\u4e2a\u77ac\u95f4\uff0c\u8fd9\u4e2a\u6349\u6478\u4e0d\u5b9a\u7684\u4eba\u5f62\u53c8\u7a81\u7136\u8bf4\u8d77\u4e86\u5b8c\u5168\u65e0\u5173\u7684\u4e1c\u897f\u3002\uff09",
    "\u554a\uff1f",
    "\u2026\u2026\u54e6\uff0c\u53ea\u662f\u8bf4\u65f6\u95f4\u7684\u8bdd\uff0c\u597d\u50cf\u662f\u8fd9\u6837\u7684\u2026\u2026",
    "\u8d70\uff0c\u6211\u4eec\u53bb\u5403\u70b9\u4e1c\u897f\u3002\u8fd9\u9644\u8fd1\u6709\u5bb6\u9910\u5385\u7684\u8089\u5377\u53e3\u5473\u5f88\u4e0d\u9519\u54e6~",
    "\u8fd9\u79cd\u5730\u65b9\u2026\u2026\uff1f\u53e3\u5473\u4e0d\u9519\uff1f",
