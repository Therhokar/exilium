[
  [
    "\uff08\u8fc7\u4e86\u4e00\u6bb5\u65f6\u95f4\uff0c\u83b1\u5a1c\u4ece\u4e8c\u7ea7\u5e73\u5c42\u5207\u51fa\uff0c\u53d6\u56de\u4e86\u9a7e\u9a76\u6743\u9650\u3002\uff09",
    "\u547c\u2026\u2026\u603b\u7b97\u89e3\u51b3\u4e86~|\u55ef\uff1f\u600e\u4e48\u4e00\u76f4\u76ef\u7740\u6211\u770b\uff1f",
    "\uff08\u610f\u8bc6\u5230\u6211\u7684\u89c6\u7ebf\uff0c\u83b1\u5a1c\u7591\u60d1\u5730\u6478\u4e86\u6478\u81ea\u5df1\u7684\u8138\u988a\u3002\uff09",
    "\u6211\u521a\u521a\u5728\u60f3\uff0c\u4f60\u4ec0\u4e48\u65f6\u5019\u80fd\u4ece\u4e8c\u7ea7\u5e73\u5c42\u5207\u51fa\u6765\u3002",
    "\u8bf6\u5440\uff0c\u8fd9\u662f\u5728\u5173\u5fc3\u6211\u5417\uff1f",
    "\u6bd5\u7adf\u4f60\u518d\u665a\u4e00\u4f1a\u5207\u51fa\u6765\uff0c\u53ef\u80fd\u5c31\u4e0d\u5728\u8f66\u4e0a\u4e86\u3002|\u6211\u4eec\u5c41\u80a1\u540e\u8fb9\uff0c\u73b0\u5728\u53ef\u70ed\u95f9\u4e86\u3002",
    "\uff08\u6211\u6307\u6307\u8eab\u540e\u5608\u6742\u58f0\u97f3\u7684\u6765\u6e90\u2014\u2014\uff09",
    "\uff08\u53ea\u89c1\u51e0\u53f0\u91cd\u578b\u8f66\u6b63\u6c14\u52bf\u6c79\u6d8c\u5730\u8ffd\u8d76\u4e0a\u6765\uff0c\u7ecf\u8fc7\u6539\u9020\u7684\u5f15\u64ce\u53d1\u51fa\u9635\u9635\u8f70\u9e23\u3002\uff09",
    "\uff08\u90a3\u4e9b\u968f\u4fbf\u710a\u63a5\u4e0a\u53bb\u7684\u88c5\u7532\u548c\u6302\u5728\u8f66\u58c1\u5916\u7684\u571f\u532a\uff0c\u65e0\u4e0d\u8bf4\u660e\u4ed6\u4eec\u7684\u8eab\u4efd\u3002\uff09",
    "\u74e6\u826f\u683c\u5e2e\uff01\u5c45\u7136\u8fd9\u4e48\u5feb\u5c31\u8ffd\u4e0a\u6765\u4e86\u2026\u2026",
    "\u9ebb\u70e6\u4e86\u5440\uff0c\u6307\u6325\u5b98\uff0c\u4f60\u770b\u90a3\u4e9b\u94a9\u5b50\u548c\u7f06\u7ef3\uff0c\u6211\u89c9\u5f97\u4ed6\u4eec\u8981\u8df3\u5e2e\u62a2\u8f66\u4e86\uff01",
    "\u770b\u8d77\u6765\u662f\u3002",
    "\u5750\u597d\u54af\uff0c\u63a5\u4e0b\u6765\u4f1a\u5f88\u523a\u6fc0\u54e6\uff01",
    "\uff08\u8bdd\u97f3\u672a\u843d\uff0c\u5979\u8e29\u4e0b\u79bb\u5408\u62c9\u8d77\u624b\u5239\uff0c\u518d\u4e00\u4e2a\u72e0\u6253\u65b9\u5411\u76d8\uff0c\u8ba9\u8f66\u5c3e\u8d27\u7bb1\u72e0\u72e0\u5730\u649e\u5230\u4e00\u8f86\u8ffd\u8f66\u7684\u9a7e\u9a76\u5ba4\u4e0a\u3002\uff09",
    "\u5c0f\u5fc3\uff01",
    "\uff08\u8ffd\u8f66\u5982\u540c\u88ab\u5de8\u5927\u6994\u5934\u62a1\u51fb\u4e86\u4e00\u822c\uff0c\u77ac\u95f4\u5931\u53bb\u4e86\u5e73\u8861\uff0c\u4f34\u968f\u7740\u731b\u70c8\u7684\u5239\u8f66\u58f0\u65cb\u8f6c\u7740\u843d\u540e\u4e0b\u53bb\u3002\uff09",
    "\uff08\u4e3a\u4e86\u907f\u514d\u78b0\u649e\uff0c\u5176\u4ed6\u74e6\u826f\u683c\u5e2e\u7684\u8f7d\u5177\u7acb\u5373\u5f00\u59cb\u4e0e\u8d27\u8f66\u62c9\u5f00\u8ddd\u79bb\u3002\uff09",
    "\u563b\u563b\u266a\u73b0\u5728\u60f3\u8eb2\u53ef\u592a\u665a\u5566\uff01",
    "\uff08\u83b1\u5a1c\u624b\u4e0a\uff0c\u90a3\u628a\u60ef\u7528\u7684\u51b2\u950b\u67aa\u5df2\u7ecf\u4e0a\u819b\uff0c\u624b\u96f7\u4e5f\u5df2\u7ecf\u6302\u597d\u3002\uff09",
    "\uff08\u5979\u63a8\u5f00\u4e86\u9a7e\u9a76\u5ba4\u7684\u8f66\u95e8\uff0c\u5c06\u534a\u4e2a\u8eab\u5b50\u4f38\u4e86\u51fa\u53bb\u3002\uff09",
    "\uff089mm\u5e15\u62c9\u8d1d\u9c81\u59c6\u5b50\u5f39\u53d1\u51fa\u6012\u543c\uff0c\u6211\u770b\u5230\u5c11\u5973\u9732\u51fa\u81ea\u4fe1\u7684\u7b11\u5bb9\u3002\uff09",
    "\uff08\u83b1\u5a1c\u7684\u626b\u5c04\u6fc0\u8d77\u4e86\u5bf9\u65b9\u7684\u56de\u51fb\uff0c\u5728\u5bf9\u65b9\u5bc6\u96c6\u7684\u5b50\u5f39\u96e8\u4e2d\uff0c\u6709\u4e2a\u8ba9\u4eba\u4e0d\u5b89\u7684\u58f0\u97f3\u4ece\u5e95\u76d8\u4e0b\u4f20\u6765\uff0c\u74e6\u826f\u683c\u5e2e\u7684\u4e09\u6d41\u5c04\u624b\u4eec\u6700\u7ec8\u8fd8\u662f\u51fb\u4e2d\u4e86\u6211\u4eec\u7684\u8f6e\u80ce\u3002\uff09",
    "\u554a\uff0c\u8fd0\u6c14\u771f\u7cdf\uff01",
    "\uff08\u83b1\u5a1c\u4e00\u628a\u7a33\u7a33\u5730\u63a7\u5236\u4f4f\u6cb9\u95e8\u548c\u65b9\u5411\u76d8\uff0c\u8c28\u614e\u7684\u64cd\u4f5c\u8ba9\u8f66\u5b50\u5e76\u6ca1\u6709\u50cf\u5927\u591a\u6570\u60c5\u51b5\u90a3\u6837\u4fa7\u7ffb\u51fa\u53bb\u3002\uff09",
    "\u8bf6\u5440\uff0c\u672c\u6765\u4e0d\u60f3\u8fd9\u4e48\u7c97\u66b4\u7684\u5462\u3002",
    "\u4e45\u8fdd\u5730\u8981\u5728\u73b0\u5b9e\u4e2d\u52a8\u52a8\u7b4b\u9aa8\u4e86\uff1f",
    "\u653e\u5fc3\u5427\uff0c\u5b9e\u6218\u7684\u6211\u53ea\u4f1a\u66f4\u52a0\u5f3a\u5927\u54e6\uff01"
  ]
]