[
  [
    "\uff08\u574d\u584c\u98ce\u66b4\u521a\u8fc7\uff0c\u4fe1\u53f7\u65ad\u65ad\u7eed\u7eed\uff0c\u5c4f\u5e55\u4e0a\u4e5f\u53ea\u662f\u663e\u793a\u7740\u201c\u58f0\u9053\u8baf\u606f\u201d\u3002\uff09",
    "\u8c22\u8c22\uff0c\u592a\u8c22\u8c22\u4e86\uff01",
    "\u6e05\u70b9\u4e00\u4e0b\u4eba\u5458\uff0c\u770b\u770b\u6709\u6ca1\u6709\u9700\u8981\u6025\u6551\u7684\uff0c\u6211\u7684\u827e\u83ab\u53f7\u4e0a\u6709\u533b\u7597\u5175\u3002",
    "\u53e6\u5916\uff0c\u7269\u8d44\u4e5f\u770b\u770b\uff0c\u4e0d\u8fc7\uff0c\u8fd9\u4e00\u70b9\u6211\u5c31\u6ca1\u529e\u6cd5\u4e86\u3002",
    "\u4f60\u80fd\u63a5\u5230\u6211\u4eec\u7684\u6c42\u6551\u4fe1\u53f7\uff0c\u8fc7\u6765\u5e2e\u6211\u4eec\u5c31\u5f88\u597d\u4e86\uff01",
    "\u5982\u679c\u6ca1\u6709\u4f60\u2014\u2014\u522b\u8bf4\u6211\u4eec\u8fd9\u4e2a\u65b0\u5efa\u6210\u7684\u5c45\u4f4f\u70b9\u80fd\u4e0d\u80fd\u8fd8\u5728\uff0c\u6211\u4eec\u8fd9\u4e9b\u4eba\u53ef\u80fd\u90fd\u8981\u5f97\u4e0aE.L.I.D.\u2026\u2026",
    "\u5c31\u662f\u2026\u2026\u916c\u91d1\u7684\u95ee\u9898\u2026\u2026",
    "\u4e0d\u7528\u4e86\uff0c\u51e0\u4e2a\u751f\u9ab8\uff0c\u5bf9\u6211\u4eec\u6765\u8bf4\u4e0d\u662f\u4ec0\u4e48\u5927\u4e8b\u3002",
    "\u2026\u2026\u4e0d\u4e0d\u4e0d\uff0c\u5176\u5b9e\u2026\u2026\u662f\u6211\u8fd8\u6709\u4e00\u4e2a\u8bf7\u6c42\u2026\u2026",
    "\u554a\uff1f"
  ],
  [
    "\uff08\u5728\u9ec4\u533a\uff0c\u201c\u5c45\u4f4f\u70b9\u201d\u8fd9\u79cd\u542c\u8d77\u6765\u5f88\u968f\u610f\u7684\u79f0\u547c\uff0c\u5b9e\u9645\u4e0a\u662f\u4ee5\u4e00\u4e2a\u96c6\u4f53\u5171\u4eab\u8d22\u5bcc\u3001\u571f\u5730\u3001\u5efa\u7b51\u3001\u751f\u4ea7\u8bbe\u5907\u3001\u751f\u5b58\u98ce\u9669\u800c\u6784\u6210\u7684\u5c0f\u578b\u4eba\u7c7b\u793e\u533a\u3002\uff09",
    "\uff08\u8fd9\u6837\u7684\u96c6\u5408\u5728\u5982\u4eca\u8fd9\u4e2a\u7834\u788e\u7684\u4e16\u754c\u4e0a\u6050\u6015\u662f\u5c11\u6570\uff0c\u4f46\u662f\u5728\u9ec4\u533a\u8fd9\u79cd\u574d\u584c\u98ce\u66b4\u3001\u751f\u9ab8\u3001\u74e6\u826f\u683c\u5e2e\u8086\u8650\u7684\u73af\u5883\u4e0b\u5374\u4e0d\u662f\u4e2a\u4f8b\u3002\uff09",
    "\uff08\u4e0d\u7ba1\u5904\u4e8e\u4ec0\u4e48\u6837\u7684\u5883\u5730\uff0c\u4eba\u7c7b\u2014\u2014\u603b\u662f\u5728\u987d\u5f3a\u5730\u5bfb\u6c42\u7740\u751f\u5b58\u3002\uff09",
    "\u6240\u4ee5\u8bf4\uff0c\u7edb\u96e8\u5c0f\u59d0\uff0c\u8fd9\u6b21\u7684\u821e\u53f0\u5267\uff0c\u8981\u4ea4\u7ed9\u4f60\u4e86\uff01",
    "\u6b38\u2014\u2014\u5c31\u7b97\u4f60\u8bf4\u4ea4\u7ed9\u6211\uff0c\u6211\u4e5f\u53ea\u662f\u6765\u51d1\u4e2a\u70ed\u95f9\u2026\u2026",
    "\u54fc\uff0c\u671d\u6656\u548c\u7eef\u600e\u4e48\u4e0d\u6765\u5440\uff0c\u8eb2\u5728\u827e\u83ab\u53f7\u4e0a\u5c31\u77e5\u9053\u5077\u61d2\u2026\u2026",
    "\u300a\u574d\u584c\u98ce\u66b4\u4e0e\u827e\u83ab\u53f7\u300b\uff0c\u6211\u76f8\u4fe1\uff0c\u8fd9\u4e00\u5b9a\u662f\u4e00\u4e2a\u7edd\u5999\u7684\u6807\u9898\uff01",
    "\u2026\u2026\u6807\u9898\uff1f",
    "\u5bf9\uff0c\u554a\u54c8\u54c8\u2026\u2026\u5267\u672c\u8fd8\u6ca1\u5199\u2026\u2026",
    "\u5176\u5b9e\uff0c\u6211\u662f\u60f3\u8bf7\u4f60\uff0c\u8bf7\u827e\u83ab\u53f7\u7684\u5b63\u98ce\u5c0f\u961f\u6765\u7ed9\u6211\u4eec\u8fd9\u91cc\u7684\u5b69\u5b50\u6392\u6f14\u4e00\u51fa\u821e\u53f0\u5267\uff0c\u4e3b\u9898\u662f\u8bb2\u4e00\u4e9b\u6c61\u67d3\u533a\u7684\u5e38\u8bc6\uff0c\u8bb2\u8bb2\u600e\u4e48\u624d\u80fd\u66f4\u597d\u5730\u9002\u5e94\u8fd9\u91cc\u7684\u73af\u5883\u3002",
    "\u4f60\u7684\u610f\u601d\u662f\uff0c\u5267\u672c\u4e5f\u8981\u8ba9\u6211\u4eec\u5199\uff1f",
    "\u6ca1\u6709\u8fd9\u4e48\u590d\u6742\uff0c\u5373\u5174\u4e5f\u53ef\u4ee5\uff1f\u5c31\u628a\u8981\u8bf4\u7684\u90a3\u4e9b\u8bf4\u5b8c\u5c31\u884c\u4e86\uff0c\u4e5f\u4e0d\u7b97\u662f\u4e2a\u4e13\u4e1a\u7684\u821e\u53f0\u5267\u3002",
    "\u54ce\u5440\uff0c\u5c31\u662f\u5f88\u53ef\u60dc\uff0c\u827e\u83ab\u53f7\u592a\u5927\u4e86\uff0c\u5230\u6211\u4eec\u8fd9\u8fb9\u6765\u6709\u70b9\u592a\u663e\u773c\uff0c\u53ea\u80fd\u505c\u5728\u9644\u8fd1\uff0c\u4e0d\u7136\u8fd8\u80fd\u642d\u4e2a\u5b9e\u666f\u3002",
    "\u7b49\u4e0b\u6b21\uff0c\u6211\u4eec\u5c45\u4f4f\u70b9\u53d1\u5c55\u8d77\u6765\u4e86\uff0c\u8bf4\u4e0d\u5b9a\u8fd8\u80fd\u4e13\u95e8\u505a\u4e00\u4e2a\u5927\u821e\u53f0\uff0c\u90a3\u65f6\u5019\uff0c\u4f1a\u518d\u9080\u8bf7\u4f60\u4eec\u6765\u3002",
    "\u554a\uff1f\u8fd8\u6709\u4e0b\u6b21\uff1f",
    "\uff08\u770b\u7740\u7edb\u96e8\u4e00\u526f\u5174\u81f4\u7f3a\u7f3a\u7684\u6837\u5b50\uff0c\u8d1f\u8d23\u4eba\u7b11\u4e86\u7b11\uff0c\u6307\u4e86\u6307\u5979\u8eab\u540e\u7684\u4e00\u4e2a\u65b9\u5411\u3002\uff09",
    "\uff08\u987a\u7740\u5979\u6307\u7684\u4f4d\u7f6e\uff0c\u7edb\u96e8\u770b\u5230\u6709\u51e0\u4e2a\u5c0f\u8111\u888b\u4ece\u4e00\u5927\u5806\u7834\u65e7\u7684\u6728\u677f\u540e\u5192\u4e86\u51fa\u6765\u3002\uff09",
    "\u6211\u4eec\u2026\u2026\u90fd\u662f\u521a\u8fdb\u5165\u6c61\u67d3\u533a\u7684\u3002\u5927\u4eba\u8fd8\u597d\uff0c\u867d\u7136\u8fd8\u9700\u8981\u9002\u5e94\uff0c\u4f46\u591a\u5c11\u6709\u4e9b\u751f\u6d3b\u7ecf\u9a8c\u2026\u2026\u8ddf\u7740\u6211\u4eec\u6765\u7684\u5b69\u5b50\u592a\u591a\u4e86\u3002",
    "\u5b69\u5b50\u4eec\u7684\u7236\u6bcd\uff0c\u57fa\u672c\u90fd\u2026\u2026\u603b\u4e4b\uff0c\u65e2\u7136\u6258\u4ed8\u7ed9\u4e86\u6211\u4eec\u8fd9\u4e9b\u5927\u4eba\uff0c\u603b\u8981\u52aa\u52aa\u529b\uff0c\u628a\u4ed6\u4eec\u517b\u5927\u3002\u4f46\u5b69\u5b50\u4eec\u5bf9\u4e8e\u600e\u4e48\u5728\u6c61\u67d3\u533a\u751f\u6d3b\u3001\u600e\u4e48\u9002\u5e94\u8fd9\u91cc\u7684\u73af\u5883\uff0c\u77e5\u9053\u7684\u592a\u5c11\u4e86\u3002",
    "\u4f60\u4eec\u521a\u521a\u624d\u6551\u4e86\u6211\u4eec\uff0c\u5c0f\u5b69\u5b50\u561b\uff0c\u603b\u4f1a\u5bf9\u4f60\u4eec\u8fd9\u4e9b\u4ed6\u4eec\u770b\u6765\u662f\u201c\u82f1\u96c4\u201d\u7684\u4eba\u7269\u611f\u5174\u8da3\uff0c\u5982\u679c\u662f\u4f60\u4eec\u51fa\u624b\uff0c\u90a3\u4e00\u5b9a\u5f88\u6709\u6548\u679c\uff01",
    "\u4f60\u770b\u90a3\u8fb9\uff0c\u6709\u4e0d\u5c11\u5b69\u5b50\u5728\u770b\u7740\u4f60\u5462\uff01\u7edb\u96e8\u5c0f\u59d0\u4e0e\u751f\u9ab8\u6218\u6597\u65f6\u7684\u82f1\u52c7\u8eab\u59ff\uff0c\u8bf7\u52a1\u5fc5\u8ba9\u5b69\u5b50\u4eec\u518d\u770b\u4e00\u6b21\uff01",
    "\uff08\u5b69\u5b50\u4eec\u7741\u7740\u4e00\u53cc\u53cc\u9ed1\u767d\u5206\u660e\u7684\u773c\u775b\uff0c\u597d\u5947\u53c8\u4e13\u6ce8\u5730\u76ef\u7740\u7edb\u96e8\u3002\uff09",
    "\u2026\u2026\u597d\u5427\u597d\u5427\uff01\u4f46\u6709\u6ca1\u6709\u4e0b\u6b21\u53e6\u8bf4\u54e6\uff01",
    "\u6ca1\u95ee\u9898\uff01\u591a\u8c22\u591a\u8c22\uff01",
    "\uff08\u8d1f\u8d23\u4eba\u677e\u4e86\u53e3\u6c14\uff0c\u79bb\u5f00\u4e86\u573a\u5730\u3002\uff09",
    "\uff08\u7edb\u96e8\u7ad9\u5728\u539f\u5730\u6101\u7709\u82e6\u8138\u5730\u601d\u8003\u4e86\u4e00\u4e0b\u3002\uff09",
    "\u6211\u8fd8\u4ee5\u4e3a\u53ea\u662f\u4e0b\u8f66\u653e\u677e\u4e00\u4e0b\u7684\u5c0f\u4efb\u52a1\uff0c\u6ca1\u60f3\u5230\u8fd8\u8981\u7f16\u5267\u672c\uff0c\u6392\u8282\u76ee\u2026\u2026\u771f\u8d39\u52b2\u554a\uff01",
    "\u8fd9\u79cd\u7a0b\u5ea6\u7684\u6f14\u51fa\uff0c\u5bf9\u4f60\u6765\u8bf4\u7b97\u4e0d\u4e0a\u8d39\u52b2\u5427\uff1f",
    "\u5267\u672c\u7684\u4e8b\uff0c\u6211\u4e5f\u4f1a\u5e2e\u5fd9\u7684\u3002",
    "\u563f\u563f\uff0c\u6211\u5c31\u7b49\u7740\u957f\u5b98\u7684\u8fd9\u53e5\u8bdd\u5462\u3002",
    "\uff08\u7edb\u96e8\u56de\u8fc7\u5934\uff0c\u5bf9\u6211\u5410\u4e86\u4e0b\u820c\u5934\u3002\uff09",
    "\uff08\u8fd9\u4e2a\u7ed1\u7740\u53cc\u9a6c\u5c3e\u7684\u5c0f\u59d1\u5a18\u8fd8\u662f\u548c\u4ee5\u524d\u4e00\u6837\uff0c\u6d3b\u529b\u6ee1\u6ee1\u3002\uff09",
    "\uff08\u4e0d\u8fdc\u5904\uff0c\u9edb\u70df\u5411\u7740\u6211\u4eec\u8d70\u6765\u3002\uff09",
    "\u7edb\u96e8\uff0c\u8d1f\u8d23\u4eba\u6765\u627e\u8fc7\u4f60\u4e86\u5427\uff1f",
    "\u8001\u59d0\uff01\u771f\u662f\u7684\u554a\uff01\u56de\u53bb\u827e\u83ab\u53f7\u4f60\u53ef\u8981\u597d\u597d\u8bf4\u8bf4\u671d\u6656\uff01\u5979\u4f5c\u4e3a\u5b63\u98ce\u5c0f\u961f\u7684\u961f\u957f\u600e\u4e48\u80fd\u8f7b\u6613\u5c31\u7b54\u5e94\u5462\uff01\u7136\u540e\u7b54\u5e94\u4e86\u8fd8\u8ba9\u6211\u548c\u4f60\u8fc7\u6765\uff01\u81ea\u5df1\u4e0d\u6765\uff01",
    "\u672c\u6765\u6211\u8fd8\u4ee5\u4e3a\u6765\u8dd1\u4e2a\u9f99\u5957\uff0c\u505a\u70b9\u6b66\u6253\u52a8\u4f5c\u6253\u6253\u9f13\u4e4b\u7c7b\u7684\uff0c\u505a\u4e3b\u6f14\u5c82\u4e0d\u662f\u5168\u5929\u90fd\u8981\u8017\u5728\u8fd9\u4e86\u2026\u2026",
    "\u8fd9\u4e2a\u59d4\u6258\uff0c\u662f\u957f\u5b98\u6279\u51c6\u7684\u54e6\u3002",
    "\u4ec0\u4e48\uff1f\u662f\u957f\u5b98\uff1f",
    "\u662f\u554a\uff0c\u6240\u4ee5\u6211\u624d\u548c\u4f60\u4eec\u4e00\u8d77\u5230\u8fd9\u91cc\u6765\u2014\u2014\u827e\u83ab\u53f7\u4e4b\u524d\u6ca1\u6709\u9047\u5230\u8fc7\u8fd9\u6837\u7684\u59d4\u6258\uff0c\u600e\u4e48\u8bf4\u5462\u2026\u2026\u6211\u89c9\u5f97\u8fd8\u7b97\u6709\u610f\u4e49\uff0c\u521a\u597d\u4e5f\u80fd\u53d1\u6325\u4f60\u4eec\u7684\u957f\u5904\uff0c\u6240\u4ee5\u5c31\u8ba9\u671d\u6656\u7b54\u5e94\u4e86\u3002",
    "\u5185\u5bb9\u7684\u8bdd\uff0c\u6211\u5df2\u7ecf\u5dee\u4e0d\u591a\u60f3\u597d\u4e86\u3002",
    "\u662f\u4ec0\u4e48\u5462\uff1f",
    "\u5c31\u50cf\u90a3\u4f4d\u8d1f\u8d23\u4eba\u6240\u8bf4\uff0c\u867d\u8bf4\u662f\u821e\u53f0\u5267\uff0c\u4e5f\u53ea\u662f\u9700\u8981\u6a21\u62df\u51e0\u4e2a\u60c5\u5883\uff0c\u7136\u540e\u544a\u8bc9\u5b69\u5b50\u4eec\u8981\u600e\u4e48\u5e94\u5bf9\u3002",
    "\u5c31\u4f9d\u7167\u6211\u4eec\u7684\u751f\u6d3b\u7ecf\u9a8c\uff0c\u603b\u7ed3\u51fa\u4e00\u4e9b\u5728\u6c61\u67d3\u533a\u91cc\u5e38\u89c1\u7684\u5371\u9669\u4e8b\u4ef6\uff0c\u7136\u540e\u4e32\u8d77\u6765\u5c31\u884c\u3002",
    "\u6709\u4e00\u4e2a\u95ee\u9898\uff0c\u90a3\u4e9b\u5b69\u5b50\u4eec\u5e74\u7eaa\u4e0d\u5927\uff0c\u6d3b\u52a8\u8303\u56f4\u5e94\u8be5\u4e5f\u5c31\u5728\u8fd9\u4e2a\u5c45\u4f4f\u70b9\u9644\u8fd1\u5427\uff1f",
    "\u4f60\u662f\u60f3\u8bf4\uff0c\u4e00\u4e9b\u4e8b\u4ef6\u5728\u6211\u4eec\u770b\u8d77\u6765\u89c9\u5f97\u5f88\u5e38\u89c1\uff0c\u4f46\u5728\u9644\u8fd1\u53ef\u80fd\u4e5f\u4e0d\u600e\u4e48\u53d1\u751f\uff0c\u5bf9\u5427\uff1f",
    "\u8fd9\u4e2a\u95ee\u9898\u6211\u4e5f\u60f3\u5230\u4e86\u3002\u6240\u4ee5\u5177\u4f53\u9009\u62e9\u54ea\u4e9b\u4e8b\u4ef6\uff0c\u6211\u4eec\u8fd8\u9700\u8981\u8c03\u67e5\u4e00\u4e0b\u9644\u8fd1\u7684\u73af\u5883\u518d\u51b3\u5b9a\u3002",
    "\u6ca1\u9519\u3002\u9664\u6b64\u4e4b\u5916\uff0c\u6211\u4eec\u8fd8\u8981\u2014\u2014",
    "\u6253\u4f4f\u6253\u4f4f\uff01\u957f\u5b98\u548c\u8001\u59d0\u7684\u610f\u601d\u90fd\u662f\u53ef\u4ee5\u53bb\u9644\u8fd1\u901b\u901b\u662f\u5427\uff1f",
    "\u597d\u8036\uff01\u90a3\u6211\u5148\u51fa\u53d1\u5566\uff01",
    "\u7edb\u96e8\uff01",
    "\uff08\u7edb\u96e8\u5bf9\u6211\u4eec\u9732\u51fa\u4e00\u4e2a\u5927\u5927\u7684\u7b11\u5bb9\uff0c\u62a2\u5148\u9009\u62e9\u4e86\u4e00\u4e2a\u65b9\u5411\uff0c\u8dd1\u8d70\u4e86\u3002\uff09",
    "\u2026\u2026\u5509\u3002",
    "\u62c5\u5fc3\u7684\u8bdd\uff0c\u6211\u4eec\u8ddf\u4e0a\u53bb\u770b\u770b\u5c31\u597d\u4e86\u3002",
    "\u597d\u3002",
    "\u4e0d\u8fc7\u8bf4\u8d77\u6765\uff0c\u6211\u4eec\u8981\u5728\u8fd9\u91cc\u7559\u51e0\u5929\uff1f\u7f8e\u73b2\u90a3\u8fb9\u6ca1\u95ee\u9898\u5417\uff1f",
