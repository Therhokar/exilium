[
  [
    "\u73af\u5883\u60c5\u62a5\u5f55\u5165\u5b8c\u6210\uff0c\u7f8e\u73b2\u4e5f\u5df2\u7ecf\u4ea4\u53c9\u786e\u8ba4\u8fc7\u3002",
    "\u55ef\uff0c\u8c03\u67e5\u5f97\u5dee\u4e0d\u591a\u4e86\uff0c\u6211\u4eec\u56de\u5c45\u4f4f\u70b9\u5427\u3002",
    "\u554a\uff1f\u8fd9\u5c31\u7ed3\u675f\u5566\uff1f\u957f\u5b98\uff0c\u518d\u7559\u4e00\u4f1a\u597d\u4e0d\u597d\u2014\u2014",
    "\uff08\u7edb\u96e8\u8bdd\u97f3\u672a\u843d\uff0c\u6211\u7684\u901a\u8baf\u7ec8\u7aef\u54cd\u4e86\u8d77\u6765\u3002\uff09",
    "\u5404\u4f4d\uff01\u51fa\u4e8b\u4e86\uff01",
    "\uff08\u2026\u2026\uff09",
    "\u2026\u2026\u60c5\u51b5\u5c31\u662f\u8fd9\u6837\u3002",
    "\u602a\u6211\u2026\u2026\u4e8b\u60c5\u592a\u591a\u4e86\uff0c\u6211\u4eec\u8fd9\u4eba\u624b\u53c8\u4e0d\u591f\uff0c\u6ca1\u770b\u4f4f\u2026\u2026",
    "\u8fd9\u3001\u8fd9\u600e\u4e48\u529e\u554a\u2026\u2026",
    "\u6211\u68b3\u7406\u4e00\u4e0b\u60c5\u51b5\u3002",
    "\u5728\u6211\u4eec\u79bb\u5f00\u8fd9\u91cc\u3001\u53bb\u9644\u8fd1\u63a2\u67e5\u671f\u95f4\uff0c\u5c45\u4f4f\u70b9\u53d7\u5230\u4e86\u4e00\u6279\u74e6\u826f\u683c\u5e2e\u7684\u88ad\u51fb\uff0c\u90e8\u5206\u7269\u8d44\u88ab\u62a2\u8d70\u4e86\u3002|\u4f60\u4eec\u90fd\u8eb2\u5230\u4e86\u4e8b\u5148\u642d\u597d\u7684\u63a9\u4f53\u4e0b\uff0c\u6709\u90e8\u5206\u4eba\u5458\u53d7\u4f24\u3002\u4f46\u5728\u74e6\u826f\u683c\u5e2e\u8d70\u540e\uff0c\u4f60\u4eec\u53d1\u73b0\u67093\u4e2a\u5b69\u5b50\u4e0d\u89c1\u4e86\uff1f",
    "\u5bf9\uff0c\u5bf9\u2026\u2026\u4ed6\u4eec\u4e0d\u4f1a\u88ab\u74e6\u826f\u683c\u5e2e\u6293\u8d70\u4e86\u5427\u2026\u2026",
    "\u53d7\u4f24\u7684\u4eba\uff0c\u60c5\u51b5\u600e\u4e48\u6837\uff1f",
    "\u2026\u2026\u90fd\u662f\u5c0f\u4f24\u2026\u2026",
    "\u654c\u65b9\u60c5\u51b5\u5982\u4f55\uff1f",
    "\u6211\u4eec\u90fd\u8eb2\u5728\u4e0d\u540c\u4f4d\u7f6e\uff0c\u770b\u4e0d\u592a\u6e05\u4ed6\u4eec\u5177\u4f53\u7684\u6570\u91cf\uff0c\u5e94\u8be5\u2026\u2026\u6709\u5341\u4e2a\uff1f\u8f66\u6709\u4e24\u8f86\u3002",
    "\u2026\u2026\u4e0d\u53ea\u662f\u544a\u8bc9\u5b69\u5b50\u4eec\u8981\u600e\u4e48\u9002\u5e94\u6c61\u67d3\u533a\u7684\u751f\u6d3b\uff0c\u53ef\u80fd\u2026\u2026\u4e0b\u4e00\u6b65\uff0c\u4f60\u4eec\u4e5f\u9700\u8981\u7740\u624b\u5b8c\u5584\u4f60\u4eec\u5c45\u4f4f\u70b9\u7684\u9632\u5fa1\u8bbe\u65bd\uff0c\u7136\u540e\uff0c\u91c7\u8d2d\u4e00\u4e9b\u6b66\u5668\u3002",
    "\u6211\u8fd9\u8fb9\u8ba4\u8bc6\u4e00\u4e9b\u6697\u5546\uff0c\u867d\u8bf4\u6253\u6298\u4e0d\u592a\u53ef\u80fd\uff0c\u4f46\u54c1\u8d28\u662f\u6709\u4fdd\u8bc1\u7684\u3002\u9700\u4e0d\u9700\u8981\u63a8\u8350\u7ed9\u4f60\uff1f",
    "\u8c22\u8c22\uff0c\u8c22\u8c22\u4f60\u2026\u2026",
    "\u6211\u4eec\u539f\u672c\u4e5f\u91c7\u8d2d\u4e86\u4e00\u4e9b\uff0c\u4f46\u8fd9\u9644\u8fd1\u574d\u584c\u98ce\u66b4\u592a\u591a\u4e86\uff0c\u4e00\u76f4\u6ca1\u9001\u8fc7\u6765\u2026\u2026\u5c31\u5728\u8fd9\u4e2a\u8282\u70b9\u4e0a\u2026\u2026\u5509\u2026\u2026",
    "\u6211\u5148\u628a\u4f69\u59ae\u6cfd\u7684\u8054\u7cfb\u65b9\u5f0f\u7ed9\u4f60\uff0c\u5979\u90a3\u8fb9\u624b\u6bb5\u591a\u4e00\u4e9b\u3002\u6025\u7528\u7684\u8bdd\uff0c\u80fd\u66f4\u5feb\u9001\u8fc7\u6765\u3002",
    "\uff08\u770b\u8d1f\u8d23\u4eba\u6709\u4e9b\u516d\u795e\u65e0\u4e3b\uff0c\u6211\u53f9\u4e86\u53e3\u6c14\u3002\uff09",
    "\u9edb\u70df\uff0c\u8054\u7edc\u7f8e\u73b2\uff0c\u6d3e\u5c0f\u961f\u8fc7\u6765\u652f\u63f4\uff0c\u8bb0\u5f97\u8ba9\u5bc7\u5c14\u8299\u5e26\u4e0a\u533b\u836f\u7bb1\u3002",
    "\u53e6\u5916\uff0c\u9edb\u70df\u4f60\u7559\u5728\u8fd9\u91cc\uff0c\u770b\u770b\u6709\u6ca1\u6709\u8981\u5e2e\u5fd9\u7684\u3002\u7edb\u96e8\u8ddf\u6211\u8d70\u3002",
    "\u957f\u5b98\u662f\u8981\u53bb\u6551\u90a3\u4e9b\u5b69\u5b50\u5417\uff1f",
    "\u55ef\uff0c\u5341\u4e2a\u4eba\uff0c\u4e0d\u662f\u5f88\u5927\u7684\u89c4\u6a21\uff0c\u6211\u548c\u7edb\u96e8\u5e94\u4ed8\u5f97\u6765\u3002",
    "\u521a\u624d\u6211\u770b\u4e86\u73af\u5883\u60c5\u62a5\uff0c\u8fd9\u91cc\u9a6c\u4e0a\u53c8\u8981\u6709\u574d\u584c\u98ce\u66b4\uff0c\u8fd9\u79cd\u60c5\u51b5\u4e0b\uff0c\u5f88\u53ef\u80fd\u6709\u751f\u9ab8\u8ddf\u7740\u98ce\u66b4\u8fc7\u6765\uff0c\u6709\u9edb\u70df\u5728\u8fd9\u91cc\uff0c\u6211\u4e5f\u653e\u5fc3\u4e00\u4e9b\u3002",
    "\u53ef\u90a3\u4e9b\u74e6\u826f\u683c\u5e2e\u4e5f\u6709\u53ef\u80fd\u6ca1\u6765\u5168\uff0c\u5982\u679c\u6570\u91cf\u4e0d\u6b62\u5341\u4e2a\u5462\uff1f",
    "\u90a3\u6837\u7684\u8bdd\uff0c\u4e0d\u5c31\u5f88\u5371\u9669\u5417\uff1f",
    "\u653e\u5fc3\u5427\u8001\u59d0\uff0c\u957f\u5b98\u8eab\u8fb9\u53ef\u662f\u6709\u6211\u5728\u554a\uff01\u6211\u4f1a\u5168\u529b\u4fdd\u62a4\u957f\u5b98\u5b89\u5168\u7684\uff01",
    "\u7edb\u96e8\u2026\u2026",
    "\u5982\u679c\u4ed6\u4eec\u4eba\u6bd4\u8f83\u591a\uff0c\u6211\u548c\u7edb\u96e8\u5c31\u5148\u63a2\u67e5\u4e00\u4e0b\u5b69\u5b50\u4eec\u7684\u60c5\u51b5\uff0c\u5c3d\u53ef\u80fd\u5148\u4fdd\u8bc1\u4ed6\u4eec\u7684\u5b89\u5168\u2014\u2014|\u6839\u636e\u5c45\u4f4f\u70b9\u8d1f\u8d23\u4eba\u7684\u8bf4\u6cd5\uff0c\u8fd8\u6709\u5730\u4e0a\u6b8b\u7559\u7684\u8f66\u8f6e\u80ce\u5370\uff0c\u8fd9\u7fa4\u74e6\u826f\u683c\u5e2e\u5728\u52ab\u63a0\u540e\uff0c\u662f\u671d\u4e1c\u5357\u65b9\u5411\u64a4\u79bb\u7684\u3002",
    "\u6211\u4eec\u53ef\u4ee5\u6839\u636e\u8f66\u8f6e\u75d5\u8ff9\u8ffd\u8e2a\uff0c\u5e76\u5b9a\u671f\u5411\u827e\u83ab\u53f7\u53d1\u4fe1\u3001\u66f4\u65b0\u4f4d\u7f6e\u4fe1\u606f\u3002|\u6211\u63a8\u6d4b\uff0c\u90a3\u4e2a\u65b9\u5411\u4e0a\u6709\u4ed6\u4eec\u7684\u636e\u70b9\u4e4b\u4e00\uff0c\u8981\u627e\u5230\u4ed6\u4eec\uff0c\u5e76\u4e0d\u56f0\u96be\u3002",
    "\u827e\u83ab\u53f7\u7684\u5176\u4ed6\u4eba\uff0c\u53ef\u4ee5\u6839\u636e\u6211\u4eec\u7684\u4f4d\u7f6e\u4fe1\u53f7\uff0c\u968f\u65f6\u51c6\u5907\u652f\u63f4\u6211\u4eec\u3002",
    "\u563f\u563f\uff0c\u53ea\u8981\u80fd\u627e\u5230\u4eba\uff0c\u74e6\u826f\u683c\u5e2e\u90a3\u79cd\u5e9f\u7269\uff0c\u6211\u4e00\u811a\u5c31\u80fd\u8e22\u98de\u5341\u4e2a\uff0c\u6551\u56de\u5b69\u5b50\u4e0d\u5728\u8bdd\u4e0b\uff01",
    "\u7edb\u96e8\uff0c\u4e0d\u53ef\u4ee5\u51b2\u52a8\u884c\u4e8b\u3002",
    "\u77e5\u9053\u77e5\u9053\uff0c\u6211\u4e00\u5b9a\u90fd\u542c\u957f\u5b98\u7684~",
    "\u8fd9\u6837\uff0c\u8001\u59d0\u80fd\u653e\u5fc3\u4e86\u5427\uff1f",
    "\uff08\u7edb\u96e8\u5411\u7740\u9edb\u70df\u7728\u4e86\u7728\u773c\u3002\uff09",
    "\u6211\u660e\u767d\u4e86\uff0c\u957f\u5b98\u7684\u5b89\u6392\u5341\u5206\u59a5\u5f53\u3002",
    "\u5728\u827e\u83ab\u53f7\u7684\u652f\u63f4\u6765\u4e4b\u524d\uff0c\u8fd9\u91cc\u90fd\u4ea4\u7ed9\u6211\u3002\u76f8\u5bf9\u7684\uff0c\u957f\u5b98\u5c31\u4ea4\u7ed9\u4f60\u4e86\uff0c\u7edb\u96e8\u3002",
    "\u653e\u5fc3\u4ea4\u7ed9\u6211\u5427\uff01\u770b\u6211\u5927\u663e\u8eab\u624b~",
    "\u55ef\uff0c\u90a3\u5c31\u51fa\u53d1\u5427\u3002"
  ]
]