[
  [
    "\uff08\u5728\u827e\u83ab\u53f7\u7684\u706b\u529b\u652f\u63f4\u548c\u7edb\u96e8\u7684\u534f\u52a9\u4e0b\uff0c\u6211\u4eec\u987a\u5229\u6551\u56de\u4e86\u90a3\u51e0\u4e2a\u5b69\u5b50\u3002\uff09",
    "\uff08\u8fd4\u56de\u5c45\u4f4f\u70b9\uff0c\u8d1f\u8d23\u4eba\u770b\u5230\u6211\u4eec\u5e26\u56de\u4e86\u5b69\u5b50\uff0c\u4e0d\u65ad\u5411\u6211\u4eec\u9053\u8c22\uff0c\u8fd8\u8ffd\u52a0\u4e86\u4e00\u7b14\u76f8\u5f53\u53ef\u89c2\u7684\u59d4\u6258\u62a5\u916c\u3002\uff09",
    "\u771f\u7684\u592a\u611f\u8c22\u4f60\u4eec\u4e86\uff0c\u5982\u679c\u4e0d\u662f\u6709\u4f60\u4eec\u5728\uff0c\u8fd9\u4e9b\u5b69\u5b50\u591a\u534a\u5c31\u56de\u4e0d\u6765\u4e86\u3002",
    "\u5ba2\u6c14\u4e86\u3002\u4f46\u5982\u679c\u4f60\u60f3\u8ba9\u5b69\u5b50\u4eec\u9002\u5e94\u5728\u6c61\u67d3\u533a\u7684\u751f\u6d3b\uff0c\u4e00\u573a\u821e\u53f0\u5267\uff0c\u6050\u6015\u6ca1\u4ec0\u4e48\u7528\u3002|\u4ed6\u4eec\u5bf9\u5371\u9669\u7684\u8ba4\u77e5\uff0c\u8fd8\u9700\u8981\u4f60\u4eec\u597d\u597d\u5f15\u2014\u2014",
    "\u52a0\u6cb9\uff01\u52a0\u6cb9\uff01\u518d\u575a\u6301\u4e00\u4e0b\uff01",
    "\uff08\u6211\u548c\u8d1f\u8d23\u4eba\u4e00\u8d77\u8d70\u51fa\u95e8\u5916\uff0c\u53d1\u73b0\u7edb\u96e8\u5c31\u5728\u95e8\u5916\u7684\u7a7a\u5730\u4e0a\u3002\uff09",
    "\uff08\u5979\u7684\u9762\u524d\u662f\u5341\u51e0\u4e2a\u9ad8\u5ea6\u4e0d\u4e00\u7684\u5c0f\u5b69\u2014\u2014\u5728\u624e\u9a6c\u6b65\u3002\uff09",
    "\u2026\u2026\u4f60\u4eec\u5728\u5e72\u4ec0\u4e48\uff1f",
    "\u563f\u563f\uff0c\u6211\u5728\u6559\u4ed6\u4eec\u6b66\u672f\u3002",
    "\u6b66\u672f\uff1f",
    "\u5bf9\u554a\uff0c\u624e\u5b9e\u7684\u9a6c\u6b65\u53ef\u662f\u6b66\u672f\u7684\u57fa\u7840\u554a\uff01",
    "\uff08\u7edb\u96e8\u8bf4\u7740\uff0c\u671d\u865a\u7a7a\u4f7f\u51fa\u4e86\u4e00\u4e2a\u6f02\u4eae\u7684\u98de\u8e22\u3002\uff09",
    "\u8981\u60f3\u8e22\u51fa\u529b\u9053\uff0c\u5c31\u5fc5\u987b\u6709\u8d85\u5f3a\u7684\u817f\u90e8\u529b\u91cf\u2014\u2014\u770b\u5230\u4e86\u5417\uff1f\u53ea\u8981\u4f60\u4eec\u575a\u6301\u7ec3\u4e60\uff0c\u65e9\u665a\u4e5f\u80fd\u505a\u51fa\u8fd9\u6837\u7684\u52a8\u4f5c\uff01",
    "\uff08\u773c\u770b\u5b69\u5b50\u4eec\u5bf9\u5e05\u6c14\u7684\u7edb\u96e8\u9762\u9732\u5d07\u62dc\uff0c\u6211\u6342\u4f4f\u4e86\u989d\u5934\u3002\uff09",
    "\u4f60\u6559\u4ed6\u4eec\u8fd9\u4e9b\uff0c\u4e07\u4e00\u2026\u2026",
    "\u6211\u4e0d\u6b62\u6559\u4e86\u8fd9\u4e9b\u54e6\uff0c\u957f\u5b98\u3002",
    "\u6211\u8fd8\u8ddf\u4ed6\u4eec\u7ea6\u597d\u4e86\uff0c\u5728\u80fd\u591f\u505a\u5230\u8fd9\u4e48\u6807\u51c6\u7684\u98de\u8e22\u524d\uff0c\u4e0d\u53ef\u4ee5\u518d\u64c5\u81ea\u884c\u52a8\uff0c\u5bb3\u5927\u5bb6\u62c5\u5fc3\u4e86\u3002|\u6bd5\u7adf\u5982\u679c\u60f3\u8981\u53bb\u505a\u4ec0\u4e48\u7684\u65f6\u5019\uff0c\u5374\u4e0d\u80fd\u4fdd\u62a4\u81ea\u5df1\uff0c\u8fd9\u6837\u53ea\u4f1a\u7ed9\u522b\u4eba\u6dfb\u9ebb\u70e6\u554a\u3002",
    "\u6211\u2014\u2014\u53ef\u662f\u6700\u61c2\u8fd9\u4e2a\u9053\u7406\u7684\u4eba\u4e86\uff01",
    "\uff08\u5728\u6211\u5f00\u53e3\u524d\uff0c\u7edb\u96e8\u671d\u6211\u72e1\u9ee0\u4e00\u7b11\u3002\uff09",
    "\u6211\u77e5\u9053\u957f\u5b98\u60f3\u8bf4\u4ec0\u4e48\uff0c\u6240\u4ee5\u6211\u5168\u90fd\u8bf4\u5b8c\u5566\uff01",
    "\u554a\u957f\u5b98\uff0c\u8001\u59d0\u548c\u95ea\u7535\u5979\u4eec\u5df2\u7ecf\u51c6\u5907\u597d\u4e86\uff0c\u6211\u4eec\u8be5\u8d70\u4e86\u54df~",
    "\u2026\u2026\u597d\u3002",
    "\uff08\u6211\u8d70\u5230\u7edb\u96e8\u8eab\u8fb9\uff0c\u770b\u7740\u5979\u7684\u9762\u5e9e\u3002\uff09",
    "\uff08\u5979\u7684\u9762\u5bb9\u548c\u8fc7\u53bb\u4e00\u6837\uff0c\u51e0\u4e4e\u6ca1\u6709\u6539\u53d8\u3002\u4f46\u4e8b\u5b9e\u4e0a\uff0c\u7ecf\u5386\u8fc7\u8fd9\u4e48\u591a\u5e74\u7684\u78e8\u7ec3\uff0c\u5979\u65e9\u5c31\u6210\u957f\u8d77\u6765\u4e86\u3002\uff09",
    "\uff08\u6709\u5979\u5728\u827e\u83ab\u53f7\uff0c\u6211\u4e5f\u53ef\u4ee5\u66f4\u8f7b\u677e\u4e00\u4e9b\u4e86\u5427\uff1f\uff09"
  ]
]