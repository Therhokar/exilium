    "\u7eb3\u7f8e\u897f\u4e1d\u8bf4\u5982\u679c\u4e0d\u662f\u7206\u70b8\u5438\u5f15\u4e86\u90a3\u4e1c\u897f\u6ce8\u610f\u529b\uff0c\u5c31\u7b97\u53ea\u5269\u4e0b\u4e09\u5206\u949f\uff0c\u5927\u5bb6\u90fd\u8981\u5168\u90e8\u66f4\u6362\u7d20\u4f53\u4e86\u3002|\u5567\uff0c\u5982\u679c\u4e0d\u662f\u90a3\u4e1c\u897f\u81ea\u5df1\u6d88\u5931\uff0c\u6211\u786e\u5b9e\u6491\u4e0d\u5230\u4f60\u4eec\u6765\u2014\u2014\u539f\u6765\u4f60\u4eec\u90a3\u91cc\u4e5f\u633a\u4e25\u91cd\u554a\u961f\u957f\uff1f",
    "\u2026\u2026\u6bc1\u706d\uff0c\u4e0d\u53ef\u53ca\u4e4b\u6df1\u6e0a\u2026\u2026",
    "\u4f60\u8bf4\u8c01\u6ca1\u7528\u5462\u7eb3\u7f8e\u897f\u4e1d\uff1f\uff01\u8ddf\u7740\u961f\u957f\u4f60\u8fd8\u80fd\u4e00\u8d77\u5168\u706d\uff1f\uff01",
    "\u95ea\u7535\uff0c\u6211\u4e0d\u77e5\u9053\u4f60\u6253\u7b97\u600e\u4e48\u89e3\u51b3\uff0c\u4f46\u7b49\u4e0b\u548c\u5bc7\u5c14\u8299\u540c\u6b65\u91cd\u8981\u4fe1\u606f\u7684\u65f6\u5019\uff0c\u5443\uff0c\u5c1d\u8bd5\u4e00\u4e0b\uff1f",
    "\u2026\u2026\u55ef\u3002",
    "\uff08\u95ea\u7535\u7ad9\u5728\u539f\u5730\uff0c\u795e\u8272\u5e73\u9759\u3002\uff09",
    "\uff08\u4ee5\u7eb3\u7f8e\u897f\u4e1d\u548c\u514b\u7f57\u4e3d\u79d1\u7684\u4e89\u5435\u4e3a\u80cc\u666f\u97f3\uff0c\u6c14\u6c1b\u4e00\u65f6\u6709\u4e9b\u51dd\u6ede\u3002\uff09",
    "\u5443\uff0c\u90a3\u4e2a\u2026\u2026\u6307\u3001\u6307\u6325\u5b98\uff0c\u68c0\u6d4b\u62a5\u544a\u2026\u2026\uff1f",
    "\u7f8e\u73b2\uff0c\u62a5\u544a\u68c0\u6d4b\u7ed3\u679c\u3002",
    "\u597d\u3001\u597d\u7684\u3002",
    "\uff08\u518d\u6b21\u67e5\u770b\u4e86\u624b\u4e2d\u7684\u62a5\u544a\uff0c\u7f8e\u73b2\u5411\u524d\u8d70\u4e86\u51e0\u6b65\u3002\uff09",
    "\uff08\u8d70\u5230\u603b\u63a7\u53f0\u524d\uff0c\u5979\u5c06\u81ea\u5df1\u7684\u4fbf\u643a\u901a\u8baf\u7ec8\u7aef\u63a5\u5165\u5230\u827e\u83ab\u53f7\u7cfb\u7edf\u4e2d\uff0c\u62a5\u544a\u4fe1\u606f\u663e\u793a\u5728\u6295\u5f71\u5c4f\u4e0a\u3002\uff09",
    "\u867d\u7136\u5df2\u7ecf\u83b7\u53d6\u4e86\u8bbf\u95ee\u6743\u9650\uff0c\u4f46\u7403\u4f53\u2014\u2014\u4e5f\u5c31\u662f\u7bb1\u5b50\u7684\u7cfb\u7edf\u4e2d\u6240\u7559\u5b58\u7684\u5927\u90e8\u5206\u6570\u636e\u90fd\u5df2\u7ecf\u88ab\u6e05\u6d17\u3002\u5df2\u7ecf\u5c1d\u8bd5\u8fdb\u884c\u6570\u636e\u6062\u590d\uff0c\u4f46\u80fd\u591f\u83b7\u5f97\u7684\u4fe1\u606f\u8fd8\u662f\u975e\u5e38\u5c11\u3002",
    "\u6211\u7efc\u5408\u5206\u6790\u4e86\u7403\u4f53\u5230\u7bb1\u5b50\u5f62\u6001\u7684\u53d8\u5f62\u673a\u5236\uff0c\u8fd9\u4e2a\u7bb1\u5b50\uff0c\u51c6\u786e\u6765\u8bf4\u662f\u8fd9\u4e2a\u4f53\u7cfb\uff0c\u5e94\u8be5\u662f\u4e00\u79cd\u4e0e\u7bb1\u5b50\u5185\u7684\u7269\u54c1\u914d\u5957\u7684\u9632\u5fa1\u4f53\u7cfb\u3002",
    "\u53ea\u4e0d\u8fc7\uff0c\u8fd9\u4e2a\u7bb1\u5b50\u5185\u50a8\u5b58\u7684\u7269\u54c1\uff0c\u5443\u2026\u2026\u4e5f\u5c31\u662f\u6d77\u4f26\u5a1c\u5c0f\u59d0\uff0c\u5979\u6709\u81ea\u4e3b\u610f\u8bc6\uff0c\u8fd9\u5957\u9632\u5fa1\u4f53\u7cfb\u9664\u4e86\u57fa\u7840\u7684\u654c\u6211\u5224\u5b9a\u5916\uff0c\u4e5f\u63a5\u5165\u4e86\u6d77\u4f26\u5a1c\u5c0f\u59d0\u7684\u610f\u8bc6\u4f5c\u4e3a\u5224\u5b9a\u6761\u4ef6\u3002",
    "\uff08\u4e0e\u7eb3\u7f8e\u897f\u4e1d\u7684\u4e89\u5435\u4ee5\u7eb3\u7f8e\u897f\u4e1d\u5355\u65b9\u9762\u9759\u97f3\u7ed3\u675f\uff0c\u514b\u7f57\u4e3d\u79d1\u7684\u6ce8\u610f\u529b\u8f6c\u56de\u5230\u7f8e\u73b2\u7684\u62a5\u544a\u4e0a\u6765\u3002\uff09",
    "\u5c31\u662f\u8bf4\uff0c\u90a3\u4e2a\u767d\u6bdb\u2014\u2014",
    "\u2026\u2026",
    "\u597d\u597d\u597d\uff0c\u662f\u6d77\u4f26\u5a1c\uff0c\u5979\u89c9\u5f97\u8c01\u5371\u9669\uff0c\u5c31\u4f1a\u8ba9\u5979\u7684\u90a3\u51e0\u4e2a\u7403\u6253\u8c01\uff1f",
    "\u662f\u7684\u3002",
    "\u5982\u679c\u4e0d\u53d7\u6d77\u4f26\u5a1c\u5c0f\u59d0\u7684\u610f\u8bc6\u5f71\u54cd\uff0c\u4e5f\u5c31\u662f\u5728\u6d77\u4f26\u5a1c\u5c0f\u59d0\u5904\u4e8e\u660f\u8ff7\u3001\u610f\u8bc6\u4e0d\u6e05\u7684\u72b6\u6001\u65f6\uff0c\u8fd9\u5957\u9632\u5fa1\u7cfb\u7edf\u4f1a\u81ea\u52a8\u653b\u51fb\u9760\u8fd1\u7bb1\u5b50\u5185\u7269\u54c1\u2014\u2014\u4e5f\u5c31\u662f\u6d77\u4f26\u5a1c\u5c0f\u59d0\u2014\u2014\u7684\u5a01\u80c1\u5355\u4f4d\u3002",
    "\u6211\u6a21\u62df\u4e86\u4e00\u4e9b\u60c5\u51b5\uff0c\u5bf9\u5b83\u8fdb\u884c\u6d4b\u8bd5\u3002\u73b0\u5728\u6682\u65f6\u6027\u7684\u7ed3\u8bba\u662f\uff0c\u6240\u6709\u52a0\u88c5\u6709\u706b\u63a7\u6838\u5fc3\u7684\u4eba\u5f62\uff0c\u8fd8\u6709\u6301\u6709\u70ed\u6b66\u5668\u7684\u5bf9\u8c61\uff0c\u90fd\u4f1a\u88ab\u8fd9\u5957\u9632\u5fa1\u7cfb\u7edf\u5224\u5b9a\u4e3a\u5a01\u80c1\u5355\u4f4d\uff0c\u5e76\u505a\u51fa\u653b\u51fb\u53cd\u5e94\u3002",
    "\u7136\u540e\u662f\u4e4b\u524d\uff0c\u95ea\u7535\u961f\u957f\u53d1\u73b0\u7684\u7279\u6b8a\u4fe1\u53f7\u2014\u2014",
    "\u8fd9\u5957\u9632\u5fa1\u7cfb\u7edf\u5728\u4f4e\u529f\u8017\u8fd0\u884c\u7684\u7bb1\u5b50\u5f62\u6001\u65f6\uff0c\u4f1a\u4ea7\u751f\u7279\u6b8a\u7684\u9891\u6bb5\uff0c\u626b\u63cf\u5e76\u76d1\u63a7\u7bb1\u5b50\u5185\u7269\u54c1\u7684\u72b6\u6001\uff1b\u5982\u679c\u662f\u5904\u4e8e\u9ad8\u529f\u8017\u8fd0\u884c\u7684\u7403\u4f53\u5f62\u6001\uff0c\u4f1a\u5728\u653b\u51fb\u65f6\u91ca\u653e\u9ad8\u9891\u6ce2\u6bb5\uff0c\u963b\u65ad\u4e00\u5b9a\u8303\u56f4\u5185\u7684\u5168\u90e8\u901a\u8baf\u3002",
    "\u6539\u5199\u654c\u6211\u8bc6\u522b\u3001\u52a0\u88c5\u4fe1\u53f7\u5c4f\u853d\u88c5\u7f6e\u662f\u5426\u53ef\u884c\uff1f",
    "\u53ef\u4ee5\u7684\uff0c\u53ea\u662f\u9700\u8981\u4e00\u70b9\u65f6\u95f4\u3002|\u5443\u2026\u2026",
    "\u8fd8\u6709\u4ec0\u4e48\u62a5\u544a\u5185\u5bb9\uff1f",
    "\u4e0d\u662f\u62a5\u544a\u2026\u2026\u6307\u6325\u5b98\u2026\u2026",
    "\u8fd9\u5957\u9632\u5fa1\u7cfb\u7edf\u7684\u79d1\u6280\u542b\u91cf\u5f88\u9ad8\uff0c\u63a8\u6d4b\u5df2\u7ecf\u8d85\u8fc7\u7eff\u533a\u5728\u53bb\u5e74\u666e\u53ca\u7684\u533a\u57df\u6027\u5bf9\u574d\u584c\u8f90\u5c04\u8b66\u6212\u88c5\u7f6e\uff0c\u5bf9\u65b9\u2026\u2026\u5e94\u8be5\u4f1a\u5f88\u96be\u5bf9\u4ed8\u5427\uff1f",
    "\u4e0d\u7ba1\u5bf9\u65b9\u662f\u8c01\uff0c\u73b0\u5728\u8fd9\u4e2a\u8ba2\u5355\u5728\u6211\u624b\u4e0a\uff0c\u8d27\u7269\u4e5f\u5df2\u7ecf\u62c6\u5c01\u3002|\u5c31\u7b97\u6211\u653e\u624b\uff0c\u5bf9\u65b9\u4e5f\u672a\u5fc5\u4f1a\u653e\u8fc7\u6211\u3002",
    "\uff08\u4e00\u65c1\uff0c\u4e00\u76f4\u6c89\u9ed8\u542c\u7740\u7f8e\u73b2\u6c47\u62a5\u7684\u95ea\u7535\u8d70\u5230\u7f8e\u73b2\u8eab\u8fb9\u3002\uff09",
    "\uff08\u5979\u5e2e\u52a9\u7f8e\u73b2\u5c06\u4fbf\u643a\u901a\u8baf\u7ec8\u7aef\u4e0e\u603b\u63a7\u53f0\u5206\u79bb\uff0c\u5e76\u5c06\u62a5\u544a\u4fe1\u606f\u8f6c\u5165\u81ea\u5df1\u7684\u901a\u8baf\u7ec8\u7aef\u3002\uff09",
    "\u554a\uff0c\u961f\u957f\uff0c\u6211\u3001\u6211\u9a6c\u4e0a\u5c31\u628a\u62a5\u544a\u4fe1\u606f\u53d1\u9001\u7ed9\u4f60\u2026\u2026",
    "\u7f8e\u73b2\uff0c\u73b0\u5728\u5927\u5bb6\u90fd\u5728\u3002|\u2026\u2026\u4e5f\u6709\u4e86\u65b0\u7684\u540c\u4f34\u52a0\u5165\uff0c\u6211\u4eec\u53ef\u4ee5\u987a\u5229\u89e3\u51b3\u8fd9\u4e2a\u8ba2\u5355\u3002",
    "\u55ef\u2026\u2026\u5927\u5bb6\uff0c\u6211\u3001\u6211\u76f8\u4fe1\u5927\u5bb6\u3002",
    "\u5bf9\u4e86\uff0c\u6307\u6325\u5b98\uff0c\u8fd9\u5957\u9632\u5fa1\u88c5\u7f6e\u7684\u5185\u90e8\uff0c\u6211\u53d1\u73b0\u4e86\u6697\u85cf\u7684\u5bc6\u95ed\u7ec6\u7ba1\uff0c\u867d\u7136\u73b0\u5728\u662f\u7a7a\u7684\uff0c\u4f46\u770b\u8d77\u6765\u5e94\u8be5\u4e4b\u524d\u88c5\u6709\u6db2\u4f53\u3002",
    "\u6211\u8bd5\u4e86\u5f88\u591a\u529e\u6cd5\u90fd\u4e0d\u80fd\u7834\u574f\u5b83\uff0c\u8fd8\u9700\u8981\u60f3\u529e\u6cd5\u63d0\u53d6\u7ec6\u7ba1\u5185\u7684\u6b8b\u7559\u7269\uff0c\u6709\u53ef\u80fd\u662f\u4ec0\u4e48\u9ebb\u70e6\u7684\u836f\u5242\u2014\u2014",
    "\u4e00\u70b9\u4e5f\u4e0d\u9ebb\u70e6\u2014\u2014\u662f\u9ebb\u9189\u5242\u54df~",
    "\u4f46\u662f\u5b89\u629a\u53d7\u60ca\u7684\u5c0f\u5b69\u5b50\u2014\u2014\u662f\u771f\u7684\u9ebb\u70e6\u8036~|\u4e0d\u8fc7\u4eba\u5bb6\u6709\u597d\u597d\u505a\u5b8c\u68c0\u67e5\u54df\uff0c\u6307\u6325\u5b98~\uff1f",
    "\u2026\u2026\u8f9b\u82e6\u4e86\u3002\u6d77\u4f26\u5a1c\u7684E.L.I.D.\u611f\u67d3\u60c5\u51b5\u600e\u4e48\u6837\uff1f",
    "\u5514\u2014\u2014",
    "\u5b8c\u2014\u2014\u5168\u2014\u2014\u6ca1\u2014\u2014\u6709\u2014\u2014\u8036~",
    "\u867d\u7136\u53d7\u5230\u9ebb\u9189\u5242\u5f71\u54cd\uff0c\u5904\u4e8e\u660f\u8ff7\u72b6\u6001\u76f8\u2014\u2014\u5f53\u957f\u65f6\u95f4\uff0c\u76f4\u5230\u6700\u8fd1\u624d\u6162\u6162\u9192\u6765\uff0c\u4f46\u8fd9\u4f4d\u6d77\u4f26\u5a1c\u5c0f\u59d0\u7684\u68c0\u67e5\u7ed3\u679c\u663e\u793a\uff0c\u5979\u5b8c\u2014\u2014\u5168\u662f\u4e00\u4e2a\u673a\u80fd\u5065\u5168\u7684\u4eba\u7c7b\u54df~\uff1f",
    "\u662f\u5b8c\u5168\u514d\u75ab\u4f53\uff1f",
    "\u2026\u2026\u76ee\u524d\u6765\u770b\uff0c\u8fd9\u79cd\u53ef\u80fd\u6027\u6700\u5927\u3002",
    "\u5b8c\u5168\u514d\u75ab\u4f53\u662f\u4ec0\u4e48\u4e1c\u897f\uff1f",
    "\u4e00\u79cd\u80fd\u591f\u5bf9\u574d\u584c\u8f90\u5c04\u5b8c\u5168\u514d\u75ab\u7684\u4eba\u7c7b\u3002",
    "\u76f8\u5173\u7684\u7814\u7a76\u867d\u7136\u5728\u4e8c\u5341\u5e74\u524d\u5c31\u5df2\u7ecf\u5f00\u59cb\u4e86\uff0c\u4f46\u73b0\u5728\uff0c\u5c45\u7136\u5df2\u7ecf\u80fd\u591f\u5236\u9020\u51fa\u8fd9\u79cd\u5b8c\u5168\u6210\u529f\u7684\u4e2a\u4f53\u4e86\uff1f",
    "\u2026\u2026\u2026\u2026\u2026\u2026\u662f\u2026\u2026\u8fd8\u662f\u2026\u2026\u6216\u8005\u2026\u2026\uff1f\u7bb1\u5b50\u7684\u53d8\u5f62\u6280\u672f\u867d\u7136\u662f\u4ed6\u4eec\u7684\uff0c\u4f46\u2026\u2026\u5b8c\u5168\u514d\u75ab\u4f53\u8fd9\u79cd\u2026\u2026",
    "\u54c8\uff1f\u662f\uff1f\u8fd8\u662f\uff1f\u4ec0\u4e48\u4e1c\u897f\uff1f\u6307\u6325\u5b98\u4f60\u8bf4\u4ec0\u4e48\uff1f",
    "\u73b0\u5728\u6211\u4e5f\u4e0d\u80fd\u786e\u5b9a\u3002\u4e0d\u8fc7\u6309\u7167\u201c\u9ebb\u9189\u5242\u5df2\u7ecf\u6d88\u8017\u5b8c\u6bd5\u201d\u8fd9\u4e00\u4fe1\u606f\u5206\u6790\uff0c\u53ef\u80fd\u6211\u4eec\u76ee\u524d\u7684\u8def\u7ebf\u4e5f\u5728\u5bf9\u65b9\u7684\u9884\u60f3\u4e4b\u5916\u3002",
    "\u7b49\u5230\u8fbe\u5b89\u5168\u4f4d\u7f6e\uff0c\u8054\u7cfb\u8ba2\u5355\u7684\u59d4\u6258\u4eba\uff0c\u8bd5\u8bd5\u770b\u80fd\u4e0d\u80fd\u83b7\u5f97\u66f4\u591a\u4fe1\u606f\u3002",
    "\u9700\u8981\u56de\u5230W_2069_0105\u51c0\u5316\u533a\uff1f",
    "\u90a3\u91cc\u786e\u5b9e\u5f88\u5b89\u5168\uff0c\u4f46\u8c01\u77e5\u9053\u6211\u4eb2\u7231\u7684\u5f17\u62c9\u57fa\u7c73\u5c14\u51c6\u5c09\u8fd8\u51c6\u5907\u4e86\u4ec0\u4e48\u60ca\u559c\u7b49\u7740\u6211\uff1f|\u6211\u53ef\u4e0d\u60f3\u518d\u8ba9\u4f60\u4eec\u7ecf\u5386\u4e00\u6b21\u5a01\u65af\u7279\u6e2f\u4e86\u3002",
    "\u5e0c\u671b\u90a3\u91cc\u7684\u751f\u9ab8\u80fd\u5728\u91cc\u9762\u8001\u8001\u5b9e\u5b9e\u2014\u2014",
    "\u6709\u5927\u6279\u751f\u9ab8\u9760\u8fd1\uff0c\u5a01\u80c1\u5ea6III\u4ee5\u4e0b\u3002|\u5e94\u8be5\u662f\u4e4b\u524d\u56e0\u6df7\u5408\u578b\u4f2f\u4ecb\u59c6\u7684\u51fa\u73b0\u800c\u805a\u96c6\u5728\u5a01\u65af\u7279\u6e2f\uff0c\u5728\u6df7\u5408\u578b\u4f2f\u4ecb\u59c6\u6d88\u5931\u540e\uff0c\u56db\u6563\u5230\u9644\u8fd1\u533a\u57df\u3002",
    "\u2026\u2026\u5567\uff0c\u51c6\u5907\u4f5c\u6218\u3002|\u7f8e\u73b2\uff0c\u7ed9\u5f17\u62c9\u57fa\u7c73\u5c14\u53d1\u6761\u6d88\u606f\uff0c\u8b66\u544a\u4ed6\u6700\u8fd1\u4e0d\u8981\u4e71\u8dd1\u3002",
    "\u554a\u3001\u597d\u7684\uff01",
    "\u5404\u4f5c\u6218\u5355\u4f4d\uff0c0010\u540e\u5f00\u59cb\u6218\u6597\u3002"
  ]
]