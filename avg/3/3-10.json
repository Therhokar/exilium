[
  [
    "\uff08\u625b\u7740\u4e00\u4e2a\u4eba\uff0c\u516d\u5206\u4eea\u7684\u52a8\u4f5c\u4ecd\u7136\u975e\u5e38\u5229\u843d\u3002\uff09",
    "\uff08\u95ea\u7535\u51e0\u4eba\u7a7f\u68ad\u5728\u6563\u4e71\u7684\u673a\u68b0\u548c\u5efa\u7b51\u6b8b\u9ab8\u4e2d\uff0c\u4e00\u8fb9\u8270\u96be\u5730\u62b5\u6297\u516d\u5206\u4eea\u72b6\u4f3c\u968f\u610f\u7684\u653b\u51fb\uff0c\u4e00\u8fb9\u52aa\u529b\u5bfb\u627e\u7740\u7a81\u7834\u53e3\u3002\uff09",
    "\uff08\u5728\u8fde\u7eed\u6570\u6b21\u653b\u51fb\u843d\u7a7a\u540e\uff0c\u7eb3\u7f8e\u897f\u4e1d\u7a81\u7136\u5f00\u53e3\u3002\uff09",
    "\u2026\u2026\u4e45\u8fdc\u2026\u2026\u6a21\u7cca\uff0c\u575a\u5b9e\u7684\u7f1d\u9699\u2026\u2026",
    "\u719f\u6089\uff1f\u4ec0\u4e48\u719f\u6089\uff1f",
    "\uff08\u518d\u6b21\u907f\u5f00\u516d\u5206\u4eea\u5e03\u8bbe\u5728\u73af\u5883\u4e2d\u7684\u653b\u51fb\u5355\u4f4d\uff0c\u514b\u7f57\u4e3d\u79d1\u4e0d\u8010\u70e6\u5730\u56de\u5e94\u3002\uff09",
    "\u2026\u2026\u5ef6\u5c55\u5206\u79bb\u2026\u2026\u754c\u7ebf\u91cd\u6784\uff0c\u5bf9\u7acb\u800c\u751f\u2026\u2026",
    "\u961f\u957f\uff0c\u7eb3\u7f8e\u897f\u4e1d\u8bf4\u5979\u4ee5\u524d\u89c1\u8fc7\u8fd9\u79cd\u653b\u51fb\u65b9\u5f0f\uff01",
    "\u2026\u2026\u706b\u7130\u3001\u7070\u70ec\uff0c\u8352\u6f20\u7d2f\u79ef\u2026\u2026",
    "\u66f4\u591a\uff1f\u66f4\u591a\u4ec0\u4e48\uff1f\u7eb3\u7f8e\u897f\u4e1d\u4f60\u8bf4\u6e05\u695a\uff01",
    "\u2026\u2026",
    "\u55ef\u2014\u2014\u514b\u7f57\u4e3d\u79d1\u518d\u597d\u597d\u60f3\u60f3\u561b~\u8bf4\u4e0d\u5b9a\u4f1a\u6709\u522b\u7684\u89e3\u8bfb\u54df\uff1f|\u8fd9\u4e2a\u516d\u5206\u4eea\u2014\u2014\u662f\u771f\u7684\u6709\u70b9\u96be\u5bf9\u4ed8\u8036~",
    "\uff08\u77ed\u6682\u65ad\u5f00\u7684\u901a\u8baf\u5668\u4e2d\uff0c\u518d\u6b21\u4f20\u6765\u6307\u6325\u5b98\u7684\u58f0\u97f3\u3002\uff09",
    "\u64a4\u9000\uff0c\u653e\u5f03\u59d4\u6258\u4eba\uff0c\u4fdd\u8bc1\u7f8e\u73b2\u5b89\u5168\u3002",
    "\u6307\u6325\u5b98\uff0c\u76ee\u524d\u5404\u4f5c\u6218\u5355\u4f4d\u53d7\u635f\u60c5\u51b5\u572810%\u4ee5\u4e0b\uff0c\u53ef\u4ee5\u7ee7\u7eed\u4f5c\u6218\u3002",
    "\u6211\u521a\u521a\u63a5\u5230\u901a\u8baf\uff0c\u8fd9\u4e2a\u540d\u5b57\u662f\u201c\u516d\u5206\u4eea\u201d\u7684\u4eba\u5f62\uff0c\u4e0d\u662f\u6211\u4eec\u73b0\u5728\u80fd\u591f\u5e94\u5bf9\u7684\u3002",
    "\u54c7~\u53c8\u662f\u6307\u6325\u5b98\u201c\u7edd\u5bf9\u53ef\u4fe1\u201d\u7684\u8001\u719f\u4eba\u561b~\uff1f",
    "\u2026\u2026\u55ef\u3002|\u95ea\u7535\uff0c\u64a4\u9000\u3002",
    "\u4e86\u89e3\u3002|\u5404\u4f5c\u6218\u5355\u4f4d\uff0c\u51c6\u5907\u64a4\u9000\uff01",
    "\uff08\u4eba\u5f62\u4eec\u53d8\u6362\u4f4d\u7f6e\uff0c\u4e0d\u518d\u5c1d\u8bd5\u4e0e\u516d\u5206\u4eea\u7f20\u6597\uff0c\u9010\u6e10\u9000\u5411\u5927\u5385\u7684\u51fa\u53e3\u65b9\u5411\u3002\uff09",
    "\uff08\u770b\u7740\u95ea\u7535\u5c0f\u961f\u79bb\u5f00\uff0c\u516d\u5206\u4eea\u505c\u6b62\u4e86\u653b\u51fb\u3002\uff09",
    "\u2014\u2014\u54b3\u3001\u54b3\u54b3\u54b3\uff01",
    "\uff08\u4e00\u76f4\u88ab\u516d\u5206\u4eea\u625b\u5728\u80a9\u4e0a\u7684\u59d4\u6258\u4eba\u9010\u6e10\u9192\u8f6c\uff0c\u5728\u5bdf\u89c9\u5230\u59d4\u6258\u4eba\u6062\u590d\u610f\u8bc6\u7684\u540c\u4e00\u65f6\u523b\uff0c\u516d\u5206\u4eea\u95ed\u4e86\u95ed\u773c\uff0c\u9762\u65e0\u8868\u60c5\u5730\u5c06\u59d4\u6258\u4eba\u653e\u5728\u4e86\u5730\u4e0a\u3002\uff09",
    "\uff08\u9192\u6765\u7684\u59d4\u6258\u4eba\u8270\u96be\u6491\u8d77\u4e0a\u534a\u8eab\uff0c\u6b63\u597d\u770b\u5230\u95ea\u7535\u5c0f\u961f\u79bb\u5f00\u5927\u5385\u3002\uff09",
    "\u4f60\u2014\u2014\u4f60\u5c45\u7136\u8ba9\u8fd9\u4e9b\u5e9f\u7269\u9003\u4e86\uff1f",
    "\uff08\u516d\u5206\u4eea\u5e76\u6ca1\u6709\u770b\u5411\u4ed6\u3002\uff09",
    "\uff08\u6ca1\u6709\u5f97\u5230\u516d\u5206\u4eea\u7684\u56de\u5e94\uff0c\u59d4\u6258\u4eba\u4ece\u5730\u4e0a\u722c\u8d77\uff0c\u56e0\u4e3a\u77ed\u6682\u7684\u7729\u6655\uff0c\u8eab\u4f53\u6643\u4e86\u6643\u3002\uff09",
    "\u4f60\u77e5\u4e0d\u77e5\u9053\u2014\u2014\u201c\u6837\u54c1\u201d\u8fd8\u5728\u8fd9\u51e0\u4e2a\u5e9f\u7269\u624b\u4e0a\uff1f",
    "\u201c\u6837\u54c1\u201d\u4e0d\u5728\u8fd9\u91cc\u3002",
    "\uff08\u516d\u5206\u4eea\u76ef\u7740\u95ea\u7535\u5c0f\u961f\u79bb\u5f00\u7684\u65b9\u5411\u3002\uff09",
    "\uff08\u5728\u5979\u8eab\u540e\uff0c\u7aed\u529b\u5bf9\u6297\u7729\u6655\u7684\u59d4\u6258\u4eba\u8868\u60c5\u72f0\u72de\u3002\uff09",
    "\u62e5\u6709\u201c\u8d4f\u91d1\u730e\u4eba\u201d\u6ce8\u518c\u8eab\u4efd\u7684\u201c\u6307\u6325\u5b98\u201d\u5df2\u7ecf\u572820\u65f618\u520640\u79d2\u524d\u548c\u201c\u6837\u54c1\u201d\u4e00\u8d77\u79bb\u5f00\uff0c\u5e76\u6ca1\u6709\u51fa\u73b0\u5728\u6b64\u533a\u57df\u5185\u3002",
    "\u90a3\u4f60\u2014\u2014\u90a3\u4f60\u8fd8\u4e0d\u8d76\u7d27\u8ffd\u8fc7\u53bb\uff1f\uff01",
    "\u901a\u8baf\u4fe1\u53f7\u4e0e\u901a\u8baf\u5185\u5bb9\u5df2\u83b7\u53d6\u6210\u529f\uff0c\u201c\u6307\u6325\u5b98\u201d\u4e0e\u201c\u6837\u54c1\u201d\u6240\u4e58\u5750\u7684\u6d32\u9645\u5217\u8f66\u76ee\u7684\u5730\u5df2\u7ecf\u67e5\u660e\u3002",
    "\u54ea\u91cc\uff1f",
    "\uff08\u516d\u5206\u4eea\u7f13\u7f13\u8f6c\u5934\uff0c\u770b\u5411\u81ea\u5df1\u9762\u524d\u7684\u59d4\u6258\u4eba\u3002\uff09",
    "\uff08\u4e24\u4eba\u8eab\u9ad8\u7684\u5dee\u8ddd\uff0c\u4f7f\u516d\u5206\u4eea\u88ab\u8feb\u7ef4\u6301\u62ac\u5934\u7684\u59ff\u52bf\uff0c\u4f46\u5979\u7684\u8138\u4e0a\u4ecd\u7136\u9762\u65e0\u8868\u60c5\u3002\uff09",
    "\u5965\u5fb7\u5345\u3002",
    "\uff08\u542c\u5230\u516d\u5206\u4eea\u7684\u56de\u7b54\uff0c\u59d4\u6258\u4eba\u8212\u4e86\u53e3\u6c14\uff0c\u91cd\u65b0\u9732\u51fa\u4e86\u50b2\u6162\u7684\u7b11\u5bb9\u3002\uff09",
    "\u5965\u5fb7\u5345\u2026\u2026\u54fc\u3002",
    "\u8fd9\u4f4d\u201c\u6307\u6325\u5b98\u201d\uff0c\u662f\u4e00\u4e2a\u4e0a\u7b49\u9975\u6599\uff0c\u80fd\u591f\u9493\u51fa\u201c\u6211\u4eec\u201d\u8fd1\u671f\u8ffd\u8e2a\u7684\u4e00\u4e2a\u91cd\u8981\u4eba\u7269\u3002\u4f5c\u4e3a\u56de\u62a5\uff0c\u201c\u5b83\u201d\u786e\u8ba4\u4e0d\u4f1a\u8ffd\u7a76\u60a8\u524d\u671f\u5931\u8d25\u7684\u201c\u6837\u54c1\u201d\u56de\u6536\u884c\u52a8\u3002\u60a8\u5e94\u8be5\u4e3a\u6b64\u611f\u5230\u5e86\u5e78\uff0c\u9601\u4e0b\u3002",
    "\u5931\u8d25\uff1f\u54c8\u3002|\u51fa\u73b0\u76ee\u524d\u8fd9\u79cd\u5c40\u9762\uff0c\u96be\u9053\u4e0d\u662f\u4f60\u4eec\u8fd9\u4e9b\u5e9f\u7269\u5728\u62d6\u7d2f\u6211\uff1f",
    "\u60a8\u9c81\u83bd\u7684\u884c\u52a8\u6240\u9020\u6210\u7684\u635f\u5931\u4ecd\u5728\u53ef\u63a7\u8303\u56f4\u5185\u3002",
    "\u4f60\u2014\u2014\uff01",
    "\uff08\u5ffd\u7565\u59d4\u6258\u4eba\u56e0\u6c14\u6124\u800c\u626d\u66f2\u7684\u8138\uff0c\u516d\u5206\u4eea\u5f00\u59cb\u5411\u5927\u5385\u7684\u51fa\u53e3\u8d70\u53bb\u3002\uff09",
    "\u5965\u5fb7\u5345\u7684\u529b\u91cf\u5df2\u7ecf\u8c03\u914d\u5b8c\u6bd5\uff0c\u9601\u4e0b\uff0c\u671f\u5f85\u60a8\u7684\u8868\u73b0\u3002",
    "\u2026\u2026",
    "\uff08\u88ab\u7559\u5728\u539f\u5730\u7684\u59d4\u6258\u4eba\u76ef\u7740\u516d\u5206\u4eea\u79bb\u5f00\u7684\u80cc\u5f71\uff0c\u6697\u81ea\u54ac\u7259\u3002\uff09",
    "\uff08\u5728\u516d\u5206\u4eea\u79bb\u5f00\u5927\u5385\u524d\uff0c\u4ed6\u5feb\u6b65\u8ffd\u4e86\u4e0a\u53bb\u3002\uff09"
  ]
]