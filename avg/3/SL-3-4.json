[
  [
    "\u8fd8\u6709\u591a\u4e45\u3002",
    "\u8ddd\u79bb\u7ea6\u5b9a\u65f6\u95f4\u8fd8\u670927\u5c0f\u65f6\uff0c\u9601\u4e0b\u3002",
    "\u4ed6\u4eec\u7684\u4f4d\u7f6e\uff0c\u67e5\u5230\u4e86\uff1f",
    "\u201c\u6837\u54c1\u201d\u7684\u4fe1\u53f7\u5e72\u6270\u5df2\u4e8e153\u65f69\u520657\u79d2\u524d\u6d88\u5931\uff0c\u8ffd\u8e2a\u5c0f\u7ec4\u5728W_2069_0782\u51c0\u5316\u533a\u9644\u8fd1\u77ed\u6682\u4fb5\u5165\u8be5\u5c0f\u961f\u8f7d\u5177\u7cfb\u7edf\u540e\uff0c\u5df2\u4e22\u5931\u8be5\u5c0f\u961f\u6240\u6709\u884c\u8e2a\u3002",
    "\u2026\u2026\u54fc\uff0c\u6211\u5236\u5b9a\u7684\u8ba1\u5212\uff0c\u5c31\u88ab\u4f60\u4eec\u6267\u884c\u6210\u8fd9\u6837\uff1f",
    "\u63a8\u6d4b\u8be5\u5c0f\u961f\u5df2\u542f\u7528\u5907\u7528\u8def\u7ebf\uff0c\u90bb\u8fd1\u51c0\u5316\u533a\u5c1a\u672a\u53d1\u73b0\u8be5\u5c0f\u961f\u901a\u884c\u4ee3\u7801\u88ab\u4f7f\u7528\u3002|\u9601\u4e0b\uff0c\u53ef\u80fd\u9700\u8981\u6392\u67e5\u9884\u5907\u51c0\u5316\u533a\uff0c\u4f30\u7b97\u8be5\u5c0f\u961f\u62b5\u8fbe\u65f6\u95f4\u3002",
    "\u6392\u67e5\uff1f\u54c8\u3002|\u4e0d\u8fc7\u662f\u4e2a\u8d4f\u91d1\u730e\u4eba\u548c\u51e0\u4e2a\u4e8c\u4ee3\u4eba\u5f62\uff0c\u53ea\u8981\u8e0f\u5165\u8fd9\u4e2a\u533a\u57df\uff0c\u6211\u7acb\u523b\u5c31\u80fd\u77e5\u9053\u3002",
    "\uff08\u59d4\u6258\u4eba\u51b7\u7b11\u4e86\u4e00\u58f0\uff0c\u5c06\u884c\u52a8\u62a5\u544a\u653e\u5728\u4e86\u5ea7\u6905\u65c1\u7684\u652f\u67b6\u578b\u684c\u677f\u4e0a\u3002\uff09",
    "\uff08\u4ed6\u7ad9\u8d77\u6765\uff0c\u5411\u76d1\u63a7\u5c4f\u5e55\u524d\u8d70\u4e86\u51e0\u6b65\u3002\uff09",
    "\uff08\u867d\u7136\u7ecf\u8fc7\u6253\u7406\uff0c\u5927\u5385\u5730\u9762\u4e0a\u4ecd\u7136\u6b8b\u7559\u6709\u4e00\u4e9b\u6742\u7269\uff0c\u6210\u4e3a\u59d4\u6258\u4eba\u884c\u8d70\u65f6\u7684\u5fae\u5c0f\u969c\u788d\u3002\uff09",
    "\uff08\u59d4\u6258\u4eba\u51e0\u4e4e\u662f\u4e0b\u610f\u8bc6\u5730\u5c06\u5b83\u4eec\u8e22\u5230\u4e86\u4e00\u65c1\u3002\uff09",
    "\u54fc\uff0c\u51e0\u6b21\u9003\u8131\u6211\u7684\u8ba1\u5212\uff0c\u4e5f\u7b97\u6709\u4e9b\u80fd\u529b\u3002|\u5916\u56f4\u5373\u5c06\u914d\u5907\u5b8c\u6210\u7684\u62c9\u5c14\u65af\u706b\u7bad\u70ae\uff0c\u5c06\u4ee3\u8868\u6211\u5bf9\u8fd9\u4f4d\u8d4f\u91d1\u730e\u4eba\u7684\u6700\u9ad8\u656c\u610f\u3002",
    "\u9601\u4e0b\uff0c\u4f9d\u636e\u201c\u6837\u54c1\u201d\u5728\u201c\u8ba1\u5212\u201d\u4e2d\u7684\u91cd\u8981\u7b49\u7ea7\uff0c\u60a8\u9700\u8981\u8c28\u614e\u8003\u8651\u56de\u6536\u4efb\u52a1\u7684\u53ef\u884c\u6027\u3002|\u56de\u6536\u5931\u8d25\u7684\u540e\u679c\uff0c\u60a8\u548c\u201c\u6211\u4eec\u201d\u90fd\u4e0d\u4f1a\u613f\u610f\u77e5\u9053\u3002",
    "\u4f60\u5728\u8d28\u7591\u6211\u7684\u5224\u65ad\uff1f",
    "\u201c\u5b83\u201d\u5c06\u4f1a\u8d28\u7591\u60a8\u7684\u5224\u65ad\u3002",
    "\u201c\u5b83\u201d\u53ea\u4f1a\u8d28\u7591\u4f60\u4eec\u8fd9\u4e9b\u201c\u970d\u6d1b\u6208\u8ba1\u5212\u201d\u7684\u4ea7\u7269\uff0c\u4f60\u4eec\uff0c\u8fd8\u4e0d\u8db3\u4ee5\u4e0e\u6211\u76f8\u63d0\u5e76\u8bba\u3002",
    "\u60a8\u53ef\u80fd\u5bf9\u201c\u8ba1\u5212\u201d\u6709\u6240\u8bef\u89e3\uff0c\u9601\u4e0b\u3002",
    "\u2014\u2014\u4f60\u5a01\u80c1\u6211\uff1f",
    "\u54fc\uff0c\u56e0\u4e3a\u4f60\u4eec\u7684\u65e0\u80fd\uff0c\u6211\u624d\u5fc5\u987b\u5192\u8fd9\u4e48\u5927\u7684\u98ce\u9669\u6765\u5230\u51c0\u5316\u533a\u8fd9\u79cd\u80ae\u810f\u7684\u5730\u65b9\u3002\u50cf\u4f60\u4eec\u8fd9\u79cd\u6d41\u6c34\u7ebf\u7684\u590d\u5236\u4ea7\u7269\uff0c\u5c45\u7136\u80fd\u5bf9\u6211\u63d0\u51fa\u8d28\u7591\uff1f",
    "\uff08\u9ed1\u8272\u4eba\u5f62\u5e76\u6ca1\u6709\u5bf9\u59d4\u6258\u4eba\u7684\u8d23\u95ee\u505a\u51fa\u53cd\u5e94\u3002\uff09",
    "\u671f\u5f85\u60a8\u7684\u8868\u73b0\uff0c\u9601\u4e0b\u3002",
    "\uff08\u5979\u5411\u59d4\u6258\u4eba\u5fae\u5fae\u4f4e\u5934\uff0c\u8f6c\u8eab\u5411\u95e8\u53e3\u5904\u8d70\u53bb\u3002\uff09",
    "\u7ad9\u4f4f\u3002",
    "\u4f9d\u636e\u5185\u90e8\u7ba1\u7406\u67b6\u6784\u53ca\u201c\u6837\u54c1\u201d\u56de\u6536\u4efb\u52a1\u4f53\u7cfb\uff0c\u60a8\u5c1a\u4e0d\u5177\u5907\u8db3\u591f\u7684\u6743\u9650\u63a7\u5236\u6211\u7684\u884c\u52a8\u3002",
    "\u795d\u60a8\u8ba1\u5212\u6267\u884c\u987a\u5229\uff0c\u9601\u4e0b\u3002",
    "\u2014\u2014\uff01",
    "\uff08\u59d4\u6258\u4eba\u968f\u624b\u62ff\u8d77\u684c\u677f\u4e0a\u7684\u884c\u52a8\u62a5\u544a\uff0c\u6454\u5230\u9ed1\u8272\u4eba\u5f62\u811a\u8fb9\u3002\uff09",
    "\uff08\u7559\u4e0b\u8eab\u540e\u6124\u6012\u7684\u59d4\u6258\u4eba\uff0c\u9ed1\u8272\u4eba\u5f62\u901f\u5ea6\u4e0d\u53d8\uff0c\u79bb\u5f00\u5927\u5385\u3002\uff09",
    "\uff08\u540c\u4e00\u65f6\u523b\u2014\u2014\uff09",
    "\uff08\u59d4\u6258\u4eba\u7684\u901a\u8baf\u7ec8\u7aef\u54cd\u8d77\uff0c\u76d1\u63a7\u5c4f\u5e55\u4e0a\u4e5f\u663e\u793a\u51fa\u9ad8\u901f\u79fb\u52a8\u7684\u7ea2\u70b9\u3002\uff09",
    "\u2014\u2014\u5e0c\u671b\u4f60\u63d0\u4f9b\u7ed9\u6211\u7684\u4f1a\u662f\u597d\u6d88\u606f\u3002",
    "\u5148\u751f\uff0c\u5165\u4fb5\u5355\u4f4d\u7684\u5f71\u50cf\u6bd4\u5bf9\u5df2\u7ecf\u5b8c\u6210\uff0c\u786e\u8ba4\u76ee\u6807\u51fa\u73b0\uff01|\u5916\u56f4\u5e03\u8bbe\u5b8c\u6210\u8fdb\u5ea660%\uff0c\u80fd\u591f\u5bf9\u76ee\u6807\u5b9e\u65bd\u521d\u6b65\u6253\u51fb\u3002",
    "\u2014\u2014\u5f88\u597d\u3002",
    "\uff08\u59d4\u6258\u4eba\u770b\u4e86\u4e00\u773c\u76d1\u63a7\u5c4f\u5e55\u3002\uff09",
    "\uff08\u4ed6\u9ad8\u50b2\u5730\u62ac\u4e86\u62ac\u4e0b\u5df4\uff0c\u6307\u5411\u5c4f\u5e55\u4e0a\u9ad8\u901f\u79fb\u52a8\u7684\u7ea2\u70b9\u3002\uff09",
    "\u53bb\u7528\u6211\u4eec\u7684\u6700\u9ad8\u793c\u8282\u62db\u5f85\u8fd9\u4f4d\u8d4f\u91d1\u730e\u4eba\u3002",
    "\u662f\uff0c\u5148\u751f\u3002"
  ]
]