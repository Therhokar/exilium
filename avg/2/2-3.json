[
  [
    "\u6307\u6325\u5b98\uff0c\u4ed6\u4eec\u8981\u64a4\u4e86\uff01",
    "\u5df2\u7ecf\u8fdb\u5165\u5b89\u5168\u8ddd\u79bb\uff0c\u957f\u57ce\u5217\u8f66\u5b88\u519b\u968f\u65f6\u4f1a\u5411\u6211\u4eec\u53d1\u8d77\u653b\u51fb\uff01",
    "\u57fa\u5730\u8f66\u524d\u8fdb\u505c\u6b62\uff0c\u4e3b\u706f\u5347\u8d77\u3002|\u95ea\u7535\uff0c\u7ef4\u6301\u8b66\u6212\u72b6\u6001\uff0c\u6ce8\u610f\u8eb2\u907f\u653b\u51fb\uff0c\u786e\u8ba4\u654c\u65b9\u79bb\u5f00\u540e\u8fd4\u56de\u57fa\u5730\u8f66\u3002",
    "\u4e86\u89e3\u3002",
    "\uff08\u827e\u83ab\u53f7\u5f00\u59cb\u51cf\u901f\u3002\uff09",
    "\uff08\u524d\u65b9\u88c5\u7f6e\u7684\u4e3b\u706f\u5347\u8d77\uff0c\u5411\u957f\u57ce\u5217\u8f66\u65b9\u5411\u4eae\u8d77\uff0c\u5f00\u59cb\u95ea\u52a8\u3002\uff09",
    "\uff08\u8fc7\u4e86\u51e0\u5206\u949f\u2014\u2014\uff09",
    "\uff08\u9694\u7740\u7eb7\u98de\u7684\u7070\u8272\u96ea\u7247\uff0c\u5bf9\u9762\u7684\u957f\u57ce\u5217\u8f66\u4e0a\u4f3c\u4e4e\u51fa\u73b0\u4e86\u5f02\u52a8\u3002\uff09",
    "\uff08\u4e0e\u6b64\u540c\u65f6\uff0c\u827e\u83ab\u53f7\u6536\u5230\u4e86\u4e00\u5219\u901a\u8baf\u8bf7\u6c42\u3002\uff09",
    "\u2014\u2014\u6307\u6325\u5b98\uff01",
    "\u2026\u2026",
    "\u5c0a\u656c\u7684\u5b88\u519b\u51c6\u5c09\u5148\u751f\uff0cB.R.I.E.F.\u6ce8\u518c\u8d4f\u91d1\u730e\u4eba\u5411\u60a8\u95ee\u597d\u3002",
    "\u54fc\uff0c\u201c\u4fdd\u630110\u516c\u91cc\u8ddd\u79bb\u201d\u2014\u2014\u6211\u8bb0\u5f97\u5df2\u7ecf\u5f3a\u8c03\u8fc7\u4e86\uff1f",
    "\u975e\u5e38\u62b1\u6b49\uff0c\u51c6\u5c09\u5148\u751f\uff0c\u8bf7\u5141\u8bb8\u6211\u5411\u60a8\u8868\u8fbe\u611f\u8c22\u3002",
    "\u611f\u8c22\u2014\u2014\u54c8\u3002",
    "\u662f\u7684\uff0c\u51c6\u5c09\u5148\u751f\u3002\u611f\u8c22\u60a8\u80fd\u591f\u51c6\u8bb8\u6211\u4eec\u7684\u8fdb\u5165\uff0c\u5e76\u4e14\u5e2e\u52a9\u6211\u4eec\u6446\u8131\u74e6\u826f\u683c\u5e2e\u3002",
    "\u2026\u2026",
    "\u2026\u2026",
    "\u4f60\u7684\u6f14\u6280\u53ef\u771f\u591f\u70c2\u7684\u2014\u2014\u6307\u6325\u5b98\uff0c\u597d\u4e45\u4e0d\u89c1\u4e86\u3002",
    "\u770b\u5230\u6211\u7684\u901a\u884c\u4ee3\u7801\u8fd8\u50cf\u6a21\u50cf\u6837\u5730\u8ba9\u6211\u4fdd\u6301\u8ddd\u79bb\uff0c\u6211\u8fd8\u7d27\u5f20\u4e86\u4e00\u4e0b\uff0c\u5f53\u4f60\u5df2\u7ecf\u79bb\u5f00\u4e86\u5462\uff0c\u5c0a\u656c\u7684\u5f17\u62c9\u57fa\u7c73\u5c14\u51c6\u5c09\u5148\u751f\uff1f",
    "\u4f60\u7684\u8fd0\u6c14\u771f\u4e0d\u9519\uff0c\u6211\u786e\u5b9e\u662f\u9a6c\u4e0a\u5c31\u8981\u79bb\u5f00\u8fd9\u91cc\u3002|\u4e0a\u4e2a\u661f\u671f\u6536\u5230\u7684\u8c03\u52a8\u4ee4\uff0c\u6211\u7ec8\u4e8e\u53ef\u4ee5\u79bb\u5f00\u8fd9\u4e2a\u9b3c\u5730\u65b9\u53bb\u7eff\u533a\u4e86\u3002",
    "\u606d\u559c\u3002\u8fd9\u4e48\u591a\u5e74\uff0c\u4f60\u90a3\u4e9b\u624b\u4e0b\uff0c\u7ec8\u4e8e\u6709\u80fd\u63a5\u66ff\u4f60\u5de5\u4f5c\u7684\u4e86\uff1f",
    "\u52c9\u5f3a\u5408\u683c\u2026\u2026\u54c8\uff0c\u5e9f\u7269\u8fd8\u662f\u5e9f\u7269\uff0c\u4f46\u4e94\u5e74\u8fc7\u53bb\uff0c\u4e5f\u603b\u8be5\u6709\u70b9\u957f\u8fdb\u4e86\u5427\uff1f|\u5012\u662f\u4f60\uff0c\u8fd9\u4e48\u4e45\u4e0d\u89c1\u2014\u2014\u8fd8\u662f\u8001\u6837\u5b50\uff1f",
    "\u4f60\u4e5f\u770b\u5230\u4e86\uff0c\u6211\u73b0\u5728\u7684\u8d22\u52a1\u72b6\u51b5\u5c31\u50cf\u6211\u73b0\u5728\u8fd9\u8f86\u7834\u70c2\u7684\u57fa\u5730\u8f66\u4e00\u6837\u3002|\u591a\u4e8f\u4e86\u4f60\uff0c\u6211\u7684\u827e\u83ab\u53f7\u624d\u80fd\u7529\u6389\u74e6\u826f\u683c\u5e2e\u90a3\u4e9b\u70e6\u4eba\u7684\u5c3e\u5df4\u3002",
    "\u8fd9\u662f\u9047\u4e0a\u4ec0\u4e48\u9ebb\u70e6\u4e86\uff1f\u74e6\u826f\u683c\u5e2e\u90a3\u79cd\u7ea7\u522b\uff0c\u5e94\u8be5\u8fd8\u4e0d\u81f3\u4e8e\u88ab\u4f60\u5f53\u505a\u5bf9\u624b\uff1f",
    "\u8fd9\u5c31\u8bf4\u6765\u8bdd\u957f\u4e86\u3002\u603b\u4e4b\uff0c\u80fd\u5728\u4f60\u8fd9\u91cc\u8eb2\u8eb2\u5c31\u597d\u3002|\u6211\u8fd9\u7834\u57fa\u5730\u8f66\u548c\u7269\u8d44\u5b58\u91cf\uff0c\u53ef\u7ecf\u4e0d\u8d77\u8fde\u7eed\u6298\u817e\u3002",
    "\u54c8\u2014\u2014\u6ca1\u60f3\u5230\u6307\u6325\u5b98\u4e5f\u80fd\u6709\u8fd9\u79cd\u65f6\u5019\u2014\u2014\u4f1f\u5927\u7684B.R.I.E.F.\u6ce8\u518c\u8d4f\u91d1\u730e\u4eba\uff0c\u8981\u4e0d\u8981\u518d\u6298\u817e\u4e00\u4e0b\uff1f",
    "\u4f60\u8fd9\u7b97\u8d26\u7684\u901f\u5ea6\u53ef\u6bd4\u6211\u60f3\u5f97\u8981\u5feb\u3002",
    "\u53ea\u662f\u4f60\u6765\u5f97\u6b63\u5de7\u3002",
    "\u4ec0\u4e48\u4e8b\uff1f",
    "\u4e5f\u4e0d\u662f\u4ec0\u4e48\u9ebb\u70e6\u7684\u4e8b\u60c5\u3002",
    "0105\u8fd9\u4e2a\u51c0\u5316\u533a\u5df2\u7ecf\u51c0\u5316\u5b8c\u6210\uff0c\u524d\u65b9\u6b63\u5728\u8fdb\u884c\u65b0\u51c0\u5316\u533a\u7684\u5730\u9762\u4fee\u6574\u4e0e\u8f68\u9053\u94fa\u8bbe\u2014\u2014\u90a3\u91cc\u751f\u9ab8\u7684\u6e05\u7406\u867d\u7136\u73b0\u5728\u8fd8\u4e0d\u5e94\u8be5\u5f52\u6211\u4eec\u7ba1\u2014\u2014\u4f46\u4ed6\u4eec\u53d1\u6765\u4e86\u6c42\u63f4\u4fe1\u606f\uff0c\u6211\u4e5f\u4e0d\u80fd\u5ffd\u7565\u4e0d\u662f\uff1f",
    "\u4f60\u90a3\u4e9b\u6709\u7528\u7684\u624b\u4e0b\u4eec\u5462\uff0c\u4ed6\u4eec\u600e\u4e48\u4e0d\u53bb\u3002",
    "\u54c8\u2014\u2014",
    "\u4eca\u5929\u6709\u96ea\u3002\u201c\u8fd9\u79cd\u5929\u6c14\uff0c\u574d\u584c\u8f90\u5c04\u66b4\u589e\uff0c\u9632\u62a4\u670d\u6839\u672c\u6ca1\u7528\u201d\uff0c\u4f60\u770b\uff0c\u8c01\u4e5f\u4e0d\u613f\u610f\u51fa\u53bb\u3002",
    "\u800c\u4e14\uff0c\u4f60\u90a3\u4e9b\u4eba\u5f62\u53c8\u4e0d\u4f1a\u6b7b\uff0c\u6211\u624b\u4e0b\u8fd9\u5e2e\u5e9f\u7269\u4eec\uff0c\u8981\u662f\u51fa\u95e8\u4e00\u8d9f\u53ef\u4e0d\u77e5\u9053\u8981\u6b7b\u591a\u5c11\u5462\uff1f|\u4e0d\u7ba1\u600e\u4e48\u8bf4\u6211\u4e5f\u662f\u4ed6\u4eec\u7684\u4e0a\u7ea7\uff0c\u6bcf\u5e74\u5e74\u672b\u7ed9\u4ed6\u4eec\u7236\u6bcd\u5199\u7684\u4fe1\u91cc\uff0c\u6211\u53ef\u90fd\u627f\u8bfa\u8fc7\u8981\u5e26\u4ed6\u4eec\u56de\u5bb6\u7684\u3002",
    "\u8fd9\u53ef\u771f\u662f\u2014\u2014\u51fa\u8272\u7684\u957f\u5b98\uff1f\u6211\u90fd\u8981\u611f\u52a8\u5f97\u6d41\u6cea\u4e86\u3002",
    "\u54e6\u2014\u2014",
    "\u4e0d\u9519\u554a\u6307\u6325\u5b98\uff0c\u53c8\u6536\u7559\u4e86\u65b0\u7684\u4eba\u5f62\u3002\u4f60\u7684\u5174\u8da3\u53ef\u771f\u662f\u4e00\u5982\u65e2\u5f80\u7684\u5e7f\u535a\uff1f",
    "\u522b\u5632\u7b11\u6211\u4e86\uff0c\u5468\u8fb9\u7684\u73af\u5883\u60c5\u62a5\u6709\u5417\uff1f",
    "\u73af\u5883\u60c5\u62a5\u2026\u2026\uff1f",
    "\u6709\u662f\u6709\u2014\u2014\u4f46\u73b0\u5728\u8fd9\u79cd\u5929\u6c14\uff0c\u6211\u8fd9\u5e2e\u5e9f\u7269\u624b\u4e0b\u8fde\u6267\u52e4\u5ba4\u90fd\u4e0d\u613f\u610f\u5446\u7740\uff0c\u5929\u77e5\u9053\u60c5\u62a5\u5b58\u5728\u54ea\u91cc\uff0c\u8981\u627e\u5230\u53ef\u662f\u5f88\u82b1\u65f6\u95f4\u7684\u2014\u2014\u4e0d\u8fc7\u6c42\u63f4\u5904\u7684\u73af\u5883\u60c5\u62a5\u6211\u5012\u8fd8\u8bb0\u5f97\uff1f",
    "\u2026\u2026\u916c\u91d1\u600e\u4e48\u7b97\uff1f",
    "\u916c\u91d1\u2014\u2014\u4f1f\u5927\u7684B.R.I.E.F.\u6ce8\u518c\u8d4f\u91d1\u730e\u4eba\u7adf\u7136\u8ba4\u4e3a\u6211\u4eec\u8d2b\u56f0\u800c\u8fb9\u7f18\u7684\u6240\u8c13\u201c\u957f\u57ce\u5217\u8f66\u5b88\u519b\u201d\u80fd\u62ff\u5f97\u51fa\u8fd9\u79cd\u4e1c\u897f\uff1f",
    "\u6ca1\u6709\u916c\u91d1\uff1f\u51c6\u5c09\u5148\u751f\uff0c\u6211\u4eec\u90fd\u5df2\u7ecf\u5728\u5feb\u997f\u6b7b\u7684\u8fb9\u7f18\u4e86\uff0c\u8fd9\u6837\u5bf9\u5f85\u8001\u670b\u53cb\u53ef\u4e0d\u591f\u539a\u9053\u5427\uff1f",
    "\u6bd5\u7adf\u4f1f\u5927\u7684B.R.I.E.F.\u6ce8\u518c\u8d4f\u91d1\u730e\u4eba\u4e5f\u4e0d\u591f\u539a\u9053\uff1f\u6258\u4f60\u7684\u798f\uff0c\u81ea\u4ece\u6211\u4eec\u8fd9\u4e9b\u4f60\u4eec\u53e3\u4e2d\u7684\u201c\u957f\u57ce\u5217\u8f66\u5b88\u519b\u201d\u88ab\u5206\u6d3e\u51fa\u6765\uff0c\u5f85\u9047\u5c31\u4e00\u843d\u5343\u4e08\u3002\u62ff\u4e0d\u5230\u916c\u91d1\u2014\u2014\u5bfc\u81f4\u8fd9\u79cd\u7ed3\u679c\u7684\u2014\u2014\u8fd8\u4e0d\u662f\u4f60\u81ea\u5df1\uff1f",
    "\u2026\u2026",
    "\u4e0d\u8fc7\u2014\u2014\u7269\u8d44\u2014\u2014\u5982\u679c\u662f\u8bf4\u5f39\u836f\u4e00\u7c7b\u7684\uff0c\u5012\u662f\u53ef\u4ee5\u5206\u4f60\u4eec\u4e00\u4e9b\u3002",
    "\u2026\u2026\u6211\u8ba4\u4e3a\u519b\u7528\u7269\u8d44\u4f7f\u7528\u7684\u5ba1\u67e5\u5e94\u8be5\u5f88\u4e25\u683c\u3002",
    "\u4e25\u683c\uff1f",
    "\u4e94\u5e74\u6765\u5c31\u6ca1\u89c1\u8fc7\u6709\u4eba\u6765\u95ee\u8fc7\u6211\u50a8\u5907\u7684\u4e8b\uff0c\u800c\u4e14\u5c31\u7b97\u7559\u4e0b\u2014\u2014\u6211\u4eec\u5e94\u8be5\u4e5f\u7528\u4e0d\u4e0a\uff1f",
    "\u786e\u5b9e\u3002\u6210\u4ea4\uff0c\u6211\u4f1a\u53bb\u6536\u62fe\u90a3\u91cc\u7684\u751f\u9ab8\u3002",
    "\u679c\u7136\u662f\u53ef\u9760\u7684\u6307\u6325\u5b98\u3002",
    "\u8fd8\u6709\u4e2a\u9644\u52a0\u6761\u4ef6\u3002",
    "\u8bf7\u5c0a\u656c\u7684\u51c6\u5c09\u5148\u751f\u82b1\u8d39\u65f6\u95f4\u4ed4\u7ec6\u5730\u56de\u5fc6\u4e00\u4e0b\uff0c\u5730\u5f62\u590d\u6742\u3001\u6c14\u8c61\u53d8\u5316\u5c11\u3001\u751f\u9ab8\u6570\u91cf\u591a\uff0c\u6709\u6ca1\u6709\u540c\u65f6\u6ee1\u8db3\u4ee5\u4e0a\u4e09\u4e2a\u6761\u4ef6\u7684\u5730\u70b9\uff1f",
    "\u53ef\u80fd\u662f\u6709\u2014\u2014\u4f46\u60c5\u62a5\u5b58\u50a8\u8def\u5f84\u590d\u6742\u2014\u2014\u7b49\u4f60\u5b8c\u6210\u6211\u7684\u59d4\u6258\u4e4b\u540e\uff0c\u53ef\u80fd\u6211\u624d\u80fd\u60f3\u8d77\u6765\uff1f",
    "\u5567\uff0c\u6211\u53ef\u4e0d\u77e5\u9053\u574d\u584c\u8f90\u5c04\u8fd8\u80fd\u5bf9\u8bb0\u5fc6\u9020\u6210\u635f\u4f24\u3002",
    "\u90a3\u53ef\u4e0d\u4e00\u5b9a\u3002",
    "\u5f17\u62c9\u57fa\u7c73\u5c14\u5148\u751f\u770b\u8d77\u6765\u6ca1\u4ec0\u4e48\u53d8\u5316\u3002",
    "\u662f\u554a\uff0c\u8fd8\u662f\u548c\u4ee5\u524d\u4e00\u6837\u65a4\u65a4\u8ba1\u8f83\u3002|\u4ed6\u4e0d\u5e94\u8be5\u5f53\u519b\u5b98\uff0c\u6697\u5546\u633a\u9002\u5408\u4ed6\u3002",
    "\u59d4\u6258\u4fe1\u606f\u53ca\u5468\u8fb9\u73af\u5883\u60c5\u62a5\u63a5\u6536\u786e\u8ba4\u3002",
    "\u51fa\u53d1\u5427\uff0c\u5e0c\u671b\u51c6\u5c09\u5148\u751f\u7684\u56de\u5fc6\u4e4b\u65c5\u80fd\u591f\u4e00\u5207\u987a\u5229\u3002"
  ]
]