[
  [
    "\u8001\u3001\u8001\u5927\u2026\u2026\u524d\u9762\u4e00\u961f\u8bf4\u6b7b\u4e86\u592a\u591a\u4eba\uff0c\u4ed6\u4eec\u4e0d\u3001\u4e0d\u5e72\u4e86\u2026\u2026",
    "\uff08\u4ed6\u9762\u524d\u7684\u201c\u8001\u5927\u201d\u731b\u7136\u7ad9\u8d77\uff0c\u4e00\u811a\u8e22\u7ffb\u4e86\u81ea\u5df1\u4e4b\u524d\u6240\u5750\u7684\u6905\u5b50\u3002\uff09",
    "\u4e0d\u5e72\u4e86\uff1f\uff01\u4ed6\u8bf4\u4e0d\u5e72\u5c31\u4e0d\u5e72\u4e86\uff1f\uff01",
    "\u4e00\u961f\u7684\u6df7\u86cb\u8fd8\u5269\u4e0b\u591a\u5c11\uff01",
    "\u8bf4\u662f\u574d\u584c\u98ce\u66b4\u8981\u6765\u4e86\uff0c\u4ed6\u4eec\u8f66\u961f\u625b\u4e0d\u4f4f\u2026\u2026\u5c31\u90fd\u64a4\u8d70\u4e86\u2026\u2026",
    "\u4ec0\u4e48\uff1f\uff01",
    "\u4f60\u5c0f\u5b50\u8dd1\u4ec0\u4e48\u8dd1\uff1f\uff01\u7ed9\u6211\u6eda\u56de\u6765\uff01",
    "\u4f3c\u4e4e\u8ba1\u5212\u8fdb\u884c\u5f97\u5e76\u4e0d\u987a\u5229\uff0c\u5e03\u745f\u5c14\u5148\u751f\u3002",
    "\u54c8\uff1f\u4f60\u53c8\u662f\u54ea\u4e2a\uff1f\uff01",
    "\u2026\u2026\u54c8\u2026\u2026",
    "\u2026\u2026\u7bb1\u5b50\uff0c\u5fc5\u987b\u5230\u624b\u3002|\u5e03\u745f\u5c14\u5148\u751f\uff0c\u60a8\u6240\u671f\u5f85\u7684\u91d1\u94b1\u3001\u88c5\u5907\u2026\u2026\u60a8\u6240\u9700\u8981\u7684\u4e00\u5207\uff0c\u90fd\u8981\u62ff\u7bb1\u5b50\u6765\u6362\u3002",
    "\u54fc\uff0c\u4f60\u5c0f\u5b50\uff0c\u5c31\u8981\u7bb1\u5b50\u662f\u5427\u3002",
    "\u5982\u679c\u60a8\u80fd\u4e86\u89e3\u5230\u8fd9\u4e00\u70b9\uff0c\u90a3\u662f\u6700\u597d\u4e0d\u8fc7\u3002",
    "\u2014\u2014\u8fd9\u7834\u7bb1\u5b50\uff0c\u642d\u8fdb\u53bb\u4e86\u6211\u4e00\u6574\u4e2a\u8f66\u961f\uff01\u4e00\u6574\u4e2a\uff01\u4f60\u77e5\u4e0d\u77e5\u9053\uff1f\u554a\uff1f\uff01",
    "\u60a8\u6240\u9700\u8981\u7684\u524d\u671f\u8865\u7ed9\uff0c\u5df2\u7ecf\u786e\u5b9e\u9001\u8fbe\u3002",
    "\u987a\u5e26\u8fd8\u6254\u4e00\u5e2e\u4e00\u961f\u90a3\u79cd\u5783\u573e\u8d27\u8272\u7ed9\u6211\uff1f",
    "\u2026\u2026\u4ee5\u5408\u4f5c\u800c\u8a00\uff0c\u6211\u9700\u8981\u7684\u7ed3\u679c\uff0c\u53ea\u662f\u56de\u6536\u7bb1\u5b50\u3002\u800c\u60a8\u9700\u8981\u7684\u7ed3\u679c\uff0c\u662f\u62e5\u6709\u6bd4\u4e4b\u524d\u66f4\u4e3a\u5f3a\u5927\u7684\u8f66\u961f\u3002|\u5982\u679c\u6210\u529f\uff0c\u60a8\u5c06\u786e\u5b9e\u8fbe\u6210\u6240\u613f\u3002",
    "\u50cf\u4f60\u4eec\u8fd9\u79cd\u754f\u754f\u7f29\u7f29\u8eb2\u5728\u540e\u9762\u7684\u5783\u573e\u2014\u2014\u4e5f\u6562\u5bf9\u8001\u5b50\u6307\u624b\u753b\u811a\uff1f\uff01",
    "\u2026\u2026\u5982\u679c\u5931\u8d25\uff0c\u6211\u4fdd\u8bc1\u60a8\u5c06\u4f1a\u548c\u60a8\u5931\u53bb\u7684\u8f66\u961f\u518d\u4f1a\u3002",
    "\u54c8\uff1f\uff01\u5582\uff01\u5582\uff1f\uff01",
    "\uff08\u65e0\u4eba\u5e94\u7b54\u3002\u901a\u8baf\u88ab\u5f3a\u884c\u6302\u65ad\uff0c\u5e03\u745f\u5c14\u5c06\u901a\u8baf\u7ec8\u7aef\u72e0\u72e0\u5730\u6454\u5728\u5730\u4e0a\u3002\uff09",
    "\u8001\u5927\uff0c\u6211\u4eec\u63a5\u4e0b\u6765\u8981\u5e72\u4ec0\u4e48\u554a\uff1f",
    "\u54fc\u2014\u2014\u5e72\u4ec0\u4e48\uff1f",
    "\u540e\u9762\u76ef\u7740\u6211\u4eec\u7684\u53ef\u4e0d\u6b62\u4e00\u961f\u4e00\u4e2a\uff01\u522b\u7ba1\u90a3\u5e2e\u70e6\u4eba\u7684\u8017\u5b50\uff0c\u7ed9\u8001\u5b50\u53bb\u76f4\u63a5\u62ff\u7bb1\u5b50\uff01"
  ]
]