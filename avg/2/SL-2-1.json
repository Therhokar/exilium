    "\u8fd9\u6837\u554a\u2014\u2014\u4e0d\u8fc7\u4ece\u7ed3\u679c\u6765\u770b\uff0c\u786e\u5b9e\u662f\u96f6\u4ef6\u5c11\u4e86\u3002|\u8ba9\u6211\u60f3\u60f3\uff0c\u4f5c\u4e3a\u60e9\u7f5a\uff0c\u7f8e\u73b2\u73b0\u5728\u7684\u6536\u85cf\u54c1\u6570\u76ee\u662f\u2026\u2026\u55ef\u2026\u2026",
    "\u545c\u5443\u3001\u4e0a\u3001\u4e0a\u6b21\u8865\u5145\u8fc7\u540e\u4e5f\u53ea\u3001\u53ea\u5269\u5c0f\u534a\u4e2a\u67dc\u5b50\u4e86\u2026\u2026\u545c\u6307\u6325\u5b98\u80fd\u4e0d\u80fd\u6362\u4e2a\u522b\u7684\u2026\u2026",
    "\u522b\u7684\u554a\u2014\u2014\u8fd9\u4e2a\u6708\u7684\u5956\u91d1\u5417\uff1f",
    "\u2014\u2014\uff01",
    "\u5956\u91d1\u7684\u8bdd\u2014\u2014\u6211\u3001\u6211\u2026\u2026\u545c\u2026\u2026",
    "\u55ef\uff1f",
    "\u545c\u545c\u6263\u6389\u5956\u91d1\u5427\uff01\u6307\u6325\u5b98\uff01",
    "\u2026\u2026\u54c8\u2026\u2026",
    "\u5982\u679c\u628a\u7f8e\u73b2\u4f60\u7684\u5956\u91d1\u6263\u6389\u5c31\u80fd\u586b\u8865\u57fa\u5730\u8f66\u7684\u5f00\u9500\u9ed1\u6d1e\uff0c\u6211\u4eec\u4e5f\u4e0d\u81f3\u4e8e\u8fd9\u4e48\u8f9b\u82e6\u3002",
    "\u2026\u2026\u5443\u545c\u3001\u554a\u3001\u554a\uff1f",
    "\u8054\u7cfb\u4f69\u59ae\u6cfd\uff0c\u65e2\u7136\u4f60\u8bf4\u6e05\u5355\u4e0a\u6709\uff0c\u90a3\u591a\u534a\u5c31\u662f\u5979\u90a3\u91cc\u51fa\u4e86\u95ee\u9898\u3002",
    "\u771f\u3001\u771f\u7684\u5417\u2026\u2026\u4e0d\u9700\u8981\u6211\u7684\u5956\u91d1\u4e86\u5417\uff1f",
    "\u4e0d\u7136\u2014\u2014\u628a\u6211\u4eec\u7684\u827e\u83ab\u53f7\u4fee\u5b8c\u540e\uff0c\u518d\u52a0\u73ed\u5236\u4f5c\u4e00\u4e2a\u5c4f\u853d\u5668\uff1f|\u90a3\u4e2a\u7bb1\u5b50\uff0c\u53ea\u9760\u4f60\u94fa\u8bbe\u7684\u9694\u7edd\u6750\u6599\uff0c\u4fe1\u53f7\u4e0d\u80fd\u5b8c\u5168\u5c4f\u853d\u3002",
    "\u554a\u3001\u554a\uff0c\u545c\u5443\u3001\u52a0\u73ed\u2026\u2026\u6ca1\u95ee\u9898\uff01\u53ef\u662f\u8981\u505a\u5c4f\u853d\u5668\u7684\u8bdd\u2026\u2026\u73b0\u5728\u7f3a\u5c11\u96f6\u4ef6\uff0c\u53ef\u80fd\u4e5f\u9700\u8981\u8865\u5145\u2026\u2026",
    "\uff08\u770b\u5230\u7f8e\u73b2\u4ecd\u7136\u6127\u759a\u4e0d\u5b89\u5730\u6298\u78e8\u624b\u4e2d\u7684\u8bb0\u5f55\u5355\uff0c\u95ea\u7535\u4e0a\u524d\u4e00\u6b65\uff0c\u5c06\u8bb0\u5f55\u5355\u4ece\u7f8e\u73b2\u624b\u4e2d\u62bd\u51fa\u629a\u5e73\uff0c\u8fd8\u7ed9\u7f8e\u73b2\u3002\uff09",
    "\u7f8e\u73b2\uff0c\u6307\u6325\u5b98\u4e0d\u662f\u5728\u8d23\u602a\u4f60\uff0c\u522b\u62c5\u5fc3\u3002",
    "\u597d\u3001\u597d\u2026\u2026\u6211\u2026\u2026\u545c\u5443\u3001\u6211\u73b0\u5728\u53bb\u8054\u7cfb\u4f69\u59ae\u6cfd\u2026\u2026",
    "\u54c7~\u5c45\u7136\u662f\u6307\u6325\u5b98\uff1f|\u53c8\u6765\u7167\u987e\u751f\u610f\uff0c\u770b\u6765\u6700\u8fd1\u751f\u5b58\u8270\u96be\uff1f",
    "\u2026\u2026\u662f\u4e0a\u6b21\u7684\u8ba2\u5355\u91cc\u6709\u51e0\u4e2a\u96f6\u4ef6\u5c11\u4e86\u3002",
    "\u5c11\u4e86\uff1f\u2014\u2014\u4e0d\u53ef\u80fd\u54e6\uff0c\u60a8\u4e13\u4e1a\u53ef\u4fe1\u8d56\u7684\u6697\u5546\u670d\u52a1\u662f\u4e0d\u53ef\u80fd\u51fa\u9519\u7684\u54e6~\uff1f",
    "\u53ef\u662f\u3001\u9001\u56de\u6765\u7684\u7269\u8d44\u7bb1\u91cc\u786e\u5b9e\u6ca1\u6709\u2026\u2026",
    "\u4ec0\u4e48\u96f6\u4ef6\uff1f",
    "\u662f\u3001\u662f\u7f16\u53f7K812\u7684\u91cd\u578b\u8f7d\u5177\u901a\u7528\u56db\u7ea7\u626d\u77e9\u5c3e\u6746\u2026\u2026\u5c11\u4e86\u6574\u65743\u4e2a\uff01",
    "\uff08\u5bf9\u9762\u7684\u4f69\u59ae\u6cfd\u4f3c\u4e4e\u662f\u7ffb\u627e\u4e86\u4e00\u4e0b\u8bb0\u5f55\u3002\uff09",
    "\u2014\u2014K812\uff1f\u54c7\uff0c\u6307\u6325\u5b98\u7ec8\u4e8e\u53d1\u73b0\u4e86\u5462~",
    "\u2026\u2026\uff1f",
    "\u5f53\u65f6\u8fd9\u4e2a\u578b\u53f7\u7684\u96f6\u4ef6\u7f3a\u8d27\uff0c\u4f5c\u4e3a\u66ff\u4ee3\uff0c\u5c31\u7ed9\u6307\u6325\u5b98\u9001\u53bb\u4e86\u4e00\u4e2a\u7a0d\u7a0d\u6709\u4e9b\u9ebb\u70e6\u7684\u5c0f\u793c\u7269~|\u6211\u8fd8\u5728\u60f3\u6307\u6325\u5b98\u600e\u4e48\u5230\u73b0\u5728\u8fd8\u6ca1\u53d1\u73b0\u5462\uff1f",
    "\u4f60\u9001\u4ec0\u4e48\u4e86\uff1f",
    "\u522b\u62c5\u5fc3\uff0c\u4e00\u5b9a\u4e0d\u4f1a\u6210\u4e3a\u6307\u6325\u5b98\u4f60\u8106\u5f31\u7684\u811a\u8e35~",
    "\u2026\u2026\u6211\u53ef\u8fd8\u4e0d\u662f\u5200\u67aa\u4e0d\u5165\u3002",
    "\u54c7\u54e6~",
    "\u7f3a\u5c11\u7684\u96f6\u4ef6\u7a0d\u540e\u5c31\u4f1a\u9001\u8fbe\u3002|\u55ef~\u4f5c\u4e3a\u201c\u5c0f\u793c\u7269\u201d\u7684\u8865\u507f\uff0c\u63d0\u4f9b\u4e00\u4e2a\u514d\u8d39\u60c5\u62a5\u7684\u8ffd\u52a0\u4fe1\u606f\uff0c\u76f8\u4fe1\u4f60\u4f1a\u975e\u5e38\u611f\u5174\u8da3\uff1f",
    "\u2026\u2026",
    "\u6307\u6325\u5b98\uff0c\u8ffd\u8e2a\u4f60\u4eec\u7684\u74e6\u826f\u683c\u5e2e\uff0c\u8fd1\u671f\u5728\u9891\u7e41\u4f7f\u7528\u6697\u5546\u7f51\u7edc\u8d2d\u4e70\u4fe1\u606f\u4f20\u9012\u670d\u52a1\u3002",
    "\u4fe1\u606f\u4f20\u9012\u670d\u52a1\u2026\u2026\u6ca1\u8bb0\u9519\u7684\u8bdd\uff0c\u4f60\u4eec\u90a3\u4e2a\u4fe1\u606f\u4f20\u9012\u670d\u52a1\uff0c\u8d39\u7528\u53ef\u662f\u9ad8\u5f97\u79bb\u8c31\uff1f",
    "\u6bd5\u7adf\u662f\u8981\u628a\u4fe1\u606f\u7ecf\u7531\u7edd\u5bc6\u6e20\u9053\u4f20\u9012\u7ed9\u53d7\u4fe1\u4eba\u3002\u76f8\u6bd4\u4e8e\u8fd9\u79cd\u96be\u5ea6\uff0c\u6536\u53d6\u4e00\u70b9\u70b9\u624b\u7eed\u8d39\u53ef\u662f\u76f8\u5f53\u201c\u5408\u7406\u201d\u5462~",
    "\u554a\uff0c\u5bf9\u4e86\u2014\u2014\u6211\u7684\u4e2a\u4eba\u4fe1\u606f\u4e5f\u5728\u552e\u5356\u4e2d\uff0c\u624b\u7eed\u8d39\u4f4e\u4e86\u4e0d\u5c11\uff0c\u6307\u6325\u5b98\u6709\u6ca1\u6709\u5174\u8da3\uff1f",
    "\u2026\u2026\u4e0d\u5fc5\u4e86\u3002",
    "\u771f\u662f\u53ef\u60dc\u3002\u50cf\u662f\u74e6\u826f\u683c\u5e2e\u8fd9\u79cd\u53e3\u7891\u6781\u5dee\u7684\u5ba2\u6237\uff0c\u7ecf\u624b\u7684\u6697\u5546\u7279\u610f\u7559\u4e86\u6863\uff0c\u6211\u53bb\u6253\u542c\u65f6\u83b7\u5f97\u4ed6\u4eec\u7684\u4fe1\u606f\u8fd8\u7b97\u8f7b\u677e\u3002",
    "\u76f8\u6bd4\u8d77\u6765\uff0c\u4ed6\u4eec\u6536\u4fe1\u4eba\u7684\u624b\u6cd5\u76f8\u5f53\u4e13\u4e1a\u3002",
    "\u8bb0\u5f55\u6e05\u9664\u2014\u2014\u6216\u8005\u8df3\u8f6c\uff1f",
    "\u8df3\u8f6c\u3002\u8fd9\u90e8\u5206\u4fe1\u606f\u7ecf\u8fc7\u5c42\u5c42\u52a0\u5bc6\uff0c\u8df3\u8f6c\u5230\u4e86\u4e00\u4e2a\u7279\u5b9a\u8282\u70b9\u4e0a\u2014\u2014\u800c\u5728\u540c\u4e00\u65f6\u95f4\uff0c\u6709\u53e6\u4e00\u4e2a\u4fe1\u53f7\u63a5\u5165\u8fd9\u4e00\u7279\u5b9a\u8282\u70b9\u3002",
    "\u8fd9\u79cd\u624b\u6cd5\u4f1a\u7559\u4e0b\u6302\u70b9\u3002\u4ee5\u6302\u70b9\u4e3a\u57fa\u7840\u8fdb\u884c\u56de\u6eaf\u6620\u5c04\uff0c\u5bf9\u8c61\u662f\u4f60\uff0c\u8fd8\u662f\u80fd\u591f\u505a\u5230\u7684\uff1f",
    "\u8fd9\u624d\u662f\u6700\u8ba9\u6211\u60ca\u8bb6\u7684\u5730\u65b9\u3002",
    "\u5355\u6b21\u4fe1\u606f\u4f20\u9001\u7684\u91cf\u7ea7\u8d85\u8fc7\u5341\u4e07\uff0c\u800c\u8fd9\u5341\u4e07\u6761\u4fe1\u606f\uff0c\u6bcf\u6b21\u540c\u63a5\u90fd\u4f1a\u8fdb\u884c\u767e\u4e07\u91cf\u7ea7\u7684\u8df3\u8f6c\uff0c\u8df3\u8f6c\u5b8c\u6210\u540e\u53c8\u4f1a\u5206\u88c2\u4e3a\u5341\u4e07\u91cf\u7ea7\u7684\u4fe1\u606f\u53d1\u9001\u51fa\u53bb\u3002\u8fd9\u79cd\u91cf\u7ea7\u7684\u6570\u636e\u4f2a\u88c5\uff0c\u4e0d\u53ef\u8ffd\u8e2a\uff0c\u4e0d\u53ef\u56de\u6eaf\u3002",
    "\u4e0d\u53ef\u8ffd\u8e2a\u2014\u2014\u8fde\u4f60\u4e5f\uff1f",
    "\u5982\u679c\u524d\u63d0\u662f\u5356\u6389\u6211\u7684\u5168\u90e8\u8f66\u961f\uff0c\u8d2d\u5165\u6574\u6574\u4e00\u4e2a\u5730\u4e0b\u57fa\u5730\u7684\u670d\u52a1\u5668\uff0c\u5012\u662f\u53ef\u4ee5\u8ffd\u8e2a\u3002",
    "\u5bf9\u624b\u53ef\u4e0d\u50cf\u662f\u666e\u901a\u4eba\u54e6\uff0c\u4e45\u8fdd\u5730\u60f9\u4e0a\u9ebb\u70e6\u7684\u5bf9\u624b\u4e86\u5462\uff1f",
