  ],
  [
    "\u2026\u2026\u4f60\u662f\u54ea\u4f4d\u3002",
    "\u521d\u6b21\u89c1\u9762~\u6211\u7684\u540d\u5b57\u53eb\u5bc7\u5c14\u8299\uff0c\u4e00\u4e2a\u666e\u901a\u7684\u6c11\u7528\u4eba\u5f62\u3002|\u6211\u662f\u4ece\u4f69\u59ae\u6cfd\u90a3\u91cc\uff0c\u82b1\u8d39\u4e86\u597d\u2014\u2014\u5927\u529f\u592b\u6765\u6295\u5954\u4f20\u95fb\u4e2d\u7684\u6307\u6325\u5b98\uff0c\u53ef\u4e0d\u662f\u4ec0\u4e48\u574f\u4eba\u54df~",
    "\u54fc\uff0c\u4f60\u8bf4\u4e0d\u662f\u5c31\u4e0d\u662f\uff1f|\u5582\u6307\u6325\u5b98\uff0c\u5e72\u8106\u5148\u62c6\u4e86\u5979\u518d\u95ee\u95ee\u4f69\u59ae\u6cfd\uff0c\u7b49\u786e\u8ba4\u6ca1\u95ee\u9898\u4e86\u518d\u88c5\u56de\u6765\u3002",
    "\u54c7\uff0c\u8fd9\u2014\u2014\u4e48\u5938\u5f20\u7684\u5417~|\u4f60\u4eec\u770b\uff0c\u4eba\u5bb6\u90fd\u6ca1\u6709\u6b66\u5668\uff0c\u771f\u7684\u662f\u6765\u5e2e\u5fd9\u7684\uff0c\u4e0d\u8981\u8fd9\u4e48\u7d27\u5f20\u561b~",
    "\uff08\u95e8\u5185\uff0c\u81ea\u79f0\u201c\u5bc7\u5c14\u8299\u201d\u7684\u4eba\u5f62\u7ef4\u6301\u8dcc\u5012\u7684\u59ff\u52bf\uff0c\u4ece\u8eab\u540e\u62ff\u51fa\u4e86\u51e0\u74f6\u9152\u3002\uff09",
    "\u7c73\u2026\u2026\u7c73\u9152\uff1f\uff01",
    "\u554a\uff0c\u770b\u6765\u8fd9\u5c31\u662f\u4f69\u59ae\u6cfd\u8bf4\u7684\uff0c\u559c\u6b22\u8fd9\u4e9b\u6db2\u4f53\u7684\u53ef\u7231\u5973\u5b69\u5b50\u4e86\u3002|\u55ef\u2014\u2014\u8fd9\u4e9b\uff0c\u5168\u2014\u2014\u90fd\u9001\u7ed9\u4f60~",
    "\u771f\u2026\u2026\u771f\u7684\u5417\uff1f\uff01|\u6211\u7684\u6536\u85cf\uff0c\u4ece\u6765\u90fd\u53ea\u6709\u88ab\u62a2\u8d70\u7684\u2026\u2026\u7adf\u7136\u4f1a\u6709\u4eba\u9001\u6211\u545c\u545c\u545c\u545c\u2026\u2026",
    "\u2026\u2026\u5443\u3001\u7f8e\u73b2\uff0c\u4f60\u3001\u4f60\u522b\u54ed\u2026\u2026",
    "\u8fd9\u4f4d\u5bc7\u5c14\u8299\u5c0f\u59d0\u4e00\u5b9a\u662f\u597d\u4eba\u545c\u545c\u545c\uff01",
    "\u6ca1\u9519\uff01\u4eba\u5bb6\u662f\u5e26\u7740\u5b8c\u5b8c\u5168\u5168\u7684\u5584\u610f\u6765\u7684~\u8981\u4e0d\u8981\u8003\u8651\u5148\u8ba9\u4eba\u5bb6\u8fdb\u6765\u518d\u8bf4\u8bdd\u5440\uff1f",
    "\u8ba9\u5979\u8fdb\u6765\u3002",
    "\u6307\u6325\u5b98\uff0c\u654c\u6211\u4e0d\u660e\u3002",
