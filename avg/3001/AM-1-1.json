[
  [
    "\uff08\u6211\u5c06\u76ee\u5149\u6295\u5411\u4e86\u6625\u7530\uff0c\u5979\u4e5f\u540c\u6837\u5fe7\u8651\u5730\u770b\u7740\u6211\u3002\uff09",
    "\uff08\u542c\u7740\u9edb\u70df\u8bb2\u5b8c\u4e86\u5979\u4e4b\u524d\u7684\u7ecf\u5386\uff0c\u6211\u9677\u5165\u4e86\u601d\u8003\u3002\uff09",
    "\uff08\u9edb\u70df\u770b\u7740\u6211\u4eec\u7684\u8868\u60c5\uff0c\u610f\u8bc6\u5230\u81ea\u5df1\u78b0\u5230\u4e86\u548c\u4ee5\u5f80\u5b8c\u5168\u4e0d\u540c\u7684\u4e8b\u4ef6\u3002\uff09",
    "\uff08\u5979\u9732\u51fa\u4e86\u60f3\u8981\u8be2\u95ee\u4f46\u53c8\u6709\u6240\u987e\u8651\u7684\u8868\u60c5\uff0c\u6211\u60f3\u4e86\u60f3\u51b3\u5b9a\u544a\u8bc9\u5979\u5b9e\u60c5\u3002\uff09",
    "\uff08\u6211\u5c06\u4e4b\u524d\u548c\u6625\u7530\u8bb2\u8ff0\u7684\u60c5\u51b5\u5927\u81f4\u4ecb\u7ecd\u4e86\u4e00\u904d\uff0c\u53c8\u628a\u521a\u624d\u884c\u52a8\u65f6\u7684\u60c5\u666f\u505a\u4e86\u63cf\u8ff0\uff0c\u9edb\u70df\u7684\u8138\u4e0a\u6d6e\u73b0\u51fa\u62c5\u5fe7\u7684\u795e\u60c5\u3002\uff09",
    "\u6211\u4eec\u7adf\u7136\u9047\u4e0a\u4e86\u8fd9\u6837\u7684\u4e8b\u2026\u2026\u5e78\u597d\u9047\u5230\u4e86\u957f\u5b98\u2026\u2026",
    "\u4e0d\u5e78\u4e2d\u7684\u4e07\u5e78\u2026\u2026\u6211\u4e5f\u5f88\u610f\u5916\u9edb\u70df\u4f60\u600e\u4e48\u4f1a\u5377\u8fdb\u8fd9\u8d77\u6848\u4ef6\u4e2d\u7684\u3002",
    "\u8001\u59d0\u7a81\u7136\u5c31\u51b2\u51fa\u53bb\u4e86\uff0c\u7279\u522b\u83ab\u540d\u5176\u5999\uff01",
    "\u662f\u554a\uff0c\u9edb\u70df\u4f60\u600e\u4e48\u4f1a\u53bb\u548c\u6211\u4eec\u7684\u4efb\u52a1\u76ee\u6807\u63a5\u89e6\uff1f",
    "\u4e0d\u77e5\u600e\u4e48\u2026\u2026\u6211\u603b\u89c9\u5f97\u8fd9\u4e2a\u4eba\u80cc\u540e\u9690\u85cf\u7740\u4ec0\u4e48\u5f88\u5927\u7684\u5371\u673a\uff0c\u6211\u4e0d\u80fd\u5c31\u8fd9\u4e48\u5750\u89c6\u4e0d\u7ba1\u2026\u2026",
    "\u7ed3\u679c\u4f60\u81ea\u5df1\u5dee\u70b9\u5c31\u88ab\u5e72\u6389\u4e86\u3002",
    "\u2026\u2026\u6211\u6ca1\u60f3\u5230\u4f1a\u6709\u4eba\u5728\u57ce\u5e02\u4e2d\u5f00\u67aa\u3002",
    "\u6211\u5f53\u65f6\u5df2\u7ecf\u63a7\u5236\u4f4f\u4e86\u90a3\u4e2a\u53eb\u57c3\u4e1d\u7279\u7684\u5973\u5b50\uff0c\u7ed3\u679c\u7a81\u7136\u95f4\u5c31\u4ece\u80cc\u540e\u88ab\u88ad\u51fb\u4e86\u2026\u2026\u554a\u2014\u2014",
    "\u600e\u4e48\u4e86\uff1f\u8001\u59d0\u4f60\u54ea\u91cc\u4e0d\u8212\u670d\u5417\uff01",
    "\u90a3\u4e2a\u57c3\u4e1d\u7279\u9003\u8dd1\u65f6\uff0c\u6709\u4e2a\u7b14\u8bb0\u672c\u843d\u4e86\u4e0b\u6765\uff0c\u6211\u5f53\u65f6\u7528\u6700\u540e\u7684\u529b\u6c14\u5c06\u5b83\u653e\u8fdb\u8863\u670d\u4e2d\u4e86\u3002",
    "\uff08\u9edb\u70df\u5f00\u59cb\u6478\u7d22\u81ea\u5df1\u8863\u670d\u5185\u7684\u53e3\u888b\uff0c\u4e0d\u4e00\u4f1a\u5c31\u62ff\u51fa\u4e86\u4e00\u4e2a\u7b14\u8bb0\u672c\u3002\uff09",
    "\u90a3\u4e2a\u4eba\u6389\u843d\u7684\u7269\u54c1\uff1f",
    "\u6b63\u662f\u8fd9\u4e2a\uff0c\u957f\u5b98\u60a8\u8981\u770b\u4e00\u4e0b\u5417\uff1f",
    "\u5927\u5bb6\u4e00\u8d77\u8fc7\u6765\u770b\u5427\uff0c\u8bf4\u4e0d\u5b9a\u80fd\u591a\u53d1\u73b0\u4e00\u4e9b\u7ebf\u7d22\u3002",
    "\uff08\u4eba\u5f62\u4eec\u90fd\u805a\u4e86\u8fc7\u6765\uff0c\u9edb\u70df\u5c06\u7b14\u8bb0\u672c\u653e\u5728\u4e86\u684c\u4e0a\uff0c\u65b9\u4fbf\u5927\u5bb6\u90fd\u80fd\u770b\u5230\u3002\uff09",
    "\uff08\u7b14\u8bb0\u672c\u7684\u5c01\u9762\u88ab\u78e8\u51fa\u4e86\u7ec6\u788e\u7684\u6bdb\u8fb9\uff0c\u4f46\u662f\u4e0a\u9762\u5374\u6ca1\u6709\u4ec0\u4e48\u6c61\u6e0d\uff0c\u770b\u4e0a\u53bb\u662f\u88ab\u7cbe\u5fc3\u4fdd\u62a4\u7740\u7684\u3002\uff09",
    "\uff08\u9edb\u70df\u770b\u4e86\u6211\u4e00\u773c\uff0c\u5728\u5f97\u5230\u786e\u8ba4\u540e\uff0c\u5f00\u59cb\u4e00\u9875\u4e00\u9875\u5730\u5f80\u4e0b\u7ffb\u3002\uff09",
    "\u54a6\uff1f\u8fd9\u4e2a\u4eba\u7684\u5b57\u8fd8\u86ee\u79c0\u6c14\u7684\u561b\uff0c\u770b\u4e0d\u51fa\u6765\u2026\u2026|\u5443\uff0c\u600e\u4e48\u540e\u9762\u7684\u5b57\u90a3\u4e48\u6f66\u8349\u554a\uff1f\u548c\u524d\u9762\u5b8c\u5168\u4e0d\u4e00\u6837\uff0c\u8fd9\u771f\u662f\u5979\u7684\u7b14\u8bb0\u672c\u5417\uff1f",
    "\u4e00\u4e2a\u4eba\u7684\u5b57\u8ff9\u53ef\u4ee5\u6709\u90a3\u4e48\u5927\u7684\u5dee\u522b\u5417\uff1f",
    "\u901a\u5e38\u6765\u8bf4\uff0c\u4eba\u7c7b\u7684\u5b57\u8ff9\u90fd\u662f\u56fa\u5b9a\u7684\uff0c\u6709\u65f6\u5019\uff0c\u5b57\u8ff9\u4e5f\u4f1a\u6210\u4e3a\u5224\u5b9a\u4eba\u7c7b\u8eab\u4efd\u7684\u6807\u8bc6\uff0c\u5982\u679c\u8981\u6a21\u4eff\u7684\u8bdd\uff0c\u7ecf\u8fc7\u957f\u65f6\u95f4\u7ec3\u4e60\u4e5f\u4e0d\u662f\u4e0d\u53ef\u4ee5\u3002",
    "\u4f46\u8fd9\u91cc\u770b\u4e0d\u51fa\u6a21\u4eff\u7684\u75d5\u8ff9\uff0c\u6211\u503e\u5411\u4e8e\u524d\u540e\u5185\u5bb9\u51fa\u81ea\u4e0d\u540c\u4eba\u4e4b\u624b\u3002|\u524d\u540e\u7684\u5185\u5bb9\u4e5f\u6709\u5f88\u5927\u7684\u4e0d\u540c\u3002",
    "\u8fd9\u662f\u2026\u2026",
    "\uff08\u7eb8\u5f20\u7ffb\u52a8\u95f4\uff0c\u4e00\u5f20\u6cdb\u9ec4\u7684\u7167\u7247\u51fa\u73b0\u5728\u5927\u5bb6\u9762\u524d\u3002\uff09",
    "\uff08\u7167\u7247\u4e2d\uff0c\u6709\u4e00\u4f4d\u62ff\u7740\u7b14\u5927\u7b11\u7684\u5973\u6027\uff0c\u5979\u7684\u8eab\u8fb9\uff0c\u6709\u4e00\u4e2a\u6487\u7740\u5634\u5f3a\u5fcd\u6cea\u6c34\u7684\u5c0f\u5973\u5b69\u3002\u4e24\u4eba\u8eab\u540e\uff0c\u662f\u4e00\u7247\u767d\u8272\u7684\u9a6c\u94c3\u85af\u82b1\u4e1b\u3002\uff09",
    "\uff08\u611f\u53d7\u5230\u7167\u7247\u80cc\u540e\u7684\u51f9\u51f8\u89e6\u611f\uff0c\u9edb\u70df\u7ffb\u8fc7\u7167\u7247\uff0c\u4e00\u5c0f\u7c07\u767d\u8272\u7684\u5e72\u82b1\u5370\u5165\u773c\u5e18\u3002\uff09",
    "\u2026\u2026\u662f\u9a6c\u94c3\u85af\u82b1\u3002",
    "\uff08\u9edb\u70df\u4e0b\u610f\u8bc6\u5730\u6478\u4e86\u6478\u9888\u95f4\u7684\u73bb\u7483\u74f6\u3002\uff09",
    "\u524d\u9762\u597d\u50cf\u90fd\u662f\u4e00\u4e9b\u968f\u7b14\u548c\u8bd7\u8bcd\uff0c\u540e\u534a\u90e8\u5206\u2026\u2026\u8fd9\u4e9b\u662f\u673a\u68b0\u548c\u5de5\u7a0b\u56fe\uff1f",
    "\u8fd9\u4e2a\u4eba\u7684\u804c\u4e1a\u5c31\u662f\u5de5\u7a0b\u5e08\u5427\uff0c\u4f30\u8ba1\u8fd9\u4e5f\u662f\u5979\u88ab\u9009\u4e2d\u7684\u7406\u7531\u3002",
    "\u55ef\u2026\u2026\u5f88\u5408\u9002\u7684\u8eab\u4efd\uff0c\u4e5f\u80fd\u6709\u8db3\u591f\u7684\u4e13\u4e1a\u80fd\u529b\u53bb\u505a\u90a3\u79cd\u4e8b\u3002",
    "\u56af\uff0c\u5c45\u7136\u6709\u4e00\u4e2a\u8fd9\u4e48\u6b63\u7ecf\u7684\u804c\u4e1a\uff1f",
    "\u770b\u770b\u91cc\u9762\u6709\u6ca1\u6709\u53ef\u7591\u5185\u5bb9\u3002",
    "\u2026\u2026\u6709\u51e0\u5f20\u7ba1\u9053\u548c\u7535\u8def\u7684\u6392\u5e03\u56fe\uff0c\u770b\u4e0d\u51fa\u662f\u54ea\u91cc\u7684\uff0c\u4f46\u4e0a\u9762\u6709\u5f88\u591a\u4e09\u89d2\u6807\u8bb0\u3002",
    "\u8be5\u4e0d\u4f1a\u662f\u7206\u70b8\u5730\u56fe\u5427\uff1f",
    "\u770b\u4e0d\u51fa\u6765\u3002\u8fd8\u6709\u597d\u51e0\u5904\u88ab\u53cd\u590d\u6d82\u62b9\u7684\u6587\u5b57\uff0c\u54a6\uff0c\u8fd9\u5757\u597d\u50cf\u6ca1\u6709\u5168\u90e8\u6d82\u6389\uff0c\u5199\u7684\u662f\u2026\u2026",
    "\uff08\u9edb\u70df\u62ff\u8d77\u7b14\u8bb0\u672c\uff0c\u60f3\u8981\u770b\u5f97\u66f4\u6e05\u695a\u4e00\u4e9b\u3002\uff09",
    "\uff08\u968f\u7740\u5979\u7684\u52a8\u4f5c\uff0c\u4e00\u5f20\u53e0\u5f97\u56db\u56db\u65b9\u65b9\u7684\u7eb8\u7247\u4ece\u672c\u5b50\u4e2d\u6389\u843d\u51fa\u6765\uff0c\u88ab\u7edb\u96e8\u773c\u75be\u624b\u5feb\u5730\u6361\u8d77\u3002\uff09",
    "\u597d\u7834\u7684\u7eb8\u5440\uff0c\u8fd8\u76b1\u76b1\u5df4\u5df4\u7684\uff0c\u4e0a\u9762\u5199\u7740\u2026\u2026\u5199\u7740\u2026\u2026",
    "\u201c\u5982\u679c\u4f60\u5fc3\u4e2d\u8fd8\u6709\u6fc0\u8361\u6cb8\u817e\u7684\u70ed\u8840\uff0c\u5982\u679c\u4f60\u8fd8\u6000\u7740\u5bf9\u2026\u2026\u4e4c\u2026\u2026\u4e4c\u624e\u7684\u601d\u5ff5\u7559\u604b\u7737\u5ff5\uff0c\u90a3\u5c31\u6765\u5427\u2026\u2026\u201d|\u8fd9\u662f\u4ec0\u4e48\u5440\uff1f\u770b\u4e0d\u61c2\u3002\u5212\u5f97\u4e71\u4e03\u516b\u7cdf\u7684\u3002",
    "\uff08\u6625\u7530\u51d1\u8fc7\u6765\u770b\u4e86\u770b\u3002\uff09",
    "\u8fd9\u4e2a\u53e5\u5f0f\u548c\u590f\u514b\u91cc\u5199\u7684\u7528\u6765\u62db\u63fd\u5ba2\u4eba\u7684\u5ba3\u4f20\u6807\u8bed\u5012\u662f\u6709\u70b9\u50cf\u3002",
    "\u4e0b\u9762\u8fd8\u5199\u4e86\u4e00\u53e5\u88ab\u62b9\u6389\u7684\u8bdd\uff1f",
    "\uff08\u6211\u4ed4\u7ec6\u7aef\u8be6\u4e86\u8d77\u6765\uff0c\u867d\u7136\u88ab\u5f88\u591a\u6a2a\u6761\u6d82\u62b9\uff0c\u4f46\u662f\u91cc\u9762\u7684\u5b57\u6837\u8fd8\u662f\u5f88\u5bb9\u6613\u5c31\u88ab\u8bc6\u522b\u4e86\u51fa\u6765\u3002\uff09",
    "\u2026\u2026\u201c\u590d\u4ec7\u8ba1\u5212\u201d\u3002",
    "\uff08\u4eba\u5f62\u4eec\u770b\u5230\u4e0a\u9762\u7684\u5b57\u6837\uff0c\u4e0d\u7531\u5730\u6c89\u9ed8\u4e86\u4e0b\u6765\u3002\uff09",
    "\u4eca\u665a\u5927\u5bb6\u5c31\u4f11\u606f\u5427\uff0c\u5269\u4e0b\u7684\u4e8b\u5c31\u4ea4\u7ed9\u6211\u4eec\u3002\u7279\u522b\u662f\u9edb\u70df\uff0c\u8ba9\u81ea\u5df1\u597d\u597d\u6062\u590d\u3002",
    "\u554a\u2026\u2026\u662f\uff0c\u957f\u5b98\u3002",
    "\uff08\u6211\u793a\u610f\u9edb\u70df\u8eba\u4e0b\uff0c\u7136\u540e\u56de\u5934\u7528\u773c\u795e\u5411\u6625\u7530\u793a\u610f\u3002\uff09",
    "\uff08\u6625\u7530\u7acb\u5373\u7406\u89e3\u4e86\u6211\u7684\u610f\u601d\uff0c\u8ddf\u968f\u6211\u8d70\u51fa\u4e86\u623f\u95f4\u3002\uff09"
  ],
  [
    "\uff08\u5496\u5561\u9986\u7684\u5927\u5385\u53ea\u5f00\u4e86\u4e00\u76cf\u53f0\u706f\uff0c\u6211\u548c\u6625\u7530\u627e\u4e86\u4e00\u5f20\u684c\u5b50\u5750\u4e0b\uff0c\u660f\u6697\u7684\u706f\u5149\u4e0d\u7531\u8ba9\u6211\u4ea7\u751f\u4e86\u75b2\u60eb\u7684\u611f\u89c9\u3002\uff09",
    "\uff08\u6211\u6df1\u6df1\u5730\u53f9\u51fa\u4e00\u53e3\u6c14\u3002\uff09",
    "\u770b\u6765\u9edb\u70df\u9047\u5230\u7684\u8fd9\u4e2a\u4eba\u767e\u5206\u767e\u5c31\u662f\u6211\u4eec\u7684\u76ee\u6807\uff0c\u7b14\u8bb0\u672c\u91cc\u900f\u9732\u51fa\u6765\u7684\u4fe1\u606f\u4e5f\u5df2\u7ecf\u786e\u5b9a\u5979\u51c6\u5907\u5e72\u4e9b\u4ec0\u4e48\u4e0d\u597d\u7684\u4e8b\u4e86\u3002|\u8fd9\u4e2a\u4eba\u6211\u4eec\u6709\u4ec0\u4e48\u65b0\u6d88\u606f\u4e48\uff1f",
    "\u5979\u7684\u6863\u6848\u6211\u5df2\u7ecf\u67e5\u5230\u4e86\u3002|\u5979\u8fd8\u6709\u5979\u7684\u540c\u4e61\u4eec\u6765\u81ea\u4e00\u4e2a\u53eb\u201c\u4e4c\u624e\u9a6c\u5fb7\u5947\u201d\u7684\u5730\u65b9\u3002",
    "\uff08\u6625\u7530\u5c06\u6253\u5370\u597d\u7684\u7eb8\u8d28\u6750\u6599\u9012\u7ed9\u4e86\u6211\uff0c\u6211\u7ffb\u9605\u4e86\u4e00\u4e0b\uff0c\u7b2c\u4e00\u9875\u5c31\u662f\u90a3\u4e2a\u57c3\u4e1d\u7279\u7684\u8d44\u6599\uff0c\u800c\u540e\u9762\u662f\u5341\u51e0\u4e2a\u5176\u4ed6\u4eba\u7684\u8d44\u6599\u3002\uff09",
    "\uff08\u8fd9\u4e9b\u4eba\u6709\u7740\u4e00\u4e2a\u660e\u663e\u7684\u5171\u901a\u70b9\u2014\u2014\u51fa\u751f\u5730\u90fd\u5728\u540c\u4e00\u4e2a\u5730\u65b9\u3002\uff09",
    "\u201c\u4e4c\u624e\u9a6c\u5fb7\u5947\u201d\u2026\u2026\u6211\u8bb0\u5f97\u90a3\u4e2a\u5730\u65b9\u5df2\u7ecf\u2026\u2026",
    "\u6ca1\u9519\uff0c\u7531\u4e8e\u51c0\u5316\u5de5\u7a0b\u7684\u63a8\u8fdb\uff0c\u4ed6\u4eec\u6545\u4e61\u6240\u5728\u7684\u5730\u65b9\u5efa\u7acb\u8d77\u4e86\u79fb\u52a8\u9694\u79bb\u5899\u7684\u8f68\u9053\uff0c\u6574\u5ea7\u5c0f\u9547\u90fd\u88ab\u5f81\u7528\u4e86\u3002|\u5931\u53bb\u6545\u4e61\u7684\u4ed6\u4eec\u53ea\u80fd\u6e38\u8f6c\u4e8e\u5404\u4e2a\u536b\u661f\u57ce\u3002",
    "\u8fd9\u6b21\u4ed6\u4eec\u662f\u4ee5\u5e86\u5178\u642d\u5efa\u5de5\u4eba\u7684\u540d\u4e49\u6765\u5230\u8fd9\u91cc\uff0c\u5f88\u591a\u5e86\u5178\u4e2d\u8981\u4f7f\u7528\u7684\u8bbe\u5907\u90fd\u662f\u51fa\u81ea\u4ed6\u4eec\uff0c\u6050\u6015\u5e86\u5178\u4e2d\u7684\u4e34\u65f6\u8bbe\u65bd\u90fd\u6709\u7206\u70b8\u7684\u53ef\u80fd\u6027\u3002",
    "\u88ab\u593a\u8d70\u4e86\u6545\u4e61\u4e48\u2026\u2026|\u8fd9\u4e9b\u4eba\u6709\u8db3\u591f\u62a5\u590d\u7f57\u8054\u7684\u52a8\u673a\uff0c\u628a\u4ed6\u4eec\u902e\u6355\u540e\u5f88\u5bb9\u6613\u5c31\u80fd\u7ed3\u6848\uff0c\u4e00\u5207\u7684\u7f6a\u8d23\u90fd\u4f1a\u52a0\u5728\u4ed6\u4eec\u7684\u5934\u4e0a\uff0c\u6050\u6015\u6700\u540e\u7684\u5224\u51b3\u4e5f\u662f\u67aa\u51b3\u4e86\u4e8b\u3002",
    "\u80cc\u540e\u7b56\u5212\u7684\u4eba\u4e3a\u4e86\u9690\u85cf\u81ea\u5df1\u82b1\u4e86\u5f88\u591a\u5fc3\u601d\u3002",
    "\u662f\u554a\uff0c\u7279\u610f\u6307\u4f7f\u8fd9\u4e9b\u53ef\u601c\u7684\u4eba\u8dd1\u5230\u8fd9\u91cc\u4ee5\u5e86\u5178\u4e3a\u65f6\u673a\u641e\u6050\u6016\u88ad\u51fb\u2026\u2026|\u8fd9\u6b21\u5e86\u5178\uff0c\u6211\u8bb0\u5f97\u662f\u4e3a\u4e86\u8ba9\u7eff\u533a\u5c45\u6c11\u652f\u6301\u51c0\u5316\u5de5\u7a0b\u653f\u7b56\u800c\u4e13\u95e8\u4e3e\u884c\u7684\u3002",
    "\u5982\u679c\u5e86\u5178\u4e0a\u53d1\u751f\u7206\u70b8\u4f1a\u5f15\u53d1\u57ce\u5e02\u6df7\u4e71\uff0c\u5982\u679c\u505c\u6b62\u4e3e\u529e\u5e86\u5178\uff0c\u7f57\u8054\u653f\u5e9c\u5728\u7eff\u533a\u4e2d\u7684\u5a01\u671b\u4f1a\u5927\u6253\u6298\u6263\u3002|\u771f\u662f\u4e00\u7bad\u53cc\u96d5\u7684\u624b\u6bb5\u554a\u3002",
    "\u60a8\u786e\u5b9a\u8fd9\u80cc\u540e\u662f\u5409\u62c9\u5c14\u96c6\u56e2\u5728\u7b56\u5212\u4e48\uff0c\u5305\u62ec\u5bf9\u9edb\u70df\u7684\u88ad\u51fb\uff1f",
    "\u88ad\u51fb\u9edb\u70df\u7684\u4eba\u4f7f\u7528\u7684\u662f\u88c5\u6709\u6d88\u97f3\u5668\u7684\u6b66\u5668\uff0c\u4ece\u5979\u7d20\u4f53\u4e2d\u53d6\u51fa\u7684\u5f39\u5934\u662f\u7279\u5236\u7684\u94a2\u82af\u91cd\u5f39\uff0c\u8fd9\u4e9b\u662f\u8fde\u7eff\u533a\u7684\u5730\u4e0b\u9ed1\u5e2e\u90fd\u5f88\u96be\u641e\u5230\u7684\u4e1c\u897f\u3002|\u4f60\u8bf4\u5462\uff1f",
    "\u5982\u679c\u662f\u548c\u5409\u62c9\u5c14\u4f59\u515a\u8fd9\u79cd\u7a77\u9014\u672b\u8def\u7684\u6b79\u5f92\u5bf9\u6297\uff0c\u6211\u4eec\u73b0\u5728\u6050\u6015\u52bf\u5355\u529b\u8584\uff0c\u662f\u4e0d\u662f\u548c\u683c\u7433\u5a1c\u90a3\u8fb9\u8054\u7cfb\u4e0b\u2026\u2026\uff1f",
    "\u5982\u679c\u5904\u7406\u8fd9\u4e48\u70b9\u4e8b\u8fd8\u8981\u8ba9\u5979\u652f\u63f4\uff0c\u4ee5\u540e\u6211\u6050\u6015\u8981\u88ab\u5979\u7b11\u4e00\u8f88\u5b50\u3002",
    "\u653e\u5fc3\u5427\uff0c\u6211\u4eec\u73b0\u5728\u6ca1\u6709\u66b4\u9732\uff0c\u6211\u6709\u8db3\u591f\u7684\u4fe1\u5fc3\u51fa\u5947\u5236\u80dc\u3002",
    "\u6211\u660e\u767d\u4e86\uff0c\u6211\u4f1a\u542c\u4ece\u6307\u6325\u5b98\u7684\u5b89\u6392\u3002",
    "\u62b1\u6b49\u6625\u7530\u2026\u2026\u6211\u662f\u4e0d\u662f\u628a\u4f60\u2026\u2026\u8fd8\u6709\u8fd9\u5bb6\u5496\u5561\u9986\u7f6e\u4e8e\u5371\u9669\u4e4b\u4e2d\u4e86\u2026\u2026",
    "\u4e0d\u8981\u4ecb\u610f\u8fd9\u4e9b\uff0c\u6307\u6325\u5b98\uff0c\u6211\u76f8\u4fe1\u60a8\u4e00\u5b9a\u80fd\u5b8c\u7f8e\u5730\u5b8c\u6210\u4efb\u52a1\u3002",
    "\u800c\u4e14\u5982\u679c\u771f\u7684\u53d1\u751f\u4ec0\u4e48\u52a8\u4e71\uff0c\u5728\u8fd9\u5ea7\u57ce\u5e02\u4e2d\u7684\u201c\u65b9\u7cd6\u201d\u52bf\u5fc5\u4e5f\u4f1a\u9677\u5165\u5371\u9669\u3002",
    "\u73b0\u5728\u91cd\u8981\u7684\u662f\u627e\u5230\u90a3\u4e2a\u5173\u952e\u7684\u4eba\uff0c\u6211\u5df2\u7ecf\u62dc\u6258\u6cbb\u5b89\u90e8\u95e8\u7528\u5168\u529b\u5bfb\u627e\u57c3\u4e1d\u7279\u7684\u4e0b\u843d\u4e86\u3002|\u4e00\u65e6\u627e\u5230\u4ed6\u4eec\u9a6c\u4e0a\u5c31\u4f1a\u53d1\u9001\u4fe1\u606f\u8fc7\u6765\u3002",
    "\u55ef\u2026\u2026\u521a\u624d\u53d1\u751f\u4e86\u90a3\u6837\u7684\u4e8b\uff0c\u73b0\u5728\u5979\u4e00\u5b9a\u8eb2\u8d77\u6765\u4e86\uff0c\u4e0d\u4f1a\u90a3\u4e48\u5bb9\u6613\u88ab\u627e\u5230\u3002",
    "\u6211\u731c\u6d4b\u9edb\u70df\u906d\u5230\u7684\u88ad\u51fb\u539f\u672c\u662f\u9488\u5bf9\u90a3\u4e2a\u57c3\u4e1d\u7279\u7684\u3002",
    "\u6211\u731c\u4e5f\u662f\u5982\u6b64\uff0c\u4e0d\u8fc7\u8ba9\u6211\u5947\u602a\u7684\u662f\uff0c\u5982\u679c\u662f\u90a3\u6837\uff0c\u5e86\u5178\u8fd8\u6ca1\u5f00\u59cb\u80cc\u540e\u7684\u4eba\u5c31\u8981\u6025\u7740\u706d\u5979\u7684\u53e3\u5417\uff1f|\u9664\u975e\u2026\u2026",
    "\u9664\u975e\u2026\u2026\u57c3\u4e1d\u7279\u80cc\u53db\u4e86\u4ed6\u4eec\u3002",
    "\u55ef\uff0c\u8fd9\u662f\u6700\u6709\u53ef\u80fd\u7684\u9009\u9879\u3002|\u5979\u4e0d\u60f3\u5e72\u8fd9\u79cd\u4e07\u52ab\u4e0d\u590d\u7684\u4e8b\uff0c\u90a3\u4e48\u5409\u62c9\u5c14\u4f59\u515a\u5c31\u4e0d\u80fd\u8ba9\u5979\u7ee7\u7eed\u6d3b\u7740\u3002",
    "\u5982\u679c\u662f\u90a3\u6837\u7684\u8bdd\uff0c\u6211\u4eec\u5f97\u5728\u57c3\u4e1d\u7279\u88ab\u5e72\u6389\u524d\u627e\u5230\u5979\uff0c\u8d8a\u65e9\u8d8a\u597d\u3002",
    "\u6211\u518d\u50ac\u4fc3\u4e0b\u6cbb\u5b89\u90e8\u95e8\uff0c\u5c3d\u5feb\u627e\u5230\u5979\u7684\u4f4d\u7f6e\u3002",
    "\u55ef\uff0c\u6211\u8fd8\u9700\u8981\u66f4\u591a\u4eba\u5f62\u3002|\u5982\u679c\u8981\u627e\u5230\u5e76\u4fdd\u62a4\u90a3\u4e2a\u57c3\u4e1d\u7279\uff0c\u4e00\u5b9a\u4f1a\u548c\u96c6\u56e2\u7684\u4eba\u4ea7\u751f\u51b2\u7a81\u3002|\u5230\u65f6\u514d\u4e0d\u4e86\u4e00\u573a\u6076\u4ed7\u3002",
    "\u6211\u4eec\u8fd8\u6709\u9664\u4e86\u590f\u514b\u91cc\u5916\u5176\u4ed6\u7684\u5e2e\u624b\u5417\uff1f",
    "\u5496\u5561\u9986\u8fd8\u6709\u4e24\u4e2a\u5e2e\u624b\uff0c\u90fd\u662f\u6709\u706b\u63a7\u6838\u5fc3\u7684\u4eba\u5f62\u3002",
    "\u662f\u4e0d\u4f1a\u8fdd\u53cd\u534f\u8bae\u7684\u4eba\u5f62\u4e48\uff1f",
    "\u662f\u6851\u6735\u83b1\u5e0c\u548c\u739b\u7eee\u6735\uff0c\u4e5f\u5c31\u662fG36\u548cWA2000\uff0c\u8fd9\u662f\u5979\u4eec\u73b0\u5728\u7684\u540d\u5b57\u3002",
    "\u662f\u5979\u4eec\u4e48\u2026\u2026\u6211\u4e0d\u60f3\u7275\u8fde\u5230\u5979\u4eec\u3002|\u8bf4\u5b9e\u8bdd\uff0c\u8981\u4e0d\u662f\u6ca1\u6709\u529e\u6cd5\uff0c\u8fde\u4f60\u6211\u4e5f\u4e0d\u60f3\u7275\u8fde\u5230\u3002",
    "\u5979\u4eec\u548c\u6211\u4e00\u6837\uff0c\u4e3a\u4e86\u6307\u6325\u5b98\u4e00\u5b9a\u4f1a\u4e0d\u60dc\u8fdd\u53cd\u534f\u8bae\u7684\u3002",
    "\u6211\u62c5\u5fc3\u7684\u5c31\u662f\u8fd9\u4e2a\u2026\u2026",
    "\u5c31\u7b97\u6307\u6325\u5b98\u518d\u4e0d\u613f\u610f\uff0c\u73b0\u5728\u60f3\u8981\u627e\u975e\u683c\u91cc\u82ac\u7684\uff0c\u8fd8\u8981\u4fe1\u5f97\u8fc7\u7684\u4eba\u5f62\u5b9e\u5728\u6709\u4e9b\u5f3a\u4eba\u6240\u96be\u3002",
    "\u5979\u4eec\u5df2\u7ecf\u5728\u56de\u57ce\u7684\u8def\u4e0a\uff0c\u53ea\u8981\u518d\u7b49\u534a\u5929\u7684\u65f6\u95f4\u2026\u2026",
    "\u5e86\u5178\u6211\u8bb0\u5f97\u662f\u660e\u5929\u665a\u4e0a\u5427\u3002",
    "\u55ef\u2026\u2026",
    "\u5c31\u7b97\u6211\u613f\u610f\u4e5f\u7b49\u4e0d\u4e86\u5979\u4eec\u90a3\u4e48\u4e45\u4e86\u3002|\u6211\u4eec\u8981\u8d76\u5728\u6cbb\u5b89\u90e8\u95e8\u6536\u7f51\u3001\u57c3\u4e1d\u7279\u88ab\u5e72\u6389\u524d\uff0c\u5c06\u5e55\u540e\u9ed1\u624b\u7ed9\u627e\u51fa\u6765\u3002|\u73b0\u5728\u9a6c\u4e0a\u5c31\u5f97\u884c\u52a8\u3002",
    "\uff08\u6625\u7530\u601d\u8003\u4e86\u4e00\u4e0b\uff0c\u5411\u6211\u70b9\u70b9\u5934\u3002\u5979\u7ad9\u8d77\u8eab\u6765\u89e3\u5f00\u4e86\u7cfb\u7740\u7684\u56f4\u88d9\u3002\uff09",
    "\u6211\u660e\u767d\u4e86\uff0c\u90a3\u63a5\u4e0b\u6765\u6211\u4e5f\u548c\u60a8\u4e00\u8d77\u884c\u52a8\u3002",
    "\u8bf7\u7b49\u4e00\u4e0b\uff01",
    "\uff08\u9edb\u70df\u548c\u5979\u4e24\u540d\u961f\u53cb\u8d70\u51fa\u4e86\u539f\u672c\u623f\u95f4\uff0c\u867d\u7136\u9edb\u70df\u8fd8\u6709\u4e9b\u7ad9\u7acb\u4e0d\u7a33\uff0c\u4f46\u5979\u8fd8\u662f\u6025\u5207\u5730\u8d70\u5230\u6211\u4eec\u9762\u524d\u3002\uff09",
    "\u9edb\u70df\uff0c\u4f60\u8fd8\u6ca1\u5b8c\u5168\u6062\u590d\uff0c\u4e0d\u80fd\u8fd9\u6837\u4e71\u52a8\uff01",
    "\u6ca1\u5173\u7cfb\u7684\u6625\u7530\u5c0f\u59d0\uff0c\u6211\u5bf9\u6211\u7684\u7d20\u4f53\u8fd8\u662f\u6709\u81ea\u4fe1\u7684\u3002",
    "\u6625\u7530\u5c0f\u59d0\u8bf7\u548c\u4ee5\u5f80\u4e00\u6837\u5f85\u5728\u540e\u65b9\u652f\u63f4\u6211\u4eec\u5427\u3002|\u6211\u548c\u7edb\u96e8\u4ee5\u524d\u662f\u524d\u7ebf\u6218\u5907\u961f\u7684\u4eba\u5f62\uff0c\u671d\u6656\u7684\u8eab\u624b\u4e5f\u4e1d\u6beb\u4e0d\u5f31\u4e8e\u6211\u4eec\u3002|\u6211\u4eec\u662f\u966a\u540c\u957f\u5b98\u4e00\u8d77\u6267\u884c\u4efb\u52a1\u66f4\u5408\u9002\u7684\u4eba\u9009\uff01",
    "\u2026\u2026\u9edb\u70df\uff1f",
    "\uff08\u6211\u6709\u4e9b\u60ca\u8bb6\u5730\u56de\u8fc7\u5934\u770b\u7740\u9edb\u70df\uff0c\u867d\u7136\u5979\u8fd8\u6709\u51e0\u5206\u865a\u5f31\uff0c\u4f46\u76ee\u5149\u5341\u5206\u575a\u5b9a\uff0c\u521a\u624d\u7684\u8fdf\u7591\u548c\u62c5\u5fe7\u5df2\u7ecf\u4e00\u626b\u800c\u7a7a\u3002\uff09",
    "\u53ef\u662f\u2026\u2026\u4f60\u4eec\u73b0\u5728\u5df2\u7ecf\u4e0d\u662f\u683c\u91cc\u82ac\u7684\u4eba\u5f62\uff0c\u800c\u6211\u4e5f\u4e0d\u662f\u683c\u91cc\u82ac\u7684\u6307\u6325\u5b98\u4e86\uff0c\u4f55\u51b5\u4ece\u534f\u8bae\u7684\u89d2\u5ea6\u6211\u4eec\u4e5f\u4e0d\u8be5\u518d\u63a5\u89e6\u4e86\u2026\u2026",
    "\u957f\u5b98\u2026\u2026\u6307\u6325\u5b98\uff01|\u8bf7\u5141\u8bb8\u6211\u518d\u548c\u60a8\u4e00\u8d77\u5e76\u80a9\u4f5c\u6218\uff0c\u4e0d\u662f\u56e0\u4e3a\u683c\u91cc\u82ac\uff0c\u800c\u662f\u56e0\u4e3a\u6211\u662f\u60a8\u9ebe\u4e0b\u7684\u6218\u672f\u4eba\u5f62\uff01",
    "\u662f\u554a\uff01\u8bf4\u5b9e\u8bdd\u76f8\u6bd4\u6253\u5de5\uff0c\u8fd8\u662f\u4f5c\u6218\u6709\u610f\u601d\u5f97\u591a\uff01|\u957f\u5b98\u8d70\u4e86\uff0c\u683c\u91cc\u82ac\u4e5f\u63a5\u4e0d\u5230\u4ec0\u4e48\u50cf\u6837\u7684\u4efb\u52a1\uff0c\u5b9e\u5728\u662f\u65e0\u804a\u6b7b\u4e86\uff01",
    "\u800c\u4e14\u8bf4\u4ec0\u4e48\u534f\u8bae\u2014\u2014\u6625\u7530\u4e0d\u4e5f\u5728\u7ed9\u957f\u5b98\u5e2e\u5fd9\u5417\uff1f|\u5979\u90fd\u53ef\u4ee5\u51ed\u4ec0\u4e48\u6211\u4eec\u4e0d\u53ef\u4ee5\u5440\uff01",
    "\u7edb\u96e8\u2026\u2026\u4e0d\u8981\u4e71\u8bf4\u8bdd\uff01",
    "\u2026\u2026\u7edb\u96e8\u8fd8\u662f\u548c\u4ee5\u524d\u4e00\u6837\u5927\u5927\u54a7\u54a7\u5462\u3002",
    "\u8bf4\u5f97\u4e5f\u662f\u5462\uff0c\u8981\u662f\u6307\u6325\u5b98\u5bf9\u6211\u8fc7\u4e8e\u504f\u5fc3\uff0c\u4ee5\u540e\u683c\u91cc\u82ac\u7684\u5927\u5bb6\u518d\u805a\u4f1a\u65f6\uff0c\u6211\u5f88\u96be\u548c\u5927\u5bb6\u89e3\u91ca\u554a\u3002",
    "\u5982\u679c\u56e0\u4e3a\u8fdd\u53cd\u534f\u8bae\u800c\u8ba9\u9edb\u70df\u5979\u4eec\u5931\u53bb\u4e86\u8eab\u4efd\uff0c\u6307\u6325\u5b98\u7528\u90a3\u8f86\u827e\u83ab\u53f7\u6536\u7559\u5979\u4eec\u4e0d\u5c31\u597d\u4e86\uff1f",
    "\u5443\u2026\u2026\u2026\u2026",
    "\u4f55\u51b5\u5982\u679c\u5e86\u5178\u90a3\u4e48\u591a\u4e2a\u4e34\u65f6\u8bbe\u65bd\u90fd\u6709\u7206\u70b8\u7684\u98ce\u9669\uff0c\u6211\u4eec\u9700\u8981\u7684\u4eba\u624b\u53ef\u8fdc\u8fdc\u4e0d\u591f\u3002",
    "\u9edb\u70df\u548c\u7edb\u96e8\u53ef\u90fd\u662f\u4ee5\u524d\u683c\u91cc\u82ac\u7684\u7cbe\u82f1\u4eba\u5f62\uff0c\u8fd9\u4f4d\u671d\u6656\u5c0f\u59d0\u770b\u8d77\u6765\u4e5f\u9887\u5177\u5b9e\u529b\u3002|\u7b97\u4e0a\u6211\u548c\u590f\u514b\u91cc\uff0c\u518d\u52a0\u4e0a\u534a\u5929\u540e\u80fd\u8d76\u6765\u7684\u5e2e\u624b\uff0c\u8fd9\u6837\u7684\u961f\u4f0d\u914d\u7f6e\u5e94\u8be5\u8db3\u591f\u6307\u6325\u5b98\u5b8c\u6210\u4efb\u52a1\u4e86\u3002",
    "\u55ef\u2026\u2026",
    "\u957f\u5b98\uff0c\u8bf7\u548c\u4ee5\u524d\u4e00\u6837\u4e0b\u547d\u4ee4\u5427\uff01",
    "\uff08\u6211\u770b\u7740\u773c\u524d\u7684\u4eba\u5f62\u4eec\u3002\uff09",
    "\uff08\u5373\u4f7f\u8fc7\u53bb\u4e86\u90a3\u4e48\u6f2b\u957f\u7684\u65f6\u5149\uff0c\u5979\u4eec\u4e5f\u4f9d\u7136\u4fe1\u4efb\u7740\u6211\uff0c\u613f\u610f\u4e3a\u6211\u594b\u4e0d\u987e\u8eab\uff0c\u6211\u4e0d\u7981\u611f\u89c9\u5230\u773c\u7736\u6709\u4e00\u4e9b\u53d1\u70ed\u3002\uff09",
    "\uff08\u6211\u8d76\u7d27\u80cc\u8fc7\u8eab\u53bb\uff0c\u4e0d\u8ba9\u5979\u4eec\u770b\u5230\u6211\u8138\u4e0a\u7684\u5f02\u5e38\u3002\uff09",
    "\u6211\u2026\u2026\u6211\u660e\u767d\u4e86\uff0c\u5927\u5bb6\u505a\u4e00\u4e0b\u4f5c\u6218\u7684\u51c6\u5907\u3002|\u7b49\u5173\u4e8e\u57c3\u4e1d\u7279\u7684\u4fe1\u606f\u4e00\u5230\uff0c\u6211\u4eec\u5c31\u7acb\u5373\u51fa\u53d1\uff0c\u8fd9\u6b21\u7b97\u6211\u6b20\u5927\u5bb6\u7684\u3002",
    "\u597d\u8036\uff01\u7ec8\u4e8e\u53ef\u4ee5\u65bd\u5c55\u62f3\u811a\u5566\uff0c\u592a\u597d\u4e86\u8001\u59d0\uff0c\u7ec8\u4e8e\u53ef\u4ee5\u56de\u5230\u6700\u5feb\u4e50\u7684\u72b6\u6001\u4e86\uff01",
    "\u597d\u4e86\u597d\u4e86\u7edb\u96e8\uff0c\u4e0d\u8981\u90a3\u4e48\u5927\u5435\u5927\u95f9\u7684\u3002",
    "\u53ea\u6709\u6211\u52a0\u5165\u4e0d\u4e86\u4f60\u4eec\u53d9\u65e7\u7684\u8bdd\u9898\u4e48\u2026\u2026",
    "\u554a\u671d\u6656\uff0c\u539f\u6765\u4f60\u5728\u554a\u3002",
    "\u6211\u4e00\u76f4\u5728\uff0c\u6211\u53ea\u662f\u627e\u4e0d\u5230\u52a0\u5165\u8bdd\u9898\u7684\u65f6\u673a\u3002",
    "\u6ca1\u4e8b\uff0c\u8fd9\u6b21\u4efb\u52a1\u540e\u4f60\u5c31\u80fd\u52a0\u5165\u4e86\uff0c\u4f60\u5f88\u5feb\u5c31\u80fd\u77e5\u9053\u957f\u5b98\u591a\u6709\u610f\u601d\u4e86\uff01",
    "\u8bf6\uff0c\u957f\u5b98\u5f88\u6709\u610f\u601d\u5417\uff1f",
    "\u2026\u2026\u7edb\u96e8\u4f60\u8fd9\u8bdd\u662f\u5728\u5938\u6211\u8fd8\u662f\u5728\u8d2c\u6211\u554a\u3002",
    "\uff08\u5c31\u5728\u8c08\u8bdd\u95f4\uff0c\u590f\u514b\u91cc\u7a81\u7136\u6253\u5f00\u623f\u95e8\u51b2\u4e86\u8fdb\u6765\uff0c\u6211\u6b63\u7591\u60d1\u521a\u624d\u590f\u514b\u91cc\u5230\u4ec0\u4e48\u5730\u65b9\u53bb\u4e86\u3002\uff09",
    "\u5e97\u957f\uff01\u4f60\u8981\u6211\u5b88\u7740\u7684\u90ae\u7bb1\u53d1\u4fe1\u606f\u6765\u4e86\uff01",
    "\u770b\u6765\u6cbb\u5b89\u90e8\u95e8\u6709\u65b0\u6d88\u606f\u6765\u4e86\u3002",
    "\uff08\u6625\u7530\u62ff\u8d77\u590f\u514b\u91cc\u9012\u7ed9\u5979\u7684\u7ec8\u7aef\u4e00\u770b\uff0c\u8138\u8272\u9661\u7136\u4e00\u53d8\u3002\uff09",
    "\u6307\u6325\u5b98\uff0c\u6cbb\u5b89\u90e8\u95e8\u6765\u6d88\u606f\uff0c\u4ed6\u4eec\u53d1\u73b0\u57c3\u4e1d\u7279\u5728\u4e00\u95f4\u5e9f\u5f03\u4ed3\u5e93\u4e2d\uff0c\u4ed3\u5e93\u4f4d\u7f6e\u5df2\u7ecf\u53d1\u9001\u8fc7\u6765\u4e86\u3002|\u6cbb\u5b89\u90e8\u95e8\u8bf4\u516d\u5c0f\u65f6\u540e\u4ed6\u4eec\u5c31\u8981\u53d1\u8d77\u6293\u6355\uff0c\u544a\u8bc9\u6211\u4eec\u6293\u7d27\u65f6\u95f4\u3002",
    "\u9edb\u70df\uff0c\u4f60\u8fd8\u8bb0\u5f97\u90a3\u4e2a\u4eba\u7684\u8138\u4e48\uff1f",
    "\u975e\u5e38\u6e05\u695a\u3002",
    "\u597d\uff0c\u9edb\u70df\u548c\u6211\u4e00\u540c\u8fdb\u5165\u5e9f\u5f03\u4ed3\u5e93\u63a7\u5236\u57c3\u4e1d\u7279\uff0c\u7edb\u96e8\u548c\u671d\u6656\u5728\u4ed3\u5e93\u5916\u56f4\u8b66\u6212\uff0c\u63d0\u9632\u5409\u62c9\u5c14\u4f59\u515a\u8fc7\u6765\u88ad\u51fb\u3002",
    "\u6625\u7530\uff0c\u4f60\u548c\u590f\u514b\u91cc\u4e3a\u6211\u4eec\u63d0\u4f9b\u884c\u52a8\u652f\u6301\uff0c\u4fdd\u6301\u5f85\u547d\u3002|\u53ea\u8981\u6293\u4f4f\u4e86\u57c3\u4e1d\u7279\uff0c\u540e\u9762\u5c31\u597d\u8bf4\u4e86\u3002",
    "\u662f\uff0c\u6307\u6325\u5b98\u3002|\u5404\u4f4d\uff0c\u6211\u4eec\u51c6\u5907\u8d77\u6765\u5427\u3002"
  ]
]