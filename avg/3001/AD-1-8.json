[
  [
    "\uff08\u7edb\u96e8\u7ec8\u4e8e\u8d39\u52b2\u5730\u593a\u53d6\u4e86\u4f0a\u4f2f\u7684\u63a7\u5236\u7cfb\u7edf\u3002\uff09",
    "\u7d2f\u2026\u2026\u7d2f\u6b7b\u6211\u4e86\u2026\u2026\u5165\u4fb5\u5b8c\u6210\uff0c\u6211\u5df2\u53d6\u5f97\u4f0a\u4f2f\u63a7\u5236\u6743\u3002",
    "\u671d\u6656\u4f60\u770b\u5230\u6ca1\u770b\u5230\u6ca1\uff0c\u6211\u521a\u624d\u7684\u52a8\u4f5c\u662f\u4e0d\u662f\u8d85\u7ea7\u5e05\uff01",
    "\u5e0c\u671b\u89c2\u4f17\u4eec\u4f1a\u89c9\u5f97\u90a3\u5c31\u662f\u201c\u70b9\u775b\u4eea\u5f0f\u201d\uff0c\u4e0d\u8981\u53d1\u73b0\u5f02\u5e38\u624d\u597d\u3002",
    "\u76f8\u6bd4\u5c31\u62ff\u6bdb\u7b14\u70b9\u4e24\u4e0b\uff0c\u8fd8\u662f\u590f\u514b\u91cc\u5728\u821e\u53f0\u4e0a\u51fa\u8272\u7684\u63a7\u573a\u624d\u662f\u6700\u68d2\u7684\u8868\u73b0\u5462~",
    "\u54c8\uff1f\u4f60\u5728\u8bf4\u5565\u5462\uff1f\u4f60\u4e5f\u4e0d\u60f3\u60f3\u9760\u8c01\u624d\u628a\u8fd9\u4e2a\u5927\u5bb6\u4f19\u7ed9\u641e\u5b9a\u7684\uff01",
    "\uff08\u4e3a\u4e86\u8bc1\u660e\u81ea\u5df1\u529f\u52b3\uff0c\u7edb\u96e8\u7528\u529b\u5730\u62cd\u4e86\u62cd\u80ef\u4e0b\u5df2\u7ecf\u505c\u6ede\u4e0d\u52a8\u7684\u4f0a\u4f2f\u3002\uff09",
    "\uff08\u5c31\u50cf\u56de\u5e94\u7edb\u96e8\u4e00\u822c\uff0c\u6709\u4e86\u773c\u775b\u7684\u4f0a\u4f2f\u53c8\u7a81\u7136\u52a8\u5f39\u4e86\u8d77\u6765\u3002\uff09",
    "\u600e\u600e\u600e\u600e\u4e48\u4e86\uff01",
    "\u600e\u4e48\u4f0a\u4f2f\u53c8\u5f00\u59cb\u79fb\u52a8\u4e86\uff1f",
    "\u8ba8\u538c\uff01\u53c8\u6709\u4eba\u5f00\u59cb\u4e89\u593a\u8fdc\u7a0b\u63a7\u5236\u6743\u4e86\uff01",
    "\u8bf6\uff1f\u201c\u70b9\u775b\u201d\u4eea\u5f0f\u53ef\u6ca1\u6709\u7b2c\u4e8c\u6b21\u4e86\u554a\uff1f",
    "\u6211\u77e5\u9053\uff01\u6211\u8ba9\u8fd9\u4e2a\u5927\u5bb6\u4f19\u56de\u5230\u539f\u672c\u7684\u653e\u7f6e\u70b9\uff0c\u6211\u4eec\u628a\u5b83\u5f7b\u5e95\u51fb\u6bc1\u4e00\u52b3\u6c38\u9038\u89e3\u51b3\u95ee\u9898\u5427\uff01",
    "\u867d\u7136\u8fd9\u4e2a\u529e\u6cd5\u663e\u5f97\u5934\u8111\u5f88\u7b80\u5355\uff0c\u4e0d\u8fc7\u6211\u5e76\u4e0d\u8ba8\u538c\u3002\u4f60\u8fd8\u80fd\u63a7\u5236\u5b83\u7684\u65b9\u5411\u5417\uff1f",
    "\u6211\u5728\u52aa\u529b\uff01",
    "\uff08\u7edb\u96e8\u8d39\u52b2\u5168\u529b\u7ec8\u4e8e\u8ba9\u4f0a\u4f2f\u5728\u539f\u5730\u8c03\u4e86\u4e00\u4e2a\u5934\uff0c\u5b83\u5f00\u59cb\u671d\u821e\u53f0\u540e\u9762\u7684\u65b9\u5411\u79fb\u52a8\u4e86\u8d77\u6765\u3002\uff09",
    "\u51fb\u6bc1\u5df2\u7ecf\u5b8c\u6210\u201c\u70b9\u775b\u201d\u7684\u4f0a\u4f2f\uff0c\u4f1a\u4e0d\u4f1a\u6709\u70b9\u4e0d\u5409\u5229\u554a\uff1f",
    "\u90fd\u5230\u8fd9\u4e2a\u65f6\u5019\u4e86\u8fd8\u8bf4\u4ec0\u4e48\u5409\u4e0d\u5409\u5229\u7684\u8bdd\u554a\uff1f\u4e3a\u4e86\u5b8c\u6210\u957f\u5b98\u7684\u4efb\u52a1\uff0c\u5c31\u8ba9\u5b83\u5b89\u5fc3\u98de\u4e0a\u5929\u5427\uff01",
    "\u55ef\uff0c\u6211\u4e5f\u89c9\u5f97\u51fb\u6bc1\u4e86\u6bd4\u8f83\u597d\uff0c\u53cd\u6b63\u70b8\u836f\u5df2\u7ecf\u90fd\u62c6\u9664\uff0c\u4e0d\u4f1a\u6784\u6210\u4ec0\u4e48\u5371\u9669\u3002",
    "\u55ef\u2026\u2026\u597d\u5427~\u53cd\u6b63\u548c\u590f\u514b\u91cc\u4e5f\u6ca1\u4ec0\u4e48\u5173\u7cfb\uff0c\u90a3\u6211\u4eec\u5c31\u8d76\u7d27\u52a8\u624b\u5427~"
  ]
]