[
  [
    "\u2026\u2026\u80e1\u841d\u535c\u3001\u571f\u8c46\u3001\u8611\u83c7\u3001\u751c\u83dc\uff0c\u8fd8\u6709\u2026\u2026",
    "\u8fd8\u6709\u9a6c\u94c3\u85af\u82b1\uff0c\u5728\u8fd9\u91cc\u3002",
    "\uff08\u56de\u5230\u9910\u9986\u540e\uff0c\u7edb\u96e8\u8ddf\u7740\u671d\u6656\u53bb\u7b79\u5907\u5f53\u665a\u7684\u6f14\u51fa\uff0c\u9edb\u70df\u5219\u5728\u540e\u53a8\u4e0e\u8001\u677f\u6838\u5bf9\u91c7\u8d2d\u6e05\u5355\u3002\uff09",
    "\u770b\u5230\u4e86\u3002\u55ef\uff0c\u6e05\u5355\u6ca1\u95ee\u9898\uff0c\u4f46\u613f\u8fd9\u4e9b\u98df\u6750\u8db3\u591f\u4e86\u2026\u2026|\u867d\u7136\u90fd\u662f\u4e9b\u6ca1\u4ec0\u4e48\u53e3\u611f\u7684\u901f\u6210\u98df\u54c1\uff0c\u4f46\u80fd\u4e70\u5230\u5df2\u7ecf\u5f88\u4e0d\u9519\u4e86\u3002",
    "\u98df\u6750\u91cf\u51e0\u4e4e\u662f\u5e73\u65f6\u7684\u4e09\u500d\u5462\uff0c\u4eca\u665a\u7684\u5ba2\u4eba\u6709\u8fd9\u4e48\u591a\u5417\uff1f",
    "\u672c\u6765\u6ca1\u6709\uff0c\u4f46\u4e0b\u5348\u63a5\u4e86\u4e00\u4e2a\u5de5\u7a0b\u961f\u7684\u5305\u573a\u9884\u5b9a\uff0c\u8bf4\u662f\u4e3a\u4e86\u5e86\u795d\u5e86\u5178\u4e3b\u821e\u53f0\u7ae3\u5de5\u3002",
    "\u6211\u8fd9\u5c0f\u9910\u9986\u7684\u5e38\u89c4\u50a8\u5907\u53ef\u5582\u4e0d\u9971\u51e0\u5341\u4e2a\u9965\u80a0\u8f98\u8f98\u7684\u5efa\u7b51\u5de5\u4eba\uff0c\u800c\u4e14\u4ed6\u4eec\u8fd8\u7279\u522b\u8981\u6c42\u996e\u54c1\u548c\u751c\u70b9\u90fd\u7528\u9a6c\u94c3\u85af\u82b1\u5236\u4f5c\uff0c\u8fd9\u4e0d\uff0c\u624d\u8ba9\u671d\u6656\u4e5f\u51fa\u53bb\u91c7\u8d2d\u4e86\u3002",
    "\u8fd9\u4e48\u591a\u4eba\uff0c\u8fd9\u91cc\u5750\u5f97\u4e0b\u5417\uff1f",
    "\u6e05\u4e00\u6e05\u573a\uff0c\u8c03\u6574\u4e00\u4e0b\u684c\u6905\u4f4d\u7f6e\u5e94\u8be5\u53ef\u4ee5\u2026\u2026\u54ce\uff0c\u8fd9\u4e9b\u5c31\u4e0d\u7528\u4f60\u64cd\u5fc3\u4e86\uff0c\u4f60\u4eec\u7ba1\u597d\u6f14\u51fa\u5c31\u53ef\u4ee5\u4e86\u3002",
    "\u8bdd\u8bf4\uff0c\u4f60\u4eec\u4f1a\u4e0d\u4f1a\u90a3\u79cd\uff0c\u5c31\u662f\u80fd\u4fc3\u8fdb\u98df\u6b32\u7684\u66f2\u5b50\u554a\uff1f",
    "\u55ef\uff1f\u554a\uff0c\u8fd9\u2026\u2026\u6211\u60f3\u5e94\u8be5\u6ca1\u6709\u8fd9\u79cd\u66f2\u5b50\u5427\uff1f",
    "\u90a3\u5c31\u6765\u51e0\u9996\u6b22\u5feb\u70b9\u7684\uff0c\u7092\u7092\u6c14\u6c1b\uff0c\u5ba2\u4eba\u5f00\u5fc3\u4e86\u591a\u5403\u51e0\u7897\uff0c\u6211\u4eec\u4e5f\u80fd\u591a\u8d5a\u70b9\u94b1\u3002",
    "\u6211\u2026\u2026\u6211\u5c3d\u91cf\u3002",
    "\u6211\u8981\u53bb\u63a5\u5f85\u5ba2\u4eba\u4e86\uff0c\u4f60\u4eec\u51c6\u5907\u4e00\u4e0b\u5427\uff0c\u554a\u3002",
    "\uff08\u79bb\u5f00\u540e\u53a8\uff0c\u9edb\u70df\u8d70\u4e0a\u821e\u53f0\u3002\uff09",
    "\uff08\u671d\u6656\u6b63\u5728\u8c03\u8bd5\u97f3\u54cd\uff0c\u7edb\u96e8\u5219\u624b\u5fd9\u811a\u4e71\u5730\u6298\u817e\u5979\u7684\u9f13\uff0c\u8033\u6735\u5374\u652f\u8d77\u6765\uff0c\u542c\u7740\u9edb\u70df\u8fd9\u8fb9\u7684\u52a8\u9759\u3002\uff09",
    "\uff08\u89c1\u9edb\u70df\u6765\u4e86\uff0c\u7edb\u96e8\u5c0f\u8dd1\u4e24\u6b65\uff0c\u7b11\u563b\u563b\u5730\u6402\u4f4f\u5979\u7684\u80f3\u818a\u3002\uff09",
    "\u563b\u563b\uff0c\u4fc3\u8fdb\u98df\u6b32\u7684\u66f2\u5b50\uff0c\u6211\u77e5\u9053\u5440\uff01",
    "\u60f3\u5403\u60f3\u5403\u60f3\u5403\uff0c\u6211\u771f\u7684\u60f3\u5403~\u660e\u660e\u4e94\u5206\u949f\u524d\uff0c\u5403\u4e86\u4e09\u7897\u996d\u2026\u2026",
    "\uff08\u9edb\u70df\u6233\u6233\u7edb\u96e8\u7684\u989d\u5934\u3002\uff09",
    "\u522b\u95f9\uff0c\u4ec0\u4e48\u5947\u5947\u602a\u602a\u7684\u6b4c\u66f2\u2026\u2026\u4f60\u4eec\u4e8b\u60c5\u505a\u5b8c\u4e86\uff1f",
    "\u4e50\u5668\u3001\u97f3\u54cd\u3001\u9ea6\u514b\u98ce\u67b6\u8bbe\u5b8c\u6bd5\uff0c\u706f\u5149\u4e5f\u6253\u597d\u4e86\uff0c\u5c31\u7b49\u8001\u59d0\u4f60\u5566\uff01|\u8fd8\u6709\u8fd8\u6709\uff0c\u4f60\u770b\u6211\u628a\u9f13\u67b6\u5728\u8fd9\u91cc\u600e\u4e48\u6837\uff1f\u4e07\u4e00\u7b49\u4f1a\u6709\u54ea\u91cc\u6572\u9519\u4e86\uff0c\u4f60\u4e5f\u80fd\u7ed9\u6211\u4f7f\u4e2a\u773c\u8272\u4ec0\u4e48\u7684\u2026\u2026",
    "\u54e6\u2014\u2014\u6211\u8bb0\u5f97\uff0c\u6211\u7684\u59b9\u59b9\u662f\u5929\u4e0d\u6015\u5730\u4e0d\u6015\u7684\u683c\u91cc\u82ac\u7cbe\u82f1\u4eba\u5f62\u6765\u7740\uff0c\u600e\u4e48\uff0c\u73b0\u5728\uff0c\u5c45\u7136\u53d8\u5f97\u7d27\u5f20\u8d77\u6765\u5566\uff1f",
    "\u662f\u524d\u2022\u683c\u91cc\u82ac\u7cbe\u82f1\u4eba\u5f62\u5566\uff01\u8001\u59d0\uff0c\u8fd9\u79cd\u65f6\u5019\u4f60\u5c31\u4e0d\u8981\u53d6\u7b11\u6211\u4e86\uff0c\u771f\u662f\u7684\u2026\u2026",
    "\u4eca\u5929\u665a\u4e0a\u6765\u4e86\u53ef\u591a\u4eba\u4e86\u5462\uff01\u4e0d\u8fc7\u2026\u2026\u611f\u89c9\u4ed6\u4eec\u6709\u70b9\u5947\u602a\uff1f\u641e\u5f97\u6211\u4e5f\u6709\u70b9\u7d27\u5f20\u4e86\u2026\u2026",
    "\u5947\u602a\uff1f|\u6b38\uff1f\u5ba2\u4eba\u90fd\u843d\u5ea7\u4e86\u5417\uff1f\u6211\u5b8c\u5168\u6ca1\u6ce8\u610f\u5230\u2026\u2026",
    "\u5bf9\u554a\u5bf9\u554a\uff0c\u6240\u4ee5\u6211\u8bf4\u5f88\u5947\u602a\u561b\u2014\u2014\u4e00\u7fa4\u4eba\uff0c\u4e5f\u4e0d\u8bf4\u8bdd\uff0c\u5c31\u82e6\u7740\u8138\u5750\u5728\u90a3\u91cc\u559d\u9152\uff0c\u8ddf\u4e92\u76f8\u6b20\u4e86\u94b1\u4e00\u6837\uff0c\u5b8c\u5168\u6ca1\u6709\u805a\u9910\u7684\u6c1b\u56f4\u561b\uff01",
    "\uff08\u542c\u7740\u7edb\u96e8\u7684\u62b1\u6028\uff0c\u9edb\u70df\u770b\u5411\u53f0\u4e0b\u7684\u89c2\u4f17\u3002\uff09",
    "\uff08\u548c\u7edb\u96e8\u63cf\u8ff0\u7684\u4e00\u6837\uff0c\u53f0\u4e0b\u5b8c\u5168\u6ca1\u6709\u805a\u9910\u7684\u6c1b\u56f4\u3002\uff09",
    "\uff08\u4e4b\u524d\u51e0\u4eba\u9047\u5230\u8fc7\u7684\u5de5\u4eba\u4e5f\u5728\u4eba\u7fa4\u4e2d\uff0c\u4f46\u6ca1\u6709\u4eba\u8bf4\u8bdd\uff0c\u6c14\u6c1b\u51dd\u91cd\u3002\uff09",
    "\uff08\u4e00\u4fa7\u7684\u89d2\u843d\u91cc\u8fd8\u6709\u4e2a\u4eba\uff0c\u684c\u4e0a\u653e\u7740\u4e00\u4e2a\u7eb8\u8d28\u7b14\u8bb0\u672c\uff0c\u4f4d\u7f6e\u4e5f\u8fdc\u79bb\u5176\u4ed6\u4eba\uff0c\u56e0\u800c\u663e\u5f97\u6709\u4e9b\u7a81\u5140\u3002\uff09",
    "\u4eca\u5929\u7684\u6c1b\u56f4\u786e\u5b9e\u6709\u70b9\u5947\u602a\uff0c\u8001\u677f\u8bf4\u662f\u5e86\u529f\u5bb4\uff0c\u53ef\u5ba2\u4eba\u4eec\u7684\u8868\u60c5\u5374\u90fd\u5f88\u4e25\u8083\u2026\u2026",
    "\u2026\u2026",
    "\u597d\u5566\uff0c\u6f14\u51fa\u5feb\u5f00\u59cb\u4e86\u3002",
    "\u7edb\u96e8\uff0c\u671d\u6656\uff0c\u63a5\u4e0b\u6765\u8981\u6f14\u594f\u7684\u66f2\u76ee\u2026\u2026\u6211\u60f3\u66f4\u6539\u4e3a\u4e4b\u524d\u4f9d\u7167\u539f\u66f2\u6539\u7f16\u7684\u90a3\u9996\u5c0f\u8c03\uff0c\u53ef\u4ee5\u5417\uff1f",
    "\u51c0\u5316\u533a\u90a3\u4e2a\u5417\uff1f",
    "\u5bf9\u3002",
    "\uff08\u5f88\u5feb\uff0c\u9910\u5385\u7684\u706f\u5149\u6697\u4e86\u4e0b\u6765\uff0c\u6f14\u594f\u5373\u5c06\u5f00\u59cb\u3002\uff09",
    "\uff08\u9edb\u70df\u7aef\u5750\u5728\u53e4\u7b5d\u524d\uff0c\u5168\u795e\u8d2f\u6ce8\u5730\u62e8\u5f26\uff0c\u5f88\u5feb\u5c31\u6c89\u6d78\u5728\u4e50\u66f2\u4e2d\u3002\uff09",
    "\uff08\u5728\u97f3\u4e50\u7684\u5b89\u629a\u4e0b\uff0c\u7edb\u96e8\u4e5f\u6e10\u6e10\u4e0d\u90a3\u4e48\u7d27\u5f20\uff0c\u5c0f\u5fc3\u7ffc\u7ffc\u5730\u5728\u4e50\u66f2\u4e2d\u52a0\u5165\u4e86\u9f13\u70b9\u3002\uff09",
    "\uff08\u4e00\u5207\u770b\u8d77\u6765\u90fd\u5f88\u6b63\u5e38\u2014\u2014\uff09",
    "\uff08\u6ca1\u60f3\u5230\uff0c\u8fd8\u672a\u6f14\u594f\u5b8c\u6bd5\uff0c\u53f0\u4e0b\u5374\u7a81\u7136\u9a9a\u52a8\u8d77\u6765\u3002\uff09",
    "\u5404\u4f4d\u5ba2\u4eba\uff0c\u6709\u8bdd\u597d\u597d\u8bf4\uff0c\u54b1\u4eec\u90fd\u5750\u4e0b\u6765\uff0c\u5750\u4e0b\u6765\u2026\u2026",
    "\u6b38\u6b38\u6b38\u90a3\u4f4d\u5ba2\u4eba\uff0c\u4e0d\u8981\u6fc0\u52a8\uff0c\u4e0d\u8981\u6454\u7897\uff01",
    "\u8c01\u662f\u5899\u5934\u8349\uff01\u6211\u662f\u88ab\u4f60\u4eec\u9a97\u8fdb\u6765\u7684\u597d\u5427\uff01",
    "\u5435\u4ec0\u4e48\u5435\uff01\u8981\u6211\u8bf4\uff0c\u4e00\u5f00\u59cb\u5c31\u4e0d\u8be5\u641e\u8fd9\u79cd\u4e8b\uff01",
    "\u54c8\uff1f\u5230\u73b0\u5728\u8fd8\u60f3\u4e34\u9635\u8131\u9003\uff0c\u4f60\u4ee5\u4e3a\u5c31\u4f60\u4e00\u4e2a\u4eba\u5192\u98ce\u9669\u5417\uff1f",
    "\u6211\u53ea\u662f\u4e0d\u60f3\u8ba9\u5927\u5bb6\u53bb\u9001\u6b7b\uff01",
    "\u95ed\u5634\uff01\u558a\u8fd9\u4e48\u5927\u58f0\u662f\u8981\u8ba9\u5168\u4e16\u754c\u90fd\u77e5\u9053\u554a\uff1f",
    "\u57c3\u4e1d\u7279\u90fd\u5df2\u7ecf\u51b3\u5b9a\u4e86\uff0c\u5979\u90fd\u8fd8\u6ca1\u53d1\u8bdd\u4f60\u4eec\u95f9\u4e2a\u4ec0\u4e48\u52b2\u554a\uff01",
    "\u5979\u61c2\u4e2a\u5c41\uff0c\u5c31\u662f\u5979\u628a\u6211\u4eec\u5377\u8fdb\u6765\u7684\uff01",
    "\u54ce\uff0c\u54ce\uff0c\u8fd9\u90fd\u662f\u4ec0\u4e48\u4e8b\u554a\uff1f|\u5404\u4f4d\uff0c\u5404\u4f4d\u51b7\u9759\u554a\u2014\u2014\u4e0d\u8981\u7838\u6211\u5e97\u91cc\u7684\u4e1c\u897f\u554a\uff01",
    "\uff08\u4e00\u65f6\u95f4\uff0c\u6454\u676f\u5b50\u3001\u6454\u6905\u5b50\u7684\u58f0\u97f3\u4e0d\u7edd\u4e8e\u8033\uff0c\u751a\u81f3\u8fd8\u9690\u7ea6\u4f20\u6765\u4e86\u54ed\u58f0\u3002\uff09",
    "\uff08\u9edb\u70df\u770b\u7740\u821e\u53f0\u4e0b\u53d1\u751f\u7684\u4e00\u5207\u6709\u4e9b\u7126\u6025\uff0c\u7136\u800c\u6ca1\u4eba\u8ba9\u5979\u505c\u6b62\u6f14\u594f\uff0c\u5979\u53ea\u80fd\u4e00\u8fb9\u5c0f\u5fc3\u7ffc\u7ffc\u5730\u89c2\u5bdf\u7740\u4eba\u7fa4\u7684\u884c\u4e3a\uff0c\u4e00\u8fb9\u7ee7\u7eed\u5c06\u66f2\u5b50\u6f14\u594f\u4e0b\u53bb\u3002\uff09",
    "\uff08\u5f53\u9edb\u70df\u5f39\u51fa\u67d0\u4e2a\u65cb\u5f8b\u65f6\uff0c\u5750\u5728\u89d2\u843d\u7684\u90a3\u4e2a\u4eba\u731b\u7136\u62ac\u5934\u3002\uff09",
    "\u8fd9\u662f\u2026\u2026\uff01\u5979\u600e\u4e48\u4f1a\u77e5\u9053\u8fd9\u9996\u66f2\u5b50\uff1f",
    "\u4f60\u4eec\u51e0\u4e2a\u5728\u90a3\u513f\u53d1\u4ec0\u4e48\u5446\u5462\uff1f\u770b\u770b\u5e97\u91cc\u90fd\u4e71\u6210\u4ec0\u4e48\u6837\u4e86\uff1f",
    "\u671d\u6656\u2014\u2014\u671d\u6656\uff01\u8fd8\u6709\u9edb\u70df\uff01\u6123\u7740\u5e72\u4ec0\u4e48\uff01\u4f60\u4eec\u51e0\u4e2a\u4e0d\u662f\u4eba\u5f62\u5417\uff1f\u5feb\u6765\u5e2e\u6211\u628a\u4eba\u62c9\u5f00\uff01\u5feb\u70b9\u5feb\u70b9\uff01",
    "\u54e6\uff0c\u54e6\u54e6\uff01",
    "\uff08\u671d\u6656\u548c\u7edb\u96e8\u8df3\u4e0b\u821e\u53f0\uff0c\u963b\u6b62\u5df2\u7ecf\u5f00\u59cb\u52a8\u624b\u7684\u987e\u5ba2\u3002\uff09",
    "\uff08\u9edb\u70df\u624b\u5fd9\u811a\u4e71\u5730\u60f3\u8981\u628a\u53e4\u7b5d\u642c\u4e0b\u821e\u53f0\uff0c\u6b63\u5f53\u5979\u51c6\u5907\u5148\u7528\u5e03\u76d6\u4f4f\u53e4\u7b5d\u65f6\uff0c\u4e00\u53ea\u624b\u91cd\u91cd\u5730\u6309\u5728\u4e86\u53e4\u7b5d\u4e0a\u3002\uff09",
    "\u521a\u624d\u90a3\u9996\u66f2\u5b50\u2026\u2026\u4f60\u662f\u6765\u76d1\u89c6\u6211\u4eec\u7684\u5417\uff01\uff1f",
    "\u4ec0\u2026\u2026\u4e48\uff1f",
    "\uff08\u7a81\u7136\u51fa\u73b0\u7684\u5973\u5b50\u6234\u7740\u5de5\u724c\uff0c\u5de5\u724c\u4e0a\u5199\u7740\u5979\u7684\u540d\u5b57\uff0c\u201c\u57c3\u4e1d\u7279\u201d\u3002\uff09",
    "\uff08\u60c5\u51b5\u53d8\u5316\u592a\u5feb\uff0c\u9edb\u70df\u4e00\u65f6\u4e0d\u77e5\u5982\u4f55\u56de\u7b54\u3002\uff09",
    "\uff08\u770b\u9edb\u70df\u6c89\u9ed8\uff0c\u8fd9\u4f4d\u540d\u53eb\u201c\u57c3\u4e1d\u7279\u201d\u7684\u5973\u5b50\u7126\u6025\u5730\u5de6\u53f3\u5f20\u671b\uff0c\u597d\u50cf\u662f\u62c5\u5fc3\u81ea\u5df1\u7684\u884c\u52a8\u88ab\u4eba\u53d1\u73b0\u3002\uff09",
    "\u8be5\u6b7b\u7684\u2026\u2026\u4f60\u4eec\u8981\u5f7b\u5e95\u903c\u6b7b\u6211\u5417\uff01",
    "\u4f60\u4eec\u4f1a\u540e\u6094\u7684\uff01",
    "\uff08\u8868\u73b0\u602a\u5f02\u7684\u5973\u5b50\u5bf9\u7740\u4e0d\u660e\u6240\u4ee5\u7684\u9edb\u70df\u653e\u4e86\u53e5\u72e0\u8bdd\uff0c\u4e4b\u540e\u8f6c\u5934\u5c31\u8d70\u3002\uff09",
    "\uff08\u7ed3\u5408\u65b9\u624d\u6240\u89c1\uff0c\u9edb\u70df\u654f\u9510\u5730\u5bdf\u89c9\u5230\u8fd9\u4ef6\u4e8b\u80cc\u540e\u53ef\u80fd\u85cf\u7740\u4ec0\u4e48\u9634\u8c0b\u3002\uff09",
    "\uff08\u2026\u2026\u5979\u6709\u4e9b\u72b9\u8c6b\u3002\uff09",
    "\uff08\u57fa\u4e8e\u5979\u76ee\u524d\u7684\u8eab\u4efd\uff0c\u5979\u5e76\u4e0d\u9700\u8981\u5bf9\u6b64\u7c7b\u4e8b\u4ef6\u8d1f\u8d23\uff0c\u4f46\u2026\u2026\uff09",
    "\uff08\u9edb\u70df\u5f88\u5feb\u5c31\u505a\u51fa\u4e86\u51b3\u5b9a\u3002\u5979\u7ad9\u8d77\u8eab\uff0c\u5e76\u540c\u65f6\u63a5\u5165\u4e86\u4e8c\u7ea7\u5e73\u5c42\u3002\uff09",
    "\u671d\u6656\uff0c\u7edb\u96e8\uff0c\u4f60\u4eec\u7559\u5728\u9910\u5385\u534f\u52a9\u8001\u677f\u5904\u7406\u73b0\u573a\u3002\u53e4\u7b5d\u5728\u821e\u53f0\u4e0a\uff0c\u5e2e\u6211\u5173\u6ce8\u4e00\u4e0b\u3002",
    "\u6709\u540d\u5973\u58eb\u8868\u73b0\u5f88\u5947\u602a\uff0c\u6211\u6000\u7591\u5979\u548c\u8fd9\u6b21\u9a9a\u4e71\u76f8\u5173\uff0c\u6211\u8ddf\u51fa\u53bb\u770b\u770b\u60c5\u51b5\u3002",
    "\u8fd9\u4e48\u7a81\u7136\uff1f",
    "\u55ef\u3002\u6709\u95ee\u9898\u968f\u65f6\u8054\u7edc\u3002",
    "\u554a\uff1f\u671d\u6656\u4f60\u2014\u2014\uff1f\u884c\u5427\uff0c\u90a3\u6709\u4e8b\u53eb\u6211\uff0c\u5148\u53eb\u6211\uff01",
    "\u55ef\uff0c\u8fd9\u91cc\u5c31\u62dc\u6258\u4f60\u4eec\u4e86\u3002",
    "\uff08\u79bb\u5f00\u9910\u5385\uff0c\u9edb\u70df\u7d27\u7d27\u8ddf\u968f\u5973\u5b50\u884c\u52a8\u3002\uff09",
    "\uff08\u5973\u5b50\u53d1\u73b0\u4e86\u8eab\u540e\u6709\u4eba\u8ddf\u8e2a\uff0c\u987f\u65f6\u52a0\u5feb\u4e86\u811a\u6b65\uff0c\u60f3\u501f\u7740\u66f2\u6298\u7684\u5df7\u5b50\u7529\u5f00\u9edb\u70df\u3002\uff09",
    "\uff08\u9edb\u70df\u4e1d\u6beb\u4e0d\u7ed9\u5973\u5b50\u9003\u79bb\u7684\u673a\u4f1a\uff0c\u76f4\u63a5\u5c06\u5979\u903c\u8fdb\u4e00\u5904\u6b7b\u80e1\u540c\u4e2d\uff0c\u5973\u5b50\u4e0d\u5f97\u4e0d\u505c\u4e0b\u6765\u76f4\u9762\u5979\u3002\uff09",
    "\uff08\u5973\u5b50\u638f\u51fa\u6000\u4e2d\u85cf\u7740\u7684\u4e00\u628a\u5c0f\u5200\uff0c\u505a\u5a01\u80c1\u72b6\u3002\uff09",
    "\u4f60\u5230\u5e95\u8981\u505a\u4ec0\u4e48\uff1f\uff01",
    "\u6211\u5e76\u6ca1\u6709\u6076\u610f\uff0c\u8fd9\u4f4d\u5973\u58eb\u3002",
    "\u60a8\u6700\u597d\u5148\u5c06\u5200\u6536\u8d77\u6765\uff0c\u4ee5\u514d\u8bef\u4f24\u5230\u81ea\u5df1\u3002",
    "\u679c\u7136\u4f60\u662f\u90a3\u8fb9\u6d3e\u6765\u76d1\u89c6\u6211\u7684\u5bb6\u4f19\u5417\uff01",
    "\u522b\u518d\u903c\u6211\u4e86\uff0c\u6211\u7b54\u5e94\u4f60\u4eec\u7684\u6211\u81ea\u4f1a\u505a\u5230\uff01",
    "\u4e4b\u540e\uff0c\u6211\u4eec\u518d\u4e5f\u4e0d\u76f8\u6b20\uff01",
    "\u2026\u2026\u6211\u4e0d\u660e\u767d\u4f60\u5728\u8bf4\u4ec0\u4e48\uff1f",
    "\u6eda\u5f00\uff01",
    "\uff08\u9edb\u70df\u6b63\u5728\u601d\u8003\u5982\u4f55\u8bf4\u670d\u773c\u524d\u8fd9\u4f4d\u66b4\u8e81\u7684\u5973\u5b50\uff0c\u8ba9\u5979\u5148\u51b7\u9759\u4e0b\u6765\uff0c\u8c01\u77e5\u5bf9\u65b9\u5df2\u7ecf\u5f00\u59cb\u4e3b\u52a8\u53d1\u96be\u3002\uff09",
    "\uff08\u5979\u63e1\u7d27\u5200\u67c4\uff0c\u5411\u7740\u9edb\u70df\u76f4\u63a5\u51b2\u649e\u8fc7\u6765\u3002\uff09",
    "\uff08\u9edb\u70df\u9762\u5bf9\u8fd9\u79cd\u7a0b\u5ea6\u7684\u5a01\u80c1\u4e1d\u6beb\u4e0d\u60e7\uff0c\u5979\u8f7b\u677e\u64a4\u6b65\uff0c\u95ea\u8eab\u5230\u4e86\u5973\u5b50\u8eab\u4fa7\uff0c\u987a\u52bf\u6293\u4f4f\u4e86\u5973\u5b50\u6301\u5200\u7684\u53f3\u624b\u3002\uff09",
    "\uff08\u5973\u5b50\u5403\u75db\uff0c\u624b\u4e2d\u7684\u5c0f\u5200\u5e94\u58f0\u843d\u5730\uff0c\u9edb\u70df\u5c06\u5979\u53cd\u624b\u64d2\u4f4f\u6309\u5728\u5899\u4e0a\uff0c\u5973\u5b50\u8bd5\u56fe\u53d1\u529b\u6323\u624e\u4f46\u53d1\u73b0\u548c\u4eba\u5f62\u7684\u529b\u91cf\u76f8\u6bd4\uff0c\u81ea\u5df1\u662f\u5982\u6b64\u7684\u5fae\u4e0d\u8db3\u9053\u3002\uff09",
    "\u8be5\u6b7b\uff01\u4f60\u4eec\u8fd9\u5e2e\u6df7\u86cb\uff01\u6211\u4ece\u4e00\u5f00\u59cb\u5c31\u77e5\u9053\u4f60\u4eec\u4e0d\u6000\u597d\u5fc3\uff01\u6709\u672c\u4e8b\u73b0\u5728\u5c31\u6740\u4e86\u6211\u5427\uff01",
    "\u6211\u53ea\u662f\u60f3\u6765\u4e86\u89e3\u4e0b\u60c5\u51b5\uff0c\u60a8\u7684\u8868\u73b0\u5f88\u4e0d\u6b63\u5e38\uff0c\u662f\u6709\u4ec0\u4e48\u96be\u8a00\u4e4b\u9690\u5417\uff1f",
    "\u4f60\u90fd\u5f39\u594f\u90a3\u9996\u66f2\u5b50\u4e86\uff0c\u8fd8\u8bf4\u4e0d\u662f\u4ed6\u4eec\u7684\u4eba\uff01",
    "\u4ed6\u4eec\uff1f\u4ed6\u4eec\u662f\u6307\u8c01\uff1f",
    "\u57c3\u4e1d\u7279\u5c0f\u59d0\uff0c\u968f\u4fbf\u4e71\u8bf4\u8bdd\u53ef\u4e0d\u662f\u4ec0\u4e48\u597d\u4e60\u60ef\u554a\u3002",
    "\uff08\u9edb\u70df\u80cc\u540e\u4f20\u6765\u4e86\u9634\u6c89\u7684\u8bdd\u97f3\uff0c\u8fd8\u6ca1\u7b49\u5979\u56de\u5934\uff0c\u968f\u7740\u51e0\u58f0\u4f4e\u95f7\u7684\u67aa\u54cd\uff0c\u9edb\u70df\u7684\u7d20\u4f53\u88ab\u5b50\u5f39\u51fb\u4e2d\u3002\uff09",
    "\uff08\u9edb\u70df\u542c\u51fa\u8fd9\u662f\u5b89\u88c5\u4e86\u6d88\u97f3\u5668\u7684\u624b\u67aa\u5f00\u67aa\u65f6\u7684\u58f0\u97f3\uff0c\u5979\u7684\u5fc3\u667a\u91cc\u5145\u65a5\u7740\u5404\u6a21\u5757\u53d7\u635f\u7684\u62a5\u9519\u3002\uff09",
    "\u522b\u8fc7\u6765\uff01\uff01",
    "\uff08\u88ab\u51fb\u4e2d\u7684\u9edb\u70df\u677e\u5f00\u624b\uff0c\u6446\u8131\u4e86\u63a7\u5236\u7684\u5973\u5b50\u7acb\u5373\u5f80\u5916\u9003\u53bb\uff0c\u5728\u614c\u5fd9\u4e2d\u5979\u8eab\u4e0a\u6709\u4ec0\u4e48\u4e1c\u897f\u6389\u843d\u4e86\u4e0b\u6765\u3002\uff09",
    "\u8fd9\u662f\u2026\u2026\u5979\u7684\u7b14\u8bb0\u672c\uff1f",
    "\uff08\u610f\u8bc6\u5df2\u7ecf\u5f00\u59cb\u6a21\u7cca\uff0c\u81ea\u5df1\u7684\u8eab\u4f53\u4e5f\u5728\u65e0\u6cd5\u63a7\u5236\u4e2d\u5012\u4e0b\u3002\uff09",
    "\uff08\u7136\u800c\u7b14\u8bb0\u672c\u5c31\u50cf\u7b49\u5f85\u7740\u81ea\u5df1\u4e00\u6837\uff0c\u843d\u5728\u81ea\u5df1\u5012\u4e0b\u7684\u4f4d\u7f6e\uff0c\u9edb\u70df\u7528\u6700\u540e\u7684\u529b\u91cf\u5c06\u7b14\u8bb0\u672c\u653e\u5165\u8863\u4e2d\u3002\uff09",
    "\uff08\u9edb\u70df\u611f\u89c9\u81ea\u5df1\u7684\u89c6\u89c9\u6a21\u5757\u5df2\u7ecf\u505c\u6b62\u5de5\u4f5c\uff0c\u53ea\u542c\u5230\u4e24\u4eba\u7684\u811a\u6b65\u58f0\u4ece\u8fdc\u5904\u9010\u6e10\u63a5\u8fd1\u81ea\u5df1\u6240\u5728\u7684\u4f4d\u7f6e\u3002\uff09",
    "\u8fd9\u4e2a\u4e61\u5df4\u4f6c\uff0c\u8dd1\u5f97\u5012\u662f\u5f88\u5feb\u3002",
    "\u5979\u8dd1\u4e0d\u4e86\u591a\u8fdc\uff0c\u8981\u4e0d\u662f\u8fd9\u4e2a\u4eba\u5f62\u66ff\u5979\u6321\u4e86\u5b50\u5f39\uff0c\u5979\u521a\u624d\u5c31\u5f97\u6b7b\u2026\u2026|\u4e0d\u8fc7\uff0c\u4e3a\u4ec0\u4e48\u4f1a\u6709\u4eba\u5f62\u51fa\u73b0\uff0c\u96be\u9053\u975e\u519b\u4e8b\u529b\u91cf\u7ba1\u7406\u5c40\u4ecb\u5165\u8fd9\u91cc\u4e86\uff1f",
    "\u548c\u7ba1\u7406\u5c40\u6709\u5173\u7684\u6863\u6848\u91cc\u6ca1\u770b\u5230\u6709\u8fd9\u6837\u7684\u4eba\u5f62\uff0c\u800c\u4e14\u7a7f\u5f97\u82b1\u91cc\u80e1\u54e8\u7684\uff0c\u4e5f\u8bb8\u53ea\u662f\u4e2a\u8def\u8fc7\u7684\u3002",
    "\u4e0d\u7ba1\u600e\u4e48\u8bf4\uff0c\u4e0d\u80fd\u8ba9\u5979\u7684\u89c6\u89c9\u8bb0\u5f55\u88ab\u4fdd\u7559\u4e0b\u6765\uff0c\u628a\u5979\u7684\u5fc3\u667a\u7834\u574f\u6389\u3002",
    "\uff08\u53c8\u662f\u4e24\u58f0\u4f4e\u95f7\u7684\u67aa\u54cd\uff0c\u9edb\u70df\u611f\u89c9\u5230\u5b50\u5f39\u51fb\u4e2d\u4e86\u81ea\u5df1\u5fc3\u667a\u6a21\u5757\u7684\u4f4d\u7f6e\u3002\u7136\u800c\u610f\u8bc6\u5374\u6ca1\u6709\u6d88\u5931\uff0c\u5b50\u5f39\u5361\u5728\u4e86\u7d20\u4f53\u7684\u8868\u5c42\u3002\uff09",
    "\u5567\uff0c\u6ca1\u6253\u7a7f\u3002|\u7ed9\u6211\u70b9\u65f6\u95f4\uff0c\u5f97\u62c6\u5f00\u7834\u574f\u3002\u8fd9\u4e2a\u4eba\u5f62\u7684\u7d20\u4f53\u7b49\u7ea7\u4e0d\u4f4e\uff0c\u624b\u67aa\u90fd\u6253\u4e0d\u7a7f\u9632\u62a4\u3002",
    "\u666e\u901a\u7684\u4eba\u5f62\u600e\u4e48\u4f1a\u6709\u8fd9\u79cd\u9632\u62a4\u6c34\u5e73\uff0c\u679c\u7136\u662f\u2014\u2014",
    "\u7b49\u4e00\u4e0b\uff0c\u6709\u4eba\u8fc7\u6765\u4e86\uff0c\u6b64\u5730\u4e0d\u5b9c\u4e45\u7559\u3002",
    "\u8fd9\u4e2a\u4eba\u5f62\u600e\u4e48\u529e\uff1f",
    "\u5979\u521a\u624d\u80cc\u5bf9\u6211\u4eec\uff0c\u80af\u5b9a\u6ca1\u770b\u5230\u6211\u4eec\u7684\u8138\uff0c\u4e0d\u7ba1\u5979\u4e86\u6211\u4eec\u8d70\uff01",
    "\uff08\u4e24\u4eba\u7684\u811a\u6b65\u58f0\u6025\u4fc3\u5730\u4ece\u9edb\u70df\u8eab\u8fb9\u8fdc\u79bb\uff0c\u53c8\u8fc7\u4e86\u4e00\u4f1a\uff0c\u51e0\u4e2a\u4e0e\u4e4b\u524d\u4e0d\u540c\u7684\u811a\u6b65\u58f0\u9010\u6e10\u671d\u5979\u9760\u8fd1\u3002\uff09",
    "\u6ca1\u627e\u5230\u76ee\u6807\uff0c\u5979\u5df2\u7ecf\u63d0\u65e9\u79bb\u5f00\u4e86\u3002\u55ef\uff0c\u6211\u4eec\u9700\u8981\u60c5\u62a5\u4e0a\u7684\u652f\u6301\u3002|\u7b49\u4e00\u4e0b\uff0c\u6211\u770b\u5230\u6709\u4e00\u540d\u4eba\u5f62\u5012\u5728\u5730\u4e0a\u2014\u2014",
    "\u8fd9\u662f\u2026\u2026\u9edb\u70df\uff1f"
  ]
]