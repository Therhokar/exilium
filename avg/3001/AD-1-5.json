[
  [
    "\uff08\u6211\u4eec\u5230\u8fbe\u4e86\u5e9f\u5f03\u4ed3\u5e93\u9644\u8fd1\u3002\uff09",
    "\uff08\u8fd9\u662f\u4e00\u680b\u4e34\u8fd1\u8f66\u7ad9\u7684\u9690\u79d8\u5efa\u7b51\uff0c\u770b\u8d77\u6765\u5c31\u5f88\u9002\u5408\u5b58\u653e\u4e0d\u5408\u6cd5\u7684\u4e1c\u897f\u3002\uff09",
    "\uff08\u7531\u4e8e\u7eff\u533a\u57ce\u5e02\u7684\u7eb5\u5411\u5e03\u5c40\uff0c\u6211\u4eec\u5f88\u5bb9\u6613\u5c31\u627e\u5230\u4e86\u4fef\u89c6\u6574\u4e2a\u4ed3\u5e93\u7684\u4f4d\u7f6e\u3002\uff09",
    "\uff08\u4ed3\u5e93\u5916\u56f4\u6709\u7740\u4e00\u4e2a\u8f83\u5927\u7684\u9662\u5b50\uff0c\u770b\u8d77\u6765\u662f\u7ed9\u8d27\u8f66\u5378\u8d27\u7528\u7684\u533a\u57df\u3002\uff09",
    "\uff08\u6b63\u9762\u7531\u4e24\u4e2a\u53cc\u5f00\u94c1\u95e8\u63a7\u5236\u7740\u552f\u4e00\u7684\u5165\u53e3\uff0c\u94c1\u95e8\u5df2\u7ecf\u9508\u8ff9\u6591\u6591\u3002\uff09",
    "\uff08\u6211\u4eec\u53ef\u4ee5\u4ece\u9ad8\u5904\u8f7b\u6613\u5730\u8fdb\u5165\u4ed3\u5e93\uff0c\u4f46\u662f\u60f3\u8981\u51fa\u53bb\u5c31\u53ea\u80fd\u7ecf\u8fc7\u552f\u4e00\u7684\u5927\u95e8\uff0c\u4e00\u65e6\u884c\u52a8\u66b4\u9732\uff0c\u60f3\u8981\u5168\u8eab\u800c\u9000\u7684\u6982\u7387\u5e76\u4e0d\u9ad8\u3002\uff09",
    "\uff08\u6240\u4ee5\u6700\u597d\u7684\u65b9\u5f0f\u5c31\u662f\u5b89\u9759\u5730\u8fdb\u53bb\uff0c\u518d\u5b89\u9759\u5730\u628a\u57c3\u4e1d\u7279\u5e26\u51fa\u6765\u3002\uff09",
    "\uff08\u4e0d\u8fc7\u4e5f\u5f97\u505a\u597d\u6700\u5dee\u7684\u6253\u7b97\u3002\uff09",
    "\u6625\u7530\uff0c\u6211\u4eec\u5df2\u7ecf\u8fbe\u5230\u76ee\u6807\u5730\u70b9\u3002|\u4e0d\u8fc7\u8fd9\u91cc\u5730\u5f62\u98ce\u9669\u8f83\u9ad8\uff0c\u80fd\u8c03\u4e00\u67b6\u80fd\u5782\u76f4\u8d77\u964d\u7684\u8f7d\u4eba\u65e0\u4eba\u673a\u6765\u5417\uff1f",
    "\u65e0\u4eba\u673a\u5417\u2026\u2026\u6709\u70b9\u56f0\u96be\uff0c\u4f46\u6211\u4f1a\u60f3\u529e\u6cd5\u7684\u3002",
    "\u62dc\u6258\u4f60\u4e86\u3002",
    "\uff08\u6211\u76d8\u7b97\u5b8c\u7b56\u7565\uff0c\u5f00\u59cb\u5411\u4eba\u5f62\u4e0b\u8fbe\u547d\u4ee4\u3002\uff09",
    "\u9edb\u70df\uff0c\u4f60\u9886\u5934\uff0c\u68c0\u67e5\u4e0b\u8fd9\u680b\u5efa\u7b51\u7684\u51fa\u5165\u53e3\uff0c\u628a\u53ef\u80fd\u7684\u9690\u85cf\u53e3\u4e5f\u627e\u51fa\u6765\u3002",
    "\u597d\u7684\uff0c\u957f\u5b98\u3002",
    "\u7edb\u96e8\uff0c\u4f60\u7559\u5728\u8fd9\u4e2a\u5236\u9ad8\u70b9\u76d1\u89c6\u5468\u56f4\u60c5\u51b5\uff0c\u5c31\u7b97\u662f\u4e00\u53ea\u8001\u9f20\u8fdb\u6765\u4e5f\u8981\u544a\u8bc9\u6211\u4eec\u3002",
    "\u7edb\u96e8\u5f97\u4ee4\uff01",
    "\u671d\u6656\uff0c\u4f60\u5728\u5efa\u7b51\u5468\u56f4\u7684\u56f4\u5899\u4e0a\u8bbe\u7f6e\u6444\u50cf\u5934\uff0c\u8865\u5145\u4e0b\u7edb\u96e8\u53ef\u80fd\u5b58\u5728\u7684\u76f2\u533a\uff0c\u7136\u540e\u627e\u4e00\u4e0b\u8fd9\u4e2a\u4ed3\u5e93\u7684\u7535\u95f8\u3002|\u4e00\u822c\u6765\u8bf4\u8fd9\u79cd\u6c11\u7528\u8bbe\u65bd\u7535\u95f8\u90fd\u4f1a\u5728\u5916\u9762\u3002",
    "\u662f\u3002|\u7edb\u96e8\uff0c\u770b\u6765\u957f\u5b98\u4e0d\u662f\u5f88\u4fe1\u4efb\u4f60\u7684\u76d1\u89c6\u80fd\u529b\u5462\u3002",
    "\u80e1\u8bf4\uff01|\u90a3\u662f\u957f\u5b98\u8003\u8651\u5468\u5bc6\uff0c\u4f5c\u6218\u4e0a\u7684\u4e8b\u542c\u957f\u5b98\u7684\u51c6\u6ca1\u9519\uff0c\u5feb\u8001\u5b9e\u7167\u529e\u5427\uff01",
    "\u901a\u8baf\u9891\u9053\u4f7f\u7528\u4ee3\u780177\uff0c\u4e09\u7ea7\u52a0\u5bc6\uff0c\u4f60\u4eec\u4e8c\u7ea7\u5e73\u5c42\u7684\u5bf9\u8bdd\u4e0d\u7528\u77e5\u4f1a\u6211\uff0c\u5f00\u59cb\u884c\u52a8\u5427\u3002",
    "\uff08\u51e0\u4e2a\u4eba\u5f62\u9690\u79d8\u800c\u8fc5\u901f\u5730\u9760\u8fd1\u5e9f\u5f03\u4ed3\u5e93\uff0c\u5f00\u59cb\u6267\u884c\u81ea\u5df1\u7684\u4efb\u52a1\u3002\uff09",
    "\uff08\u6211\u4e5f\u8df3\u4e0b\u5230\u9662\u5b50\u4e2d\uff0c\u7136\u540e\u8fc5\u901f\u79fb\u52a8\u5230\u4e86\u4ed3\u5e93\u5899\u8fb9\u7684\u9690\u853d\u4f4d\u7f6e\u3002\uff09",
    "\uff08\u8ba9\u6211\u610f\u5916\u7684\u662f\uff0c\u4ed3\u5e93\u4e2d\u4f20\u51fa\u4e86\u6cb8\u626c\u7684\u5435\u95f9\u58f0\uff0c\u800c\u5916\u56f4\u7adf\u7136\u8fde\u4e00\u4e2a\u653e\u54e8\u7684\u4eba\u90fd\u6ca1\u6709\u3002\uff09",
    "\u957f\u5b98\uff0c\u5df2\u7ecf\u641c\u7d22\u5b8c\u6bd5\uff0c\u4ed3\u5e93\u4e00\u5171\u5c31\u4e24\u4e2a\u51fa\u5165\u53e3\uff0c\u6ca1\u6709\u53d1\u73b0\u6697\u95e8\u3002",
    "\u91cc\u9762\u597d\u5435\uff01\u8fd9\u662f\u5927\u534a\u591c\u6050\u6016\u5206\u5b50\u4eec\u5728\u5f00\u4f1a\u5417\uff1f",
    "\u8bf4\u4e0d\u5b9a\u91cc\u9762\u5728\u5185\u8ba7\uff0c\u4e0d\u9700\u8981\u6211\u4eec\u51fa\u624b\u4ed6\u4eec\u5c31\u81ea\u751f\u81ea\u706d\u4e86\u3002",
    "\u54c8\uff1f\u600e\u4e48\u53ef\u80fd\uff1f",
    "\u671d\u6656\u8bf4\u7684\u8fd9\u79cd\u60c5\u51b5\u7684\u786e\u5f88\u6709\u53ef\u80fd\u3002|\u5982\u679c\u57c3\u4e1d\u7279\u5728\u8fd9\u5ea7\u4ed3\u5e93\u2026\u2026\u91cc\u9762\u53c8\u6709\u5f88\u591a\u5176\u4ed6\u4eba\uff0c\u6211\u5927\u6982\u80fd\u731c\u5230\u5979\u60f3\u505a\u4ec0\u4e48\u4e86\u3002",
    "\u957f\u5b98\uff0c\u6444\u50cf\u5934\u8bbe\u7f6e\u5b8c\u4e86\uff0c\u53e6\u5916\u6211\u627e\u5230\u4e86\u4ed3\u5e93\u7684\u603b\u7535\u95f8\u4e86\u3002",
    "\u505a\u5f97\u597d\uff0c\u4e00\u4f1a\u7b49\u6211\u7684\u547d\u4ee4\uff0c\u4e00\u5230\u65f6\u673a\u4f60\u5c31\u66ff\u6211\u65ad\u7535\u3002",
    "\u660e\u767d\u3002",
    "\u554a\u554a\uff0c\u957f\u5b98\uff0c\u80fd\u7ed9\u6211\u70b9\u522b\u7684\u4efb\u52a1\u5417\uff1f|\u53ea\u662f\u5e72\u77aa\u773c\u597d\u65e0\u804a\u554a\u2026\u2026",
    "\u76d1\u89c6\u533a\u57df\u53ef\u662f\u5f88\u91cd\u8981\u7684\u4efb\u52a1\uff0c\u4e00\u4f1a\u64a4\u79bb\u662f\u5426\u987a\u5229\u5c31\u5168\u9760\u4f60\u4e86\uff0c\u5148\u5fcd\u8010\u4e0b\u5427\uff1f",
    "\u597d\u5427\u2026\u2026\u957f\u5b98\u90fd\u8fd9\u4e48\u8bf4\u4e86\u2026\u2026",
    "\u770b\u6765\u90fd\u51c6\u5907\u5f97\u5dee\u4e0d\u591a\u4e86\uff0c\u6625\u7530\uff0c\u80fd\u542c\u5230\u5417\uff1f",
    "\u6211\u5728\uff0c\u6307\u6325\u5b98\uff0c\u6211\u5411\u6cbb\u5b89\u90e8\u95e8\u501f\u8c03\u4e86\u4e00\u67b6\u8b66\u7528\u65e0\u4eba\u673a\uff0c\u7b49\u5f85\u60a8\u7684\u547d\u4ee4\u968f\u65f6\u8d77\u964d\u3002",
    "\u660e\u767d\uff0c\u9ebb\u70e6\u4f60\u4e86\u3002",
    "\u9edb\u70df\uff0c\u6211\u671d\u4f60\u90a3\u8fb9\u6765\u4e86\u3002|\u6211\u4eec\u80fd\u4ece\u540e\u95e8\u6e9c\u8fdb\u53bb\u4e48\uff1f",
    "\u6211\u89c9\u5f97\u5e94\u8be5\u6ca1\u95ee\u9898\uff0c\u540e\u95e8\u6ca1\u6709\u4efb\u4f55\u4eba\u628a\u5b88\u3002",
    "\u5e0c\u671b\u8fd9\u4e0d\u662f\u4e2a\u9677\u9631\u2026\u2026",
    "\uff08\u6211\u548c\u9edb\u70df\u5728\u540e\u95e8\u5904\u6c47\u5408\uff0c\u9edb\u70df\u8f7b\u8f7b\u63a8\u5f00\u540e\u95e8\u3002\uff09",
    "\uff08\u5979\u7528\u81ea\u5df1\u654f\u9510\u7684\u542c\u89c9\u671d\u5185\u611f\u77e5\u4e86\u4e00\u5c0f\u4f1a\uff0c\u91cc\u9762\u6beb\u65e0\u52a8\u9759\u3002\uff09",
    "\uff08\u9edb\u70df\u5411\u6211\u793a\u610f\uff0c\u6211\u7d27\u7d27\u5730\u8ddf\u5728\u5979\u540e\u9762\u6301\u67aa\u8fdb\u5165\u4e86\u4ed3\u5e93\u3002\uff09",
    "\uff08\u6211\u4eec\u6e9c\u5165\u4e86\u4ed3\u5e93\u5185\u90e8\uff0c\u91cc\u9762\u7684\u7a7a\u95f4\u6bd4\u9884\u60f3\u4e2d\u8981\u5927\u5f88\u591a\u3002\uff09",
    "\uff08\u4ed3\u5e93\u7684\u5e03\u5c40\u5341\u5206\u7b80\u5355\uff0c\u4e2d\u95f4\u662f\u4e00\u4e2a\u5de8\u5927\u7684\u5b58\u50a8\u7a7a\u95f4\uff0c\u6b63\u95e8\u548c\u540e\u95e8\u5404\u4e00\u6761\u901a\u9053\uff0c\u800c\u901a\u9053\u5219\u6709\u697c\u68af\u8054\u901a\u5411\u4e0a\u7684\u697c\u5c42\u3002\uff09",
    "\uff08\u7531\u4e8e\u4ed3\u5e93\u5e76\u4e0d\u9ad8\uff0c\u4e00\u5171\u5c31\u4e09\u5c42\uff0c\u6211\u4eec\u51b3\u5b9a\u4e0d\u76f4\u63a5\u8fdb\u5165\u90a3\u4eba\u58f0\u6700\u55a7\u6742\u7684\u5b58\u50a8\u7a7a\u95f4\uff0c\u800c\u662f\u722c\u5230\u4e86\u6700\u9ad8\u7684\u697c\u5c42\uff0c\u6084\u6084\u5730\u5f80\u4e0b\u770b\u53bb\u3002\uff09",
    "\uff08\u5230\u4e86\u6700\u9ad8\u5904\uff0c\u6211\u4eec\u627e\u5230\u4e86\u4e00\u4e2a\u80cc\u5149\u7684\u4f4d\u7f6e\u63a2\u5934\u4e0b\u671b\uff0c\u773c\u524d\u7684\u60c5\u51b5\u5b8c\u5168\u51fa\u4e4e\u4e86\u6211\u7684\u610f\u6599\u3002\uff09",
    "\uff08\u8fd9\u4e2a\u5e9f\u5f03\u7684\u4ed3\u5e93\u91cc\u9762\u7adf\u7136\u8fd8\u642d\u4e86\u4e00\u4e2a\u50cf\u6a21\u50cf\u6837\u7684\u821e\u53f0\uff0c\u4e0b\u9762\u81f3\u5c11\u805a\u96c6\u7740\u51e0\u5341\u4e2a\u4eba\u3002\uff09",
    "\uff08\u800c\u5728\u53f0\u4e0a\u7684\u90a3\u4e2a\u4eba\u6b63\u62ff\u7740\u9ea6\u514b\u98ce\u671d\u4e0b\u5ba3\u8bb2\u7740\u4ec0\u4e48\u3002\uff09",
    "\u821e\u53f0\u4e0a\u7684\u90a3\u4f4d\u2026\u2026\u4e0d\u5c31\u662f\u57c3\u4e1d\u7279\u5c0f\u59d0\u5417\uff1f",
    "\u8fd9\u5c31\u662f\u57c3\u4e1d\u7279\u5417\uff1f|\u548c\u7167\u7247\u6bd4\u770b\u8d77\u6765\u4e0d\u592a\u4e00\u6837\u3002",
    "\u55ef\uff0c\u548c\u8d44\u6599\u4e2d\u7684\u7167\u7247\u6bd4\uff0c\u5979\u6ca7\u6851\u4e86\u5f88\u591a\u3002",
    "\u8fd9\u4e48\u5927\u5f20\u65d7\u9f13\uff0c\u770b\u6765\u5979\u4e5f\u662f\u51c6\u5907\u6700\u540e\u4e00\u640f\u4e86\u3002",
    "\uff08\u6211\u626b\u89c6\u4e86\u821e\u53f0\u5468\u56f4\u7684\u60c5\u51b5\u3002\u5927\u90e8\u5206\u4eba\u7684\u7a7f\u7740\u770b\u8d77\u6765\u90fd\u662f\u5de5\u4eba\uff0c\u4f46\u662f\u4eba\u7fa4\u4e2d\u6709\u597d\u51e0\u4e2a\u4eba\u770b\u8d77\u6765\u5374\u6709\u4e00\u4e9b\u5f02\u6837\u3002\uff09",
    "\u957f\u5b98\u2026\u2026",
    "\uff08\u9edb\u70df\u4e5f\u53d1\u73b0\u4e86\u90a3\u51e0\u4e2a\u884c\u4e3a\u8be1\u5f02\u7684\u4eba\uff0c\u5979\u6307\u4e86\u6307\u4ed6\u4eec\u5411\u6211\u505a\u51fa\u63d0\u793a\u3002\uff09",
    "\u4f60\u4e5f\u53d1\u73b0\u4e86\u4e48\u2026\u2026",
    "\u55ef\uff0c\u4ed6\u4eec\u7684\u52a8\u4f5c\u770b\u8d77\u6765\u63a5\u53d7\u8fc7\u8bad\u7ec3\uff0c\u773c\u795e\u548c\u59ff\u6001\u4e0e\u524d\u9762\u90a3\u4e9b\u5de5\u4eba\u5bf9\u6bd4\u533a\u522b\u592a\u660e\u663e\u4e86\u3002",
    "\u6709\u53ef\u80fd\u662f\u5409\u62c9\u5c14\u4f59\u515a\u7684\u4eba\uff0c\u770b\u6765\u4ed6\u4eec\u4e5f\u6e17\u900f\u8fdb\u6765\u4e86\u3002",
    "\u6211\u4eec\u63a5\u4e0b\u6765\u8be5\u600e\u4e48\u529e\uff1f",
    "\u8981\u5728\u90a3\u4e48\u591a\u4eba\u9762\u524d\u63a7\u5236\u57c3\u4e1d\u7279\u8fd8\u771f\u4e0d\u662f\u4ef6\u5bb9\u6613\u7684\u4e8b\uff0c\u5409\u62c9\u5c14\u4f59\u515a\u4e00\u5b9a\u51c6\u5907\u505a\u4e9b\u4ec0\u4e48\u3002|\u6211\u4eec\u5148\u5728\u6697\u4e2d\u89c2\u5bdf\u4e00\u4e0b\uff0c\u7b49\u5f85\u65f6\u673a\u3002",
    "\uff08\u9edb\u70df\u70b9\u4e86\u70b9\u5934\uff0c\u770b\u5411\u4e86\u821e\u53f0\u4e0a\u7684\u90a3\u4e2a\u4eba\u3002\uff09",
    "\uff08\u57c3\u4e1d\u7279\u5728\u53f0\u4e0a\uff0c\u4ece\u4e0a\u65b9\u53ef\u4ee5\u5f88\u6e05\u695a\u5730\u770b\u5230\u5979\u6025\u4fc3\u7d27\u5f20\u7684\u8868\u60c5\u3002\uff09",
    "\uff08\u5979\u6df1\u5438\u4e86\u51e0\u53e3\u6c14\uff0c\u8bd5\u56fe\u8ba9\u81ea\u5df1\u51b7\u9759\u4e0b\u6765\u3002\uff09",
    "\uff08\u7ec8\u4e8e\uff0c\u5979\u4e0b\u5b9a\u4e86\u51b3\u5fc3\uff0c\u62ff\u8d77\u4e86\u9ea6\u514b\u98ce\uff0c\u51c6\u5907\u5bf9\u4e0b\u65b9\u55a7\u95f9\u7684\u542c\u4f17\u8fdb\u884c\u6f14\u8bb2\u3002\uff09",
    "\uff08\u4e0b\u65b9\u7684\u4eba\u7fa4\u770b\u5230\u5979\u6709\u8bdd\u8981\u8bf4\uff0c\u4e5f\u9010\u6e10\u5b89\u9759\u4e86\u4e0b\u6765\u3002\uff09",
    "\u4eca\u5929\u53ec\u96c6\u5927\u5bb6\u8fc7\u6765\uff0c\u786e\u5b9e\u662f\u4e3a\u4e86\u5f15\u7206\u88c5\u7f6e\u7684\u4e8b\u3002",
    "\u6211\u8ba4\u771f\u601d\u8003\u4e86\u5f88\u4e45\uff0c\u5f15\u7206\u4f0a\u4f2f\u8fd9\u4ef6\u4e8b\u8981\u7ec8\u6b62\uff0c\u6211\u4eec\u4e0d\u80fd\u505a\u8fd9\u79cd\u4e8b\uff01",
    "\uff08\u57c3\u4e1d\u7279\u7684\u8bdd\u521a\u4e00\u51fa\u53e3\u53f0\u4e0b\u5c31\u70b8\u5f00\u4e86\u9505\uff0c\u521a\u521a\u624d\u5b89\u9759\u4e0b\u6765\u7684\u7a7a\u95f4\u4e00\u77ac\u95f4\u5c31\u55a7\u95f9\u4e86\u8d77\u6765\u3002\uff09",
    "\u5927\u59d0\u5934\u4f60\u4e0d\u4f1a\u662f\u60f3\u4e34\u9635\u9000\u7f29\u5427\uff1f",
    "\u90fd\u4ec0\u4e48\u65f6\u5019\u4e86\uff0c\u73b0\u5728\u600e\u4e48\u53ef\u80fd\u505c\u4e0b\u6765\u554a\uff01",
    "\u662f\u554a\uff0c\u6211\u4eec\u5c31\u7b49\u7740\u660e\u5929\u4e86\uff0c\u4eca\u5929\u8fde\u89c9\u90fd\u7761\u4e0d\u7740\u4e86\uff01",
    "\u5728\u5e86\u5178\u65f6\u5f15\u7206\u90a3\u4e48\u591a\u70b8\u5f39\uff0c\u4f60\u4eec\u60f3\u60f3\u8fd9\u4f1a\u6b7b\u591a\u5c11\u4eba\uff1f",
    "\u5927\u59d0\u5934\u4f60\u592a\u8f6f\u5f31\u4e86\uff01\u60f3\u60f3\u7f57\u8054\u662f\u600e\u4e48\u5bf9\u6211\u4eec\u7684\uff01",
    "\u4f46\u662f\u7f57\u8054\u600e\u4e48\u505a\u548c\u666e\u901a\u7684\u5e02\u6c11\u6709\u4ec0\u4e48\u5173\u7cfb\uff1f\uff01",
    "\u90a3\u5173\u6211\u4eec\u4ec0\u4e48\u4e8b\uff1f|\u6211\u4eec\u5fc5\u987b\u8981\u7ed9\u7f57\u8054\u4e00\u70b9\u989c\u8272\u770b\u770b\uff01\u8ba9\u4ed6\u4eec\u77e5\u9053\u4e4c\u624e\u4eba\u7684\u5389\u5bb3\uff01",
    "\u5c31\u662f\uff01\u4ed6\u4eec\u5f97\u4e3a\u6b64\u4ed8\u51fa\u4ee3\u4ef7\uff01",
    "\u4e0d\u662f\u8fd9\u6837\u7684\uff01\u8fd9\u79cd\u4e8b\u60c5\u600e\u4e48\u60f3\u90fd\u4e0d\u5bf9\u52b2\u554a\uff01|\u5927\u5bb6\u597d\u597d\u60f3\u4e00\u60f3\uff0c\u6211\u4eec\u662f\u4e0d\u662f\u6389\u8fdb\u9677\u9631\u91cc\u4e86\uff1f",
    "\u4ec0\u4e48\u9677\u9631\uff1f\u80fd\u62a5\u590d\u7f57\u8054\u90a3\u5e2e\u8822\u732a\uff0c\u8fd8\u80fd\u62ff\u5230\u4e00\u5927\u7b14\u94b1\uff0c\u8fd9\u79cd\u597d\u4e8b\u5230\u54ea\u91cc\u53bb\u627e\uff1f",
    "\u5c31\u662f\u56e0\u4e3a\u8fd9\u6837\uff0c\u8fd9\u79cd\u4e8b\u624d\u5f88\u5947\u602a\u597d\u5417\uff1f|\u4f60\u4eec\u60f3\u60f3\u90a3\u4e48\u591a\u94b1\u662f\u4ece\u54ea\u91cc\u6765\u7684\uff1f",
    "\uff08\u53f0\u4e0b\u7684\u4eba\u7fa4\u5f00\u59cb\u8bae\u8bba\u7eb7\u7eb7\uff0c\u53cd\u5bf9\u7684\u52bf\u5934\u6ca1\u6709\u6700\u5f00\u59cb\u90a3\u4e48\u76db\u4e86\u3002\uff09",
    "\uff08\u7136\u800c\u8d28\u7591\u7684\u58f0\u97f3\u6765\u5f97\u5f88\u5feb\uff0c\u7acb\u5373\u6709\u4eba\u5f00\u59cb\u548c\u57c3\u4e1d\u7279\u5bf9\u8d28\u8d77\u6765\u3002\uff09",
    "\u4f46\u8fd9\u4ef6\u4e8b\u7684\u53d1\u8d77\u4eba\u4e0d\u5c31\u662f\u5927\u59d0\u5934\u4f60\u5417\uff1f",
    "\u662f\u554a\uff0c\u8fd9\u4e0d\u662f\u81ea\u5df1\u5728\u6253\u81ea\u5df1\u7684\u8138\uff0c\u4ee5\u540e\u5927\u5bb6\u8fd8\u600e\u4e48\u76f8\u4fe1\u4f60\u8bf4\u7684\u8bdd\u554a\uff1f",
    "\u6211\u5f53\u7136\u77e5\u9053\u53d1\u8d77\u4eba\u662f\u6211\uff0c\u6240\u4ee5\u53d1\u73b0\u95ee\u9898\u4e86\u6211\u624d\u6765\u53ec\u96c6\u5927\u5bb6\u6765\u554a\uff01",
    "\u5c31\u5728\u4eca\u5929\u4e0b\u5348\uff0c\u6709\u4eba\u8fc7\u6765\u671d\u6211\u5f00\u67aa\uff01\u6446\u660e\u4e86\u5c31\u662f\u77e5\u9053\u6211\u4eec\u628a\u70b8\u5f39\u90e8\u7f72\u597d\u4e86\uff0c\u6240\u4ee5\u60f3\u706d\u53e3\uff01|\u5e78\u597d\u6211\u9003\u5f97\u5feb\u624d\u4fdd\u4e0b\u81ea\u5df1\u4e00\u6761\u547d\uff01",
    "\uff08\u4e3a\u4e86\u8bc1\u660e\u81ea\u5df1\u88ab\u88ad\u51fb\uff0c\u57c3\u4e1d\u7279\u62c9\u8d77\u8896\u5b50\u8ba9\u53f0\u4e0b\u4eba\u770b\u5230\u624b\u81c2\u4e0a\u7684\u6de4\u75d5\u3002\uff09",
    "\u4e0b\u5348\u8fbe\u8499\u4ed6\u4eec\u53bb\u5403\u996d\u540e\u5c31\u518d\u4e5f\u6ca1\u56de\u6765\uff0c\u542c\u8bf4\u88ab\u6cbb\u5b89\u90e8\u95e8\u7684\u4eba\u5e26\u8d70\u4e86\uff0c\u5c31\u662f\u90a3\u4e2a\u65f6\u5019\u53d1\u751f\u7684\u4e8b\u5417\uff1f",
    "\u6ca1\u9519\uff01\u6211\u4eec\u57cb\u8bbe\u597d\u7684\u70b8\u836f\uff0c\u7136\u540e\u88ad\u51fb\u5c31\u6765\u4e86\uff0c\u6cbb\u5b89\u90e8\u95e8\u7684\u4eba\u4e5f\u6765\u4e86\uff01\u54ea\u6709\u8fd9\u4e48\u5de7\u7684\u4e8b\uff01",
    "\u5927\u59d0\u5934\u90fd\u88ab\u88ad\u51fb\u4e86\u2026\u2026\u5929\u554a\uff0c\u8fd9\u4e0d\u4f1a\u771f\u7684\u662f\u9677\u9631\u5427\u2026\u2026",
    "\uff08\u53f0\u4e0b\u7684\u4eba\u7fa4\u518d\u6b21\u5f00\u59cb\u8bae\u8bba\uff0c\u57c3\u4e1d\u7279\u89c1\u81ea\u5df1\u8bf4\u7684\u8bdd\u6709\u6548\u679c\uff0c\u51c6\u5907\u4e00\u9f13\u4f5c\u6c14\u7ee7\u7eed\u8bf4\u4e0b\u53bb\u3002\uff09",
    "\uff08\u7136\u800c\uff0c\u53f0\u4e0b\u54cd\u4eae\u800c\u523a\u8033\u7684\u58f0\u97f3\u6253\u65ad\u4e86\u5979\u7684\u6f14\u8bf4\u3002\uff09",
    "\u4f60\u662f\u4e0d\u662f\u88ab\u7f57\u8054\u6536\u4e70\u4e86\uff1f\u4f60\u80af\u5b9a\u662f\u628a\u6211\u4eec\u90fd\u5356\u4e86\u5427\uff01",
    "\u4ec0\u4e48\uff1f",
    "\u4f60\u80af\u5b9a\u662f\u6536\u4e86\u94b1\u5427\uff01|\u4f5c\u4e3a\u4e4c\u624e\u4eba\u7adf\u7136\u66ff\u7f57\u8054\u8bf4\u597d\u8bdd\uff0c\u4f60\u77e5\u9053\u8fd9\u6b21\u7684\u590d\u4ec7\u8ba1\u5212\u6211\u4eec\u7b49\u4e86\u591a\u4e45\u5417\uff1f",
    "\u8ba9\u6211\u4eec\u53bb\u505a\u70b8\u5f39\u7684\u662f\u4f60\uff0c\u73b0\u5728\u8bf4\u4e0d\u8981\u5e72\u7684\u4e5f\u662f\u4f60\uff01|\u4f60\u662f\u628a\u6211\u4eec\u5f53\u7334\u800d\u5417\uff1f",
    "\u5c31\u7b97\u8fd9\u6837\u6211\u4eec\u4e5f\u4e0d\u80fd\u6740\u4eba\u2014\u2014",
    "\u90a3\u662f\u6d3b\u8be5\uff01\u7f57\u8054\u8be5\u6b7b\uff0c\u8fd9\u4e9b\u4eba\u4e5f\u8be5\u6b7b\uff01",
    "\u5c31\u662f\uff01\u90a3\u5e2e\u987a\u6c11\u7ad9\u5728\u7f57\u8054\u90a3\u8fb9\uff0c\u4ed6\u4eec\u6d3b\u8be5\u5f53\u796d\u54c1\uff0c\u7528\u4ed6\u4eec\u7684\u547d\u6765\u8ba9\u7f57\u8054\u77e5\u9053\u6211\u4eec\u7684\u6124\u6012\uff01|\u6211\u4eec\u8fd9\u662f\u6b63\u4e49\u7684\u590d\u4ec7\uff01\u505a\u4ec0\u4e48\u90fd\u662f\u7406\u6240\u5e94\u5f53\u7684\uff01",
    "\u4f60\u4eec\u2026\u2026\u4f60\u4eec\u75af\u4e86\u5417\uff1f",
    "\u4f60\u624d\u75af\u4e86\uff01|\u8fd9\u4e8b\u662f\u4f60\u5f00\u7684\u5934\uff0c\u73b0\u5728\u5374\u60f3\u8fd9\u4e48\u7b80\u5355\u5c31\u6d17\u8131\u81ea\u5df1\uff0c\u54ea\u6709\u90a3\u4e48\u5bb9\u6613\u7684\u4e8b\uff01",
    "\u7f57\u8054\u6bc1\u4e86\u6211\u4eec\u7684\u6545\u4e61\uff0c\u6211\u4eec\u4e5f\u5fc5\u987b\u6bc1\u4e86\u7f57\u8054\uff01",
    "\u662f\u554a\uff01\uff01\u662f\u554a\uff01\uff01\u5e72\u6389\u7f57\u8054\uff0c\u628a\u4ed6\u4eec\u90fd\u70b8\u4e0a\u5929\uff01\uff01",
    "\uff08\u53f0\u4e0b\uff0c\u4eba\u7fa4\u518d\u6b21\u9a9a\u4e71\u4e86\u8d77\u6765\u3002\u90a3\u4e00\u5f20\u5f20\u8138\uff0c\u6df7\u5408\u7740\u6124\u6012\u3001\u5931\u671b\u3001\u618e\u6068\uff0c\u4e94\u5b98\u626d\u66f2\uff0c\u9762\u76ee\u53ef\u618e\u3002\uff09",
    "\uff08\u770b\u7740\u8fd9\u4e9b\u8138\uff0c\u9edb\u70df\u4e0d\u7531\u81ea\u4e3b\u5730\u60f3\u8d77\u4e86\u5728\u5404\u4e2a\u536b\u661f\u57ce\u5de5\u4f5c\u7684\u65f6\u5019\uff0c\u751a\u81f3\u66f4\u8fdc\u4e00\u4e9b\uff0c\u5728\u683c\u91cc\u82ac\u7684\u65f6\u5019\u2026\u2026\u90a3\u4e9b\u4e0e\u5979\u76f8\u5904\u7684\u4eba\u7c7b\u7684\u8138\u2026\u2026\uff09",
    "\uff08\u957f\u5b98\u3001\u683c\u7433\u5a1c\u3001\u8d6b\u4e3d\u5b89\uff0c\u8bb0\u5fc6\u4e2d\uff0c\u90a3\u4e9b\u9762\u5e9e\u603b\u662f\u6e29\u67d4\u800c\u548c\u5584\u3002\uff09",
    "\uff08\u4f46\u662f\u53f0\u4e0b\u7684\u8fd9\u4e9b\u4eba\u7c7b\uff0c\u660e\u660e\u5728\u505a\u7740\u9519\u8bef\u7684\u4e8b\u60c5\uff0c\u5374\u90a3\u4e48\u7406\u6240\u5f53\u7136\u5730\u8086\u610f\u6563\u53d1\u7740\u6076\u610f\uff0c\u8fd9\u80a1\u6076\u610f\u5145\u65a5\u7740\u9edb\u70df\u7684\u5fc3\u667a\uff0c\u8ba9\u5979\u51e0\u4e4e\u65e0\u6cd5\u601d\u8003\u3002\uff09",
    "\u9edb\u70df\uff0c\u4f60\u6ca1\u4e8b\u5427\uff1f",
    "\u6211\u6ca1\u4e8b\u957f\u5b98\u2026\u2026\u6211\u53ea\u662f\uff0c\u6709\u70b9\u96be\u4ee5\u9002\u5e94\u2026\u2026",
    "\uff08\u9edb\u70df\u610f\u8bc6\u5230\u81ea\u5df1\u7684\u865a\u5f31\uff0c\u5979\u8d76\u7d27\u91cd\u65b0\u805a\u795e\u8ba9\u81ea\u5df1\u9547\u5b9a\u4e0b\u6765\uff0c\u4f46\u662f\u6211\u8fd8\u662f\u80fd\u4ece\u5979\u7684\u8868\u60c5\u4e2d\u770b\u51fa\u5979\u7684\u54c0\u4f24\u3002\uff09",
    "\u4e3a\u4ec0\u4e48\u8fd9\u4e9b\u4eba\u6709\u90a3\u4e48\u5927\u7684\u6076\u610f\uff0c\u4e3a\u4e86\u5ba3\u6cc4\u81ea\u5df1\u7684\u60c5\u7eea\u4e0d\u60dc\u53bb\u4f24\u5bb3\u65e0\u8f9c\u7684\u4eba\u2026\u2026|\u4e4c\u624e\u9a6c\u5fb7\u5947\u672c\u8eab\u4e5f\u5728\u6c61\u67d3\u533a\u4e2d\uff0c\u7f57\u8054\u5efa\u7acb\u79fb\u52a8\u9694\u79bb\u5899\u4e0d\u5c31\u662f\u4e3a\u4e86\u51c0\u5316\u4ed6\u4eec\u7684\u6545\u4e61\u5417\uff1f",
    "\u4e0d\u662f\u6bcf\u4e2a\u4eba\u90fd\u80fd\u7406\u667a\u5730\u53bb\u601d\u8003\u8fd9\u4e9b\uff0c\u5bf9\u4ed6\u4eec\u800c\u8a00\u7f57\u8054\u94fa\u8bbe\u7684\u8f68\u9053\u5c06\u4ed6\u4eec\u9010\u51fa\u6545\u4e61\uff0c\u5149\u8fd9\u4e00\u6761\u5c31\u5df2\u7ecf\u7f6a\u4e0d\u53ef\u6055\u4e86\u3002",
    "\u4f46\u662f\u5f53\u90a3\u5757\u571f\u5730\u88ab\u51c0\u5316\u540e\uff0c\u4ed6\u4eec\u7684\u6545\u4e61\u4e0d\u4f1a\u53d8\u5f97\u6bd4\u53d7\u5230\u6c61\u67d3\u65f6\u66f4\u597d\u5417\uff1f",
    "\u4ee5\u540e\u7684\u4e8b\u60c5\u5bf9\u4ed6\u4eec\u6765\u8bf4\u592a\u8fdc\u4e86\uff0c\u6ca1\u4eba\u77e5\u9053\u7f57\u8054\u5230\u5e95\u80fd\u4e0d\u80fd\u5b8c\u6210\u51c0\u5316\uff0c\u4ed6\u4eec\u80fd\u5728\u4e4e\u7684\u53ea\u6709\u73b0\u5728\u3002",
    "\u800c\u73b0\u5728\u7684\u60c5\u51b5\u5c31\u662f\u2014\u2014\u4ed6\u4eec\u7684\u6545\u4e61\u56e0\u4e3a\u51c0\u5316\u5de5\u7a0b\u6d88\u5931\u4e86\u3002|\u66f4\u4f55\u51b5\uff0c\u7fa4\u4f53\u5931\u53bb\u7406\u667a\u7684\u80cc\u540e\u4e00\u5b9a\u6709\u4eba\u5728\u63a8\u6ce2\u52a9\u6f9c\uff0c\u800c\u4e14\u8fd9\u6837\u7684\u63a8\u624b\u8fd8\u4e0d\u5c11\u3002",
    "\uff08\u6211\u6307\u4e86\u6307\u4eba\u7fa4\u4e2d\u6b63\u5728\u632f\u81c2\u9ad8\u547c\u4e14\u55d3\u95e8\u6700\u54cd\u7684\u51e0\u4e2a\u4eba\uff0c\u9edb\u70df\u7acb\u5373\u8ba4\u51fa\u4ed6\u4eec\u6b63\u662f\u521a\u624d\u53d1\u73b0\u7684\u72b6\u6001\u5f02\u5e38\u7684\u4eba\u3002\uff09",
    "\u5409\u62c9\u5c14\u4f59\u515a\u2026\u2026\u5728\u717d\u52a8\u8fd9\u4e9b\u5de5\u4eba\uff1f",
    "\u57c3\u4e1d\u7279\u5df2\u7ecf\u5728\u52aa\u529b\u8bf4\u670d\u5979\u7684\u8001\u4e61\u4eec\u4e86\uff0c\u4f46\u98ce\u5411\u6839\u672c\u4e0d\u662f\u5979\u6240\u80fd\u63a7\u5236\u7684\u3002",
    "\u8fd9\u4e9b\u4eba\u5f88\u4e13\u4e1a\uff0c\u4e00\u770b\u5c31\u4e0d\u662f\u7b2c\u4e00\u6b21\u505a\u8fd9\u79cd\u4e8b\u3002|\u4f20\u64ad\u8c23\u8a00\uff0c\u5728\u80cc\u540e\u717d\u52a8\u4ec7\u6068\uff0c\u8ba9\u8fd9\u4e9b\u65e0\u77e5\u53c8\u53ef\u601c\u7684\u5de5\u4eba\u66ff\u4ed6\u4eec\u5f53\u6253\u624b\uff0c\u597d\u8ba9\u81ea\u5df1\u9003\u8131\u7f6a\u8d23\u3002",
    "\u867d\u7136\u6211\u4e5f\u8ba8\u538c\u7f57\u8054\uff0c\u4e0d\u8fc7\u6211\u66f4\u8ba8\u538c\u90a3\u4e9b\u8eb2\u5728\u9ed1\u6697\u4e2d\u7684\u86c6\u866b\u3002|\u548c\u4ee5\u524d\u6211\u78b0\u5230\u8fc7\u7684\u654c\u4eba\u4e00\u6837\uff0c\u4ed6\u4eec\u7528\u4ec7\u6068\u6495\u88c2\u4eba\u7fa4\uff0c\u7528\u5351\u9119\u7684\u8c0e\u8a00\u548c\u6240\u8c13\u7684\u771f\u76f8\uff0c\u5728\u80cc\u540e\u5506\u4f7f\u4e86\u4e00\u573a\u53c8\u4e00\u573a\u4e0d\u5fc5\u8981\u7684\u51b2\u7a81\u3002",
    "\u591a\u5c11\u4eba\u56e0\u6b64\u65e0\u8c13\u5730\u727a\u7272\u4e86\uff0c\u800c\u8fd9\u4e9b\u727a\u7272\u4e0d\u8fc7\u662f\u4ed6\u4eec\u83b7\u53d6\u5229\u76ca\u7684\u7b79\u7801\uff0c\u4e0d\u53ef\u539f\u8c05\u7684\u5bb6\u4f19\u2026\u2026",
    "\uff08\u770b\u5230\u6211\u7684\u7709\u5934\u7d27\u76b1\uff0c\u9edb\u70df\u8f7b\u8f7b\u6478\u4e86\u6478\u6211\u6525\u7d27\u7684\u62f3\u5934\u3002\uff09",
    "\uff08\u610f\u8bc6\u5230\u81ea\u5df1\u6709\u4e9b\u5931\u53bb\u51b7\u9759\uff0c\u6211\u6df1\u5438\u4e86\u4e00\u53e3\u6c14\uff0c\u8ba9\u81ea\u5df1\u5c3d\u91cf\u5e73\u9759\u4e0b\u6765\u3002\uff09",
    "\u8fc7\u53bb\u4e86\u5341\u5e74\uff0c\u9047\u5230\u7684\u654c\u4eba\u5374\u6beb\u65e0\u65b0\u610f\uff0c\u8fd9\u4e2a\u4e16\u754c\u771f\u7684\u6709\u5728\u53d8\u597d\u5417\uff1f",
    "\u6b63\u56e0\u5982\u6b64\uff0c\u624d\u66f4\u9700\u8981\u957f\u5b98\u8fd9\u6837\u7684\u4eba\u3002",
    "\u771f\u7684\u662f\u8fd9\u6837\u4e48\u2026\u2026",
    "\uff08\u6211\u518d\u6b21\u505a\u4e86\u4e00\u6b21\u6df1\u547c\u5438\uff0c\u901a\u8fc7\u901a\u8baf\u7ec8\u7aef\u793a\u610f\u5916\u56f4\u7684\u4eba\u5f62\u2014\u2014\u6211\u4eec\u5c06\u6709\u6240\u884c\u52a8\u3002\uff09",
    "\u505a\u597d\u51c6\u5907\u9edb\u70df\uff0c\u52a8\u624b\u7684\u65f6\u673a\u5c31\u8981\u6765\u4e86\u3002",
    "\uff08\u9edb\u70df\u671d\u6211\u70b9\u4e86\u70b9\u5934\uff0c\u6211\u4eec\u4e00\u5e76\u671d\u697c\u4e0b\u770b\u53bb\u3002\uff09",
    "\uff08\u53f0\u4e0b\u7684\u4eba\u7fa4\u8d8a\u6765\u8d8a\u66b4\u8e81\uff0c\u5435\u95f9\u7684\u547c\u53eb\u58f0\u4e00\u6ce2\u63a5\u7740\u4e00\u6ce2\uff0c\u57c3\u4e1d\u7279\u7565\u5fae\u540e\u9000\u4e86\u51e0\u6b65\uff0c\u5979\u660e\u767d\u573a\u9762\u5373\u5c06\u5931\u63a7\u3002\uff09",
    "\u8be5\u6b7b\u2026\u2026\u6211\u8be5\u600e\u4e48\u529e\uff1f"
  ]
]