[
  [
    "\uff08\u5c31\u50cf\u56de\u5e94\u57c3\u4e1d\u7279\u7684\u9000\u5374\u4e00\u822c\uff0c\u6124\u6012\u7684\u4eba\u4eec\u51b2\u4e0a\u53f0\u53bb\u5c06\u57c3\u4e1d\u7279\u67b6\u4f4f\uff0c\u800c\u5979\u505a\u7740\u65e0\u529b\u800c\u5f92\u52b3\u7684\u6323\u624e\u3002\uff09",
    "\u4f60\u4eec\u5728\u505a\u4ec0\u4e48\uff01\u653e\u5f00\u6211\uff0c\u4f60\u4eec\u90fd\u75af\u4e86\uff01",
    "\u8001\u5b9e\u70b9\uff0c\u4f60\u8fd9\u6761\u7f57\u8054\u7684\u72d7\uff01",
    "\u545c\u554a\u2026\u2026\uff01\uff01\u54b3\u54b3\u54b3\uff01\uff01",
    "\uff08\u57c3\u4e1d\u7279\u8fd8\u5728\u8bd5\u56fe\u6323\u5f00\u675f\u7f1a\uff0c\u4f46\u662f\u67b6\u4f4f\u5979\u7684\u4eba\u5f80\u5979\u7684\u4e0b\u8179\u72e0\u72e0\u5730\u6253\u4e86\u4e00\u62f3\uff0c\u5267\u70c8\u7684\u75bc\u75db\u8ba9\u57c3\u4e1d\u7279\u51e0\u4e4e\u65e0\u6cd5\u7ad9\u4f4f\u3002\uff09",
    "\uff08\u75bc\u75db\u5e26\u6765\u7684\u75c9\u631b\u8ba9\u5979\u7684\u8eab\u4f53\u4e0d\u81ea\u89c9\u5730\u5f2f\u66f2\uff0c\u57c3\u4e1d\u7279\u751a\u81f3\u65e0\u6cd5\u62ac\u8d77\u81ea\u5df1\u7684\u5934\u9885\u3002\uff09",
    "\uff08\u6124\u6012\u7684\u5492\u9a82\u548c\u5267\u70c8\u7684\u75bc\u75db\u8ba9\u5979\u4e00\u9635\u5931\u795e\uff0c\u4e00\u5207\u90fd\u50cf\u653e\u7740\u6162\u955c\u5934\u7684\u7535\u5f71\u7247\u6bb5\u4e00\u822c\u3002\uff09",
    "\uff08\u5979\u76f4\u76ef\u76ef\u5730\u770b\u7740\u5730\u9762\uff0c\u751a\u81f3\u65e0\u6cd5\u547c\u5438\uff0c\u76f4\u5230\u4e00\u53cc\u76ae\u978b\u671d\u81ea\u5df1\u8d8a\u8d70\u8d8a\u8fd1\u3002\uff09",
    "\uff08\u5979\u611f\u5230\u81ea\u5df1\u7684\u5934\u53d1\u88ab\u6293\u4f4f\uff0c\u5934\u9885\u731b\u5730\u88ab\u62ce\u4e86\u8d77\u6765\u3002\uff09",
    "\u57c3\u4e1d\u7279\uff0c\u4f60\u505a\u70b8\u5f39\u7684\u624b\u827a\u4e0d\u9519\uff0c\u53ef\u60dc\u6f14\u8bb2\u7684\u6c34\u5e73\u5b9e\u5728\u662f\u592a\u5dee\u4e86\u3002",
    "\uff08\u4e00\u4e2a\u4e00\u5934\u957f\u53d1\uff0c\u8138\u4e0a\u5e26\u7740\u9634\u6c89\u8868\u60c5\u7684\u7537\u4eba\uff0c\u8d6b\u7136\u51fa\u73b0\u5728\u57c3\u4e1d\u7279\u7684\u773c\u524d\u3002\uff09",
    "\uff08\u4ed6\u7528\u4e0d\u5c51\u7684\u773c\u795e\u770b\u7740\u57c3\u4e1d\u7279\uff0c\u8a00\u8bed\u4e2d\u5939\u6742\u7740\u9119\u89c6\u3002\uff09",
    "\u6211\u7ed9\u4e86\u4f60\u673a\u4f1a\u4e3a\u5bb6\u4e61\u590d\u4ec7\uff0c\u6ca1\u60f3\u5230\u4f60\u5374\u8981\u5f53\u53db\u5f92\uff0c\u8981\u51fa\u5356\u6211\u4eec\uff0c\u8981\u80cc\u53db\u548c\u4f60\u540c\u751f\u540c\u957f\u7684\u4eba\u4eec\u3002",
    "\u5f17\u2026\u2026\u5f17\u62c9\u683c\uff01\u4f60\u600e\u4e48\u5728\u8fd9\u91cc\uff1f\uff01",
    "\u6211\u4e3a\u4ec0\u4e48\u4e0d\u80fd\u5728\u8fd9\u91cc\uff1f\u6211\u548c\u6211\u7684\u5144\u5f1f\u59d0\u59b9\u4eec\u540c\u805a\u4e00\u5802\u6709\u4ec0\u4e48\u5947\u602a\u5417\uff1f",
    "\uff08\u5f17\u62c9\u683c\u5c55\u5f00\u53cc\u624b\uff0c\u4eff\u4f5b\u793a\u610f\u4e00\u822c\u671d\u56db\u5468\u8f6c\u4e86\u4e00\u5708\uff0c\u5468\u56f4\u7684\u4eba\u7fa4\u7acb\u5373\u4e3a\u4ed6\u559d\u8d77\u5f69\u6765\u3002\uff09",
    "\u5f17\u62c9\u683c\u5927\u54e5\u65e9\u5c31\u7b97\u5230\u57c3\u4e1d\u7279\u8981\u80cc\u53db\u6211\u4eec\uff01",
    "\u5e78\u597d\u6709\u5927\u54e5\u6307\u5f15\u6211\u4eec\uff01\u4e0d\u7136\u590d\u4ec7\u8ba1\u5212\u5c31\u8981\u524d\u529f\u5c3d\u5f03\u4e86\uff01",
    "\u5f17\u62c9\u683c\u5927\u54e5\uff01\u6211\u4eec\u8be5\u600e\u4e48\u5904\u7f6e\u8fd9\u4e2a\u53db\u5f92\uff1f",
    "\uff08\u57c3\u4e1d\u7279\u610f\u8bc6\u5230\u81ea\u5df1\u7684\u540c\u4e61\u5df2\u7ecf\u88ab\u8fd9\u540d\u53eb\u5f17\u62c9\u683c\u7684\u7537\u5b50\u5b8c\u5168\u6d17\u8111\uff0c\u751a\u81f3\u8fde\u4e00\u4e2a\u613f\u610f\u542c\u5979\u8bf4\u8bdd\u7684\u4eba\u90fd\u6ca1\u6709\u3002\uff09",
    "\u957f\u5b98\uff01\u8fd9\u4e2a\u4eba\u7684\u58f0\u97f3\u2026\u2026\u4ed6\u5c31\u662f\u90a3\u5929\u5728\u80cc\u540e\u671d\u6211\u5f00\u67aa\u7684\u4eba\uff01",
    "\u4ed6\u5fc5\u7136\u662f\u5409\u62c9\u5c14\u8fd9\u6b21\u4e8b\u4ef6\u80cc\u540e\u7684\u6267\u884c\u8005\u4e4b\u4e00\uff0c\u8bb0\u5f55\u4e0b\u4ed6\u7684\u8138\u3002",
    "\u662f\uff0c\u8bb0\u5f55\u5f97\u975e\u5e38\u6e05\u695a\u3002",
    "\u671d\u6656\uff0c\u968f\u65f6\u51c6\u5907\u62c9\u95f8\uff0c\u9edb\u70df\uff0c\u706d\u706f\u540e\u5c06\u57c3\u4e1d\u7279\u5e26\u51fa\u4ed3\u5e93\uff0c\u6211\u4eec\u5728\u540e\u95e8\u6c47\u5408\u3002",
    "\u597d\u3002",
    "\u662f\u3002",
    "\u65e2\u7136\u4f60\u5f53\u4e86\u53db\u5f92\uff0c\u5c31\u8981\u505a\u597d\u4e3a\u6b64\u4ed8\u51fa\u4ee3\u4ef7\u7684\u51c6\u5907\u3002|\u771f\u662f\u53ef\u60dc\uff0c\u6211\u539f\u672c\u4ee5\u4e3a\u6211\u4eec\u90fd\u662f\u90a3\u79cd\u80f8\u6000\u6012\u706b\u7684\u6218\u58eb\u2026\u2026",
    "\u600e\u4e48\uff0c\u4f60\u8981\u5728\u8fd9\u91cc\u6740\u4e86\u6211\u5417\uff1f|\u5c31\u7b97\u4f60\u6740\u6b7b\u6211\uff0c\u6211\u7684\u540c\u4e61\u4e5f\u4f1a\u628a\u6240\u6709\u7684\u8bc1\u636e\u90fd\u4ea4\u7ed9\u6cbb\u5b89\u90e8\u95e8\uff0c\u4f60\u662f\u4e0d\u4f1a\u5f97\u901e\u7684\uff01",
    "\u4f60\u786e\u5b9a\u4f60\u8fd8\u6709\u540c\u4e61\u4f1a\u5e2e\u4f60\uff1f|\u522b\u81ea\u6b3a\u6b3a\u4eba\u4e86\uff0c\u57c3\u4e1d\u7279\u3002",
    "\u6211\u7ed9\u4e86\u4f60\u505a\u4eba\u7684\u673a\u4f1a\uff0c\u4f46\u4f60\u5374\u6700\u7ec8\u9009\u62e9\u5f53\u72d7\u2014\u2014\u5f53\u7f57\u8054\u7684\u72d7\u3002",
    "\u4f60\u5e76\u4e0d\u662f\u88ab\u9009\u4e2d\u7684\u90a3\u4e2a\u4eba\uff0c\u4f60\u53ea\u662f\u4e00\u7c92\u7070\u5c18\u4e00\u9897\u5c18\u57c3\uff0c\u6c38\u522b\u4e86\uff0c\u57c3\u4e1d\u7279\u3002",
    "\uff08\u53eb\u5f17\u62c9\u683c\u7684\u4eba\u5bf9\u7740\u57c3\u4e1d\u7279\u4e3e\u8d77\u67aa\uff0c\u4ed3\u5e93\u5185\u53d8\u5f97\u9e26\u96c0\u65e0\u58f0\uff0c\u6240\u6709\u4eba\u90fd\u5728\u5c4f\u606f\u7b49\u5f85\u884c\u5211\u7684\u90a3\u4e00\u523b\u3002\uff09",
    "\uff08\u6211\u8ba4\u4e3a\u673a\u4f1a\u6765\u4e86\u3002\uff09",
    "\u671d\u6656\uff0c\u62c9\u95f8\u3002",
    "\u4ec0\u4e48\uff1f",
    "\uff08\u201c\u5494\u201d\u7684\u4e00\u58f0\u540e\uff0c\u4ed3\u5e93\u5185\u4e00\u7247\u6f06\u9ed1\uff0c\u4eba\u7fa4\u9677\u5165\u4e86\u6df7\u4e71\u3002\uff09",
    "\uff08\u9ed1\u6697\u4e2d\uff0c\u5f17\u62c9\u683c\u7acb\u5373\u8df3\u4e0b\u53f0\uff0c\u9690\u85cf\u8fdb\u4e86\u4eba\u7fa4\u4e2d\uff0c\u800c\u67b6\u4f4f\u57c3\u4e1d\u7279\u7684\u5de5\u4eba\u4e5f\u56e0\u4e3a\u60ca\u614c\u5931\u63aa\u800c\u6709\u6240\u677e\u61c8\u3002\uff09",
    "\uff08\u57c3\u4e1d\u7279\u9a6c\u4e0a\u6293\u4f4f\u673a\u4f1a\uff0c\u7528\u811a\u8e39\u5f00\u4e86\u8eab\u8fb9\u7684\u5de5\u4eba\u6323\u8131\u4e86\u675f\u7f1a\uff0c\u671d\u540e\u95e8\u9003\u53bb\u3002\uff09",
    "\u5c31\u8d81\u73b0\u5728\u2026\u2026\uff01",
    "\uff08\u5c31\u5728\u6b64\u65f6\u9edb\u70df\u4ece\u697c\u4e0a\u8df3\u4e0b\uff0c\u4e0d\u504f\u4e0d\u501a\u5730\u843d\u5728\u4e86\u57c3\u4e1d\u7279\u9762\u524d\u3002\u57c3\u4e1d\u7279\u5927\u5403\u4e00\u60ca\uff0c\u4e00\u77ac\u95f4\u50f5\u4f4f\u7ad9\u5728\u4e86\u539f\u5730\u3002\uff09",
    "\u4f60\uff01\u4f60\u662f\u6628\u665a\u7684\u90a3\u4e2a\u2014\u2014",
    "\u62b1\u6b49\u4e86\u57c3\u4e1d\u7279\u5c0f\u59d0\u3002",
    "\uff08\u9edb\u70df\u8bf4\u5b8c\u4e00\u8bb0\u624b\u5200\u6253\u5728\u4e86\u57c3\u4e1d\u7279\u7684\u540e\u9888\u4e0a\uff0c\u57c3\u4e1d\u7279\u968f\u5373\u660f\u8ff7\u4e86\u8fc7\u53bb\u3002\uff09",
    "\uff08\u9edb\u70df\u625b\u8d77\u57c3\u4e1d\u7279\u5c31\u5f80\u540e\u95e8\u5904\u8dd1\u53bb\uff0c\u800c\u6211\u5df2\u7ecf\u8dd1\u4e0b\u697c\u68af\u548c\u5979\u6c47\u5408\u3002\uff09",
    "\u957f\u5b98\uff0c\u57c3\u4e1d\u7279\u5c0f\u59d0\u5df2\u7ecf\u63a7\u5236\u4f4f\u4e86\u3002",
    "\u597d\uff0c\u6211\u4eec\u7acb\u5373\u79bb\u5f00\u3002\u7edb\u96e8\uff0c\u5916\u9762\u60c5\u51b5\u5982\u4f55\uff1f",
    "\uff08\u6b64\u65f6\u706f\u5149\u7a81\u7136\u6062\u590d\u4e86\uff0c\u4eae\u5149\u8ba9\u6211\u4e0d\u7531\u5730\u772f\u773c\uff0c\u540c\u65f6\u6211\u610f\u8bc6\u5230\u60c5\u51b5\u6709\u4e9b\u4e0d\u5999\u3002\uff09",
    "\u5567\u2026\u2026\u52a8\u4f5c\u6bd4\u6211\u9884\u60f3\u7684\u5feb\u3002",
    "\u957f\u5b98\uff0c\u4ed6\u4eec\u4f7f\u7528\u4e86\u5907\u7528\u7535\u6e90\uff0c\u9700\u8981\u6211\u518d\u53bb\u62c9\u4e00\u6b21\u95f8\u5417\uff1f",
    "\u98ce\u9669\u592a\u9ad8\u4e86\uff0c\u9a6c\u4e0a\u5230\u540e\u95e8\u8fd9\u8fb9\u548c\u6211\u4eec\u6c47\u5408\u3002",
    "\u660e\u767d\u3002",
    "\uff08\u6062\u590d\u706f\u5149\u540e\uff0c\u4eba\u4eec\u53d1\u73b0\u57c3\u4e1d\u7279\u65e9\u5df2\u4e0d\u5728\u53f0\u4e0a\u3002\uff09",
    "\uff08\u57c3\u4e1d\u7279\u7684\u6d88\u5931\u8ba9\u4f17\u4eba\u6124\u6012\u4e86\u8d77\u6765\uff0c\u4ed6\u4eec\u7acb\u5373\u81ea\u53d1\u5730\u641c\u5bfb\u8d77\u57c3\u4e1d\u7279\u3002\uff09",
    "\u57c3\u4e1d\u7279\u5462\uff1f\u57c3\u4e1d\u7279\u8dd1\u4e86\uff01",
    "\u5979\u8dd1\u4e0d\u4e86\u591a\u8fdc\uff01\u628a\u5979\u6293\u56de\u6765\uff01\uff01",
    "\u4e0d\u597d\u4e86\u957f\u5b98\uff0c\u597d\u591a\u4eba\u90fd\u5728\u5f80\u5916\u7a9c\uff0c\u597d\u50cf\u5728\u627e\u4f60\u4eec\uff01\u6211\u8be5\u600e\u4e48\u529e\uff1f",
    "\u80fd\u5236\u9020\u4e00\u4e9b\u52a8\u9759\u628a\u4ed6\u4eec\u5f15\u5f00\u5417\uff1f",
    "\u54e6\u54e6\u597d\uff0c\u6211\u8bd5\u8bd5\uff01",
    "\u6625\u7530\uff01\u6211\u4eec\u9700\u8981\u65e0\u4eba\u673a\u64a4\u79bb\uff0c\u64a4\u79bb\u4f4d\u7f6e\u662f\u2026\u2026\u2026\u671d\u6656\uff0c\u5e2e\u6211\u4eec\u627e\u4e00\u4e2a\u9002\u5408\u7684\u964d\u843d\u4f4d\u7f6e\uff01",
    "\u9a6c\u4e0a\u3002",
    "\u6307\u6325\u5b98\uff0c\u65e0\u4eba\u673a\u5df2\u7ecf\u671d\u4f60\u4eec\u8fc7\u53bb\u4e86\uff0c\u9884\u8ba13\u5206\u949f\u540e\u964d\u843d\u3002",
    "\u957f\u5b98\uff01\u8fd9\u4e9b\u4eba\u600e\u4e48\u56de\u4e8b\uff0c\u4ed6\u4eec\u90fd\u4e0d\u8981\u547d\u4e86\u5417\uff1f\u7adf\u7136\u6562\u548c\u6211\u8fc7\u62db\uff01",
    "\u4ec0\u4e48\uff1f",
    "\uff08\u6211\u6253\u5f00\u7ec8\u7aef\uff0c\u770b\u5230\u6444\u50cf\u5934\u62cd\u5230\u7edb\u96e8\u8df3\u5230\u4eba\u7fa4\u4e2d\u5927\u58f0\u6311\u8845\uff0c\u5e76\u5c06\u51b2\u4e0a\u6765\u7684\u4eba\u5f53\u4f17\u5236\u670d\u5728\u5730\u3002\uff09",
    "\uff08\u5373\u4fbf\u5982\u6b64\uff0c\u6124\u6012\u7684\u4eba\u7fa4\u8fd8\u662f\u5728\u671d\u5979\u51b2\u53bb\uff0c\u7edb\u96e8\u4e3a\u4e86\u4e0d\u88ab\u6293\u4f4f\u4e0d\u5f97\u4e0d\u8fde\u8fde\u540e\u9000\u3002\uff09",
    "\u8be5\u6b7b\u7684\u4eba\u5f62\uff0c\u662f\u57c3\u4e1d\u7279\u7684\u4fdd\u9556\u5417\uff01\uff1f",
    "\u628a\u5979\u548c\u57c3\u4e1d\u7279\u4e00\u8d77\u6254\u8fdb\u5783\u573e\u573a\u91cc\uff01\uff01",
    "\u5c31\u51ed\u4f60\u4eec\uff0c\u60f3\u90fd\u4e0d\u8981\u60f3\uff01",
    "\u7edb\u96e8\uff01\u5c0f\u5fc3\u5440\uff01",
    "\u653e\u5fc3\u5427\u8001\u59d0\uff0c\u8fd9\u70b9\u5c0f\u4e8b\u96be\u4e0d\u5012\u6211\uff01"
  ],
  [
    "\u6307\u6325\u5b98\uff0c\u65e0\u4eba\u673a\u5df2\u7ecf\u6309\u7167\u671d\u6656\u53d1\u9001\u7684\u5750\u6807\u964d\u843d\uff0c\u4f60\u4eec\u8d76\u7d27\u64a4\u79bb\uff01",
    "\u660e\u767d\uff0c\u6240\u6709\u4eba\u767b\u673a\uff0c\u671d\u6656\u4f60\u6765\u64cd\u4f5c\u65e0\u4eba\u673a\uff01",
    "\u6536\u5230\u3002",
    "\uff08\u65e0\u4eba\u673a\u5df2\u7ecf\u98de\u5230\u6211\u4eec\u5934\u4e0a\uff0c\u98a4\u98a4\u5dcd\u5dcd\u5f00\u59cb\u964d\u843d\uff0c\u7136\u800c\u7edb\u96e8\u8fd8\u5728\u8fdc\u5904\uff0c\u4f46\u79bb\u6211\u4eec\u8f83\u8fd1\u7684\u5de5\u4eba\u5df2\u7ecf\u542c\u5230\u4e86\u58f0\u97f3\u3002\uff09",
    "\u8fd9\u91cc\uff01\u8fd9\u91cc\u6709\u65e0\u4eba\u673a\u964d\u843d\uff01\u522b\u7ba1\u90a3\u4e2a\u4eba\u5f62\u4e86\uff0c\u57c3\u4e1d\u7279\u8981\u8dd1\u4e86\uff01",
    "\uff08\u542c\u5230\u547c\u558a\u58f0\u7684\u4eba\u7fa4\u7acb\u5373\u6389\u5934\u671d\u65e0\u4eba\u673a\u7684\u4f4d\u7f6e\u51b2\u4e86\u8fc7\u6765\uff0c\u867d\u7136\u65e0\u4eba\u673a\u5df2\u7ecf\u964d\u843d\uff0c\u4f46\u7edb\u96e8\u8fd8\u6ca1\u80fd\u8d76\u56de\u6765\u3002\uff09",
    "\u8fd9\u4e9b\u4eba\u592a\u96be\u7f20\u4e86\uff01\u957f\u5b98\u4f60\u4eec\u5148\u8d70\uff0c\u4e00\u4f1a\u6211\u81ea\u5df1\u60f3\u529e\u6cd5\uff01",
    "\u7edb\u96e8\uff01",
    "\u4e0d\u884c\uff0c\u4e00\u8d77\u64a4\u79bb\uff01",
    "\uff08\u7136\u800c\u4eba\u7fa4\u5df2\u7ecf\u51e0\u4e4e\u51b2\u5230\u4e86\u6211\u4eec\u773c\u524d\uff0c\u518d\u8fd9\u6837\u4e0b\u53bb\u6211\u4eec\u5c31\u8981\u88ab\u4ed6\u4eec\u6309\u5012\u5728\u5730\u3002\uff09",
    "\uff08\u9edb\u70df\u5e26\u7740\u660f\u8ff7\u7684\u57c3\u4e1d\u7279\u94bb\u8fdb\u4e86\u65e0\u4eba\u673a\u4e2d\uff0c\u6211\u548c\u671d\u6656\u9760\u5728\u8237\u8fb9\u770b\u7740\u4eba\u7fa4\u8d8a\u6765\u8d8a\u8fd1\u3002\u671d\u6656\u4e0d\u81ea\u89c9\u5730\u62e8\u5f00\u4e86\u4fdd\u9669\uff0c\u5979\u671d\u6211\u770b\u6765\uff0c\u6211\u6447\u4e86\u6447\u5934\u3002\uff09",
    "\u957f\u5b98\uff0c\u8bf7\u9a6c\u4e0a\u8d77\u98de\uff0c\u7edb\u96e8\u5f80\u697c\u4e0a\u8dd1\uff01",
    "\u597d\uff01\u671d\u6656\u5feb\uff01",
    "\uff08\u6211\u4eec\u6240\u6709\u4eba\u7acb\u523b\u660e\u767d\u4e86\u9edb\u70df\u7684\u610f\u601d\uff0c\u671d\u6656\u968f\u5373\u62c9\u8d77\u4e86\u65e0\u4eba\u673a\u3002\uff09",
    "\uff08\u5c31\u5728\u4eba\u7fa4\u6293\u5230\u6211\u4eec\u524d\u7684\u4e00\u77ac\u95f4\uff0c\u65e0\u4eba\u673a\u98de\u4e86\u8d77\u6765\u3002\u5feb\u901f\u5347\u7a7a\u5e26\u8d77\u7684\u5927\u98ce\u751a\u81f3\u5c06\u51b2\u5728\u524d\u9762\u7684\u51e0\u4e2a\u4eba\u6380\u7ffb\u5728\u5730\u3002\uff09",
    "\u8001\u59d0\uff0c\u63a5\u597d\u6211\uff01",
    "\uff08\u6500\u767b\u4e09\u5c42\u697c\u9ad8\u7684\u4ed3\u5e93\u5bf9\u7edb\u96e8\u6765\u8bf4\u8f7b\u800c\u6613\u4e3e\uff0c\u5979\u5e73\u5730\u8df3\u8d77\u6293\u4f4f\u4e86\u4e8c\u697c\u5899\u4e0a\u7684\u62a4\u680f\uff0c\u518d\u4e00\u4e2a\u7528\u529b\u5c06\u81ea\u5df1\u7529\u5230\u4e86\u4ed3\u5e93\u697c\u9876\u3002\uff09",
    "\uff08\u65e0\u4eba\u673a\u6b64\u65f6\u4ece\u5979\u9762\u524d\u98de\u8fc7\uff0c\u5979\u7eb5\u8eab\u4e00\u8dc3\u98de\u8fdb\u4e86\u673a\u8231\u5185\uff0c\u521a\u597d\u8df3\u8fdb\u9edb\u70df\u7684\u6000\u91cc\u3002\uff09",
    "\u545c\u54c7\uff01",
    "\uff08\u867d\u7136\u9edb\u70df\u505a\u597d\u4e86\u63a5\u4f4f\u7edb\u96e8\u7684\u51c6\u5907\uff0c\u4f46\u5979\u8fd8\u662f\u88ab\u7edb\u96e8\u7684\u52a8\u4f5c\u5413\u4e86\u4e00\u8df3\u3002\u786e\u8ba4\u5979\u6ca1\u4e8b\u540e\uff0c\u9edb\u70df\u8d76\u7d27\u5c06\u6000\u4e2d\u7684\u7edb\u96e8\u6276\u5230\u4e86\u65e0\u4eba\u673a\u7684\u5ea7\u6905\u4e0a\u3002\uff09",
    "\u547c\u2026\u2026\u771f\u662f\u5343\u94a7\u4e00\u53d1\u5462~\u8fd8\u597d\u8d76\u4e0a\u4e86\u3002",
    "\u63a5\u5f97\u597d\u9edb\u70df\u3002",
    "\u771f\u662f\u7684\u592a\u5192\u9669\u4e86\uff0c\u7edb\u96e8\uff01\u4e0b\u6b21\u4e0d\u51c6\u518d\u81ea\u5df1\u505a\u8fd9\u79cd\u4e8b\u4e86\uff01",
    "\u8fd9\u4e0d\u662f\u4e3a\u4e86\u5b8c\u6210\u957f\u5b98\u7684\u4efb\u52a1\u561b~",
    "\u5b8c\u6210\u4efb\u52a1\u4e5f\u53ef\u4ee5\u7528\u66f4\u5b89\u5168\u7684\u65b9\u5f0f\u554a\uff01",
    "\u597d\u5566\u597d\u5566\uff0c\u4e0b\u6b21\u518d\u8bf4\u5427\u3002",
    "\uff08\u4e00\u770b\u9edb\u70df\u53c8\u8981\u8fdb\u5165\u8bf4\u6559\u6a21\u5f0f\uff0c\u7edb\u96e8\u8d76\u7d27\u9003\u5230\u6211\u8fd9\u8fb9\uff0c\u8eb2\u5728\u6211\u8eab\u540e\u671d\u9edb\u70df\u505a\u9b3c\u8138\u3002\uff09",
    "\uff08\u6211\u53ea\u80fd\u65e0\u53ef\u5948\u4f55\u5730\u770b\u7740\u9edb\u70df\u6447\u6447\u5934\uff0c\u7136\u540e\u63a5\u901a\u548c\u6625\u7530\u7684\u901a\u8baf\u3002\uff09",
    "\u6625\u7530\uff0c\u76ee\u6807\u5df2\u7ecf\u5230\u624b\uff0c\u6240\u6709\u4eba\u90fd\u5230\u9f50\u4e86\uff0c\u73b0\u5728\u5f00\u59cb\u8fd4\u56de\u3002",
    "\u505a\u5f97\u597d\u6307\u6325\u5b98\uff0c\u6211\u7b49\u5f85\u5927\u5bb6\u7684\u5f52\u6765\u3002",
    "\u671d\u6656\uff0c\u6211\u4eec\u8fd4\u822a\u5427\u3002",
    "\u597d\u7684\u3002",
    "\uff08\u65e0\u4eba\u673a\u4e00\u4e2a\u8f6c\u5411\uff0c\u5feb\u901f\u5730\u79bb\u5f00\u4e86\u4ed3\u5e93\u4e0a\u7a7a\u3002\uff09",
    "\uff08\u5728\u55a7\u95f9\u7740\u7684\u4eba\u7fa4\u4e4b\u540e\uff0c\u5f17\u62c9\u683c\u62ac\u624b\u6309\u4e0b\u4e86\u624b\u8fb9\u51e0\u4e2a\u4eba\u4e3e\u8d77\u7684\u67aa\uff0c\u90a3\u4e9b\u4eba\u7acb\u5373\u606d\u656c\u5730\u6536\u8d77\u4e86\u6b66\u5668\u3002\uff09",
    "\uff08\u5f17\u62c9\u683c\u770b\u7740\u65e0\u4eba\u673a\u98de\u8d70\u7684\u65b9\u5411\uff0c\u638f\u51fa\u4e86\u901a\u8baf\u7ec8\u7aef\uff0c\u62e8\u901a\u4e86\u67d0\u4e2a\u53f7\u7801\u3002\uff09",
    "\u6211\u6709\u4ef6\u4e8b\u60f3\u544a\u8bc9\u4f60\uff0c\u6211\u5728CHE-02\u89c1\u5230\u4e86\u67d0\u4e2a\u4eba\u3002|\u867d\u7136\u53ea\u662f\u8fdc\u8fdc\u5730\u770b\u89c1\uff0c\u4e0d\u8fc7\u90a3\u4e2a\u8eab\u5f71\u2026\u2026",
    "\uff08\u5f17\u62c9\u683c\u638f\u51fa\u4e86\u4e00\u652f\u70df\uff0c\u4ed6\u7684\u624b\u4e0b\u7acb\u5373\u66ff\u4ed6\u70b9\u4e0a\u3002\uff09",
    "\uff08\u4ed6\u731b\u5730\u5438\u4e86\u4e00\u53e3\uff0c\u6545\u5f04\u7384\u865a\u822c\u5730\u505c\u987f\u4e86\u4e00\u4f1a\uff0c\u5728\u5bf9\u9762\u5df2\u7ecf\u5feb\u4e0d\u8010\u70e6\u65f6\uff0c\u4ed6\u624d\u7ee7\u7eed\u521a\u624d\u7684\u8bdd\u9898\u3002\uff09",
    "\u6211\u4e0d\u4f1a\u770b\u9519\u7684\uff0c\u6b63\u662f\u90a3\u4e2a\u6d88\u5931\u4e86\u5341\u5e74\u7684\u5bb6\u4f19\u3002",
    "\u770b\u6765\u6211\u4eec\u9493\u5230\u4e86\u5927\u9c7c\uff0c\u63a5\u4e0b\u6765\u8bf7\u4f60\u7b49\u5f85\u6211\u7684\u597d\u6d88\u606f\u3002|\u54fc\uff0c\u8bb0\u4f4f\u6211\u8bf4\u7684\u8bdd\uff0c\u6211\u4e0d\u4f1a\u8ba9\u96c6\u56e2\u5931\u671b\u7684\u3002",
    "\uff08\u2026\u2026\uff09",
    "\uff08\u57c3\u4e1d\u7279\u6162\u6162\u5730\u7741\u5f00\u773c\u775b\uff0c\u9633\u5149\u900f\u8fc7\u6811\u836b\u6591\u9a73\u5730\u7167\u8000\u5728\u5979\u7684\u8138\u4e0a\u3002\uff09",
    "\uff08\u5979\u4f38\u51fa\u624b\u906e\u6321\u4f4f\u4e86\u4e9b\u8bb8\u9633\u5149\uff0c\u8bd5\u56fe\u8ba9\u81ea\u5df1\u722c\u8d77\u8eab\u6765\u3002\uff09",
    "\u4f60\u9192\u4e86\uff1f",
    "\uff08\u4e00\u4f4d\u6e29\u67d4\u7684\u5987\u4eba\u629a\u6478\u7740\u5979\u7684\u989d\u5934\uff0c\u4e00\u79cd\u5f3a\u70c8\u7684\u5b89\u5fc3\u611f\u50cf\u88ab\u5b50\u4e00\u6837\u8986\u76d6\u4e86\u57c3\u4e1d\u7279\u7684\u5168\u8eab\u3002\uff09",
    "\uff08\u5987\u4eba\u4f38\u624b\u6458\u4e0b\u8eab\u65c1\u7684\u82b1\u6735\uff0c\u5c06\u82b1\u6735\u63d2\u5728\u4e86\u57c3\u4e1d\u7279\u7684\u5934\u53d1\u4e0a\uff0c\u82b1\u9999\u6251\u9762\u800c\u6765\u3002\uff09",
    "\uff08\u57c3\u4e1d\u7279\u5c06\u82b1\u6735\u4ece\u5934\u4e0a\u53d6\u4e0b\uff0c\u5728\u5979\u624b\u4e2d\u7684\u662f\u4e00\u6735\u767d\u8272\u7684\u9a6c\u94c3\u85af\u82b1\u3002\uff09",
    "\u57c3\u4e1d\u7279\uff0c\u4f60\u8981\u597d\u597d\u6d3b\u4e0b\u53bb\u3002",
    "\uff08\u5987\u4eba\u7ad9\u8d77\u8eab\u6765\uff0c\u671d\u8fdc\u5904\u8d70\u53bb\uff0c\u5979\u7684\u8eab\u5f71\u662f\u5982\u6b64\u6a21\u7cca\uff0c\u57c3\u4e1d\u7279\u51e0\u4e4e\u65e0\u6cd5\u770b\u6e05\u5979\u7684\u6837\u5b50\u3002\u4f46\u662f\u57c3\u4e1d\u7279\u5374\u5341\u5206\u6e05\u695a\uff0c\u8fd9\u4f4d\u5987\u4eba\u5230\u5e95\u662f\u8c01\u3002\uff09",
    "\u5988\u5988\uff1f\u8bf7\u7b49\u4e00\u4e0b\uff01",
    "\uff08\u5987\u4eba\u6ca1\u6709\u56de\u5e94\u57c3\u4e1d\u7279\u7684\u547c\u5524\uff0c\u5979\u53ea\u662f\u8d8a\u8d70\u8d8a\u8fdc\uff0c\u4eff\u4f5b\u8d70\u8fdb\u4e86\u4e00\u7247\u7a7a\u767d\u7684\u4e16\u754c\u4e2d\u3002\uff09",
    "\uff08\u57c3\u4e1d\u7279\u8bd5\u56fe\u4f38\u624b\u633d\u7559\uff0c\u4f46\u5374\u53d1\u73b0\u81ea\u5df1\u65e0\u8bba\u5982\u4f55\u90fd\u65e0\u6cd5\u722c\u8d77\u8eab\u6765\u3002\uff09",
    "\u5988\u5988\uff0c\u522b\u629b\u4e0b\u6211\uff01\u6c42\u4f60\u4e86\uff0c\u5988\u5988\u2014\u2014",
    "\u5988\u5988\uff01",
    "\uff08\u57c3\u4e1d\u7279\u7a81\u7136\u9192\u6765\uff0c\u8eab\u4f53\u7684\u75bc\u75db\u8ba9\u5979\u4e00\u9635\u7729\u6655\uff0c\u5979\u5403\u529b\u5730\u73af\u987e\u56db\u5468\uff0c\u8fd9\u662f\u4e00\u4e2a\u964c\u751f\u7684\u5730\u65b9\u3002\uff09",
    "\uff08\u5934\u4e0a\u6709\u6e29\u6696\u800c\u67d4\u548c\u7684\u611f\u89c9\uff0c\u6709\u8c01\u6b63\u5750\u5728\u5e8a\u8fb9\u629a\u6478\u7740\u5979\u7684\u5934\u53d1\u3002\uff09",
    "\u8fd9\u91cc\u662f\u2026\u2026",
    "\u60a8\u9192\u4e86\u5417\uff0c\u57c3\u4e1d\u7279\u5c0f\u59d0\uff1f",
    "\uff08\u5b88\u5728\u57c3\u4e1d\u7279\u5e8a\u8fb9\u7684\u6b63\u662f\u9edb\u70df\uff0c\u5979\u6b63\u54fc\u7740\u4e00\u9996\u57c3\u4e1d\u7279\u65e0\u6bd4\u719f\u6089\u7684\u65cb\u5f8b\u3002\uff09",
    "\uff08\u770b\u5230\u57c3\u4e1d\u7279\u9192\u6765\uff0c\u9edb\u70df\u8c03\u6574\u4e86\u4e00\u4e0b\u5750\u59ff\uff0c\u8ba9\u57c3\u4e1d\u7279\u80fd\u6e05\u695a\u5730\u770b\u5230\u81ea\u5df1\u3002\uff09",
    "\u4f60\u662f\u2026\u2026\u4f60\u662f\u90a3\u4e2a\u5728\u9910\u9986\u91cc\u6f14\u594f\u53e4\u7b5d\u7684\u4eba\u5f62\u2026\u2026",
    "\u554a\uff01\u521a\u624d\u5c31\u662f\u4f60\u628a\u6211\u6253\u660f\u8fc7\u53bb\uff01\u8fd8\u6709\u6628\u665a\u4e5f\u662f\u4f60\u7f20\u7740\u6211\uff01",
    "\uff08\u57c3\u4e1d\u7279\u4e00\u7d27\u5f20\uff0c\u731b\u5730\u4ece\u5e8a\u4e0a\u5f39\u4e86\u8d77\u6765\u3002\u7136\u800c\u5979\u7684\u77e5\u89c9\u8fd8\u6ca1\u5b8c\u5168\u6062\u590d\uff0c\u6839\u672c\u65e0\u6cd5\u597d\u597d\u652f\u6491\u4f4f\u81ea\u5df1\u7684\u8eab\u4f53\u3002\uff09",
    "\uff08\u9edb\u70df\u8d76\u7d27\u5c06\u5979\u6276\u6b63\u5750\u5728\u4e86\u5e8a\u4e0a\u3002\uff09",
    "\u62b1\u6b49\uff0c\u5f53\u65f6\u4e8b\u51fa\u7d27\u6025\uff0c\u6240\u4ee5\u6211\u53ea\u80fd\u8ba9\u60a8\u53d7\u4e86\u70b9\u4f24\u5bb3\u3002|\u73b0\u5728\u8eab\u4f53\u6709\u4ec0\u4e48\u4e0d\u8212\u670d\u7684\u5730\u65b9\u4e48\uff1f",
    "\u2026\u2026\u2026\u2026",
    "\uff08\u57c3\u4e1d\u7279\u6ca1\u6709\u56de\u7b54\u9edb\u70df\u7684\u95ee\u9898\u3002\uff09",
    "\uff08\u6211\u542c\u5230\u91cc\u9762\u7684\u52a8\u9759\uff0c\u6572\u4e86\u6572\u95e8\uff0c\u5728\u5f97\u5230\u9edb\u70df\u7684\u7b54\u590d\u540e\uff0c\u6211\u6253\u5f00\u623f\u95e8\u8d70\u4e86\u8fdb\u53bb\u3002\uff09",
    "\uff08\u57c3\u4e1d\u7279\u8b66\u60d5\u5730\u770b\u7740\u9edb\u70df\u548c\u521a\u8fdb\u5165\u5c4b\u5b50\u4e2d\u7684\u6211\uff0c\u4e00\u8a00\u4e0d\u53d1\u3002\uff09",
    "\u5979\u9192\u4e86\uff1f",
    "\u55ef\uff0c\u57c3\u4e1d\u7279\u5c0f\u59d0\u5979\u521a\u9192\u3002",
    "\u4f60\u8fd8\u597d\u5417\uff1f",
    "\uff08\u65e0\u89c6\u4e86\u6211\u5bf9\u5979\u7684\u95ee\u5019\uff0c\u57c3\u4e1d\u7279\u4f4e\u7740\u5934\u770b\u7740\u5e8a\u88ab\uff0c\u7ee7\u7eed\u4e00\u8a00\u4e0d\u53d1\u3002\uff09",
    "\u57c3\u4e1d\u7279\u5c0f\u59d0\u73b0\u5728\u53ef\u80fd\u8fd8\u6709\u4e00\u4e9b\u6df7\u4e71\u2026\u2026",
    "\u9047\u5230\u8fd9\u6837\u7684\u4e8b\u4e5f\u96be\u514d\uff0c\u4e0d\u8fc7\u6211\u4eec\u65f6\u95f4\u6709\u9650\u2026\u2026",
    "\uff08\u9edb\u70df\u62c5\u5fc3\u5730\u7aef\u8be6\u7740\u57c3\u4e1d\u7279\u7684\u8138\u5e9e\uff0c\u867d\u7136\u57c3\u4e1d\u7279\u5f3a\u538b\u81ea\u5df1\u7684\u60c5\u7eea\u5047\u88c5\u9762\u65e0\u8868\u60c5\uff0c\u4f46\u773c\u6cea\u5374\u6b63\u6b62\u4e0d\u4f4f\u5730\u4ece\u5979\u7684\u773c\u7736\u5f80\u5916\u6d41\u3002\uff09",
    "\u60a8\u5728\u54ed\u4e48\uff1f",
    "\uff08\u57c3\u4e1d\u7279\u7528\u529b\u62c9\u8d77\u88ab\u5355\u64e6\u62ed\u6389\u81ea\u5df1\u7684\u773c\u6cea\uff0c\u7136\u800c\u773c\u6cea\u4e1d\u6beb\u6ca1\u6709\u505c\u6b62\u7684\u610f\u601d\u3002\u843d\u4e0b\u7684\u6cea\u6c34\u6cbe\u6e7f\u4e86\u8138\u4e0b\u7684\u88ab\u5355\uff0c\u4f46\u662f\u5979\u4f9d\u7136\u5014\u5f3a\u5730\u4e00\u8a00\u4e0d\u53d1\u3002\uff09",
    "\uff08\u9edb\u70df\u5411\u6211\u6295\u6765\u4e86\u8be2\u95ee\u7684\u76ee\u5149\uff0c\u6211\u731c\u5230\u4e86\u5979\u60f3\u600e\u4e48\u505a\u3002\u7565\u4f5c\u601d\u8003\uff0c\u6211\u5411\u9edb\u70df\u70b9\u4e86\u70b9\u5934\uff0c\u540c\u610f\u4e86\u5979\u7684\u60f3\u6cd5\u3002\uff09",
    "\uff08\u9edb\u70df\u770b\u5411\u4e00\u8a00\u4e0d\u53d1\u7684\u57c3\u4e1d\u7279\uff0c\u6e05\u4e86\u6e05\u55d3\u5b50\uff0c\u968f\u5373\u5f00\u53e3\u8bf4\u9053\u3002\uff09",
    "\u201c\u6211\u5e26\u7740\u6765\u81ea\u5730\u4e0b\u6df1\u5904\u7684\u6c34\uff0c\u5c06\u5b83\u4eec\u4e00\u52fa\u4e00\u52fa\u6d47\u704c\u5728\u9ed1\u571f\u4e0a\uff0c\u7b49\u5f85\u7ea4\u7ec6\u7684\u65b0\u82bd\uff0c\u7834\u571f\u800c\u51fa\u2026\u2026\u201d",
    "\uff08\u57c3\u4e1d\u7279\u731b\u5730\u62ac\u8d77\u5934\uff0c\u7528\u4e0d\u53ef\u601d\u8bae\u7684\u773c\u795e\u770b\u5411\u9edb\u70df\u3002\uff09",
    "\u201c\u2026\u2026\u4f60\u8bf4\uff0c\u9a6c\u94c3\u85af\u82b1\u5f00\u7684\u65f6\u5019\uff0c\u4f60\u5c31\u4f1a\u56de\u6765\u2026\u2026\u201d",
    "\u6211\u7684\u7b14\u8bb0\u672c\uff01",
    "\uff08\u9edb\u70df\u4ece\u6000\u4e2d\u53d6\u51fa\u4e86\u90a3\u672c\u7cbe\u81f4\u7684\u7b14\u8bb0\u672c\uff0c\u770b\u5230\u7b14\u8bb0\u672c\u7684\u57c3\u4e1d\u7279\u7acb\u5373\u6fc0\u52a8\u4e86\u8d77\u6765\u3002\uff09",
    "\u90a3\u662f\u6211\u7684\u7b14\u8bb0\u672c\uff0c\u5feb\u8fd8\u7ed9\u6211\uff01",
    "\u8981\u8fd8\u7ed9\u4f60\u4e5f\u4e0d\u662f\u4e0d\u884c\uff0c\u6211\u4eec\u53ea\u662f\u60f3\u95ee\u4f60\u4e00\u4e9b\u95ee\u9898\uff0c\u53ea\u8981\u4f60\u2014\u2014",
    "\u54b3\u54b3\u3002",
    "\uff08\u9edb\u70df\u5c11\u89c1\u5730\u7528\u54b3\u55fd\u6253\u65ad\u4e86\u6211\u7684\u8bdd\uff0c\u671d\u6211\u6447\u4e86\u6447\u5934\u3002\u610f\u8bc6\u5230\u8fd9\u4e48\u505a\u6709\u4e9b\u4e0d\u59a5\uff0c\u6211\u8bc6\u8da3\u5730\u95ed\u5634\u5f80\u540e\u9000\u4e86\u4e00\u6b65\u3002\uff09",
    "\u57c3\u4e1d\u7279\u5c0f\u59d0\uff0c\u8fd9\u662f\u4e0b\u5348\u6211\u4eec\u9047\u88ad\u65f6\u60a8\u9057\u5931\u5728\u5730\u4e0a\u7684\u7b14\u8bb0\u672c\uff0c\u5e78\u8fd0\u7684\u662f\u5f53\u65f6\u88ab\u6211\u53d1\u73b0\u5e76\u4fdd\u7ba1\u4e86\u8d77\u6765\u3002|\u8fd9\u4e2a\u7b14\u8bb0\u672c\u5bf9\u60a8\u6765\u8bf4\u610f\u4e49\u91cd\u5927\u5427\uff1f",
    "\u4f60\u60f3\u600e\u4e48\u6837\uff1f",
    "\u6211\u60f3\u65e2\u7136\u5bf9\u60a8\u975e\u5e38\u91cd\u8981\uff0c\u90a3\u8fd8\u662f\u7269\u5f52\u539f\u4e3b\u6bd4\u8f83\u597d\u3002",
    "\uff08\u9edb\u70df\u5c06\u7b14\u8bb0\u672c\u9012\u7ed9\u57c3\u4e1d\u7279\uff0c\u57c3\u4e1d\u7279\u6709\u4e9b\u65e0\u6cd5\u7f6e\u4fe1\uff0c\u751a\u81f3\u6ca1\u6709\u62ac\u624b\u53bb\u63a5\u3002\uff09",
    "\u4f60\u2026\u2026\u76f4\u63a5\u5c31\u8fd8\u7ed9\u6211\uff1f",
    "\u662f\u7684\uff0c\u57c3\u4e1d\u7279\u5c0f\u59d0\u3002",
    "\uff08\u8fd9\u624d\u53cd\u5e94\u8fc7\u6765\u7684\u57c3\u4e1d\u7279\u4e00\u628a\u5c06\u7b14\u8bb0\u672c\u62a2\u8fc7\u6765\u62b1\u5728\u6000\u91cc\uff0c\u7b14\u8bb0\u672c\u5931\u800c\u590d\u5f97\u8ba9\u5979\u8138\u4e0a\u6062\u590d\u4e86\u4e00\u4e9b\u8840\u8272\u3002\uff09",
    "\uff08\u5979\u7acb\u5373\u7ffb\u5f00\u7b14\u8bb0\u672c\uff0c\u770b\u5230\u90a3\u5f20\u6cdb\u9ec4\u7684\u7167\u7247\u8fd8\u5728\uff0c\u57c3\u4e1d\u7279\u518d\u4e5f\u65e0\u6cd5\u6291\u5236\u81ea\u5df1\u7684\u60c5\u7eea\uff0c\u62b1\u7740\u88ab\u5b50\u75db\u54ed\u4e86\u8d77\u6765\u3002\uff09",
    "\u5988\u5988\u2026\u2026\u5988\u5988\u6211\u4ee5\u4e3a\u6211\u518d\u4e5f\u89c1\u4e0d\u5230\u4f60\u4e86\u2026\u2026",
    "\uff08\u770b\u7740\u7167\u7247\u7684\u57c3\u4e1d\u7279\u6cea\u6c34\u50cf\u6eaa\u6c34\u822c\u987a\u7740\u8138\u5e9e\u6d41\u6dcc\u4e0b\u6765\uff0c\u5979\u62fc\u547d\u7528\u8896\u53e3\u62ed\u53bb\u6cea\u6c34\uff0c\u8896\u53e3\u8fb9\u5df2\u7ecf\u5b8c\u5168\u88ab\u6cea\u6c34\u6d78\u900f\uff0c\u7136\u800c\u5979\u5374\u5b8c\u5168\u65e0\u6cd5\u63a7\u5236\u81ea\u5df1\u3002\uff09",
    "\u4e3a\u4ec0\u4e48\uff0c\u4e3a\u4ec0\u4e48\u4f1a\u53d8\u6210\u8fd9\u6837\uff01|\u6211\u53ea\u662f\u4e0d\u60f3\u505a\u574f\u4e8b\uff01\u6211\u53ea\u662f\u60f3\u6d3b\u7740\u56de\u5230\u4e4c\u624e\uff01",
    "\uff08\u57c3\u4e1d\u7279\u7528\u5c3d\u5168\u529b\u6293\u4f4f\u88ab\u5355\uff0c\u4f3c\u4e4e\u8981\u5c06\u81ea\u5df1\u6240\u6709\u7684\u59d4\u5c48\u548c\u6124\u61d1\u90fd\u53d1\u6cc4\u51fa\u6765\u3002\uff09",
    "\u6211\u4e5f\u2026\u2026\u6211\u4e5f\u5f88\u5bb3\u6015\u2026\u2026\u53ef\u662f\u53ea\u6709\u6211\u80fd\u633a\u8eab\u800c\u51fa\uff01|\u6211\u4eec\u4e0d\u80fd\u5e26\u7740\u522b\u4eba\u7684\u51a4\u9b42\u56de\u5bb6\uff01\u4e3a\u4ec0\u4e48\u5927\u5bb6\u5c31\u662f\u4e0d\u660e\u767d\u5462\uff01",
    "\uff08\u5979\u770b\u5411\u90a3\u5f20\u6cdb\u9ec4\u7684\u7167\u7247\uff0c\u4eff\u4f5b\u8981\u4ece\u7167\u7247\u4e2d\u627e\u5230\u80fd\u62ef\u6551\u81ea\u5df1\u7684\u7b54\u6848\u3002\uff09",
    "\u5988\u5988\u2026\u2026\u6211\u8be5\u600e\u4e48\u529e\u2026\u2026\u6211\u8be5\u600e\u4e48\u529e\u624d\u597d\u2026\u2026",
    "\uff08\u9edb\u70df\u8f7b\u8f7b\u5730\u62cd\u7740\u57c3\u4e1d\u7279\u7684\u540e\u80cc\uff0c\u5728\u5979\u8eab\u8fb9\u66ff\u5979\u7528\u7eb8\u5dfe\u64e6\u53bb\u6cea\u6c34\u3002\uff09",
    "\uff08\u4e0d\u77e5\u8fc7\u4e86\u591a\u4e45\uff0c\u57c3\u4e1d\u7279\u4ece\u5927\u54ed\u6162\u6162\u53d8\u6210\u4e86\u62bd\u6ce3\uff0c\u5979\u7ec8\u4e8e\u63a7\u5236\u4f4f\u4e86\u81ea\u5df1\u7684\u60c5\u7eea\u3002\uff09",
    "\uff08\u6211\u548c\u9edb\u70df\u5728\u5979\u8eab\u8fb9\u9ed8\u4e0d\u4f5c\u58f0\uff0c\u7b49\u5f85\u7740\u57c3\u4e1d\u7279\u81ea\u5df1\u5f00\u53e3\u7684\u90a3\u4e00\u523b\u3002\uff09",
    "\uff08\u57c3\u4e1d\u7279\u7684\u547c\u5438\u9010\u6e10\u5e73\u548c\u4e86\u4e0b\u6765\uff0c\u5979\u770b\u7740\u6211\u4eec\u7684\u8138\uff0c\u60f3\u8981\u8bf4\u8bdd\u4f46\u53c8\u6709\u4e9b\u72b9\u8c6b\u3002\uff09",
    "\uff08\u5979\u7684\u76ee\u5149\u5feb\u901f\u5728\u6211\u4eec\u8eab\u4e0a\u626b\u8fc7\uff0c\u6700\u7ec8\u8fd8\u662f\u505c\u7559\u5728\u624b\u4e2d\u7684\u7b14\u8bb0\u672c\u4e0a\u3002\uff09",
    "\u4f60\u2026\u2026\u4f60\u4eec\u770b\u8fc7\u7b14\u8bb0\u672c\u7684\u5185\u5bb9\u4e86\uff1f",
    "\u5bf9\u4e0d\u8d77\uff0c\u6211\u4eec\u7684\u786e\u64c5\u81ea\u7ffb\u770b\u4e86\u91cc\u9762\u7684\u5185\u5bb9\u3002|\u90a3\u91cc\u9762\uff0c\u6709\u88ab\u4f60\u5212\u6389\u4e86\u65e0\u6570\u6b21\u7684\u5b57\u773c\u2014\u2014\u201c\u590d\u4ec7\u8ba1\u5212\u201d\u3001\u201c\u706b\u836f\u201d\u3001\u201c\u7206\u7834\u70b9\u201d\u2026\u2026",
    "\u2026\u2026\u2026\u2026",
    "\u901a\u8fc7\u521a\u624d\u5728\u4ed3\u5e93\u91cc\uff0c\u60a8\u7684\u8a00\u884c\u2026\u2026\u60a8\u5b9e\u9645\u7684\u201c\u5584\u884c\u201d\u4e0e\u7b14\u8bb0\u672c\u4e2d\u7684\u201c\u6076\u5ff5\u201d\u5b8c\u5168\u4e0d\u7b26\u3002",
    "\u8981\u77e5\u9053\u6587\u5b57\u3001\u7ed8\u753b\u548c\u97f3\u4e50\u4e00\u6837\uff0c\u90fd\u80fd\u53cd\u6620\u521b\u4f5c\u8005\u7684\u5fc3\u5883\uff0c\u90a3\u4e9b\u53cd\u590d\u6d82\u62b9\u7684\u75d5\u8ff9\u4e5f\u4ee3\u8868\u7740\u60a8\u5e76\u4e0d\u8ba4\u540c\u201c\u590d\u4ec7\u8ba1\u5212\u201d\u5427\uff1f",
    "\u6211\u5f53\u7136\u4e0d\u8ba4\u540c\u2026\u2026\u6211\u600e\u4e48\u53ef\u80fd\u8ba4\u540c\uff01",
    "\uff08\u57c3\u4e1d\u7279\u518d\u6b21\u770b\u5411\u90a3\u5f20\u7167\u7247\uff0c\u5979\u6469\u6332\u7740\u7167\u7247\uff0c\u76ee\u5149\u67d4\u8f6f\u4e86\u4e0b\u6765\u3002\uff09",
    "\u7167\u7247\u4e0a\u7684\u8fd9\u4f4d\u662f\u60a8\u7684\u6bcd\u4eb2\u5427\uff1f",
    "\u55ef\u3002",
    "\u5979\u4e5f\u5728\u8fd9\u5ea7\u57ce\u5e02\u4e48\uff1f",
    "\u5979\u5df2\u7ecf\u6b7b\u4e86\u3002",
    "\u2026\u2026\u62b1\u6b49\uff0c\u8bf7\u8282\u54c0\u3002",
    "\uff08\u57c3\u4e1d\u7279\u6df1\u5438\u4e86\u4e00\u53e3\u6c14\uff0c\u5979\u7ec8\u4e8e\u9f13\u8d77\u52c7\u6c14\u62ac\u8d77\u5934\u6b63\u89c6\u6211\u548c\u9edb\u70df\u3002\uff09",
    "\u7167\u7247\u91cc\u7684\u662f\u6211\u6bcd\u4eb2\u3002|\u7b14\u8bb0\u672c\u91cc\uff0c\u524d\u534a\u90e8\u5206\u662f\u6211\u6bcd\u4eb2\u5199\u7684\u968f\u7b14\uff0c\u6211\u7684\u6bcd\u4eb2\uff0c\u5c31\u5728\u201c\u4e4c\u624e\u201d\u957f\u5927\uff0c\u548c\u7236\u4eb2\u7ed3\u8bc6\uff0c\u5e76\u751f\u4e0b\u4e86\u6211\u3002",
    "\u4e4c\u624e\uff0c\u662f\u6211\u7684\u6545\u4e61\uff0c\u5168\u540d\u662f\u201c\u4e4c\u624e\u9a6c\u5fb7\u5947\u201d\uff0c\u90a3\u4e2a\u8352\u829c\u4e4b\u5730\u539f\u672c\u8fde\u4e2a\u5730\u540d\u90fd\u6ca1\u6709\uff0c\u662f\u6211\u7684\u957f\u8f88\u4eec\u628a\u90a3\u91cc\u5f00\u62d3\u4e86\u51fa\u6765\uff0c\u5e76\u7ed9\u4e86\u5979\u540d\u5b57\u3002",
    "\u5728\u6211\u7684\u8bb0\u5fc6\u91cc\uff0c\u6545\u4e61\u662f\u4e00\u9897\u5728\u8352\u6f20\u91cc\u7684\u773c\u6cea\uff0c\u603b\u6709\u767d\u8272\u7684\u9a6c\u94c3\u85af\u82b1\u5728\u968f\u98ce\u6447\u66f3\u3002|\u800c\u6211\u6700\u719f\u6089\u7684\u6c14\u5473\uff0c\u4e5f\u662f\u9a6c\u94c3\u85af\u70e4\u719f\u540e\u7684\u9999\u6c14\uff0c\u90a3\u91cc\u6709\u7740\u6211\u7ae5\u5e74\u7684\u4e00\u5207\u3002",
    "\u4f46\u662f\u56e0\u4e3a\u51c0\u5316\u533a\u7684\u63a8\u8fdb\uff0c\u540e\u6765\u57c3\u4e1d\u7279\u5c0f\u59d0\u548c\u540c\u4e61\u4eec\u4e00\u8d77\u79bb\u5f00\u4e86\u4e4c\u624e\uff0c\u5bf9\u5417\uff1f",
    "\u5475\u2026\u2026\u5982\u679c\u4e0d\u662f\u88ab\u7f57\u8054\u5f3a\u8feb\uff0c\u8c01\u4f1a\u613f\u610f\u79bb\u5f00\u81ea\u5df1\u7684\u6545\u4e61\u5462\uff1f",
    "\u6211\u4eec\u7684\u957f\u8f88\u4eec\u7528\u4e86\u5341\u51e0\u5e74\u5f00\u62d3\u90a3\u4e2a\u5730\u65b9\uff0c\u5373\u4f7f\u540e\u6765\u73af\u5883\u4e00\u518d\u6076\u5316\uff0c\u5927\u5bb6\u4e5f\u8fd8\u662f\u613f\u610f\u7559\u5728\u90a3\u91cc\u3002",
    "\u4f46\u662f\uff0c\u957f\u57ce\u5217\u8f66\u6765\u4e86\u2026\u2026\u90a3\u4e9b\u94a2\u94c1\u602a\u7269\u7834\u574f\u4e86\u6211\u4eec\u7684\u519c\u7530\u3001\u7834\u574f\u4e86\u6211\u4eec\u7684\u623f\u5c4b\uff0c\u7834\u574f\u4e86\u6211\u4eec\u7684\u6545\u4e61\uff01",
    "\u7f57\u8054\u627f\u8bfa\u4f1a\u8ba9\u6211\u4eec\u8fdb\u5165\u7eff\u533a\u57ce\u5e02\u751f\u6d3b\uff0c\u4f46\u5927\u591a\u6570\u4eba\u8eab\u4e0a\u7684\u574d\u584c\u8f90\u5c04\u90fd\u8d85\u6807\uff0c\u6839\u672c\u6ca1\u8d44\u683c\u8fdb\u5165\u536b\u661f\u57ce\uff01",
    "\u6211\u4eec\u539f\u672c\u6709\u597d\u51e0\u767e\u4eba\uff0c\u6700\u540e\u80fd\u8fdb\u5230\u8fd9\u5ea7\u536b\u661f\u57ce\u91cc\uff0c\u53ea\u5269\u4e0b\u4e0d\u5230\u4e00\u767e\u4eba\uff01|\u5176\u4ed6\u4eba\u5462\uff0c\u4ed6\u4eec\u53bb\u54ea\u91cc\u4e86\uff1f\u6211\u4e0d\u77e5\u9053\uff0c\u6211\u4e5f\u6ca1\u529e\u6cd5\u77e5\u9053\u3002",
    "\u2026\u2026\u5bf9\u4e0d\u8d77\uff0c\u8ba9\u60a8\u60f3\u5230\u4e86\u96be\u8fc7\u7684\u4e8b\u60c5\u3002",
    "\u2026\u2026\u8fd9\u4e9b\u8bdd\uff0c\u6211\u5df2\u7ecf\u5728\u5fc3\u91cc\u618b\u5f88\u4e45\u4e86\u3002",
    "\u6211\u8fd8\u7b97\u5e78\u8fd0\uff0c\u6211\u7684\u6bcd\u4eb2\u662f\u8fd9\u4e0d\u5230\u4e00\u767e\u4eba\u4e2d\u7684\u4e00\u5458\u3002|\u6211\u4eec\u597d\u4e0d\u5bb9\u6613\u5728\u536b\u661f\u57ce\u5b89\u987f\u4e86\u4e0b\u6765\uff0c\u72ed\u5c0f\u7684\u7a7a\u95f4\uff1f\u4e25\u5bc6\u7684\u76d1\u63a7\uff1f|\u6ca1\u5173\u7cfb\uff0c\u53ea\u8981\u6bcd\u4eb2\u8fd8\u5728\uff0c\u53ea\u8981\u540c\u4e61\u7684\u4f19\u4f34\u4eec\u8fd8\u5728\uff0c\u6211\u5c31\u53ef\u4ee5\u575a\u6301\u4e0b\u53bb\u3002",
    "\u6211\u4eec\u5728\u8fd9\u5ea7\u57ce\u5e02\u4e2d\u7a7f\u68ad\u5fd9\u788c\u7740\uff0c\u8d54\u7740\u7b11\u8138\u3001\u770b\u7740\u90a3\u4e9b\u57ce\u91cc\u4eba\u7684\u773c\u8272\u8fc7\u6d3b\u3002|\u5728\u4ed6\u4eec\u773c\u91cc\uff0c\u6211\u4eec\u548c\u4eba\u5f62\u6ca1\u4ec0\u4e48\u533a\u522b\uff0c\u751a\u81f3\u4e0d\u5982\u4eba\u5f62\u597d\u7528\u2026\u2026",
    "\u4f46\u5c31\u7b97\u5982\u6b64\u6211\u4eec\u4e5f\u8981\u6d3b\u4e0b\u53bb\uff0c\u8981\u6709\u5c0a\u4e25\u5730\u6d3b\u4e0b\u53bb\uff0c\u6211\u4eec\u7ec4\u5efa\u4e86\u7531\u540c\u4e61\u7ec4\u6210\u7684\u5de5\u7a0b\u961f\uff0c\u505a\u8d77\u4e86\u5c5e\u4e8e\u6211\u4eec\u81ea\u5df1\u7684\u751f\u610f\u3002",
    "\u540e\u6765\u7531\u4e8e\u6211\u624b\u827a\u4e0a\u7684\u5929\u8d4b\uff0c\u6211\u5f00\u59cb\u88ab\u5927\u5bb6\u63a8\u4e3e\u9886\u5bfc\u8d77\u540c\u4e61\u4eec\u7ec4\u6210\u7684\u5de5\u7a0b\u961f\u3002|\u6162\u6162\u7684\uff0c\u751f\u610f\u6709\u4e86\u8d77\u8272\uff0c\u5927\u5bb6\u7ec8\u4e8e\u80fd\u5403\u70b9\u597d\u7684\u4e86\u3002",
    "\u4f46\u662f\uff0c\u5728\u4e4b\u524d\u65e5\u590d\u4e00\u65e5\u7684\u5de5\u4f5c\u4e2d\uff0c\u6211\u7684\u6bcd\u4eb2\u62d6\u57ae\u4e86\u8eab\u5b50\uff0c\u6b7b\u4e8e\u8f90\u5c04\u75c7\u3002",
    "\u8fd9\u662f\u538b\u57ae\u6211\u7684\u6700\u540e\u4e00\u6839\u7a3b\u8349\u3002",
    "\uff08\u57c3\u4e1d\u7279\u54ac\u4e86\u54ac\u7259\uff0c\u88ab\u5355\u518d\u6b21\u5728\u5979\u7684\u624b\u4e2d\u88ab\u634f\u6210\u4e00\u56e2\u3002\uff09",
    "\u7136\u540e\u90a3\u4e2a\u5bb6\u4f19\u2026\u2026\u90a3\u4e2a\u53eb\u5f17\u62c9\u683c\u7684\u5bb6\u4f19\u5c31\u627e\u4e0a\u4e86\u6211\u3002|\u8bf4\u4ec0\u4e48\u4ed6\u4e5f\u662f\u4e4c\u624e\u51fa\u8eab\uff0c\u4ed6\u548c\u6211\u8bf4\u4e86\u5f88\u591a\u7f57\u8054\u505a\u8fc7\u7684\u5351\u9119\u65e0\u803b\u7684\u4e8b\uff0c\u6211\u5f53\u65f6\u4e00\u65f6\u6c14\u6124\u5c31\u7b54\u5e94\u4e86\u4ed6\u7684\u8981\u6c42\u3002",
    "\u4ed6\u627f\u8bfa\u4e8b\u6210\u540e\u4f1a\u7ed9\u6211\u4eec\u4e00\u5927\u7b14\u94b1\u3002|\u6211\u5f53\u65f6\u6ca1\u591a\u60f3\uff0c\u5c31\u53ec\u96c6\u4e86\u6211\u7684\u540c\u4e61\u4eec\u5f00\u59cb\u90e8\u7f72\u5728\u5e86\u5178\u4e0a\u5f15\u7206\u70b8\u5f39\u8fd9\u4ef6\u4e8b\u3002",
    "\u4f46\u662f\u4e8b\u540e\u6211\u8d8a\u60f3\u8d8a\u89c9\u5f97\u4e0d\u5bf9\u52b2\u3002|\u4ed6\u81ea\u79f0\u662f\u4e4c\u624e\u4eba\uff0c\u4f46\u6211\u8eab\u8fb9\u6ca1\u6709\u4e00\u4e2a\u4eba\u4ee5\u524d\u8ba4\u8bc6\u4ed6\uff0c\u66f4\u4f55\u51b5\u90a3\u4e48\u591a\u70b8\u836f\u548c\u94b1\u4e0d\u53ef\u80fd\u662f\u4e4c\u624e\u4eba\u80fd\u641e\u5f97\u5230\u7684\u3002",
    "\u6240\u4ee5\u6211\u5c31\u60f3\u963b\u6b62\u4ed6\u4eec\u7684\u8ba1\u5212\uff0c\u6ca1\u60f3\u5230\u4e00\u5f00\u59cb\u884c\u52a8\uff0c\u90a3\u4e2a\u5f17\u62c9\u683c\u5c31\u53c8\u51fa\u73b0\u4e86\uff0c\u53ea\u4e0d\u8fc7\u8fd9\u6b21\u51fa\u73b0\u662f\u4e3a\u4e86\u8ffd\u6740\u6211\u3002",
    "\u6211\u672c\u6765\u60f3\u9003\u5230\u6211\u4eec\u4ee5\u524d\u7684\u79d8\u5bc6\u907f\u96be\u6240\uff0c\u8ba9\u540c\u4e61\u4eec\u914d\u5408\u6211\u4e00\u8d77\u963b\u6b62\u7206\u70b8\u3002|\u4f46\u521a\u624d\u7684\u60c5\u51b5\u4f60\u4e5f\u770b\u5230\u4e86\uff0c\u90a3\u4e2a\u5f17\u62c9\u683c\u7684\u4eba\u65e9\u5c31\u6e17\u900f\u8fdb\u6765\u4e86\uff0c\u6211\u6839\u672c\u5c31\u963b\u6b62\u4e0d\u4e86\u4ed6\u4eec\u5728\u5e86\u5178\u4e0a\u5f15\u53d1\u7206\u70b8\u3002",
    "\u540e\u6765\u7684\u4e8b\u4f60\u4eec\u4e5f\u77e5\u9053\u4e86\uff0c\u6211\u7684\u540c\u4e61\u4eec\u5df2\u7ecf\u5f7b\u5e95\u88ab\u4ec7\u6068\u8499\u853d\u4e86\u53cc\u773c\uff0c\u4e0d\u7ba1\u6211\u8bf4\u4ec0\u4e48\u90fd\u5df2\u7ecf\u6ca1\u6709\u7528\u4e86\u3002|\u7136\u540e\u2026\u2026\u6211\u5c31\u88ab\u4f60\u4eec\u5e26\u5230\u8fd9\u4e2a\u5730\u65b9\u6765\u4e86\u3002",
    "\uff08\u57c3\u4e1d\u7279\u8bf4\u5b8c\u4e86\u81ea\u5df1\u7684\u6545\u4e8b\uff0c\u5979\u770b\u5411\u6211\uff0c\u8138\u4e0a\u9732\u51fa\u590d\u6742\u7684\u795e\u60c5\u3002\uff09",
    "\u6551\u6211\u51fa\u6765\u7684\u4e8b\u662f\u4f60\u7684\u4e3b\u610f\u5427\uff1f\u8fd9\u4e2a\u4eba\u5f62\u662f\u4f60\u7684\u624b\u4e0b\uff1f",
    "\uff08\u57c3\u4e1d\u7279\u6307\u4e86\u6307\u9edb\u70df\u3002\uff09",
    "\u6211\u542c\u5979\u53eb\u4f60\u957f\u5b98\u3002",
    "\u7b97\u662f\u5427\uff0c\u4f60\u53ef\u4ee5\u53eb\u6211\u6307\u6325\u5b98\uff0c\u5979\u7684\u8bdd\u2026\u2026\u53eb\u5979\u9edb\u70df\u5c31\u884c\u3002",
    "\uff08\u57c3\u4e1d\u7279\u770b\u5411\u9edb\u70df\uff0c\u9edb\u70df\u5fae\u7b11\u5730\u5411\u5979\u70b9\u5934\u793a\u610f\u3002\uff09",
    "\u6307\u6325\u5b98\u2026\u2026\u548c\u9edb\u70df\u3002",
    "\uff08\u57c3\u4e1d\u7279\u9ed8\u5ff5\u4e86\u51e0\u58f0\u6211\u4eec\u7684\u540d\u5b57\uff0c\u62ac\u8d77\u5934\u5411\u6211\u63d0\u95ee\u3002\uff09",
    "\u4f60\u4eec\u4e0d\u662f\u5f17\u62c9\u683c\u7684\u4eba\u5427\uff1f",
    "\u4f60\u89c9\u5f97\u5462\uff1f",
    "\u80af\u5b9a\u4e0d\u662f\uff0c\u4e0d\u7136\u6211\u65e9\u5c31\u5df2\u7ecf\u6b7b\u4e86\u3002|\u4f46\u4f60\u4eec\u770b\u8d77\u6765\u4e5f\u4e0d\u50cf\u662f\u6cbb\u5b89\u90e8\u95e8\u7684\u4eba\u2026\u2026",
    "\u6211\u4eec\u53ea\u662f\u70ed\u5fc3\u7684\u597d\u5e02\u6c11\uff0c\u770b\u5230\u4f60\u9047\u5230\u4e86\u5371\u9669\u6240\u4ee5\u51fa\u624b\u5e2e\u4e86\u4f60\u4e00\u628a\u800c\u5df2\u3002",
    "\u522b\u9a97\u6211\u4e86\uff0c\u597d\u5e02\u6c11\u600e\u4e48\u4f1a\u6709\u6218\u672f\u4eba\u5f62\uff1f",
    "\u90a3\u53ef\u4e0d\u597d\u8bf4\uff0c\u5de5\u7a0b\u5e08\u8fd8\u5728\u5236\u9020\u70b8\u5f39\u5462\u3002",
    "\uff08\u542c\u5230\u70b8\u5f39\u8fd9\u4e2a\u8bcd\uff0c\u57c3\u4e1d\u7279\u7684\u7709\u5934\u6311\u4e86\u4e00\u4e0b\uff0c\u5979\u77e5\u9053\u81ea\u5df1\u7406\u4e8f\uff0c\u4e0d\u518d\u4f5c\u58f0\u3002\uff09",
    "\u4e0d\u5f00\u73a9\u7b11\u4e86\u3002|\u6211\u4eec\u53ea\u662f\u60f3\u4e86\u89e3\u4e00\u4e9b\u4f60\u521a\u624d\u8bf4\u7684\uff0c\u5f17\u62c9\u683c\u8fd8\u6709\u548c\u4ed6\u6709\u5173\u7684\u4fe1\u606f\u3002|\u53ea\u8981\u4f60\u628a\u4f60\u6240\u77e5\u9053\u7684\u90fd\u544a\u8bc9\u6211\u4eec\uff0c\u6211\u4eec\u53ef\u4ee5\u786e\u4fdd\u4f60\u7684\u5b89\u5168\u3002",
    "\u65e2\u7136\u4f60\u4eec\u4e0d\u662f\u6cbb\u5b89\u90e8\u95e8\u7684\u4eba\uff0c\u4e3a\u4ec0\u4e48\u8981\u6551\u6211\uff1f|\u8fd8\u662f\u8bf4\u4f60\u6709\u4ec0\u4e48\u522b\u7684\u76ee\u7684\uff1f",
    "\u6211\u53ea\u662f\u4e2a\u968f\u5904\u53ef\u89c1\u7684\u8d4f\u91d1\u730e\u4eba\u3002|\u4e4b\u6240\u4ee5\u63ba\u548c\u8fd9\u4ef6\u4e8b\uff0c\u662f\u56e0\u4e3a\u90a3\u4e2a\u5f17\u62c9\u683c\u80cc\u540e\u7684\u96c6\u56e2\u5728\u915d\u917f\u7740\u4e00\u4e2a\u5de8\u5927\u7684\u9634\u8c0b\uff0c\u800c\u6211\u53d7\u96c7\u6765\u8c03\u67e5\u8fd9\u4e00\u7cfb\u5217\u7684\u6848\u4ef6\u3002",
    "\u8d4f\u91d1\u730e\u4eba\uff1f",
    "\u6ca1\u9519\uff0c\u666e\u666e\u901a\u901a\u7684\u8d4f\u91d1\u730e\u4eba\u3002|\u81f3\u4e8e\u4e3a\u4ec0\u4e48\u6551\u4f60\uff0c\u600e\u4e48\u8bf4\u5462\u2026\u2026\u53ef\u80fd\u662f\u56e0\u4e3a\u770b\u4e0d\u60ef\u5427\u3002",
    "\u4ec0\u4e48\u2026\u2026\u610f\u601d\uff1f",
    "\u56e0\u4e3a\u6211\u770b\u4e0d\u60ef\u8fd9\u4e9b\u4eba\u7684\u4f0e\u4fe9\uff0c\u4f60\u88ab\u4ed6\u4eec\u5229\u7528\u4e86\uff0c\u4f60\u548c\u4f60\u7684\u90a3\u4e9b\u540c\u4e61\u90fd\u662f\u3002|\u4ed6\u4eec\u5229\u7528\u4f60\u4eec\u5bf9\u7f57\u8054\u7684\u4ec7\u6068\u6765\u4e3a\u4ed6\u4eec\u83b7\u53d6\u80ae\u810f\u7684\u5229\u76ca\uff0c\u501f\u7740\u4f60\u4eec\u7684\u53cc\u624b\u6765\u7834\u574f\u73b0\u5728\u6765\u4e4b\u4e0d\u6613\u7684\u548c\u5e73\u3002",
    "\u5f15\u53d1\u4ec7\u6068\u662f\u6700\u5bb9\u6613\u9a71\u4f7f\u4eba\u7684\u624b\u6bb5\uff0c\u5c06\u4f60\u4eec\u5185\u5fc3\u4e2d\u7684\u6076\u4e4b\u706b\u70b9\u71c3\uff0c\u4f60\u4eec\u5c31\u4f1a\u5728\u4e0d\u81ea\u89c9\u4e2d\u4f9b\u4ed6\u4eec\u4e3a\u6240\u6b32\u4e3a\u3002",
    "\u53ef\u662f\u7f57\u8054\u7684\u786e\u6bc1\u706d\u4e86\u6211\u4eec\u7684\u6545\u4e61\uff0c\u90a3\u4e9b\u5b98\u5458\u5c31\u8fd9\u6837\u968f\u610f\u5730\u628a\u6211\u4eec\u8d76\u51fa\u4e86\u6545\u4e61\uff01|\u6211\u4eec\u4ec0\u4e48\u90fd\u505a\u4e0d\u4e86\uff0c\u53ea\u80fd\u773c\u7741\u7741\u770b\u7740\u6211\u4eec\u7684\u5bb6\u4e61\u88ab\u94f2\u4e3a\u5e73\u5730\uff01",
    "\u5982\u679c\u7f57\u8054\u771f\u7684\u548c\u5b83\u7684\u53e3\u53f7\u558a\u7684\u4e00\u6837\uff0c\u662f\u4e3a\u4e86\u6539\u53d8\u8fd9\u4e2a\u4e16\u754c\uff0c\u90a3\u4e3a\u4ec0\u4e48\u4ee3\u4ef7\u5c31\u8981\u6211\u4eec\u8fd9\u4e9b\u666e\u901a\u4eba\u6765\u627f\u62c5\u5462\uff1f",
    "\u90a3\u662f\u56e0\u4e3a\u2026\u2026",
    "\uff08\u6211\u4e00\u65f6\u8bed\u585e\uff0c\u6211\u7adf\u627e\u4e0d\u5230\u4efb\u4f55\u7406\u7531\u4e3a\u7f57\u8054\u8fa9\u62a4\u3002\u6211\u4e5f\u66fe\u7ecf\u60f3\u6539\u53d8\u4e9b\u4ec0\u4e48\uff0c\u4f46\u90a3\u65f6\u7684\u6211\u7ec8\u7a76\u4ec0\u4e48\u90fd\u6ca1\u505a\u5230\u3002\uff09",
    "\uff08\u8fd9\u6837\u7684\u6211\uff0c\u771f\u7684\u6709\u8d44\u683c\u53bb\u5bf9\u773c\u524d\u8fd9\u4f4d\u5973\u5b50\u8bf4\u6559\u5417\uff1f\uff09",
    "\u4f46\u662f\u2026\u2026\u5373\u4f7f\u5982\u6b64\uff0c\u6211\u4e5f\u4e0d\u60f3\u770b\u5230\u522b\u7684\u666e\u901a\u4eba\u56e0\u4e3a\u6211\u4eec\u6240\u8c13\u7684\u590d\u4ec7\u800c\u6b7b\u3002|\u593a\u8d70\u5c5e\u4e8e\u522b\u4eba\u7684\u5e78\u798f\u2026\u2026\u5982\u679c\u53d1\u751f\u8fd9\u6837\u7684\u4e8b\uff0c\u6211\u4eec\u7684\u6240\u4f5c\u6240\u4e3a\u548c\u7f57\u8054\u53c8\u6709\u4ec0\u4e48\u533a\u522b\u5462\uff1f",
    "\u5982\u679c\u5927\u5bb6\u90fd\u80fd\u50cf\u60a8\u8fd9\u6837\u51b7\u9759\u4e0b\u6765\u5c31\u597d\u4e86\u3002|\u6211\u60f3\u4ec7\u6068\u53ea\u4f1a\u5e26\u6765\u65b0\u7684\u4ec7\u6068\uff0c\u60f3\u8981\u8d70\u51fa\u4ee5\u5f80\u7684\u56f0\u5883\uff0c\u6211\u4eec\u5e94\u8be5\u7528\u66f4\u597d\u7684\u65b9\u5f0f\u53bb\u6539\u53d8\u4e16\u754c\u3002",
    "\u6ca1\u9519\u5427\uff0c\u957f\u5b98\uff1f",
    "\uff08\u9edb\u70df\u6df1\u60c5\u5730\u770b\u7740\u6211\uff0c\u6211\u77e5\u9053\u5979\u7684\u8bdd\u8bed\u4e0d\u4ec5\u4ec5\u53ea\u662f\u8bf4\u7ed9\u57c3\u4e1d\u7279\u542c\u3002\u6211\u770b\u7740\u5979\u7684\u53cc\u7738\uff0c\u671d\u5979\u9732\u51fa\u4e86\u82e6\u6da9\u7684\u5fae\u7b11\u3002\uff09",
    "\u662f\u554a\u2026\u2026\u867d\u7136\u5931\u53bb\u4e86\u4e00\u5207\u6240\u80fd\u5931\u53bb\u7684\uff0c\u4f46\u53ea\u8981\u8fd8\u80fd\u6d3b\u4e0b\u53bb\u5c31\u80fd\u6709\u65b0\u7684\u5f00\u59cb\u3002",
    "\u6211\u4e5f\u60f3\u6d3b\u4e0b\u53bb\u2026\u2026\u53ef\u662f\u4e0d\u7ba1\u6211\u53bb\u7834\u574f\u70b8\u5f39\uff0c\u8fd8\u662f\u52a8\u5458\u540c\u4e61\u90fd\u5931\u8d25\u4e86\u3002|\u6211\u4e4b\u524d\u8fd8\u8bd5\u56fe\u62a5\u8fc7\u8b66\uff0c\u4f46\u662f\u6cbb\u5b89\u90e8\u95e8\u7684\u4eba\u6839\u672c\u5c31\u4e0d\u7406\u6211\uff0c\u6211\u731c\u90a3\u91cc\u4e5f\u65e9\u5c31\u88ab\u5f17\u62c9\u683c\u4e70\u901a\u4e86\u3002",
    "\u5f17\u62c9\u683c\u80cc\u540e\u7684\u96c6\u56e2\u6709\u7740\u8fd9\u5ea7\u536b\u661f\u57ce\u6cbb\u5b89\u90e8\u95e8\u4e5f\u60f9\u4e0d\u8d77\u7684\u52bf\u529b\uff0c\u6240\u4ee5\u4ed6\u4eec\u4e0d\u6562\u8f7b\u4e3e\u5984\u52a8\u3002|\u4f46\u662f\u603b\u8981\u6709\u4eba\u7ad9\u51fa\u6765\u5236\u88c1\u4ed6\u4eec\uff0c\u5c31\u50cf\u4f60\u5728\u90a3\u4e48\u5371\u9669\u7684\u60c5\u51b5\u4e0b\u8fd8\u9009\u62e9\u7ad9\u51fa\u6765\u4e00\u6837\u3002",
    "\u5c31\u50cf\u6211\u2026\u2026",
    "\uff08\u57c3\u4e1d\u7279\u7565\u52a0\u601d\u8003\uff0c\u5979\u7684\u773c\u795e\u53d8\u5f97\u6108\u53d1\u575a\u5b9a\u3002\uff09",
    "\u4f60\u662f\u8981\u963b\u6b62\u5f17\u62c9\u683c\uff0c\u8981\u963b\u6b62\u5e86\u5178\u4e0a\u7684\u7206\u70b8\u53d1\u751f\u5bf9\u5427\uff1f",
    "\u6ca1\u9519\uff0c\u6700\u597d\u8fd8\u80fd\u6293\u5230\u4ed6\u3002",
    "\u867d\u7136\u6211\u4e0d\u89c9\u5f97\u6211\u80fd\u9003\u8131\u6cd5\u5f8b\u7684\u5236\u88c1\uff0c\u4f46\u81f3\u5c11\uff0c\u6211\u60f3\u8ba9\u6211\u548c\u6211\u7684\u540c\u4e61\u4eec\u4e0d\u8981\u72af\u4e0b\u4e0d\u53ef\u9976\u6055\u7684\u7f6a\u884c\u3002|\u6211\u53ef\u4ee5\u5c06\u6211\u6240\u77e5\u9053\u7684\u4e00\u5207\u544a\u8bc9\u4f60\uff0c\u4f60\u80fd\u5e2e\u52a9\u6211\u963b\u6b62\u90a3\u4e2a\u5f17\u62c9\u683c\u8981\u8fdb\u884c\u7684\u8ba1\u5212\u5417\uff1f",
    "\u5982\u679c\u4f60\u613f\u610f\u534f\u52a9\u6211\u4eec\u7684\u8bdd\u3002|\u4e0d\u8fc7\u5176\u4e2d\u7684\u5371\u9669\u6027\u2026\u2026\u6211\u5c31\u4e0d\u7528\u5f3a\u8c03\u4e86\u5427\uff1f",
    "\u6211\u4e0d\u6015\u5371\u9669\uff01\u6211\u4e0d\u80fd\u53ea\u5f85\u5728\u8fd9\u91cc\uff0c\u8bf7\u4f60\u5e26\u4e0a\u6211\uff0c\u53ea\u6709\u6211\u6700\u6e05\u695a\u90a3\u4e9b\u70b8\u5f39\u7684\u4f4d\u7f6e\u548c\u90e8\u7f72\u65b9\u5f0f\uff01",
    "\uff08\u6211\u548c\u9edb\u70df\u5bf9\u89c6\u4e86\u4e00\u773c\uff0c\u6211\u77e5\u9053\u6211\u4eec\u90fd\u5df2\u7ecf\u4ece\u5185\u5fc3\u91cc\u8ba4\u53ef\u4e86\u57c3\u4e1d\u7279\u3002\u9edb\u70df\u968f\u5373\u6400\u6276\u57c3\u4e1d\u7279\u4e0b\u5e8a\u3002\uff09",
    "\u6211\u4eec\u884c\u52a8\u8d77\u6765\u5427\u57c3\u4e1d\u7279\uff0c\u65f6\u95f4\u5df2\u7ecf\u4e0d\u591a\u4e86\u3002"
  ]
]