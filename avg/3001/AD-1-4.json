[
  [
    "\uff08\u4e00\u4e2a\u5c0f\u65f6\u524d\u3002\uff09",
    "\uff08\u521a\u7ecf\u5386\u8fc7\u4e00\u573a\u77ed\u65f6\u4eba\u5de5\u964d\u96e8\uff0c\u5496\u5561\u9986\u95e8\u524d\u7684\u77f3\u677f\u8def\u88ab\u51b2\u5237\u5f97\u5341\u5206\u5e72\u51c0\u3002\uff09",
    "\uff08\u590f\u514b\u91cc\u5938\u5f20\u5730\u6df1\u5438\u4e00\u53e3\u6c14\uff0c\u51b2\u7740\u591c\u8272\u4f38\u4e86\u4e2a\u5927\u5927\u7684\u61d2\u8170\u3002\uff09",
    "\u547c\u554a\u2014\u2014\u7e41\u5fd9\u7684\u4e00\u5929\u7ed3\u675f\u4e86\uff0c\u4eca\u5929\u7684\u6218\u7ee9\u4e5f\u5341\u5206\u4e0d\u9519\u5462\u2665|\u5982\u679c\u6ca1\u6709\u508d\u665a\u90a3\u4e2a\u5c0f\u63d2\u66f2\u5c31\u597d\u4e86\u2026\u2026",
    "\uff08\u590f\u514b\u91cc\u4f38\u624b\u628a\u95e8\u53e3\u5199\u7740\u62db\u724c\u996e\u54c1\u7684\u8367\u5149\u706f\u724c\u7535\u6e90\u5173\u6389\uff0c\u51c6\u5907\u642c\u8fdb\u5e97\u5185\u3002\uff09",
    "\uff08\u5728\u5979\u884c\u52a8\u7684\u8fc7\u7a0b\u4e2d\uff0c\u7565\u663e\u6c89\u91cd\u7684\u811a\u6b65\u58f0\u4ece\u5979\u8eab\u540e\u4f20\u6765\u3002\uff09",
    "\u4e0d\u597d\u610f\u601d\u5462\uff0c\u8fd9\u4f4d\u5ba2\u4eba\uff0c\u5c0f\u5e97\u4eca\u5929\u5df2\u7ecf\u6253\u70ca\u4e86\u54e6\u2665|\u660e\u5929\u8bf7\u65e9\u2026\u2026",
    "\u8fd9\u91cc\u2026\u2026\u5c31\u662f\u201c\u65b9\u7cd6\u201d\u5496\u5561\u9986\u5417\uff1f",
    "\uff08\u590f\u514b\u91cc\u628a\u62db\u724c\u653e\u4e0b\uff0c\u4e0a\u4e0b\u6253\u91cf\u4e86\u6211\u4e00\u756a\uff0c\u5f97\u51fa\u4e86\u81ea\u5df1\u7684\u5224\u65ad\u3002\uff09",
    "\u8fd9\u4f4d\u5ba2\u4eba\u2026\u2026\u60a8\u4e0d\u662f\u6765\u559d\u5496\u5561\u7684\u5427\uff1f",
    "\u771f\u662f\u4e2a\u806a\u660e\u7684\u4eba\u5f62\uff0c\u6625\u7530\u5c0f\u59d0\u5728\u5417\uff1f",
    "\u539f\u6765\u662f\u627e\u5e97\u957f\u5440\uff0c\u8bf7\u5f80\u91cc\u9762\u6765~",
    "\uff08\u8ddf\u968f\u7740\u8fd9\u540d\u4e2a\u5b50\u4e0d\u9ad8\u7684\u4eba\u5f62\uff0c\u6211\u8d70\u8fdb\u4e86\u8fd9\u5bb6\u88c5\u4fee\u4e0d\u7b97\u6c14\u6d3e\u4f46\u5341\u5206\u6e29\u99a8\u7684\u5496\u5561\u9986\u3002\uff09",
    "\uff08\u5e97\u5185\u5e03\u5c40\u9887\u50cf\u66fe\u7ecf\u683c\u91cc\u82ac\u57fa\u5730\u91cc\u7684\u5496\u5561\u5385\uff0c\u8ba9\u4eba\u4eff\u4f5b\u56de\u5230\u4e86\u8fc7\u53bb\u7684\u65f6\u5149\u3002\uff09",
    "\uff08\u5c31\u5728\u6211\u7684\u76ee\u5149\u8fd8\u6e38\u8d70\u5728\u5e97\u5185\u65f6\uff0c\u6625\u7530\u5feb\u6b65\u4ece\u540e\u53a8\u8d70\u4e86\u51fa\u6765\u3002\uff09",
    "\u6307\u6325\u5b98\uff1f",
    "\uff08\u5979\u65e9\u5df2\u6536\u5230\u6211\u4f1a\u524d\u6765\u7684\u6d88\u606f\uff0c\u4e0d\u8fc7\u8138\u4e0a\u8fd8\u662f\u9732\u51fa\u6b23\u559c\u7684\u795e\u8272\u3002\u867d\u7136\u8fc7\u53bb\u4e86\u5341\u5e74\uff0c\u4f46\u65e0\u8bba\u662f\u8863\u7740\u8fd8\u662f\u6c14\u8d28\uff0c\u548c\u5f53\u5e74\u76f8\u6bd4\u6ca1\u6709\u4e1d\u6beb\u4e0d\u540c\u3002\uff09",
    "\uff08\u5979\u5e26\u7740\u6211\u8d70\u5230\u4e86\u4e00\u4e2a\u4e34\u7a97\u7684\u5ea7\u4f4d\u524d\uff0c\u671d\u7a97\u5916\u770b\u53bb\uff0c\u8857\u4e0a\u7684\u9713\u8679\u7167\u5c04\u7740\u536b\u661f\u57ce\u7684\u9ad8\u697c\u548c\u9694\u79bb\u5899\u3002\uff09",
    "\uff08\u6625\u7530\u771f\u662f\u627e\u4e86\u4e00\u4e2a\u597d\u5730\u65b9\u5462\uff0c\u6211\u5fc3\u91cc\u8fd9\u6837\u60f3\u7740\u3002\u968f\u540e\uff0c\u5979\u7684\u8bdd\u8bed\u5c06\u6211\u62c9\u56de\u4e86\u73b0\u5b9e\u3002\uff09",
    "\u6307\u6325\u5b98\uff0c\u8bf7\u5750\u5427\u3002",
    "\u55ef\uff0c\u8c22\u8c22\u3002",
    "\uff08\u6211\u548c\u6625\u7530\u62c9\u5f00\u6905\u5b50\u5750\u5230\u4e86\u684c\u524d\uff0c\u800c\u590f\u514b\u91cc\u50cf\u77a7\u51c6\u65f6\u673a\u822c\u5730\u62b1\u7740\u83dc\u5355\u51b2\u4e86\u8fc7\u6765\u3002\uff09",
    "\u8fd9\u4f4d\u5ba2\u4eba\uff0c\u590f\u514b\u91cc\u53ef\u4ee5\u53eb\u60a8\u6307\u6325\u5b98\u5bf9\u5427\uff1f\u65e2\u7136\u90fd\u6765\u5e97\u91cc\u4e86\uff0c\u6307\u6325\u5b98\u4e0d\u5982\u6765\u4e00\u676f\u8d85\u7ea7\u7126\u7cd6\u739b\u5947\u6735\u5427\uff1f\u8fd9\u53ef\u662f\u6211\u4eec\u5e97\u91cc\u7684\u62f3\u5934\u4ea7\u54c1\u54e6\uff01",
    "\u5982\u679c\u60a8\u4e0d\u70b9\u7684\u8bdd\u2026\u2026\u8fd9\u4e2a\u6708\u7684\u8425\u4e1a\u989d\u5c31\u4e0d\u8fbe\u6807\u4e86\u2026\u2026\u6211\u4eec\u8fd9\u5bb6\u5e97\u53ef\u662f\u7a77\u5f97\u8981\u5012\u95ed\u4e86\u5462\u2026\u2026",
    "\u6307\u6325\u5b98\uff0c\u60a8\u4e0d\u5fcd\u5fc3\u770b\u5230\u8fd9\u6837\u7684\u60c5\u51b5\u51fa\u73b0\u5427\uff1f",
    "\uff08\u590f\u514b\u91cc\u9732\u51fa\u4e86\u4e00\u526f\u8981\u54ed\u7684\u8868\u60c5\uff0c\u867d\u7136\u6211\u5df2\u7ecf\u5bf9\u4eba\u5f62\u8fd9\u6837\u7684\u8868\u6f14\u4e60\u4ee5\u4e3a\u5e38\uff0c\u4f46\u662f\u73b0\u5728\u8fd8\u662f\u8bc6\u76f8\u5730\u914d\u5408\u4e00\u4e0b\u5979\u5427\u3002\uff09",
    "\u5443\u2026\u2026\u55ef\uff0c\u90a3\u5c31\u9ebb\u70e6\u6765\u4e24\u676f\u5427\u3002",
    "\u611f\u8c22\u4e0b\u5355~\u590f\u514b\u91cc\u73b0\u5728\u5c31\u53bb\u505a\uff01",
    "\u600e\u4e48\u80fd\u8ba9\u6307\u6325\u5b98\u4ed8\u94b1\u5462\uff01\u590f\u514b\u91cc\u2014\u2014",
    "\u5e97\u957f\uff0c\u4eba\u5bb6\u4e5f\u662f\u4e3a\u4e86\u5496\u5561\u9986\u8003\u8651\u7684\u2026\u2026",
    "\u800c\u4e14\uff0c\u5e97\u957f\u5230\u73b0\u5728\u90fd\u6ca1\u6709\u6b63\u5f0f\u4ecb\u7ecd\u590f\u514b\u91cc\u5462\u3002",
    "\uff08\u6625\u7530\u4e00\u8138\u62b1\u6b49\u5730\u770b\u5411\u6211\uff0c\u65e0\u5948\u5730\u53f9\u4e86\u53e3\u6c14\u3002\uff09",
    "\u4ecb\u7ecd\u4e00\u4e0b\uff0c\u8fd9\u4f4d\u590f\u514b\u91cc\u662f\u6211\u5e97\u91cc\u7684\u9a7b\u573a\u5076\u50cf\u3002",
    "\u771f\u662f\u4e2a\u53ef\u7231\u7684\u5076\u50cf\u3002\u6709\u4f60\u5728\u60f3\u5fc5\u5e97\u91cc\u4f1a\u591a\u5f88\u591a\u751f\u610f\u5427\uff1f",
    "\u90a3\u662f\u81ea\u7136\uff01\u6709\u8d85\u7ea7\u5076\u50cf\u590f\u514b\u91cc\u5728\uff0c\u5927\u5bb6\u7684\u5fc3\u90fd\u4f1a\u88ab\u878d\u5316\u54e6\u2665",
    "\uff08\u590f\u514b\u91cc\u505a\u4e86\u4e00\u4e2a\u4e00\u770b\u5c31\u7ec3\u4e60\u4e86\u5f88\u4e45\u7684\u62db\u724c\u59ff\u52bf\uff0c\u81ea\u79f0\u5076\u50cf\u662f\u73b0\u5728\u4eba\u5f62\u5f88\u6d41\u884c\u7684\u98ce\u5c1a\u5417\uff1f\uff09",
    "\u60a8\u4e0d\u8981\u4ecb\u610f\uff0c\u5979\u5c31\u662f\u8fd9\u6837\u7684\u6027\u683c\u3002",
    "\u6ca1\u4e8b\uff0c\u5df2\u7ecf\u4e60\u60ef\u4e86\uff0c\u5728\u6211\u7684\u8f66\u4e0a\u4e5f\u6709\u8fd9\u6837\u7684\u4eba\u5f62\u3002",
    "\u662f\u8fd9\u6837\u4e48\uff1f\u4e0d\u6127\u662f\u6307\u6325\u5b98\u5462\u2026\u2026",
    "\uff08\u6625\u7530\u82e5\u6709\u6240\u601d\u5730\u70b9\u4e86\u70b9\u5934\uff0c\u8f6c\u800c\u770b\u5411\u4e86\u590f\u514b\u91cc\u3002\uff09",
    "\u590f\u514b\u91cc\uff0c\u8d76\u7d27\u53bb\u5e2e\u6307\u6325\u5b98\u5236\u4f5c\u5496\u5561\u5427\uff1f",
    "\u597d\uff0c\u7acb\u5373\u51fa\u53d1\uff01",
    "\uff08\u590f\u514b\u91cc\u4e00\u6e9c\u70df\u8dd1\u5230\u4e86\u540e\u53a8\uff0c\u6211\u73af\u987e\u56db\u5468\uff0c\u5427\u53f0\u524d\u5370\u7740\u8fd9\u95f4\u5496\u5561\u9986\u7684\u5e97\u540d\u2014\u2014\u201c\u65b9\u7cd6\u201d\u3002\uff09",
    "\u201c\u65b9\u7cd6\u201d\u5496\u5561\u9986\uff0c\u6625\u7530\uff0c\u4f60\u7684\u68a6\u60f3\u5df2\u7ecf\u5b9e\u73b0\u4e86\u5462\u3002",
    "\u6258\u5927\u5bb6\u7684\u798f\uff0c\u6211\u7ec8\u4e8e\u8fc8\u51fa\u4e86\u5c0f\u5c0f\u7684\u4e00\u6b65\u3002",
    "\u771f\u662f\u95f4\u98ce\u683c\u4e0d\u9519\u7684\u5496\u5561\u9986\uff0c\u80fd\u5b9e\u73b0\u81ea\u5df1\u7684\u76ee\u6807\u771f\u662f\u592a\u597d\u4e86\u3002",
    "\uff08\u4e5f\u8bb8\u662f\u6355\u6349\u5230\u6211\u8a00\u8bed\u4e2d\u5fae\u5c0f\u7684\u843d\u5bde\uff0c\u6625\u7530\u9732\u51fa\u4e86\u5979\u6807\u5fd7\u6027\u7684\u6e29\u67d4\u7b11\u5bb9\uff0c\u5979\u524d\u503e\u4e86\u4e0b\u8eab\u5b50\u5411\u6211\u9760\u8fd1\u4e86\u4e00\u4e9b\u3002\uff09",
    "\u6307\u6325\u5b98\u4e0d\u4e5f\u5728\u5b9e\u73b0\u81ea\u5df1\u7684\u76ee\u6807\u4e48\uff1f",
    "\u6211\uff1f\u6211\u53ea\u662f\u5728\u82df\u4e14\u5077\u751f\u7f62\u4e86\u3002",
    "\u6307\u6325\u5b98\u5728\u7528\u6307\u6325\u5b98\u7684\u65b9\u5f0f\u524d\u8fdb\u7740\u3002|\u6211\u660e\u767d\uff0c\u73b0\u5728\u7684\u5c40\u9762\u662f\u56e0\u4e3a\u60a8\u5f53\u521d\u66ff\u6211\u4eec\u627f\u62c5\u4e86\u6240\u6709\u4ee3\u4ef7\u3002\u6ca1\u6709\u60a8\u5f53\u521d\u7684\u51b3\u5b9a\uff0c\u6211\u4eec\u8fd9\u4e9b\u4eba\u5f62\u4ec0\u4e48\u90fd\u505a\u4e0d\u4e86\uff0c\u66f4\u4e0d\u8981\u8bf4\u5f00\u8fd9\u6837\u4e00\u95f4\u5496\u5561\u9986\u3002",
    "\uff08\u6625\u7530\u73af\u987e\u4e86\u4e0b\u81ea\u5df1\u7684\u5496\u5561\u9986\uff0c\u4ece\u5979\u7684\u773c\u795e\u91cc\uff0c\u6211\u80fd\u611f\u53d7\u5230\u5979\u662f\u5982\u6b64\u5730\u73cd\u60dc\u8fd9\u91cc\u3002\uff09",
    "\u201c\u65b9\u7cd6\u201d\u7684\u5efa\u7acb\uff0c\u6211\u80fd\u8fc8\u51fa\u5b9e\u73b0\u68a6\u60f3\u7684\u90a3\u4e00\u6b65\uff0c\u8fd9\u91cc\u9762\u4e5f\u6709\u6307\u6325\u5b98\u60a8\u7684\u4e00\u4efd\u529f\u52b3\u3002",
    "\u6240\u4ee5\u8bf7\u4e0d\u8981\u8bf4\u81ea\u5df1\u82df\u4e14\u5077\u751f\uff0c\u4e5f\u8bb8\u5bf9\u60a8\u4e0d\u4e86\u89e3\u7684\u4eba\u4f1a\u8fd9\u4e48\u60f3\uff0c\u4f46\u662f\u60a8\u662f\u65e0\u6cd5\u7792\u8fc7\u6211\u7684\u3002",
    "\u2026\u2026\u2026\u2026",
    "\u80fd\u542c\u5230\u4f60\u8fd9\u4e48\u8bf4\uff0c\u6211\u5fc3\u91cc\u8212\u670d\u591a\u4e86\u3002",
    "\uff08\u8bf4\u5b8c\uff0c\u6211\u82e6\u7b11\u4e00\u4e0b\u3002\u6625\u7530\u611f\u53d7\u5230\u4e86\u6211\u7684\u60c5\u7eea\uff0c\u7a0d\u7a0d\u5b89\u5fc3\u4e86\u4e00\u4e9b\u3002\uff09",
    "\u4e0d\u8fc7\uff0c\u6211\u6ca1\u9884\u6599\u5230\u6307\u6325\u5b98\u60a8\u80fd\u6765\u8fd9\u91cc\uff0c\u6211\u5f88\u60ca\u8bb6\u4ed6\u4eec\u4f1a\u5141\u8bb8\u60a8\u8fdd\u53cd\u534f\u8bae\u3002",
    "\u683c\u7433\u5e2e\u4e86\u6211\u5f88\u5927\u7684\u5fd9\uff0c\u800c\u4e14\u4ed6\u4eec\u53c8\u5f00\u59cb\u9700\u8981\u4eba\u53bb\u505a\u4e00\u4e9b\u4ed6\u4eec\u4e0d\u60f3\u505a\u7684\u4e8b\u4e86\u3002",
    "\u6307\u6325\u5b98\u2026\u2026\u60a8\u5f53\u65f6\u79bb\u5f00\u7684\u521d\u8877\u5c31\u662f\u4e0d\u60f3\u518d\u88ab\u4ed6\u4eec\u6446\u5e03\u5427\uff1f\u867d\u7136\u6211\u4eec\u90fd\u5f88\u820d\u4e0d\u5f97\u60a8\uff0c\u4f46\u6211\u4eec\u540c\u6837\u4e0d\u5e0c\u671b\u60a8\u6b21\u6b21\u90fd\u7f6e\u8eab\u4e8e\u5371\u9669\u4e2d\u3002|\u7136\u800c\u73b0\u5728\u2026\u2026",
    "\u6709\u5404\u79cd\u5404\u6837\u7684\u539f\u56e0\u5427\uff0c\u73b0\u5728\u4e00\u8a00\u96be\u5c3d\u3002\u4f46\u4e0d\u7ba1\u600e\u4e48\u8bf4\uff0c\u683c\u7433\u9700\u8981\u6211\uff0c\u6211\u5c31\u5fc5\u987b\u652f\u6301\u5979\u3002|\u4f60\u4e5f\u4f1a\u652f\u6301\u6211\u4eec\u5427\uff1f",
    "\u90a3\u662f\u4e00\u5b9a\u7684\uff0c\u5982\u679c\u6709\u4efb\u4f55\u547d\u4ee4\uff0c\u8bf7\u60a8\u5429\u5490\u3002",
    "\u683c\u7433\u8bf4\u4f60\u5728\u8fd9\u5ea7\u57ce\u5e02\u7684\u6cbb\u5b89\u90e8\u95e8\u4e2d\u6709\u4e00\u4e9b\u8d44\u6e90\uff0c\u6211\u9700\u8981\u901a\u8fc7\u7cfb\u7edf\u627e\u4e00\u4e2a\u4eba\u7684\u4f4d\u7f6e\u3002",
    "\u662f\u4ec0\u4e48\u4eba\uff1f",
    "\u6211\u6b63\u5728\u8ffd\u67e5\u6709\u5173\u6211\u4eec\u7684\u654c\u4eba\u2014\u2014\u5409\u62c9\u5c14\u96c6\u56e2\u8d44\u91d1\u8d70\u5411\u7684\u7ebf\u7d22\uff0c\u4f60\u6709\u542c\u8bf4\u8fc7\u8fd9\u4e2a\u540d\u5b57\u4e48\uff1f",
    "\u5409\u62c9\u5c14\u96c6\u56e2\u2026\u2026\u8fd9\u662f\u6700\u8fd1\u8fd8\u633a\u6d41\u884c\u7684\u540d\u5b57\uff0c\u4ed6\u4eec\u66fe\u5728CHE-02\u8bbe\u7acb\u591a\u5bb6\u5206\u516c\u53f8\uff0c\u5e26\u52a8\u4e86\u5927\u91cf\u4eba\u6d41\u8fdb\u5165\u8fd9\u5ea7\u57ce\u5e02\u3002|\u5bf9\u6211\u4eec\u800c\u8a00\u5012\u7684\u786e\u63d0\u5347\u4e86\u8425\u4e1a\u989d\uff0c\u53ef\u662f\u4e4b\u524d\u6211\u770b\u5230\u5965\u5fb7\u5345\u5dde\u52a8\u4e71\u7684\u65b0\u95fb\u4e86\u2026\u2026\u8fd9\u91cc\u7684\u5206\u516c\u53f8\u4e5f\u7eb7\u7eb7\u8fdb\u5165\u505c\u4e1a\u6574\u987f\u3002",
    "\u55ef\uff0c\u4ed6\u4eec\u4ece\u6765\u5c31\u4e0d\u662f\u6765\u505a\u6148\u5584\u7684\uff0c\u800c\u662f\u5728\u5404\u4e2a\u57ce\u5e02\u915d\u917f\u7740\u9634\u8c0b\u3002|\u867d\u7136\u5965\u5fb7\u5345\u5dde\u5df2\u7ecf\u5f00\u59cb\u5bf9\u4ed6\u4eec\u8fdb\u884c\u6e05\u7b97\uff0c\u4f46\u5176\u4ed6\u5dde\u5927\u6982\u7387\u8fd8\u5b58\u6709\u5176\u4f59\u515a\u3002|\u683c\u7433\u67e5\u5230\u4e86\u5409\u62c9\u5c14\u96c6\u56e2\u4e4b\u524d\u8d2d\u4e70\u975e\u6cd5\u6b66\u88c5\u7684\u8fdd\u89c4\u8d44\u91d1\u51fa\u5165\u8fd9\u5ea7\u57ce\u5e02\u7684\u8bb0\u5f55\u3002",
    "\u9664\u4e86\u5f02\u5e38\u7684\u8d44\u91d1\u8d70\u5411\u5916\uff0c\u8fd8\u6709\u4e00\u4e9b\u7ba1\u5236\u7269\u8d44\u2026\u2026|\u4e00\u4e9b\u80fd\u7528\u6765\u5236\u9020\u7206\u70b8\u7269\u7684\u5316\u5b66\u7269\u8d28\uff0c\u4e5f\u901a\u8fc7\u5730\u4e0b\u6e20\u9053\u8fdb\u5165\u8fd9\u5ea7\u57ce\u5e02\u3002\u800c\u8fd9\u4e9b\u7269\u8d44\uff0c\u901a\u8fc7\u8fd0\u8f93\u8bb0\u5f55\u53ef\u4ee5\u5224\u65ad\uff0c\u6700\u540e\u6d41\u5411\u7684\u662f\u4e00\u7fa4\u4ece\u53e6\u5916\u536b\u661f\u57ce\u6765\u7684\u5de5\u7a0b\u961f\u3002",
    "\u8fd9\u5ea7\u536b\u661f\u57ce\u4eba\u5458\u7684\u5f80\u6765\u5f88\u5bc6\u96c6\uff0c\u5bf9\u4e8e\u4e2a\u4f53\u7684\u5b89\u5168\u7ba1\u63a7\u6bd4\u8d77\u5176\u4ed6\u57ce\u5e02\u66f4\u4e3a\u5bbd\u677e\u3002",
    "\u662f\u7684\uff0c\u8fd9\u662f\u4e00\u4e2a\u6f0f\u6d1e\u3002|\u6b63\u5de7\u8fd9\u5ea7\u57ce\u5e02\u9a6c\u4e0a\u5c31\u8981\u4e3e\u529e\u5927\u578b\u5e86\u5178\u6d3b\u52a8\uff0c\u6211\u4eec\u6000\u7591\u8fd9\u4e9b\u8d44\u91d1\u548c\u7269\u8d44\u4f1a\u88ab\u7528\u5728\u4e00\u4e9b\u6fc0\u8fdb\u7684\u72af\u7f6a\u884c\u4e3a\u4e0a\uff0c\u800c\u8d1f\u8d23\u8fd9\u4e00\u72af\u7f6a\u884c\u4e3a\u7684\u5c31\u662f\u4e00\u4e2a\u53eb\u505a\u57c3\u4e1d\u7279\u7684\u5973\u5b50\u8fd8\u6709\u5979\u7684\u4e00\u7fa4\u540c\u50da\u3002",
    "\u2026\u2026\u4ed6\u4eec\u7684\u76ee\u7684\u662f\u8981\u7834\u574f\u5e86\u5178\uff1f",
    "\u8fd9\u53ea\u662f\u7b2c\u4e00\u6b65\uff0c\u6050\u6015\u501f\u7740\u8fd9\u6b21\u72af\u7f6a\u884c\u4e3a\u9020\u6210\u7684\u6076\u6027\u5f71\u54cd\uff0c\u540e\u7eed\u8fd8\u4f1a\u6709\u66f4\u591a\u62a5\u590d\u6027\u52a8\u4f5c\u3002",
    "\u6211\u5927\u6982\u660e\u767d\u4e86\uff0c\u542c\u8d77\u6765\u662f\u4e2a\u5f88\u4e25\u91cd\u7684\u6848\u5b50\u3002|\u4e0d\u8fc7\uff0c\u8fd9\u6837\u7684\u6848\u5b50\u662f\u4e0d\u662f\u4ea4\u7531\u6cbb\u5b89\u90e8\u95e8\u53bb\u5904\u7406\u4f1a\u66f4\u4e3a\u59a5\u5f53\uff1f",
    "\u6cbb\u5b89\u90e8\u95e8\u5f88\u5feb\u5c31\u4f1a\u5bf9\u5728\u540d\u5355\u4e0a\u7684\u4eba\u5b9e\u65bd\u6293\u6355\uff0c\u4f46\u662f\u88ab\u902e\u6355\u7684\u53ea\u4f1a\u662f\u90a3\u4e9b\u88ab\u63a8\u5230\u524d\u9762\u7684\u66ff\u7f6a\u7f8a\u3002\u771f\u6b63\u7684\u5e55\u540e\u9ed1\u624b\u4ed6\u4eec\u6293\u4e0d\u5230\uff0c\u4e5f\u4e0d\u613f\u610f\u53bb\u6293\uff0c\u8fd9\u80cc\u540e\u7684\u52bf\u529b\u4e0d\u662f\u4ed6\u4eec\u80fd\u5f97\u7f6a\u7684\u3002",
    "\u55ef\u2026\u2026\u542c\u8d77\u6765\u5f88\u68d8\u624b\uff0c\u6211\u660e\u767d\u4e3a\u4ec0\u4e48\u6307\u6325\u5b98\u4f1a\u6765\u8fd9\u91cc\u4e86\u3002",
    "\u6211\u4f1a\u548c\u8fd9\u5ea7\u57ce\u5e02\u7684\u6cbb\u5b89\u90e8\u95e8\u6c9f\u901a\uff0c\u8bf7\u4ed6\u4eec\u5c3d\u53ef\u80fd\u914d\u5408\u6211\u4eec\u3002|\u90a3\u540d\u53eb\u57c3\u4e1d\u7279\u7684\u5973\u5b50\u2026\u2026\u6211\u4f1a\u5411\u6cbb\u5b89\u90e8\u95e8\u8bf4\u660e\u60c5\u51b5\uff0c\u5e94\u8be5\u5f88\u5feb\u5c31\u80fd\u88ab\u76d1\u63a7\u627e\u5230\u3002",
    "\u53e6\u5916\u8fd9\u5ea7\u57ce\u5e02\u4e2d\u6709\u53ef\u4ee5\u8c03\u7528\u7684\u3001\u4fe1\u5f97\u8fc7\u7684\u3001\u4e0d\u8fdd\u53cd\u534f\u8bae\u7684\u6218\u672f\u4eba\u5f62\u5417\uff1f",
    "\u95ea\u7535\u5c0f\u961f\u6ca1\u548c\u60a8\u4e00\u8d77\u6765\u5417\uff1f",
    "\u4f60\u77e5\u9053\uff1f\u6625\u7530\u7684\u60c5\u62a5\u679c\u7136\u5f88\u7075\u901a\u554a\u2026\u2026",
    "\u6307\u6325\u5b98\u7684\u4e8b\u5927\u5bb6\u90fd\u5f88\u5173\u5fc3\uff0c\u6240\u4ee5\u6211\u5076\u5c14\u542c\u5230\u4e86\u800c\u5df2\u3002",
    "\u95ea\u7535\u5c0f\u961f\u53bb\u6267\u884c\u522b\u7684\u4efb\u52a1\u4e86\u3002|\u4e3a\u4e86\u5c3d\u53ef\u80fd\u4f4e\u8c03\uff0c\u8fd9\u6b21\u6765\u8fd9\u5ea7\u57ce\u5e02\uff0c\u6211\u6ca1\u6709\u5e26\u522b\u7684\u4eba\u5f62\u6765\uff0c\u540e\u9762\u91c7\u53d6\u884c\u52a8\u65f6\u6700\u597d\u6709\u8db3\u591f\u6218\u529b\u7684\u4eba\u5f62\u6765\u652f\u6301\u3002",
    "\u7b26\u5408\u90a3\u4e48\u591a\u6761\u4ef6\u7684\u6218\u672f\u4eba\u5f62\u7684\u8bdd\uff0c\u5012\u7684\u786e\u2014\u2014",
    "\u4e24\u676f\u8d85\u5927\u676f\u8d85\u7ea7\u7126\u7cd6\u739b\u5947\u6735\u6765\u5566\uff01\u590f\u514b\u91cc\u8fd8\u7279\u610f\u5728\u6307\u6325\u5b98\u90a3\u676f\u4e2d\u65bd\u5c55\u4e86\u4f1a\u8ba9\u60a8\u5e78\u798f\u7684\u9b54\u6cd5\u54e6\u2665",
    "\uff08\u590f\u514b\u91cc\u7aef\u7740\u76d8\u5b50\u4ece\u540e\u53a8\u4e00\u6e9c\u70df\u8dd1\u4e86\u51fa\u6765\uff0c\u5979\u4ee5\u4e00\u4e2a\u6d6e\u5938\u7684\u59ff\u52bf\u5c06\u4e24\u676f\u5496\u5561\u653e\u5230\u4e86\u6211\u4eec\u9762\u524d\u7684\u684c\u4e0a\uff0c\u53cc\u624b\u5bf9\u6211\u6bd4\u4e86\u4e00\u4e2a\u7231\u5fc3\u7684\u624b\u52bf\u3002\uff09",
    "\u773c\u524d\u5c31\u6709\u4e00\u4e2a\u5462\u3002",
    "\uff08\u6211\u548c\u6625\u7530\u540c\u65f6\u770b\u5411\u590f\u514b\u91cc\uff0c\u8fd9\u8ba9\u5979\u7a81\u7136\u6709\u79cd\u4e0d\u7965\u7684\u9884\u611f\u3002\uff09",
    "\u600e\u2026\u2026\u600e\u4e48\u4e86\uff1f"
  ],
  [
    "\u6307\u6325\u5b98\uff0c\u80fd\u542c\u5230\u5417\uff1f",
    "\u5f88\u6e05\u695a\uff0c\u8bf7\u8bb2\u3002",
    "\u57c3\u4e1d\u7279\u7684\u4f4d\u7f6e\u6211\u5df2\u7ecf\u53d1\u9001\u7ed9\u60a8\u4e86\u3002|\u6839\u636e\u6cbb\u5b89\u90e8\u95e8\u7ed9\u7684\u76d1\u63a7\u8bb0\u5f55\uff0c\u5979\u5e94\u8be5\u8fd8\u5728\u90a3\u4e2a\u4f4d\u7f6e\u7684\u9910\u5385\u548c\u5979\u7684\u540c\u4e8b\u4e00\u8d77\u805a\u9910\u3002",
    "\u6307\u6325\u5b98\uff0c\u60a8\u6709\u8db3\u591f\u7684\u65f6\u95f4\u63a7\u5236\u5979\u3002",
    "\u5979\u6709\u643a\u5e26\u6b66\u5668\u6216\u8005\u62a4\u536b\u4ec0\u4e48\u7684\u5417\uff1f",
    "\u6ca1\u6709\uff0c\u5979\u53ea\u662f\u4e00\u4e2a\u666e\u901a\u7684\u5de5\u7a0b\u5e08\uff0c\u6211\u731c\u5979\u53ea\u662f\u88ab\u9009\u4e2d\u7684\u66ff\u7f6a\u7f8a\u3002|\u5979\u672c\u8eab\u6ca1\u6709\u72af\u7f6a\u8bb0\u5f55\uff0c\u4e5f\u6ca1\u6709\u770b\u5230\u6709\u8fc7\u6218\u6597\u76f8\u5173\u7684\u5c65\u5386\uff0c\u662f\u4e2a\u975e\u5e38\u6b63\u5e38\u7684\u666e\u901a\u4eba\u3002",
    "\u666e\u901a\u4eba\u5374\u8981\u9020\u70b8\u5f39\u2026\u2026\u73b0\u5728\u7684\u4e16\u9053\u771f\u662f\u626d\u66f2\u3002",
    "\u4e5f\u8bb8\u5979\u88ab\u5a01\u80c1\u4e86\uff0c\u7b49\u6211\u4eec\u63a7\u5236\u5979\u4e4b\u540e\u53ef\u4ee5\u6162\u6162\u8be2\u95ee\u3002",
    "\u8bf4\u7684\u4e5f\u662f\u3002|\u5916\u56f4\u7684\u51c6\u5907\u5982\u4f55\u4e86\uff1f",
    "\u6211\u90fd\u6c9f\u901a\u597d\u4e86\uff0c\u6cbb\u5b89\u90e8\u95e8\u63a5\u4e0b\u6765\u534a\u5c0f\u65f6\u5185\u4e0d\u4f1a\u5728\u90a3\u4e2a\u533a\u57df\u5de1\u903b\u3002|\u53e6\u5916\u8fd0\u8f7d\u7528\u7684\u8f66\u8f86\u4e5f\u968f\u65f6\u5f85\u547d\uff0c\u7b49\u6307\u6325\u5b98\u60a8\u7684\u547d\u4ee4\u3002",
    "\u55ef\uff0c\u4fdd\u6301\u8054\u7cfb\uff0c\u5b8c\u6bd5\u3002",
    "\uff08\u6211\u5173\u95ed\u4e86\u901a\u8baf\uff0c\u770b\u7740\u5ea7\u4f4d\u53e6\u4e00\u5934\u7684\u590f\u514b\u91cc\uff0c\u5979\u6b63\u770b\u7740\u7a97\u5916\u5634\u91cc\u788e\u788e\u5ff5\u7740\u3002\uff09",
    "\u8fd9\u51e0\u5929\u600e\u4e48\u8fd9\u4e48\u5012\u9709\u2026\u2026\u8f9b\u8f9b\u82e6\u82e6\u505a\u7684\u5175\u8681\u88ab\u4eba\u6253\u574f\uff0c\u4e0b\u73ed\u4e86\u8fd8\u8981\u88ab\u6d3e\u51fa\u6765\u5e72\u5916\u52e4\u2026\u2026",
    "\u590f\u514b\u91cc\u4f60\u6709\u5728\u8bf4\u4ec0\u4e48\u5417\uff1f",
    "\u6ca1\u6709\u6ca1\u6709\uff0c\u6211\u5728\u8bf4\u80fd\u548c\u6307\u6325\u5b98\u4e00\u8d77\u51fa\u6765\u975e\u5e38\u8363\u5e78\uff01",
    "\u771f\u7684\u5417\uff1f\u73b0\u5728\u4e00\u526f\u6709\u5fc3\u4e8b\u6837\u5b50\u7684\u4eba\u5f62\u6bd4\u4ee5\u524d\u8981\u591a\u5f88\u591a\u554a\uff0c\u662f\u6700\u8fd1\u7684\u6d41\u884c\uff1f",
    "\u6ca1\u6709\u6ca1\u6709\uff0c\u590f\u514b\u91cc\u624d\u6ca1\u6709\u5fc3\u4e8b\u5462~",
    "\u5f00\u4e2a\u73a9\u7b11\u800c\u5df2\uff0c\u603b\u89c9\u5f97\u4f60\u6709\u4e00\u4e9b\u7d27\u5f20\uff0c\u662f\u7b2c\u4e00\u6b21\u63a5\u89e6\u8fd9\u6837\u7684\u4e8b\u5427\u3002",
    "\u590f\u514b\u91cc\u4ee5\u524d\u662f\u5076\u50cf\u561b\uff0c\u554a\u5f53\u7136\u73b0\u5728\u4e5f\u662f\uff01|\u6240\u4ee5\u6293\u574f\u4eba\u8fd9\u79cd\u4e8b\u8fd8\u662f\u6709\u70b9\u964c\u751f\u7684\u2026\u2026",
    "\u4e0d\u8fc7\u65e2\u7136\u5e97\u957f\u5df2\u7ecf\u628a\u4efb\u52a1\u4ea4\u7ed9\u6211\u4e86\uff0c\u90a3\u6211\u4e00\u5b9a\u4f1a\u5168\u529b\u5b8c\u6210\uff01\u8fd9\u4e5f\u662f\u5076\u50cf\u7684\u804c\u8d23\u2665",
    "\u539f\u6765\u8fd9\u4e5f\u662f\u5076\u50cf\u7684\u804c\u8d23\u554a\u2026\u2026\u4e0d\u8fc7\u4eb2\u548c\u529b\u9ad8\u7684\u4eba\u5f62\u8bf4\u4e0d\u5b9a\u4f1a\u6709\u66f4\u597d\u7684\u6548\u679c\u3002",
    "\u590f\u514b\u91cc\uff0c\u4e00\u4f1a\u628a\u6b66\u5668\u9690\u85cf\u8d77\u6765\uff0c\u6211\u4eec\u5148\u4ee5\u6c9f\u901a\u4e3a\u524d\u63d0\u548c\u5bf9\u65b9\u63a5\u89e6\u3002|\u5b9e\u5728\u4e0d\u884c\u518d\u52a8\u7528\u6b66\u529b\u3002",
    "\u597d\u54a7\uff0c\u590f\u514b\u91cc\u660e\u767d\uff01",
    "\u8bdd\u8bf4\u6307\u6325\u5b98\uff0c\u4e00\u4f1a\u6211\u4eec\u628a\u90a3\u4e2a\u53eb\u57c3\u4e1d\u7279\u7684\u5bb6\u4f19\u6293\u8d77\u6765\uff0c\u4e0d\u4f1a\u88ab\u6cbb\u5b89\u90e8\u95e8\u53d1\u73b0\u5427\uff1f",
    "\u6625\u7530\u5df2\u7ecf\u548c\u8fd9\u9644\u8fd1\u7684\u6cbb\u5b89\u90e8\u95e8\u6c9f\u901a\u8fc7\u4e86\uff0c\u4ed6\u4eec\u4e0d\u4f1a\u963b\u6320\u6211\u4eec\u7684\u884c\u52a8\u3002",
    "\u90a3\u4e2a\u5bb6\u4f19\u7684\u4f4d\u7f6e\u4e5f\u662f\u6cbb\u5b89\u90e8\u95e8\u63d0\u4f9b\u7684\uff0c\u4ed6\u4eec\u65e9\u5c31\u5df2\u7ecf\u5173\u6ce8\u5230\u8fd9\u4e9b\u4eba\uff0c\u53ea\u662f\u4e00\u76f4\u6ca1\u4e0b\u624b\u7f62\u4e86\u3002",
    "\u4e3a\u4ec0\u4e48\uff1f\u6293\u574f\u4eba\u4e0d\u5e94\u8be5\u662f\u6cbb\u5b89\u90e8\u95e8\u7684\u804c\u8d23\u5417\uff1f",
    "\u2026\u2026\u8fd9\u91cc\u9762\u6709\u5f88\u590d\u6742\u7684\u539f\u56e0\u3002|\u603b\u4e4b\uff0c\u6211\u4eec\u66ff\u4ed6\u4eec\u6392\u9664\u98ce\u9669\uff0c\u4ed6\u4eec\u611f\u8c22\u6211\u4eec\u8fd8\u6765\u4e0d\u53ca\u3002",
    "\u542c\u8d77\u6765\u6709\u4e00\u4e9b\u590d\u6742\u2026\u2026\u590f\u514b\u91cc\u4e0d\u592a\u80fd\u7406\u89e3\u2026\u2026",
    "\u4e0d\u80fd\u7406\u89e3\u4e5f\u6ca1\u5173\u7cfb\uff0c\u672c\u6765\u4e5f\u4e0d\u662f\u4ec0\u4e48\u5f88\u91cd\u8981\u7684\u4e8b\u3002",
    "\uff08\u5f88\u5feb\u6211\u4eec\u5c31\u5230\u8fbe\u4e86\u76ee\u7684\u5730\u9644\u8fd1\u3002\uff09",
    "\uff08\u4e0b\u8f66\u540e\uff0c\u6625\u7530\u63d0\u4f9b\u7ed9\u6211\u7684\u60c5\u62a5\u4e2d\u663e\u793a\u7684\u9910\u9986\u5c31\u5728\u773c\u524d\u3002\uff09",
    "\uff08\u7136\u800c\u548c\u60f3\u8c61\u4e2d\u4e0d\u540c\uff0c\u9910\u9986\u4e2d\u6b63\u53d1\u751f\u7740\u6fc0\u70c8\u7684\u4e89\u5435\u3002\uff09",
    "\u600e\u4e48\u56de\u4e8b\uff1f",
    "\u91cc\u9762\u7684\u4eba\u597d\u50cf\u5728\u6253\u67b6\uff01",
    "\u6625\u7530\uff0c\u9910\u9986\u4e2d\u53d1\u751f\u4e86\u6597\u6bb4\uff0c\u60c5\u51b5\u53ef\u80fd\u6709\u53d8\u3002|\u6211\u5148\u8fdb\u53bb\u770b\u770b\u3002",
    "\u597d\u7684\u6307\u6325\u5b98\uff0c\u6cbb\u5b89\u90e8\u95e8\u8fd9\u8fb9\u6211\u4f1a\u6c9f\u901a\u597d\u914d\u5408\uff0c\u60a8\u6ce8\u610f\u5b89\u5168\u3002",
    "\u660e\u767d\uff0c\u5b8c\u6bd5\u3002",
    "\uff08\u6211\u548c\u590f\u514b\u91cc\u5feb\u6b65\u8d70\u5230\u5e97\u95e8\u524d\uff0c\u5f80\u91cc\u770b\u53bb\uff0c\u91cc\u9762\u957f\u76f8\u5404\u6837\u7684\u4eba\u6b63\u5728\u63a8\u6421\u4e89\u5435\u7740\uff0c\u800c\u8001\u677f\u6b63\u5728\u4e00\u65c1\u82e6\u82e6\u54c0\u6c42\u4ed6\u4eec\u4f4f\u624b\u3002\uff09",
    "\u522b\u6253\u4e86\u6211\u7684\u5929\u554a\u522b\u6253\u554a\uff01\u7edb\u96e8\uff0c\u671d\u6656\uff01\u4f60\u4eec\u5feb\u63a7\u5236\u573a\u9762\u554a\uff01",
    "\u5728\u63a7\u5236\u554a\uff01\u4ed6\u4eec\u5341\u591a\u4e2a\u4eba\u5462\uff0c\u6211\u4eec\u624d\u4e24\u4e2a\uff01",
    "\uff08\u8ba9\u6211\u60ca\u8bb6\u7684\u662f\uff0c\u4ee5\u524d\u540d\u4e3a97\u5f0f\u7684\u4eba\u5f62\u6b63\u6309\u4f4f\u4e24\u4e2a\u5927\u6c49\uff0c\u5979\u8eab\u8fb9\u8fd8\u6709\u4e00\u4e2a\u672a\u66fe\u89c1\u8fc7\u7684\u4eba\u5f62\u4f3c\u4e4e\u5728\u548c\u8001\u677f\u8ba8\u4ef7\u8fd8\u4ef7\u3002\uff09",
    "\u8fd9\u4e2a\u7b97\u989d\u5916\u7684\u5de5\u94b1\u5417\uff1f|\u6211\u4eec\u63d0\u4f9b\u7684\u670d\u52a1\u4e2d\u53ef\u6ca1\u5305\u542b\u5b89\u4fdd\u3002",
    "\u54c8\uff1f\u73b0\u5728\u662f\u63d0\u5de5\u94b1\u7684\u65f6\u5019\u5417\uff1f",
    "\u4eba\u5f62\u53ef\u4e0d\u80fd\u968f\u4fbf\u4f24\u5bb3\u4eba\u7c7b\uff0c\u4e07\u4e00\u6cbb\u5b89\u90e8\u95e8\u7684\u4eba\u6765\u4e86\u53ef\u5c31\u5f88\u96be\u89e3\u91ca\u4e86\uff0c\u6211\u4eec\u5f97\u83b7\u5f97\u4e0e\u98ce\u9669\u76f8\u5bf9\u5e94\u7684\u62a5\u916c\u3002",
    "\uff08\u590f\u514b\u91cc\u4e5f\u88ab\u6df7\u4e71\u7684\u573a\u9762\u5413\u4e86\u4e00\u8df3\uff0c\u4f46\u662f\u5979\u7acb\u5373\u8ba4\u51fa\u4e86\u521a\u624d\u8fd8\u5728\u548c\u5979\u8ba8\u4ef7\u8fd8\u4ef7\u7684\u53cc\u9a6c\u5c3e\u4eba\u5f62\u3002\uff09",
    "\u554a\uff01\u8fd9\u4e0d\u662f\u62c6\u4e86\u590f\u514b\u91cc\u6700\u5fc3\u7231\u5175\u8681\u7684\u90a3\u4e2a\u5192\u5931\u9b3c\u4eba\u5f62\u5417\uff01",
    "\u5514\u554a\uff01\u706f\u7b3c\u5973\uff1f\u4f60\u3001\u4f60\u600e\u4e48\u4f1a\u5728\u8fd9\u91cc\uff01",
    "\u4ec0\u4e48\u706f\u7b3c\u5973\u2026\u2026\u592a\u6ca1\u6709\u793c\u8c8c\u4e86\u5427\uff1f\u590f\u514b\u91cc\u53ef\u662f\u201c\u65b9\u7cd6\u201d\u5496\u5561\u9986\u7684\u62db\u724c\u5076\u50cf\uff01",
    "\u4f60\u6765\u5e72\u4ec0\u4e48\uff01\uff1f\u4e0d\u4f1a\u6765\u8981\u8d26\u7684\u5427\uff1f\u6211\u4eec\u8bf4\u8fc7\u4f1a\u8d54\u507f\u7684\uff0c\u4e5f\u4e0d\u7528\u90a3\u4e48\u7740\u6025\u5427\uff01",
    "\u8c01\u662f\u6765\u8981\u8d26\u7684\uff01\u6211\u662f\u548c\u6307\u6325\u5b98\u4e00\u8d77\u6765\u6267\u884c\u4efb\u52a1\u7684\uff01",
    "\u957f\u5b98\uff01\uff1f",
    "\uff08\u7edb\u96e8\u4e00\u77ac\u95f4\u50f5\u4f4f\u4e86\uff0c\u5979\u5b8c\u5168\u6ca1\u6709\u9884\u6599\u5230\u81ea\u5df1\u4f1a\u5728\u8fd9\u79cd\u573a\u9762\u4e0b\u4e0e\u6211\u91cd\u9022\u3002\uff09",
    "\uff08\u6211\u5feb\u901f\u5730\u626b\u8fc7\u5728\u573a\u4eba\u7c7b\u7684\u9762\u5e9e\uff0c\u6ca1\u6709\u90a3\u4e2a\u53eb\u57c3\u4e1d\u7279\u7684\u5bb6\u4f19\uff0c\u60c5\u62a5\u4e0d\u51c6\u786e\u5417\uff1f\uff09",
    "\uff08\u6211\u91cd\u65b0\u5c06\u6ce8\u610f\u529b\u8f6c\u56de\u5979\u7684\u8eab\u4e0a\uff0c97\u8fd8\u5728\u9707\u60ca\u4e2d\uff0c\u6ca1\u56de\u8fc7\u795e\u6765\u3002\uff09",
    "\uff08\u6211\u8d76\u7d27\u5728\u5979\u773c\u524d\u6446\u4e86\u6446\u624b\uff0c\u8bd5\u56fe\u628a\u5979\u7684\u610f\u8bc6\u62c9\u56de\u73b0\u5b9e\u3002\uff09",
    "97\uff1f\u4f60\u600e\u4e48\u4f1a\u5728\u8fd9\u91cc\uff1f",
    "\u957f\u957f\u957f\u957f\u957f\u5b98\uff01\u771f\u7684\u662f\u957f\u5b98\u554a\uff01",
    "\u662f\u554a\uff0c\u771f\u7684\u662f\u6211\uff0c97\u4f60\u600e\u4e48\u5728\u8fd9\u91cc\u548c\u4eba\u6253\u67b6\uff1f",
    "\u4eba\u5bb6\u73b0\u5728\u53c8\u53eb\u56de\u7edb\u96e8\u4e86\u5566\uff01|\u800c\u4e14\u6211\u624d\u6ca1\u5728\u6253\u67b6\uff0c\u662f\u8fd9\u7fa4\u4eba\u5728\u95f9\u4e8b\uff0c\u6211\u5728\u6b63\u5f53\u9632\u536b\uff01",
    "\u5443\u597d\uff0c\u7edb\u96e8\uff0c\u8d76\u7d27\u628a\u573a\u9762\u63a7\u5236\u4f4f\uff0c\u6211\u6709\u8bdd\u60f3\u95ee\u4f60\u3002",
    "\u54e6\u54e6\uff0c\u957f\u5b98\u7b49\u6211\u4e00\u5206\u949f\uff01",
    "\uff08\u7edb\u96e8\u8bf4\u5b8c\u7acb\u5373\u6539\u53d8\u4e86\u521a\u624d\u51fa\u5de5\u4e0d\u51fa\u529b\u7684\u6837\u5b50\uff0c\u5979\u8fc5\u901f\u5c55\u73b0\u51fa\u60ca\u4eba\u7684\u4f53\u672f\uff0c\u5c06\u90a3\u4e9b\u6597\u6bb4\u7684\u4eba\u7c7b\u4e00\u4e00\u653e\u5012\u5728\u5730\uff0c\u518d\u987a\u624b\u63b0\u4e0b\u9644\u8fd1\u6905\u80cc\u4e0a\u7684\u91d1\u5c5e\u6746\u3002\uff09",
    "\uff08\u91d1\u5c5e\u6746\u5728\u7edb\u96e8\u7684\u529b\u9053\u4e0b\uff0c\u5c31\u50cf\u7ef3\u5b50\u4e00\u822c\u7f20\u4f4f\u4e86\u8fd8\u8bd5\u56fe\u6323\u624e\u7684\u4eba\u7684\u53cc\u81c2\uff0c\u8ba9\u4ed6\u4eec\u4e1d\u6beb\u65e0\u6cd5\u52a8\u5f39\u3002\uff09",
    "\u554a\u554a\u554a\uff0c\u6211\u7684\u9ad8\u7ea7\u65f6\u5c1a\u6905\u554a\uff01",
    "\u77e5\u8db3\u5427\uff0c\u8981\u662f\u8fd9\u4e9b\u4eba\u7ee7\u7eed\u641e\u7834\u574f\uff0c\u4f60\u7684\u635f\u5931\u53ef\u4e0d\u6b62\u51e0\u628a\u6905\u5b50\u4e86\u3002",
    "\uff08\u7edb\u96e8\u65e0\u89c6\u8001\u677f\u7684\u60e8\u53eb\uff0c\u4e00\u8e66\u4e00\u8df3\u8dd1\u5230\u6211\u7684\u9762\u524d\uff0c\u5728\u5979\u7684\u80cc\u540e\u6211\u770b\u5230\u4e86\u4e00\u628a\u719f\u6089\u7684\u53e4\u7b5d\u3002\uff09",
    "\uff0895\u4e5f\u6765\u8fd9\u91cc\u4e86\uff1f\uff09",
    "\u7edb\u96e8\uff0c\u8fd9\u91cc\u53d1\u751f\u4e86\u4ec0\u4e48\uff1f",
    "\u6211\u548c\u8001\u59d0\uff0c\u8fd8\u6709\u2026\u2026\u554a\u5bf9\u4e86\uff0c\u8fd9\u4f4d\u662f\u671d\u6656\u3002|\u6211\u4eec\u4e00\u8d77\u5728\u8fd9\u5ea7\u9910\u9986\u8868\u6f14\uff0c\u7ed3\u679c\u53f0\u4e0b\u8fd9\u4e9b\u805a\u9910\u7684\u4eba\u7a81\u7136\u6253\u8d77\u6765\u4e86\uff0c\u6211\u4eec\u4e5f\u6b63\u6478\u4e0d\u7740\u5934\u8111\u5462\u3002",
    "\u8fd9\u91cc\u6709\u6ca1\u6709\u4e00\u4e2a\u53eb\u57c3\u4e1d\u7279\u7684\u4eba\uff1f",
    "\u597d\u50cf\u521a\u624d\u8868\u6f14\u7684\u65f6\u5019\u9690\u7ea6\u542c\u5230\u8fc7\u8fd9\u4e2a\u540d\u5b57\uff1f",
    "\u521a\u624d\u6709\u4e00\u4e2a\u4eba\u7a81\u7136\u8dd1\u5230\u9edb\u70df\u9762\u524d\uff0c\u53c8\u7a81\u7136\u51b2\u4e86\u51fa\u53bb\u3002|\u9edb\u70df\u8ddf\u4e0a\u53bb\u4e86\uff0c\u8bf4\u4e0d\u5b9a\u5c31\u662f\u8fd9\u4e2a\u4eba\u3002",
    "\u9edb\u70df\uff0c\u662f95\u5417\u2026\u2026\u5979\u8ffd\u51fa\u53bb\u4e86\uff1f",
    "\uff08\u6211\u611f\u89c9\u6709\u4e9b\u4e0d\u5999\uff0c\u76f4\u89c9\u544a\u8bc9\u6211\u90a3\u4e2a\u8dd1\u51fa\u53bb\u7684\u4eba\u5c31\u662f\u57c3\u4e1d\u7279\u3002\uff09",
    "\uff08\u5982\u679c\u771f\u662f\u5982\u6b64\uff0c\u90a3\u9edb\u70df\u6050\u6015\u4f1a\u6709\u5371\u9669\u3002\uff09",
    "\u7edb\u96e8\uff0c\u4f60\u7684\u706b\u63a7\u6838\u5fc3\u8fd8\u5728\u5417\uff1f",
    "\u5f53\u7136\u662f\u5b8c\u597d\u65e0\u635f\uff01",
    "\u597d\uff0c\u4f60\u62ff\u4e0a\u6b66\u5668\uff0c\u548c\u590f\u514b\u91cc\u4e00\u8d77\u8ddf\u6211\u6765\u3002",
    "\u54e6\u54e6\u597d\u3002",
    "\u6625\u7530\uff0c\u6211\u4eec\u5df2\u7ecf\u5c06\u73b0\u573a\u6597\u6bb4\u4eba\u5458\u90fd\u63a7\u5236\u4e86\uff0c\u4e00\u4f1a\u7b49\u6211\u4eec\u79bb\u5f00\u540e\uff0c\u8ba9\u6cbb\u5b89\u90e8\u95e8\u7684\u4eba\u628a\u4ed6\u4eec\u90fd\u5e26\u8d70\u3002|\u8bf4\u4e0d\u5b9a\u8fd9\u4e9b\u4eba\u77e5\u9053\u4e9b\u4ec0\u4e48\u3002",
    "\u660e\u767d\uff0c\u6709\u53d1\u73b0\u57c3\u4e1d\u7279\u7684\u8e2a\u8ff9\u4e48\uff1f",
    "\u5979\u63d0\u65e9\u79bb\u5f00\u9910\u9986\u4e86\uff0c\u63a5\u4e0b\u6765\u6211\u4f1a\u53bb\u8ffd\u8e2a\uff0c\u770b\u80fd\u5426\u622a\u4f4f\u5979\u3002",
    "\u6536\u5230\uff0c\u8f7d\u5177\u968f\u65f6\u5f85\u547d\u3002",
    "\u55ef\uff0c\u7b49\u6211\u6d88\u606f\uff0c\u5b8c\u6bd5\u3002",
    "\uff08\u5173\u95ed\u901a\u8baf\u540e\uff0c\u6211\u51c6\u5907\u91c7\u53d6\u63a5\u4e0b\u6765\u7684\u884c\u52a8\uff0c\u800c\u89c6\u7ebf\u4e2d\u90a3\u540d\u4e0d\u8ba4\u8bc6\u7684\u4eba\u5f62\u8d70\u5230\u4e86\u6211\u7684\u9762\u524d\u3002\uff09",
    "\u8fd9\u4f4d\u662f\u53eb\u671d\u6656\u2026\u2026\uff1f|\u4e00\u4f1a\u6cbb\u5b89\u90e8\u95e8\u7684\u4eba\u4f1a\u8fc7\u6765\uff0c\u5728\u6b64\u4e4b\u524d\u8bf7\u4f60\u63a7\u5236\u597d\u8fd9\u4e2a\u73b0\u573a\u3002",
    "\u4e0d\u6127\u662f\u957f\u5b98\uff0c\u5bf9\u4e0d\u8ba4\u8bc6\u7684\u4eba\u5f62\u4e5f\u7acb\u5373\u53d1\u53f7\u65bd\u4ee4\u8d77\u6765\u4e86\u5462\u3002",
    "\u7279\u6b8a\u60c5\u51b5\uff0c\u5728\u573a\u7684\u4eba\u4e00\u4e2a\u90fd\u4e0d\u80fd\u79bb\u5f00\uff0c\u6211\u80fd\u62dc\u6258\u4f60\u5417\uff1f",
    "\u662f\uff0c\u6211\u4f1a\u5b8c\u6210\u3002",
    "\u5979\u4f1a\u641e\u5b9a\u7684\uff01|\u957f\u5b98\u6211\u4eec\u8d76\u7d27\u8d70\uff0c\u8001\u59d0\u662f\u4e0d\u662f\u6709\u5371\u9669\u554a\uff1f\uff01",
    "\u8bf4\u4e0d\u5b9a\u662f\u3002",
    "\uff08\u7edb\u96e8\u6253\u65ad\u4e86\u6211\u548c\u671d\u6656\u7684\u5bf9\u8bdd\uff0c\u5979\u7ffb\u51fa\u5305\u4e2d\u7684\u6b66\u5668\uff0c\u6025\u4e0d\u53ef\u8010\u5730\u5f80\u5916\u51b2\u3002\uff09",
    "\uff08\u6211\u548c\u590f\u514b\u91cc\u7acb\u5373\u8ddf\u968f\u5979\u8dd1\u51fa\u4e86\u9910\u9986\u3002\uff09",
    "\u4f60\u77e5\u9053\u9edb\u70df\u7684\u4f4d\u7f6e\u5417\uff1f",
    "\u8001\u59d0\u548c\u6211\u4e00\u76f4\u8fde\u7740\u4e8c\u7ea7\u5e73\u5c42\uff0c\u5979\u5c31\u5728\u524d\u9762\u4e0d\u8fdc\u7684\u4f4d\u7f6e\uff01",
    "\uff08\u6211\u4eec\u62d0\u8fdb\u4e86\u4e00\u4e2a\u5c0f\u5df7\u5b50\u4e2d\uff0c\u660e\u660e\u5750\u6807\u5c31\u5728\u773c\u524d\uff0c\u4f46\u5c0f\u5df7\u5b50\u5230\u5904\u90fd\u662f\u6b7b\u8def\uff0c\u6211\u4eec\u8fdf\u8fdf\u4e0d\u80fd\u8dd1\u5230\u5750\u6807\u7684\u4f4d\u7f6e\u3002\uff09",
    "\uff08\u6bcf\u5230\u4e00\u4e2a\u6b7b\u80e1\u540c\uff0c\u7edb\u96e8\u90fd\u6025\u5f97\u76f4\u8dfa\u811a\u3002\uff09",
    "\u8fd9\u4ec0\u4e48\u7834\u5730\u65b9\uff01\u54ea\u6765\u90a3\u4e48\u591a\u6b7b\u80e1\u540c\uff01",
    "\uff08\u7830\uff01\u7830\uff01\uff09",
    "\u8fd9\u662f\u67aa\u58f0\uff1f",
    "\uff08\u6211\u654f\u9510\u5730\u611f\u77e5\u5230\uff0c\u8fd9\u79cd\u4f4e\u6c89\u7684\u97f3\u8272\u4e0d\u662f\u666e\u901a\u7684\u67aa\u58f0\uff0c\u8fd9\u662f\u88c5\u6709\u6d88\u97f3\u5668\u7684\u624b\u67aa\u5f00\u67aa\u7684\u58f0\u97f3\u3002\uff09",
    "\uff08\u80fd\u4f7f\u7528\u6d88\u97f3\u5668\u7684\u654c\u4eba\u4e00\u5b9a\u4e0d\u662f\u4e00\u822c\u7684\u52bf\u529b\u3002\uff09",
    "\u628a\u6b66\u5668\u4e0a\u819b\uff0c\u6211\u4eec\u5f80\u67aa\u58f0\u90a3\u8d76\uff01",
    "\uff08\u6709\u58f0\u97f3\u7684\u6307\u5f15\uff0c\u6211\u4eec\u5feb\u901f\u63a5\u8fd1\u4e86\u5750\u6807\u6240\u5728\u7684\u4f4d\u7f6e\u3002\uff09",
    "\uff08\u53c8\u662f\u4e24\u58f0\u6c89\u95f7\u7684\u67aa\u58f0\uff0c\u5df7\u5b50\u7684\u5c3d\u5934\u6709\u4e24\u4e2a\u6a21\u7cca\u7684\u4eba\u5f71\u3002\uff09",
    "\uff08\u542c\u5230\u4e86\u6211\u4eec\u7684\u811a\u6b65\u58f0\uff0c\u4e24\u4e2a\u4eba\u5f71\u7acb\u5373\u79bb\u5f00\uff0c\u5728\u5df7\u5b50\u4e2d\u6d88\u5931\u5f97\u65e0\u5f71\u65e0\u8e2a\u3002\uff09",
    "\u8dd1\u5f97\u771f\u5feb\u2026\u2026",
    "\u8981\u8ffd\u5417\u6307\u6325\u5b98\uff1f",
    "\u4e0d\u8ffd\uff0c\u60c5\u51b5\u4e0d\u660e\uff0c\u8c28\u614e\u4e00\u4e9b\u3002",
    "\u6625\u7530\uff0c\u6211\u4eec\u6ca1\u627e\u5230\u76ee\u6807\uff0c\u5979\u5df2\u7ecf\u63d0\u65e9\u79bb\u5f00\u4e86\u3002",
    "\u5979\u5e94\u8be5\u8fd8\u5728\u9644\u8fd1\uff0c\u9700\u8981\u8054\u7cfb\u6cbb\u5b89\u90e8\u95e8\u8c03\u53d6\u9644\u8fd1\u76d1\u63a7\u5417\uff1f",
    "\u55ef\uff0c\u6211\u4eec\u9700\u8981\u60c5\u62a5\u4e0a\u7684\u652f\u6301\u3002",
    "\uff08\u6211\u9010\u6e10\u9760\u8fd1\u521a\u624d\u6709\u4eba\u5f71\u5728\u7684\u4f4d\u7f6e\uff0c\u5374\u7a81\u7136\u53d1\u73b0\u5730\u4e0a\u6709\u5012\u4e0b\u7684\u4eba\u5f62\u3002\uff09",
    "\u7b49\u4e00\u4e0b\uff0c\u6211\u770b\u5230\u6709\u4e00\u540d\u4eba\u5f62\u5012\u5728\u5730\u4e0a\u2014\u2014",
    "\u8fd9\u662f\u2026\u2026\u9edb\u70df\uff1f",
    "\u9edb\u70df\u5728\u73b0\u573a\uff1f\uff01",
    "\u5979\u7684\u7d20\u4f53\u53d7\u635f\u4e25\u91cd\uff0c\u6211\u4eec\u5f97\u7acb\u5373\u5e26\u5979\u79bb\u5f00\uff01\u8ba9\u8f7d\u5177\u9a6c\u4e0a\u8fc7\u6765\uff01",
    "\u597d\u7684\uff0c\u6211\u7acb\u5373\u5b89\u6392\uff01",
    "\u9edb\u70df\u59d0\uff01",
    "\uff08\u7edb\u96e8\u770b\u5230\u5012\u5728\u5730\u4e0a\u7684\u9edb\u70df\uff0c\u5c31\u8981\u594b\u4e0d\u987e\u8eab\u5730\u51b2\u8fc7\u53bb\uff0c\u6211\u89c1\u72b6\u7acb\u5373\u559d\u6b62\u4f4f\u4e86\u5979\u7684\u884c\u4e3a\u3002\uff09",
    "\u522b\u8fc7\u53bb\uff01\u654c\u4eba\u53ef\u80fd\u8fd8\u5728\u9644\u8fd1\uff01\u68c0\u67e5\u89d2\u843d\uff01",
    "\u662f\u2026\u2026\u662f\uff01",
    "\u590f\u514b\u91cc\u4f60\u770b\u4f4f\u6211\u4eec\u7684\u540e\u65b9\uff0c\u7edb\u96e8\uff0c\u6211\u4eec\u5404\u8d1f\u8d23\u4e00\u4e2a\u8f6c\u89d2\u3002",
    "\u597d\u2026\u2026\u597d\u7684\u6307\u6325\u5b98\uff01",
    "\uff08\u7edb\u96e8\u4e3e\u8d77\u67aa\u8c28\u614e\u5730\u68c0\u67e5\u4e86\u5df7\u5b50\u7684\u51e0\u4e2a\u51fa\u53e3\uff0c\u800c\u6211\u68c0\u67e5\u5b8c\u89d2\u843d\u540e\u5c0f\u5fc3\u7ffc\u7ffc\u5730\u786e\u8ba4\u4e86\u9edb\u70df\u8eab\u4e0a\u6ca1\u6709\u88ab\u88c5\u4e0a\u8be1\u96f7\u4e00\u7c7b\u7684\u4e1c\u897f\u3002\uff09",
    "\u957f\u5b98\uff0c\u6ca1\u6709\u770b\u5230\u6709\u5a01\u80c1\u7684\u5355\u4f4d\u2026\u2026\u6211\u662f\u4e0d\u662f\u53ef\u4ee5\u2026\u2026",
    "\u55ef\uff0c\u9edb\u70df\u53ef\u4ee5\u79fb\u52a8\u3002|\u5979\u53d7\u635f\u4e25\u91cd\uff0c\u6211\u4eec\u5f97\u7acb\u5373\u5e26\u5979\u53bb\u7ef4\u4fee\u3002",
    "\uff08\u7edb\u96e8\u7acb\u5373\u6025\u5207\u5730\u51b2\u5230\u9edb\u70df\u9762\u524d\uff0c\u5f00\u59cb\u4e3a\u5979\u68c0\u67e5\u53d7\u635f\u90e8\u4f4d\u3002\uff09",
    "\u8001\u59d0\uff01\u8001\u59d0\u4f60\u6ca1\u4e8b\u5427\uff0c\u4f60\u53ef\u522b\u5413\u6211\uff01",
    "\u8fd9\u4e0d\u662f\u521a\u624d\u7684\u201c\u5bb6\u957f\u201d\u4eba\u5f62\u5417\uff1f\u600e\u4e48\u88ab\u6253\u6210\u8fd9\u6837\uff01\uff1f",
    "\u654c\u4eba\u7684\u884c\u52a8\u901f\u5ea6\u6bd4\u6211\u4eec\u9884\u60f3\u7684\u8fd8\u8981\u66f4\u5feb\uff0c\u7edb\u96e8\u4f60\u628a\u9edb\u70df\u80cc\u4e0a\uff0c\u8f7d\u5177\u9a6c\u4e0a\u5c31\u5230\u3002",
    "\u590f\u514b\u91cc\uff0c\u4f60\u56de\u53bb\u901a\u77e5\u521a\u624d\u90a3\u4f4d\u671d\u6656\uff0c\u8ba9\u5979\u628a\u6240\u6709\u884c\u674e\u5e26\u4e0a\u8ddf\u4e0a\u6211\u4eec\u3002|\u6211\u4eec\u8981\u5728\u6cbb\u5b89\u90e8\u95e8\u8d76\u6765\u8fd9\u91cc\u4e4b\u524d\u79bb\u5f00\u3002",
    "\u597d\u7684\uff0c\u6211\u9a6c\u4e0a\u5c31\u53bb\uff01",
    "\u8be5\u6b7b\u2026\u2026\u9edb\u70df\uff0c\u575a\u6301\u4f4f\uff0c\u4f60\u5343\u4e07\u4e0d\u8981\u6709\u4e8b\uff01"
  ]
]