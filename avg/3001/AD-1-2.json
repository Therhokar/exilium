[
  [
    "\uff08\u5f53\u9edb\u70df\u548c\u671d\u6656\u8d76\u5230\u65f6\uff0c\u82b1\u8f66\u7684\u961f\u4f0d\u5df2\u7ecf\u505c\u4e86\u4e0b\u6765\u3002\uff09",
    "\uff08\u961f\u4f0d\u4e2d\u6bb5\u7684\u4f4d\u7f6e\u56f4\u4e86\u4e00\u5927\u7fa4\u4eba\uff0c\u6b63\u4ea4\u5934\u63a5\u8033\u5730\u8c08\u8bba\u7740\u4ec0\u4e48\uff0c\u4e00\u4e2a\u751c\u7f8e\u7684\u58f0\u97f3\u5e26\u7740\u59d4\u5c48\u4ece\u5305\u56f4\u5708\u4e2d\u5fc3\u4f20\u51fa\u3002\uff09",
    "\u545c\u545c\u545c\uff0c\u90fd\u8bf4\u4e86\u8fd9\u4e9b\u624d\u4e0d\u662f\u4ec0\u4e48\u94c1\u8840\u72d7\uff0c\u90fd\u662f\u590f\u514b\u91cc\u53ef\u7231\u7684\u5ba0\u7269\u5175\u8681\u5440\uff01|\u590f\u514b\u91cc\u82b1\u4e86\u597d\u591a\u94b1\uff0c\u8fd8\u6539\u88c5\u4e86\u5f88\u4e45\uff0c\u5c31\u662f\u8981\u7ed9\u5927\u5bb6\u5e26\u53bb\u6b22\u4e50\u2014\u2014",
    "\u6211\u3001\u6211\u53c8\u4e0d\u662f\u6545\u610f\u7684\uff01\u4e0d\u662f\u5df2\u7ecf\u8ddf\u4f60\u9053\u6b49\u4e86\u561b\uff01",
    "\u8fd9\u4e2a\u58f0\u97f3\u2026\u2026\u662f\u7edb\u96e8\uff01",
    "\uff08\u4e8c\u4eba\u8fc5\u901f\u6324\u8fdb\u4eba\u7fa4\uff0c\u4eba\u7fa4\u4e2d\u592e\uff0c\u7edb\u96e8\u6b63\u5728\u4e0e\u4e00\u540d\u7a7f\u7740\u5938\u5f20\u7684\u4eba\u5f62\u5bf9\u5cd9\u3002\uff09",
    "\u9053\u6b49\uff1f\u9053\u6b49\u6709\u7528\u7684\u8bdd\u2014\u2014",
    "\uff08\u81ea\u79f0\u201c\u590f\u514b\u91cc\u201d\u7684\u4eba\u5f62\u7a81\u7136\u8f6c\u5934\u76ef\u7740\u5468\u56f4\u7a83\u7a83\u79c1\u8bed\u7684\u4eba\u7fa4\uff0c\u4f3c\u4e4e\u5728\u5206\u6790\u4eba\u4eec\u7684\u8bf4\u8bdd\u5185\u5bb9\u3002\uff09",
    "\u9053\u6b49\u6709\u7528\u7684\u8bdd\u5c31\u2014\u2014\u5c31\u592a\u597d\u5566\uff01",
    "\u545c\u545c\uff0c\u53ef\u662f\u590f\u514b\u91cc\u7684\u6f14\u51fa\u88ab\u7834\u574f\u4e86\uff0c\u7cbe\u5fc3\u51c6\u5907\u7684\u6f14\u51fa\u670d\u90fd\u53d8\u5f97\u7834\u7834\u70c2\u70c2\uff0c\u53d1\u578b\u4e5f\u4e71\u6210\u4e00\u56e2\uff0c\u5ba0\u7269\u5175\u8681\u4eec\u4e5f\u90fd\u56db\u811a\u671d\u5929\u4e86\uff01",
    "\u8fd9\u4e9b\u5f69\u5e26\u53ef\u662f\u590f\u514b\u91cc\u82b1\u4e86\u4e00\u6574\u4e2a\u665a\u4e0a\u624d\u88c5\u8fdb\u5b83\u4eec\u809a\u5b50\u91cc\u7684\uff01\u672c\u6765\u8fd8\u60f3\u7b49\u8d70\u5230\u65b9\u7cd6\u5496\u5561\u9986\u7684\u65f6\u5019\u5410\u5f69\u5e26\u7ed9\u5e97\u957f\u770b\u7684\uff0c\u73b0\u5728\uff0c\u545c\u545c\u2026\u2026\u5168\u90fd\u2026\u2026\u6ca1\u6709\u4e86\uff01",
    "\uff08\u4eba\u5f62\u6307\u7740\u53ef\u601c\u5730\u8eba\u5728\u5730\u4e0a\u7684\u56db\u4e94\u53ea\u5ba0\u7269\u5175\u8681\u5f00\u59cb\u62bd\u6ce3\u3002\uff09",
    "\uff08\u5175\u8681\u7684\u5c0f\u77ed\u817f\u65e0\u529b\u5730\u5728\u7a7a\u4e2d\u6447\u6446\uff0c\u50cf\u662f\u4e3a\u4e86\u5370\u8bc1\u4eba\u5f62\u7684\u8bdd\uff0c\u65f6\u4e0d\u65f6\u4ece\u53e3\u4e2d\u5410\u51fa\u4e00\u6761\u5f69\u5e26\u3002\uff09",
    "\uff08\u4eba\u5f62\u4ece\u751f\u6c14\u5230\u695a\u695a\u53ef\u601c\u7684\u8f6c\u53d8\u8fc7\u4e8e\u7a81\u5140\uff0c\u62bd\u6ce3\u7684\u59ff\u52bf\u4e5f\u5f88\u5938\u5f20\uff0c\u5c31\u7b97\u662f\u9edb\u70df\uff0c\u4e5f\u6123\u5728\u4e86\u539f\u5730\u3002\uff09",
    "\u4f60\u88c5\u4ec0\u4e48\u88c5\u554a\uff01\u800c\u4e14\u4f60\u8bf4\u4ec0\u4e48\uff0c\u5ba0\u7269\u5175\u8681\uff1f\u5175\u8681\uff1f\u5ba0\u7269\uff1f\u4f60\u77e5\u9053\u662f\u5175\u8681\u8fd8\u5f53\u5b83\u662f\u5ba0\u7269\u4f60\u5230\u5e95\u662f\u60f3\u5e72\u4ec0\u4e48\uff1f\uff01",
    "\u545c\u545c\u2014\u2014\u545c\u545c\u54c7\u54c7\u54c7\u2014\u2014\uff01",
    "\uff08\u4eba\u5f62\u5e72\u8106\u5750\u4e86\u4e0b\u6765\uff0c\u6342\u7740\u8138\u5f00\u59cb\u5927\u58f0\u54ed\u6ce3\u3002\uff09",
    "\uff08\u9edb\u70df\u7acb\u523b\u62c9\u4f4f\u4e86\u60f3\u8981\u7ee7\u7eed\u53cd\u51fb\u7684\u7edb\u96e8\u3002\uff09",
    "\u4e0d\u597d\u610f\u601d\uff0c\u5b9e\u5728\u662f\u62b1\u6b49\uff01",
    "\u7edb\u96e8\u5979\u786e\u5b9e\u4e0d\u662f\u6545\u610f\u7684\uff0c\u8fd9\u91cc\u9762\u5b58\u5728\u4e00\u4e9b\u2026\u2026\u8bef\u4f1a\uff01|\u4f5c\u4e3a\u59d0\u59d0\uff0c\u6211\u4ee3\u66ff\u6211\u8fd9\u83bd\u649e\u7684\u59b9\u59b9\u518d\u6b21\u5411\u60a8\u9053\u6b49\uff01",
    "\u554a\uff1f\u8001\u59d0\uff0c\u4f60\u8fc7\u6765\u4e86\uff1f\u53ef\u4f60\u770b\u8fd9\u4e5f\u4e0d\u80fd\u602a\u6211\u554a\uff1f\u4f60\u4e5f\u770b\u5230\u4e86\u5427\u8fd9\u4e2a\u94c1\u8840\u6d82\u88c5\u2014\u2014",
    "\u2026\u2026\u597d\u5427\u597d\u5427\uff0c\u6211\u662f\u4e2a\u6210\u719f\u7684\u4eba\u5f62\u4e86\uff0c\u8981\u6709\u81ea\u5236\u529b\uff0c\u4e0d\u80fd\u8ddf\u8fd9\u79cd\u5c0f\u59d1\u5a18\u4e00\u822c\u89c1\u8bc6\u2026\u2026",
    "\uff08\u7edb\u96e8\u505a\u51fa\u4e86\u62c9\u62c9\u94fe\u7684\u52a8\u4f5c\uff0c\u7528\u624b\u6342\u4f4f\u4e86\u5634\uff0c\u7136\u540e\u5979\u732b\u7740\u8eab\u5b50\u8eb2\u5230\u4e86\u671d\u6656\u8eab\u540e\u3002\uff09",
    "\uff08\u671d\u6656\u51b7\u7740\u8138\u4e00\u624b\u628a\u5979\u62ce\u4e86\u51fa\u6765\uff0c\u6309\u4e0b\u8111\u888b\u3002\uff09",
    "\u2026\u2026\u2026\u2026",
    "\u671d\u6656\u4f60\u5e72\u561b\u6309\u6211\u5934\uff01|\u884c\u5427\u884c\u5427\u6211\u9519\u4e86\u6211\u9519\u4e86\u2014\u2014\u4f60\u522b\u54ed\u4e86\u884c\u4e0d\u884c\uff1f",
    "\u545c\u545c\u54c7\u54c7\u54c7\u2014\u2014\uff01\u590f\u514b\u91cc\u597d\u96be\u8fc7\u2014\u2014\uff01",
    "\u4f60\u3001\u4f60\u6709\u4ec0\u4e48\u6761\u4ef6\uff0c\u4f60\u8bf4\uff01",
    "\u545c\u545c\u2026\u2026\u5927\u5bb6\u90fd\u770b\u5230\u4e86\u5427\uff0c\u590f\u514b\u91cc\u53ef\u662f\u7eaf\u7eaf\u7684\u201c\u53d7\u5bb3\u8005\u201d\u3002",
    "\u4e0d\u8fc7\uff0c\u590f\u514b\u91cc\u4e5f\u4e0d\u662f\u4ec0\u4e48\u4e0d\u8fd1\u4eba\u60c5\u7684\u5c0f\u6076\u9b54\uff0c\u4e0d\u4f1a\u6545\u610f\u4e3a\u96be\u4eba\u7684\uff0c\u53ea\u8981\u652f\u4ed8\u8fd9\u2014\u2014\u4e2a\u6570\u7684\u8d54\u507f\u91d1\uff0c\u590f\u514b\u91cc\u5c31\u539f\u8c05\u60a8\uff0c\u600e\u4e48\u6837\uff1f",
    "\uff08\u590f\u514b\u91cc\u8000\u6b66\u626c\u5a01\u5730\u4f38\u51fa\u4e24\u6839\u624b\u6307\u3002\uff09",
    "\u8428\u72c4\u65af\u91d12\u76ce\u53f8\uff1f|\u8fd8\u597d\u5566\uff0c\u6211\u8fd8\u662f\u2026\u2026\u53ef\u4ee5\u4ed8\u5f97\u8d77\uff0c\u6ca1\u95ee\u9898\uff01",
    "20\u54e6\u3002",
    "\u4f60\u62a2\u94b1\uff1f\uff01",
    "\uff08\u7edb\u96e8\u77aa\u7740\u773c\u775b\u903c\u8fd1\u590f\u514b\u91cc\uff0c\u671d\u6656\u8d70\u4e0a\u524d\u62e6\u4f4f\u4e86\u5979\uff0c\u7136\u540e\u8e72\u4e0b\u6765\u67e5\u770b\u5175\u8681\u7684\u53d7\u635f\u60c5\u51b5\u3002\uff09",
    "\u8fd9\u4e9b\u5ba0\u7269\u5175\u8681\uff0c\u5e76\u672a\u5b8c\u5168\u635f\u574f\uff0c\u5b8c\u5168\u53ef\u4ee5\u4fee\u590d\u3002|\u4f60\u7684\u670d\u88c5\u548c\u9053\u5177\uff0c\u4e5f\u4e0d\u662f\u4ec0\u4e48\u540d\u8d35\u6750\u6599\uff0c\u4ef7\u94b1\u65b9\u9762\uff0c\u5e94\u8be5\u8fd8\u6709\u8f6c\u571c\u7684\u4f59\u5730\u5427\uff1f",
    "\u5e72\u3001\u5e72\u561b\u5566\uff0c\u590f\u514b\u91cc\u7684\u6f14\u51fa\u670d\u53ef\u662f\u7eaf\u624b\u5de5\u5236\u4f5c\uff0c\u662f\u65e0\u4ef7\u7684\u5462\uff01|\u8fd8\u6709\u6211\u7684\u5b9d\u8d1d\u5175\u8681\u4eec\uff0c\u53ef\u90fd\u662f\u6700\u65b0\u6b3e\u5462\u2026\u2026",
    "\uff08\u590f\u514b\u91cc\u88ab\u51b7\u7740\u8138\u5f00\u53e3\u7684\u671d\u6656\u5413\u5230\uff0c\u5411\u540e\u9000\u4e86\u4e00\u6b65\u3002\uff09",
    "\uff08\u4f46\u5979\u7acb\u523b\u53c9\u8d77\u8170\uff0c\u6c14\u9f13\u9f13\u5730\u6485\u8d77\u5634\u3002\uff09",
    "\u8fd9\u90fd\u662f\u590f\u514b\u91cc\u73cd\u8d35\u7684\u5b9d\u7269\uff01\u4e0d\u80fd\u7528\u5e38\u7406\u770b\u5f85\uff01",
    "\u590f\u514b\u91cc\u5c0f\u59d0\uff0c\u65e2\u7136\u635f\u574f\u4e86\u60a8\u7684\u7269\u54c1\uff0c\u6211\u4eec\u4e00\u5b9a\u4f1a\u8fdb\u884c\u8d54\u507f\uff0c\u53ea\u662f\u8d54\u507f\u7684\u6570\u989d\u548c\u4ea4\u4ed8\u8d54\u507f\u91d1\u7684\u65f6\u95f4\u5e0c\u671b\u80fd\u591f\u2026\u2026",
    "\u524d\u9762\u7684\uff0c\u4f60\u8fd8\u8d70\u4e0d\u8d70\u554a\uff1f\u540e\u9762\u7684\u961f\u4f0d\u53ef\u90fd\u6392\u961f\u7b49\u7740\u5462\uff01",
    "\uff08\u4e24\u4eba\u7684\u5bf9\u8bdd\u88ab\u6b64\u8d77\u5f7c\u4f0f\u7684\u50ac\u4fc3\u58f0\u6253\u65ad\uff0c\u540e\u9762\u7684\u82b1\u8f66\u8f66\u5934\u4e5f\u5df2\u7ecf\u8d34\u4e0a\u4e86\u590f\u514b\u91cc\u6240\u5728\u82b1\u8f66\u7684\u5c3e\u5df4\u3002\uff09",
    "\u597d\u5566\u597d\u5566\uff0c\u4e0d\u8981\u518d\u50ac\u4e86\uff0c\u590f\u514b\u91cc\u8fd9\u5c31\u5f00\u52a8\u54e6~|\u5514\uff0c\u60a8\u521a\u624d\u8bf4\u4ec0\u4e48\uff1f",
    "\u5173\u4e8e\u6211\u59b9\u59b9\u7edb\u96e8\u7ed9\u60a8\u9020\u6210\u7684\u635f\u5931\uff0c\u662f\u5426\u53ef\u4ee5\u8fdb\u884c\u4e00\u4e2a\u66f4\u52a0\u5ba2\u89c2\u5408\u7406\u7684\u8bc4\u4f30\u5462\uff1f",
    "\uff08\u590f\u514b\u91cc\u5fc3\u4e0d\u5728\u7109\u5730\u6446\u6446\u624b\uff0c\u7136\u540e\u6025\u5306\u5306\u5730\u5728\u6f14\u51fa\u670d\u91cc\u6478\u7d22\u4e86\u4e00\u9635\uff0c\u62ff\u51fa\u4e00\u5f20\u76b1\u5df4\u5df4\u7684\u5ba3\u4f20\u5355\uff0c\u4e0d\u77e5\u9053\u4ece\u54ea\u91cc\u53c8\u638f\u51fa\u4e00\u53ea\u7709\u7b14\uff0c\u5728\u5ba3\u4f20\u5355\u4e0a\u7b7e\u4e86\u540d\u540e\u4e00\u628a\u585e\u8fdb\u9edb\u70df\u6000\u91cc\u3002\uff09",
    "\u6765\u4e0d\u53ca\u4e86\u6765\u4e0d\u53ca\u4e86\uff01\u73b0\u5728\u53ef\u4e0d\u662f\u780d\u4ef7\u7684\u65f6\u5019\uff0c\u7b49\u51c6\u5907\u597d\u8d54\u507f\u91d1\uff0c\u5c31\u6765\u8fd9\u4e0a\u9762\u7684\u5730\u5740\u5427\uff01",
    "\u4e8b\u5148\u8b66\u544a\u60a8\u54e6\uff0c\u53ef\u4e0d\u8981\u60f3\u7740\u8d56\u6389\u590f\u514b\u91cc\u7684\u8d26\uff0c\u590f\u514b\u91cc\u5df2\u7ecf\u628a\u6240\u6709\u4eba\u7684\u6837\u5b50\u90fd\u5b8c\u2014\u2014\u5168\u8bb0\u4f4f\u4e86\uff01",
    "\uff08\u8868\u60c5\u4e25\u8083\u5730\u8bf4\u5b8c\uff0c\u590f\u514b\u91cc\u5411\u5175\u8681\u53d1\u51fa\u6307\u4ee4\u3002\uff09",
    "\uff08\u5730\u4e0a\u7684\u5ba0\u7269\u5175\u8681\u4e00\u7638\u4e00\u62d0\u5730\u722c\u4e0a\u4e86\u82b1\u8f66\uff0c\u5b9e\u5728\u65e0\u6cd5\u52a8\u5f39\u7684\uff0c\u4e5f\u88ab\u590f\u514b\u91cc\u4e00\u624b\u4e00\u4e2a\uff0c\u8270\u96be\u5730\u4e22\u4e86\u4e0a\u53bb\u3002\uff09",
    "\u597d\u5566\u597d\u5566\uff0c\u63a5\u4e0b\u6765\u662f\u2014\u2014Show\u2665Time~",
    "\u8001\u59d0\uff0c\u671d\u6656\uff0c\u4f60\u4eec\u770b\u5230\u4e86\u5417\uff1f\u4e00\u9635\u70df\u96fe\u7684\u65f6\u95f4\uff0c\u5979\u5c31\u6362\u4e0a\u4e86\u65b0\u7684\u6f14\u51fa\u670d\uff01|\u6211\u4eec\u80fd\u4e0d\u80fd\u4e70\u4e9b\u8fd9\u79cd\u5389\u5bb3\u7684\u9053\u5177\u554a\u2026\u2026\u8fd9\u6837\u89c2\u4f17\u6570\u91cf\u4e00\u5b9a\u4f1a\u7ffb\u500d\u7684\uff01",
    "\uff08\u76ee\u9001\u82b1\u8f66\u6e10\u6e10\u5f00\u8fdc\uff0c\u671d\u6656\u8038\u4e86\u8038\u80a9\uff0c\u6ca1\u6709\u8bf4\u8bdd\uff0c\u53ea\u662f\u7528\u4e0b\u5df4\u6307\u4e86\u6307\u4e00\u65c1\u6b63\u5728\u53f9\u6c14\u7684\u9edb\u70df\u3002\uff09",
    "\u6211\u60f3\u4e50\u961f\u7684\u652f\u51fa\u5e94\u8be5\u4e5f\u4f1a\u7ffb\u500d\u7684\u3002|\u73b0\u5728\u2026\u2026\u6700\u91cd\u8981\u7684\u662f\u53ca\u65f6\u8d76\u56de\u9910\u5385\u3002",
    "\u4eca\u665a\u7684\u6f14\u51fa\u4e0d\u8981\u8fdf\u5230\uff0c\u597d\u5417\uff1f\u6211\u4eb2\u7231\u7684\u59b9\u59b9\u3002"
  ]
]