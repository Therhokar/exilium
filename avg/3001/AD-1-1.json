[
  [
    "\u827e\u83ab\u53f7\u2026\u2026\u8fd9\u4e48\u591a\u5e74\u8fd8\u662f\u6ca1\u53d8\u5462\u3002",
    "\u548c\u5f53\u5e74\u6bd4\u8fd8\u662f\u7834\u65e7\u4e86\u4e0d\u5c11\uff0c\u5e0c\u671b\u4f60\u4e0d\u8981\u4ecb\u610f\u5c31\u597d\u3002",
    "\u600e\u4e48\u53ef\u80fd\u4ecb\u610f\u5462\uff0c\u80fd\u56de\u5230\u8fd9\u91cc\u662f\u6211\u7684\u8363\u5e78\u624d\u5bf9\u3002",
    "\uff08\u9edb\u70df\u73af\u987e\u56db\u5468\uff0c\u8fd9\u4e2a\u719f\u6089\u53c8\u964c\u751f\u7684\u5730\u65b9\u4eff\u4f5b\u53c8\u5c06\u5979\u5e26\u56de\u4e86\u5f53\u521d\u7684\u5c81\u6708\u3002\uff09",
    "\uff08\u79bb\u5f00\u683c\u91cc\u82ac\u540e\uff0c\u5341\u6570\u5e74\u7684\u65f6\u5149\u662f\u5982\u6b64\u7684\u6f2b\u957f\uff0c\u4f46\u5728\u89c1\u5230\u6211\u65f6\uff0c\u5979\u53c8\u4eff\u4f5b\u7a7f\u68ad\u56de\u4e86\u8fc7\u53bb\u3002\uff09",
    "\uff08\u65c5\u7a0b\u5e26\u6765\u7684\u75b2\u60eb\u548c\u4e0d\u5b89\u5728\u9010\u6e10\u6d88\u53bb\uff0c\u968f\u4e4b\u800c\u6765\u7684\u662f\u6d8c\u4e0a\u80f8\u53e3\u7684\u503e\u8bc9\u4e4b\u60c5\u3002\uff09",
    "\uff08\u9edb\u70df\u660e\u767d\uff0c\u6211\u6709\u5f88\u591a\u6545\u4e8b\u60f3\u4e0e\u5979\u5206\u4eab\uff0c\u800c\u5979\u4e5f\u6709\u5f88\u591a\u6545\u4e8b\u60f3\u5411\u6211\u8bc9\u8bf4\u3002\uff09",
    "\uff08\u8fd9\u4e9b\u5e74\u7684\u70b9\u70b9\u6ef4\u6ef4\uff0c\u8be5\u4ece\u4f55\u8bf4\u8d77\uff1f\uff09",
    "\uff08\u9edb\u70df\u6709\u4e00\u4e9b\u72b9\u8c6b\uff0c\u5979\u8bd5\u63a2\u6027\u5730\u770b\u5411\u4e86\u6211\uff0c\u6211\u4e5f\u6e29\u67d4\u5730\u7b11\u7740\u671d\u5979\u70b9\u4e86\u70b9\u5934\u3002\uff09",
    "\uff08\u6211\u5c06\u503e\u8bc9\u7684\u673a\u4f1a\u8ba9\u7ed9\u4e86\u5979\u3002\uff09",
    "\u957f\u5b98\uff0c\u60a8\u8fd8\u8bb0\u5f97\u5f53\u521d\u6211\u4eec\u5728CHE-02\u536b\u661f\u57ce\u76f8\u89c1\u65f6\u53d1\u751f\u7684\u4e8b\u5417\uff1f",
    "\u6709\u5370\u8c61\uff0c\u90a3\u65f6\u53ef\u771f\u662f\u60ca\u5fc3\u52a8\u9b44\u5462\u3002",
    "\u662f\u554a\uff0c\u6ca1\u60f3\u5230\u5341\u5e74\u540e\u4f1a\u5728\u90a3\u6837\u7684\u60c5\u51b5\u4e0b\u76f8\u89c1\u2026\u2026",
    "\u90a3\u65f6\u5019\uff0c\u6211\u8bb0\u5f97\u9edb\u70df\u8fd8\u5728\u6e38\u5386\u5404\u4e2a\u4e0d\u540c\u7684\u57ce\u5e02\u5427\uff1f",
    "\u662f\u7684\uff0c\u6211\u60f3\u62e5\u6709\u66f4\u591a\u7684\u7ecf\u5386\uff0c\u7136\u540e\u628a\u8fd9\u4e9b\u7ecf\u5386\u878d\u8fdb\u6211\u6240\u521b\u4f5c\u7684\u97f3\u4e50\u4e2d\u3002|\u5982\u679c\u80fd\u8ba9\u957f\u5b98\u542c\u5230\uff0c\u6211\u60f3\u60a8\u4e00\u5b9a\u80fd\u5bdf\u89c9\u5230\u6211\u7684\u6210\u957f\u3002",
    "\u55ef\uff0c\u4e0d\u7ba1\u662f\u6211\u8fd8\u662f\u5927\u5bb6\u90fd\u80fd\u4ece\u4e50\u66f2\u4e2d\u611f\u53d7\u5230\u9edb\u70df\u4f60\u6240\u611f\u77e5\u5230\u7684\u4e00\u5207\u3002|\u5c24\u5176\u662f\u90a3\u9996\u536b\u661f\u57ce\u4e2d\u6240\u521b\u4f5c\u7684\u66f2\u5b50\u2014\u2014",
    "\u90a3\u9996\u2026\u2026\u300a\u9a6c\u94c3\u85af\u82b1\u5f00\u7684\u65f6\u5019\u300b\u5417\uff1f",
    "\u662f\u554a\uff0c\u90a3\u662f\u9996\u6709\u7740\u5f88\u591a\u611f\u60c5\u7684\u66f2\u5b50\u5462\u2026\u2026",
    "\u55ef\uff0c\u4e5f\u6709\u5f88\u591a\u56de\u5fc6\u2026\u2026",
    "\uff08\u5373\u4f7f\u73b0\u5728\u56de\u60f3\u8d77\u6765\uff0c\u5f53\u65f6\u7684\u7ecf\u5386\u4f9d\u7136\u8ba9\u9edb\u70df\u6709\u4e9b\u5fc3\u6709\u4f59\u60b8\u3002\uff09",
    "\uff08\u5979\u5c06\u89c6\u7ebf\u6295\u5f80\u7a97\u5916\uff0c\u601d\u7eea\u5f00\u59cb\u56de\u5230\u5f53\u65f6\u7684\u65f6\u5149\u2026\u2026\uff09",
    "\uff08\u2026\u2026\uff09",
    "\u8fd9\u91cc\u662f\u2026\u2026\uff1f",
    "\uff08\u9edb\u70df\u5728\u4e00\u4e2a\u964c\u751f\u7684\u623f\u95f4\u9192\u6765\u3002\uff09",
    "\uff08\u5979\u7684\u5fc3\u667a\u8fd8\u5728\u5267\u70c8\u5730\u6ce2\u52a8\uff0c\u5979\u8bd5\u56fe\u8fdb\u884c\u81ea\u68c0\u6765\u7a33\u5b9a\u7cfb\u7edf\uff0c\u4f46\u662f\u5404\u79cd\u5f02\u5e38\u7684\u62a5\u9519\u8ba9\u5979\u65e0\u6cd5\u51b7\u9759\u4e0b\u6765\u3002\uff09",
    "\u6211\u2026\u2026\u91cd\u542f\u4e86\uff1f\u8fd9\u662f\u4ec0\u4e48\u5730\u65b9\uff1f",
    "\uff08\u65e5\u5fd7\u4e2d\u51fa\u73b0\u53d7\u635f\u7684\u62a5\u544a\uff0c\u4f46\u5979\u5374\u65e0\u6cd5\u60f3\u8d77\u4e4b\u524d\u53d1\u751f\u4e86\u4ec0\u4e48\u3002\uff09",
    "\uff08\u9edb\u70df\u52c9\u5f3a\u5730\u652f\u6491\u8d77\u81ea\u5df1\u7684\u8eab\u4f53\u3002\uff09",
    "\uff08\u5979\u53d1\u73b0\u81ea\u5df1\u8eab\u5904\u4e00\u4e2a\u7b80\u6613\u7684\u7ef4\u4fee\u4ed3\u5185\uff0c\u8eab\u4e0a\u6709\u660e\u663e\u7684\u4fee\u590d\u75d5\u8ff9\uff0c\u4e07\u5e78\u7684\u662f\u4f24\u53e3\u90fd\u5df2\u7ecf\u5904\u7406\u59a5\u5f53\u4e86\u3002\uff09",
    "\uff08\u9edb\u70df\u5f00\u59cb\u5bfb\u627e\u8eab\u8fb9\u7684\u7269\u54c1\uff0c\u6b66\u5668\u2026\u2026\u8fd8\u6709\u53e4\u7b5d\u90fd\u4e0d\u89c1\u4e86\u3002\uff09",
    "\uff08\u4e4b\u524d\u4e00\u5b9a\u53d1\u751f\u4e86\u4ec0\u4e48\u4e0d\u597d\u7684\u4e8b\uff0c\u81ea\u5df1\u884c\u8d70\u8fc7\u90a3\u4e48\u591a\u57ce\u5e02\u90fd\u4ece\u6765\u6ca1\u6709\u53d1\u751f\u8fc7\u8fd9\u6837\u7684\u72b6\u51b5\u3002\uff09",
    "\uff08\u5468\u56f4\u7684\u4e00\u5207\u90fd\u90a3\u4e48\u7684\u964c\u751f\uff0c\u8fd9\u4e0d\u7981\u8ba9\u9edb\u70df\u6709\u4e00\u4e9b\u5bb3\u6015\u3002\uff09",
    "\uff08\u66f4\u8ba9\u5979\u62c5\u5fe7\u7684\u662f\uff0c\u5979\u7684\u540c\u4f34\u73b0\u5728\u4e0d\u5728\u8eab\u8fb9\u3002\uff09",
    "\u7edb\u96e8\u2026\u2026\u671d\u6656\u2026\u2026\u4f60\u4eec\u5728\u54ea\u91cc\uff1f",
    "\uff08\u623f\u95f4\u91cc\u4e00\u4e2a\u4eba\u90fd\u6ca1\u6709\uff0c\u53ea\u6709\u7ef4\u4fee\u4ed3\u53d1\u51fa\u7684\u4f4e\u6c89\u7684\u566a\u97f3\u3002\uff09",
    "\uff08\u9edb\u70df\u5c3d\u529b\u8ba9\u81ea\u5df1\u51b7\u9759\u4e0b\u6765\uff0c\u968f\u65f6\u51c6\u5907\u5e94\u5bf9\u53ef\u80fd\u53d1\u751f\u7684\u60c5\u51b5\u3002\uff09",
    "\u4e0d\u7528\u62c5\u5fc3\u5979\u4eec\uff0c\u5979\u4eec\u73b0\u5728\u90fd\u5f88\u5b89\u5168\u3002",
    "\u8fd9\u4e2a\u58f0\u97f3\u662f\uff1f",
    "\uff08\u9edb\u70df\u987a\u7740\u58f0\u97f3\u770b\u53bb\uff0c\u8ba9\u5979\u4e07\u5206\u60ca\u8bb6\u7684\u662f\uff0c\u4e00\u5f20\u719f\u6089\u7684\u9762\u5e9e\u51fa\u73b0\u5728\u81ea\u5df1\u773c\u524d\u3002\uff09",
    "\u6625\u7530\u2026\u2026\u6625\u7530\u5c0f\u59d0\uff01",
    "\uff08\u6625\u7530\u5e26\u7740\u719f\u6089\u7684\u7b11\u5bb9\u8d70\u8fdb\u4e86\u623f\u95f4\uff0c\u770b\u7740\u8fd9\u7b11\u5bb9\uff0c\u9edb\u70df\u7ec8\u4e8e\u7a0d\u7a0d\u653e\u4e0b\u5fc3\u6765\u3002\uff09",
    "\u6625\u7530\u5c0f\u59d0\uff1f\u662f\u683c\u91cc\u82ac\u7684\u6625\u7530\u5c0f\u59d0\u5417\uff1f",
    "\u662f\u7684\uff0c\u7684\u786e\u662f\u6211\u3002\u4e0d\u4f1a\u8fc7\u4e86\u5341\u5e74\u5df2\u7ecf\u8ba4\u4e0d\u51fa\u6211\u6765\u4e86\u5427\uff1f",
    "\uff08\u8fde\u58f0\u97f3\u4e5f\u662f\u90a3\u719f\u6089\u7684\u611f\u89c9\uff0c\u4f1a\u8fd9\u6837\u8bf4\u8bdd\u7684\u4eba\u5f62\u9664\u4e86\u6625\u7530\u5c0f\u59d0\u4e0d\u4f1a\u518d\u6709\u522b\u4eba\u4e86\u3002\uff09",
    "\uff08\u9edb\u70df\u7ec8\u4e8e\u5b8c\u5168\u653e\u4e0b\u4e86\u6212\u5fc3\uff0c\u6697\u4e2d\u9632\u5fa1\u7684\u59ff\u6001\u7f13\u89e3\u4e86\u4e0b\u6765\u3002\u6625\u7530\u4e5f\u53d1\u73b0\u4e86\u8fd9\u4e00\u70b9\uff0c\u5fae\u7b11\u7740\u671d\u9edb\u70df\u8f7b\u8f7b\u70b9\u4e86\u70b9\u5934\u3002\uff09",
    "\u597d\u4e45\u4e0d\u89c1\uff0c\u9edb\u70df\u3002|\u5e78\u597d\u6211\u8fd9\u91cc\u6709\u7ef4\u4fee\u4ed3\uff0c\u867d\u7136\u7b80\u6613\uff0c\u4e0d\u8fc7\u529f\u80fd\u8fd8\u7b97\u591f\u7528\u3002",
    "\u2026\u2026\u6211\u8fd9\u662f\uff1f",
    "\u4f60\u7684\u7ef4\u4fee\u521a\u521a\u624d\u5b8c\u6210\uff0c\u7cfb\u7edf\u8fd8\u6ca1\u6709\u5b8c\u5168\u7a33\u5b9a\uff0c\u4e00\u4f1a\u6211\u4f1a\u518d\u66ff\u4f60\u8c03\u8bd5\u4e00\u4e0b\u3002",
    "\uff08\u5c31\u5728\u548c\u6625\u7530\u5bf9\u8bdd\u65f6\uff0c\u4e24\u4e2a\u8eab\u5f71\u7a81\u7136\u4ece\u6625\u7530\u7684\u80cc\u540e\u7a9c\u4e86\u51fa\u6765\uff0c\u6025\u5306\u5306\u5730\u8dd1\u5230\u9edb\u70df\u773c\u524d\u3002\uff09",
    "\u8001\u59d0\uff01\u4f60\u8fd8\u597d\u5427\uff1f\u4f60\u521a\u624d\u5413\u6b7b\u6211\u4e86\uff01",
    "\u62b1\u6b49\u9edb\u70df\uff0c\u6211\u5b9e\u5728\u662f\u62c9\u4e0d\u4f4f\u5979\uff0c\u539f\u672c\u5e94\u8be5\u8ba9\u4f60\u597d\u597d\u7ef4\u4fee\u7684\u3002",
    "\u7edb\u96e8\uff0c\u671d\u6656\uff0c\u4f60\u4eec\u90fd\u6ca1\u4e8b\u5427\uff1f",
    "\u6211\u4eec\u5f53\u7136\u6ca1\u4e8b\u554a\uff01\u5012\u662f\u8001\u59d0\u4f60\uff0c\u81ea\u5df1\u72ec\u81ea\u8dd1\u51fa\u53bb\uff0c\u7136\u540e\u5c31\u518d\u4e5f\u6ca1\u6d88\u606f\u4e86\uff0c\u5413\u6b7b\u6211\u4e86\uff01",
    "\u6211\u4eec\u90fd\u5f88\u62c5\u5fc3\u4f60\uff0c\u521a\u624d\u53d1\u751f\u4ec0\u4e48\u4e86\uff1f",
    "\u2026\u2026\u6211\uff0c\u6211\u8bb0\u4e0d\u592a\u6e05\u4e86\u3002",
    "\u770b\u6765\u6a21\u5757\u7684\u517c\u5bb9\u6027\u8fd8\u6709\u4e9b\u95ee\u9898\u2026\u2026\u4f60\u521a\u624d\u88ab\u88ad\u51fb\u4e86\uff0c\u5e78\u597d\u88ab\u6307\u6325\u5b98\u6551\u4e0b\uff0c\u4e0d\u7136\u5f53\u65f6\u60c5\u51b5\u5c31\u5371\u9669\u4e86\u3002",
    "\u6307\u2026\u2026\u6307\u6325\u5b98\uff1f",
    "\u662f\u6211\u60f3\u7684\u90a3\u4f4d\u201c\u6307\u6325\u5b98\u201d\u5417\uff1f",
    "\uff08\u811a\u6b65\u58f0\u4ece\u95e8\u5916\u4f20\u6765\uff0c\u9edb\u70df\u5ffd\u7136\u7d27\u5f20\u4e86\u8d77\u6765\uff0c\u5979\u6025\u5fd9\u5750\u8d77\u8eab\u6765\uff0c\u68b3\u7406\u7740\u81ea\u5df1\u6709\u4e00\u4e9b\u6742\u4e71\u7684\u5934\u53d1\u3002\uff09",
    "\uff08\u8fd8\u6ca1\u7b49\u5979\u51c6\u5907\u597d\uff0c\u9edb\u70df\u6240\u671f\u5f85\u7684\u90a3\u4e2a\u8eab\u5f71\u5df2\u7ecf\u8d70\u4e86\u8fdb\u6765\u3002\uff09",
    "\u6625\u7530\uff0c\u9edb\u70df\u60c5\u51b5\u5982\u4f55\u4e86\u3002",
    "\u957f\u5b98\uff0c\u771f\u7684\u662f\u60a8\u5417\uff01\uff1f",
    "\u554a\uff0c\u4f60\u5df2\u7ecf\u6062\u590d\u4e86\u554a\u3002",
    "\u55ef\u2026\u2026\u6211\uff0c\u6211\u5df2\u7ecf\u6ca1\u4e8b\u4e86\u957f\u5b98\u3002",
    "\u563f\u563f\uff0c\u8001\u59d0\u4e00\u770b\u5230\u957f\u5b98\u8fde\u6001\u5ea6\u90fd\u53d8\u4e86\u5462\uff0c\u660e\u660e\u5e73\u65f6\u90fd\u662f\u6446\u51fa\u4e00\u526f\u957f\u8f88\u7684\u6837\u5b50~",
    "\u7edb\u96e8\uff01",
    "\u770b\u8d77\u6765\u5df2\u7ecf\u6ca1\u4e8b\u4e86\u3002\u6625\u7530\uff0c\u9edb\u70df\u7684\u72b6\u51b5\u5982\u4f55\uff1f",
    "\u8fd8\u6709\u70b9\u7cfb\u7edf\u4e0a\u7684\u95ee\u9898\uff0c\u9a6c\u4e0a\u5c31\u80fd\u8c03\u8bd5\u5b8c\u3002",
    "\u9ebb\u70e6\u4f60\u4e86\u6625\u7530\u3002|\u90a3\u4e2a95\u2026\u2026\u73b0\u5728\u662f\u53eb\u9edb\u70df\u4e86\u5427\uff0c\u9edb\u70df\uff0c\u597d\u4e45\u4e0d\u89c1\uff0c\u5e94\u8be5\u6709\u2026\u2026\u5341\u5e74\u4e86\u5427\uff1f",
    "\u6ca1\u9519\uff0c\u5df2\u7ecf\u5341\u5e74\u4e86\u2026\u2026\u6211\u7ec8\u4e8e\u518d\u6b21\u89c1\u5230\u60a8\u4e86\u2026\u2026",
    "\u771f\u7684\u5df2\u7ecf\u8fc7\u53bb\u597d\u4e45\u4e86\uff0c\u6ca1\u60f3\u5230\u4f1a\u5728\u6211\u6b63\u5728\u8ffd\u67e5\u7684\u6848\u4ef6\u4e2d\u9047\u5230\u4f60\u2026\u2026\u6216\u8bb8\u8fd9\u4e5f\u662f\u67d0\u79cd\u7f18\u5206\uff1f",
    "\u6848\u4ef6\uff1f",
    "\u6ca1\u9519\uff0c\u6848\u4ef6\u3002|\u4f60\u88ab\u7275\u626f\u5230\u4e86\u67d0\u4e2a\u6848\u4ef6\u4e2d\uff0c\u521a\u521a\u906d\u9047\u4e86\u88ad\u51fb\uff0c\u8fd8\u8bb0\u5f97\u4e4b\u524d\u53d1\u751f\u7684\u4e8b\u4e48\uff1f",
    "\u2026\u2026\u6211\u52aa\u529b\u56de\u5fc6\u3002",
    "\u7cfb\u7edf\u7684\u7a33\u5b9a\u6027\u6a21\u5757\u5df2\u7ecf\u66ff\u4f60\u91cd\u65b0\u8986\u76d6\u4e86\uff0c\u73b0\u5728\u9edb\u70df\u2026\u2026\u4f60\u7684\u5404\u9879\u7cfb\u7edf\u5e94\u8be5\u90fd\u80fd\u6b63\u5e38\u8fd0\u884c\u4e86\u5427\u3002",
    "\uff08\u9edb\u70df\u611f\u89c9\u81ea\u5df1\u7684\u5fc3\u667a\u7a33\u5b9a\u4e86\u4e0b\u6765\uff0c\u521a\u624d\u56e0\u4e3a\u60ca\u614c\u800c\u6a21\u7cca\u7684\u8bb0\u5fc6\u4e5f\u5f00\u59cb\u6062\u590d\u3002\uff09",
    "\uff08\u5979\u5411\u6625\u7530\u548c\u6211\u70b9\u4e86\u70b9\u5934\uff0c\u5f00\u59cb\u5728\u5fc3\u667a\u4e2d\u68c0\u7d22\u521a\u624d\u6240\u7ecf\u5386\u7684\u4e00\u5207\u3002\uff09",
    "\u90a3\u4e48\u9edb\u70df\uff0c\u4f60\u80fd\u5c06\u521a\u624d\u53d1\u751f\u7684\u4e8b\u544a\u8bc9\u6211\u5417\uff1f",
    "\u597d\u7684\uff0c\u957f\u5b98\u3002\u6211\u5df2\u7ecf\u90fd\u56de\u60f3\u8d77\u6765\u4e86\uff0c\u8bf7\u60a8\u542c\u6211\u8bb2\u8ff0\u5427\u3002",
    "\uff08CHE-02\u536b\u661f\u57ce\uff0cB3\u533a\u2014\u2014\uff09",
    "\uff08\u591c\u5e55\u964d\u4e34\uff0c\u767d\u65e5\u5b89\u9759\u7684\u8857\u9053\u9010\u6e10\u53d8\u5f97\u7199\u7199\u6518\u6518\u3002\uff09",
    "\uff08\u9053\u8def\u65c1\uff0c\u5de5\u4eba\u4eec\u4e58\u7740\u5347\u964d\u68af\uff0c\u5c06\u7cfb\u7740\u5404\u8272\u88c5\u9970\u7684\u5c3c\u9f99\u7ef3\u6302\u5728\u8def\u706f\u4e4b\u95f4\u3002\uff09",
    "\uff08\u901a\u7535\u540e\uff0c\u706f\u5149\u4f9d\u6b21\u4eae\u8d77\uff0c\u5411\u8fdc\u5904\u873f\u8712\u7740\u5f62\u6210\u4e86\u4e00\u6761\u5149\u7684\u6cb3\u6d41\u3002\uff09",
    "\uff08\u6b63\u62ce\u7740\u5927\u5305\u5c0f\u5305\u8d76\u8def\u7684\u9edb\u70df\u88ab\u706f\u5149\u5438\u5f15\uff0c\u4e0d\u7531\u505c\u4e0b\u4e86\u5306\u5306\u7684\u811a\u6b65\u3002\uff09",
    "\u54c7\u2026\u2026\u597d\u7f8e\u554a\uff0c\u8fd9\u79cd\u201c\u5f20\u706f\u7ed3\u5f69\u201d\u7684\u573a\u666f\uff0c\u4eb2\u773c\u6240\u89c1\uff0c\u53ef\u8981\u6bd4\u5f71\u89c6\u8d44\u6599\u91cc\u58ee\u89c2\u591a\u4e86\u3002",
    "\u54fc\u54fc~\u6211\u5c31\u77e5\u9053\u8001\u59d0\u4f60\u4f1a\u559c\u6b22\u8fd9\u4e2a\u3002|\u600e\u4e48\u6837\uff1f\u6bd4\u8d77\u5728\u9910\u9986\u91cc\u626b\u5730\u5237\u76d8\u5b50\uff0c\u8ddf\u6211\u51fa\u95e8\u91c7\u8d2d\u624d\u662f\u6b63\u786e\u7684\u9009\u62e9\u5427\uff1f",
    "\u4f60\u53ef\u522b\u5fd8\u4e86\uff0c\u662f\u671d\u6656\u5e2e\u6211\u4eec\u63fd\u4e0b\u4e86\u6240\u6709\u7684\u7b79\u5907\u5de5\u4f5c\uff0c\u6211\u4eec\u624d\u6709\u673a\u4f1a\u51fa\u6765\u7684\u3002\u5f85\u4f1a\u513f\u53ef\u5f97\u597d\u597d\u611f\u8c22\u5979\u3002",
    "\u77e5\u9053\u4e86\u77e5\u9053\u4e86\uff0c\u611f\u8c22\u671d\u6656\u7684\u65e0\u79c1\u5949\u732e\uff01\u8001\u59d0\uff0c\u6211\u4eec\u53bb\u90a3\u8fb9\u2014\u2014\uff01",
    "\u6b38\uff0c\u6162\u7740\uff01",
    "\uff08\u9edb\u70df\u773c\u75be\u624b\u5feb\u5730\u63ea\u4f4f\u50cf\u6ce5\u9cc5\u4e00\u6837\u60f3\u8981\u7a9c\u8d70\u7684\u7edb\u96e8\u3002\uff09",
    "\u7edb\u96e8\u2014\u2014\u4f60\u5fd8\u4e86\u6211\u4eec\u7684\u9996\u8981\u4efb\u52a1\u662f\u91c7\u8d2d\u7269\u8d44\u5417\uff1f\u4f60\u518d\u7ffb\u7ffb\u6e05\u5355\uff0c\u6211\u4eec\u8fd8\u6709\u597d\u51e0\u6837\u4e1c\u897f\u6ca1\u4e70\u5230\u5462\u3002",
    "\u8fd9\u4e9b\u2026\u2026\u4e70\u8d77\u6765\u4e5f\u633a\u5feb\u7684\u5427\uff01|\u54ce\u5440\uff0c\u53cd\u6b63\u65f6\u95f4\u8fd8\u65e9\uff0c\u6211\u4eec\u5148\u56db\u5904\u8f6c\u8f6c\u561b\uff0c\u63d0\u524d\u611f\u53d7\u4e00\u4e0b\u5e86\u5178\u6c1b\u56f4~",
    "\u4e0d\u53ef\u4ee5\uff0c\u6211\u4eec\u73b0\u5728\u8fd8\u662f\u5728\u5de5\u4f5c\u4e2d\uff0c\u5f97\u8d76\u7d27\u5b8c\u6210\u91c7\u8d2d\u5c06\u4e1c\u897f\u9001\u56de\u53bb\uff0c\u5f71\u54cd\u5230\u9910\u9986\u7684\u6b63\u5e38\u8425\u4e1a\u5c31\u4e0d\u597d\u4e86\u3002",
    "\u53ef\u662f\u6211\u4eec\u4e00\u56de\u53bb\u5c31\u8981\u5f00\u59cb\u6f14\u594f\u4e86\u5440\uff0c\u5de5\u4f5c\u7ed3\u675f\u8857\u4e0a\u65e9\u5c31\u6ca1\u4eba\u4e86\uff01",
    "\u7edb\u96e8\u2026\u2026\u8981\u662f\u4e0d\u80fd\u53ca\u65f6\u628a\u7f3a\u5c11\u7684\u6750\u6599\u4e70\u56de\u53bb\uff0c\u4f1a\u5f71\u54cd\u9910\u9986\u8425\u4e1a\u7684\uff0c\u8981\u662f\u9910\u9986\u5012\u95ed\u4e86\uff0c\u6211\u4eec\u7684\u6f14\u51fa\u673a\u4f1a\u4e0d\u5c31\u4e5f\u6ca1\u6709\u4e86\u5417\uff1f|\u4f60\u8981\u77e5\u9053\u73b0\u5728\u77ed\u671f\u5de5\u662f\u5f88\u96be\u627e\u7684\uff0c\u6211\u4eec\u6765\u8fd9\u5ea7\u536b\u661f\u57ce\u8fd9\u4e48\u4e45\u624d\u627e\u5230\u2026\u2026",
    "\u201c\u4e00\u5bb6\u613f\u610f\u96c7\u7528\u6211\u4eec\u7684\u9910\u9986\u201d\uff0c\u8001\u59d0\uff01\u8fd9\u53e5\u8bdd\u4f60\u5df2\u7ecf\u8bf4\u4e86\u597d\u51e0\u904d\u4e86\uff01",
    "\u90a3\u4f60\u8bb0\u4f4f\u4e86\u5417\uff1f",
    "\u8bb0\u4f4f\u4e86\u8bb0\u4f4f\u4e86\uff01",
    "\u554a\u554a\uff01\u4e00\u5f00\u59cb\u660e\u660e\u662f\u56e0\u4e3a\u8001\u59d0\u8bf4\u8fd9\u5ea7\u536b\u661f\u57ce\u6709\u4ec0\u4e48\u5e86\u5178\uff0c\u60f3\u6765\u611f\u53d7\u4e00\u4e0b\uff0c\u91c7\u91c7\u98ce\uff0c\u6211\u4eec\u624d\u6765\u7684\u561b\uff01\u4f46\u6765\u4e86\u5c31\u662f\u6253\u5de5\u3001\u6f14\u51fa\u3001\u6253\u5de5\uff01\u8fd9\u53eb\u2026\u2026\u8fd9\u53eb\u2026\u2026\u672c\u672b\u5012\u7f6e\uff01\u5bf9\uff01\u672c\u672b\u5012\u7f6e\uff01\u5b9e\u5728\u662f\u592a\u65e0\u8da3\u4e86\uff01",
    "\uff08\u7edb\u96e8\u6cae\u4e27\u5730\u4f4e\u7740\u5934\uff0c\u8e22\u7740\u811a\u8fb9\u7684\u77f3\u5b50\uff0c\u77f3\u5b50\u987a\u7740\u5979\u7684\u529b\u9053\u5411\u4e00\u65c1\u98de\u4e86\u51fa\u53bb\u3002\uff09",
    "\uff08\u7edb\u96e8\u8ddf\u7740\u5c0f\u77f3\u5b50\u98de\u51fa\u7684\u65b9\u5411\u770b\u4e86\u770b\uff0c\u7a81\u7136\uff0c\u5979\u597d\u50cf\u53d1\u73b0\u4e86\u611f\u5174\u8da3\u7684\u4e1c\u897f\uff0c\u5174\u594b\u5730\u5c0f\u8dd1\u4e24\u6b65\uff0c\u5b8c\u5168\u5fd8\u8bb0\u4e86\u81ea\u5df1\u521a\u624d\u7684\u62b1\u6028\u3002\uff09",
    "\u8001\u59d0\u4f60\u770b\uff01\u90a3\u8fb9\u6709\u4e2a\u5947\u602a\u7684\u5927\u5bb6\u4f19\uff01\u957f\u5f97\u6709\u70b9\u773c\u719f\u554a\u2026\u2026",
    "\u90a3\u662f\u2026\u2026\u9cc4\u9762\u9a6c\u9e7f\uff1f\uff01\u57ce\u5e02\u91cc\u600e\u4e48\u4f1a\u6709\u751f\u9ab8\uff1f",
    "\u554a\uff1f\u662f\u53eb\u8fd9\u4e2a\u540d\u5b57\u5417\uff1f|\u6211\u600e\u4e48\u8bb0\u5f97\u2026\u2026\u6c61\u67d3\u533a\u770b\u5230\u7684\u8fd9\u79cd\u751f\u9ab8\u6ca1\u8fd9\u4e48\u9ad8\u5427\uff1f\u8fd9\u90fd\u6709\u516d\u4e03\u7c73\u4e86\u5427\uff1f",
    "\u4ed4\u7ec6\u770b\u770b\uff0c\u5b83\u7684\u5934\u4e0a\u2026\u2026\u6302\u7684\u662f\u5f69\u5e26\uff1f\u6216\u8bb8\u662f\u2026\u2026\u5e86\u5178\u7528\u7684\u9053\u5177\uff1f",
    "\u8001\u59d0\uff0c\u4e0d\u7136\u6211\u4eec\u8fc7\u53bb\u770b\u770b\u5427\uff1f",
    "\uff08\u9edb\u70df\u770b\u4e86\u770b\u65f6\u95f4\uff0c\u6b63\u5de7\u770b\u5230\u671d\u6656\u53d1\u6765\u7684\u65b0\u6d88\u606f\u3002\uff09",
    "\u53ef\u80fd\u4e0d\u884c\u4e86\u54e6\uff0c\u671d\u6656\u521a\u624d\u53d1\u4fe1\u606f\u6765\uff0c\u8bf4\u8001\u677f\u8ffd\u52a0\u4e86\u4e00\u4e9b\u91c7\u8d2d\u9700\u6c42\uff0c\u5979\u62c5\u5fc3\u6211\u4eec\u62ff\u4e0d\u52a8\uff0c\u7b49\u4e0b\u4f1a\u6765\u548c\u6211\u4eec\u6c47\u5408\u3002",
    "\u6211\u628a\u6211\u4eec\u7684\u5750\u6807\u53d1\u7ed9\u671d\u6656\uff0c\u7269\u8d44\u8981\u5c3d\u5feb\u91c7\u8d2d\u5b8c\u6bd5\u3002\u5df2\u7ecf6\u70b9\u534a\u4e86\uff0c8\u70b9\u524d\u6211\u4eec\u8981\u56de\u5230\u9910\u9986\u3002",
    "\u597d\u597d\u597d\uff01\u65f6\u95f4\u8fd8\u65e9\uff0c\u6ca1\u95ee\u9898\u7684\u5566\uff01"
  ],
  [
    "\uff08\u6cbf\u7740\u5c0f\u8def\uff0c\u9edb\u70df\u548c\u7edb\u96e8\u8d70\u5411\u90a3\u53ea\u7ad9\u5728\u7a7a\u5730\u4e0a\u3001\u770b\u8d77\u6765\u6781\u5176\u663e\u773c\u7684\u5de8\u517d\u3002\uff09",
    "\u4ec0\u4e48\u561b\uff0c\u8fd9\u660e\u663e\u770b\u7740\u5c31\u662f\u4eba\u9020\u7684\u4e1c\u897f\u561b\u2026\u2026|\u5b83\u8eab\u4e0a\u8fd9\u662f\u4ec0\u4e48\uff1f\u600e\u4e48\u8fd8\u82b1\u91cc\u80e1\u54e8\u7684\uff1f\u751f\u9ab8\u8fd8\u6d41\u884c\u7eb9\u8eab\u5417\uff1f",
    "\u8fd9\u79cd\u6837\u5f0f\uff0c\u5e94\u8be5\u662f\u201c\u4e91\u7eb9\u201d\u3002|\u4e0d\u8fc7\uff0c\u6c61\u67d3\u533a\u91cc\u90a3\u79cd\u51f6\u6076\u7684\u751f\u9ab8\uff0c\u56e0\u4e3a\u8eab\u4e0a\u5e03\u6ee1\u4e86\u82b1\u82b1\u7eff\u7eff\u7684\u7ef8\u5e26\u548c\u5938\u5f20\u7684\u5f69\u7ed8\uff0c\u52a0\u4e0a\u773c\u775b\u90e8\u5206\u662f\u7a7a\u767d\uff0c\u53cd\u800c\u770b\u8d77\u6765\u6709\u4e9b\u61a8\u539a\u4e86\uff0c\u771f\u6709\u610f\u601d\u3002",
    "\u2026\u2026\u6211\u600e\u4e48\u89c9\u5f97\u8001\u59d0\u4f60\u662f\u5728\u505a\u4e00\u4e9b\u5947\u5947\u602a\u602a\u7684\u89e3\u8bf4\uff1f",
    "\u6709\u5417\uff1f\u9519\u89c9\u5427\u3002",
    "\uff08\u4e24\u4eba\u8d70\u5230\u4e86\u5de8\u517d\u811a\u4e0b\u3002\uff09",
    "\uff08\u56e0\u65bd\u5de5\u800c\u642d\u8bbe\u7684\u811a\u624b\u67b6\u963b\u62e6\u4f4f\u4e86\u4e24\u4eba\u7684\u811a\u6b65\u3002\u9edb\u70df\u9694\u7740\u811a\u624b\u67b6\u62ac\u8d77\u5934\uff0c\u4ed4\u7ec6\u5730\u89c2\u5bdf\u8d77\u6765\u3002\uff09",
    "\u6211\u8bb0\u5f97\uff0c\u5728\u53e4\u8001\u7684\u4f20\u8bf4\u4e2d\uff0c\u4eba\u4eec\u4f1a\u5728\u6625\u5929\u5230\u6765\u4e4b\u524d\uff0c\u5c06\u5f62\u4f3c\u745e\u517d\u7684\u7269\u4ef6\u6216\u8005\u526a\u753b\u6302\u5728\u95e8\u524d\uff0c\u963b\u6321\u5384\u8fd0\u5165\u4fb5\uff0c\u4ee5\u6c42\u6765\u5e74\u5e73\u5b89\u987a\u9042\u3001\u98ce\u8c03\u96e8\u987a\uff0c\u6d41\u4f20\u5230\u73b0\u5728\uff0c\u5c31\u6f14\u53d8\u6210\u4e86\u4e00\u79cd\u8ba8\u5f69\u5934\uff0c\u6c42\u5409\u7965\u7684\u5e86\u5178\u6d3b\u52a8\u3002",
    "\u6211\u770b\u8fc7\u4e00\u4e9b\u8d44\u6599\uff0c\u8fd9\u5ea7\u536b\u661f\u57ce\uff0c\u56e0\u4e3a\u5468\u8fb9\u6709\u519c\u7530\uff0c\u4fbf\u5728\u57ce\u5e02\u5e86\u5178\u7684\u6d3b\u52a8\u4e2d\u52a0\u5165\u4e86\u8fd9\u4e00\u73af\u8282\uff0c\u636e\u8bf4\u5df2\u7ecf\u6301\u7eed\u597d\u51e0\u5e74\u4e86\u3002\u770b\u8d77\u6765\uff0c\u8fd9\u91cc\u5e94\u8be5\u662f\u628a\u201c\u9cc4\u9762\u9a6c\u9e7f\u201d\u5f53\u6210\u745e\u517d\u4e86\u3002",
    "\u771f\u7a00\u5947\uff0c\u4f60\u8fd8\u77e5\u9053\u8fd9\u4e9b\uff1f",
    "\uff08\u7edb\u96e8\u548c\u9edb\u70df\u7684\u5bf9\u8bdd\u5438\u5f15\u4e86\u6b63\u5728\u201c\u9cc4\u9762\u9a6c\u9e7f\u201d\u8eab\u4e0a\u6d82\u88c5\u7684\u5de5\u4eba\u3002\uff09",
    "\uff08\u4ed6\u8df3\u4e0b\u6765\uff0c\u817f\u4e00\u76d8\u5750\u5728\u811a\u624b\u67b6\u4e0a\uff0c\u4e0a\u4e0b\u6253\u91cf\u8d77\u4e24\u4eba\u3002\uff09",
    "\u662f\u4eba\u5f62\uff1f\u96be\u5f97\u4ece\u4f60\u4eec\u8fd9\u4e9b\u4eba\u5f62\u53e3\u4e2d\u542c\u5230\u8fd9\u4e9b\u4e60\u4fd7\u3002|\u4f60\u4eec\u4e0d\u662f\u5929\u5929\u90fd\u662f\u4e00\u5957\u6a21\u5f0f\u201c\u4f60\u597d\u8c22\u8c22\u518d\u89c1\u6b22\u8fce\u4e0b\u6b21\u5149\u4e34\u201d\u5417\uff1f\u600e\u4e48\u8fd8\u80fd\u77e5\u9053\u8fd9\u4e9b\uff1f",
    "\u4eba\u5f62\u6709\u5f88\u591a\u4e0d\u540c\u7684\u7c7b\u578b\uff0c\u4e5f\u4f1a\u8d1f\u8d23\u4e0d\u540c\u7684\u5de5\u4f5c\u3002\u60a8\u63d0\u5230\u7684\u90a3\u79cd\u4eba\u5f62\uff0c\u5927\u591a\u662f\u5728\u5404\u7c7b\u9910\u9986\u5de5\u4f5c\u7684\u3002|\u6211\u77e5\u9053\u8fd9\u4e9b\uff0c\u53ea\u662f\u6070\u597d\u770b\u8fc7\u4e00\u4e9b\u8d44\u6599\uff0c\u7ed3\u5408\u8fd9\u53f0\u8bbe\u5907\u7684\u6837\u8c8c\uff0c\u505a\u51fa\u4e86\u4e00\u4e9b\u5408\u7406\u63a8\u6d4b\u3002",
    "\u679c\u7136\u662f\u673a\u68b0\u8111\u5b50\uff0c\u770b\u5230\u5c31\u80fd\u8bb0\u5f97\u3002\u54c8~\u8bf4\u8bf4\u770b\uff0c\u4f60\u662f\u505a\u4ec0\u4e48\u7684\uff1f\u600e\u4e48\u8fd8\u80fd\u770b\u5230\u90a3\u4e9b\u4f20\u8bf4\uff1f\u8fd9\u4e2a\u5e74\u4ee3\uff0c\u90a3\u4e9b\u8d44\u6599\u53ef\u662f\u5f88\u96be\u89c1\u5230\u554a\u3002",
    "\u2026\u2026\u6211\u4eec\uff0c\u73b0\u5728\u5728\u4e00\u5bb6\u9910\u9986\u5de5\u4f5c\u3002",
    "\u54c8\uff1f",
    "\uff08\u5de5\u4eba\u9732\u51fa\u7591\u60d1\u7684\u795e\u60c5\uff0c\u4ed6\u76b1\u7740\u7709\u5934\u53cd\u590d\u6253\u91cf\u4e24\u4eba\u3002\uff09",
    "\u2026\u2026\u521a\u624d\u4f60\u4e0d\u662f\u8bf4\u90a3\u79cd\u4eba\u5f62\u5c31\u662f\u9910\u9986\u4eba\u5f62\uff1f\u6211\u542c\u9519\u4e86\uff1f",
    "\u9910\u9986\u2014\u2014\u9910\u9986\u5de5\u4f5c\u4e5f\u4f1a\u6709\u5404\u79cd\u7c7b\u578b\u7684\u5566\uff01\u6211\u4eec\u662f\u8d1f\u8d23\u5728\u9910\u9986\u6f14\u594f\u7684\u54e6\uff01",
    "\u54e6\uff0c\u884c\u5427\u884c\u5427\u3002|\u867d\u7136\u662f\u4eba\u5f62\uff0c\u4e0d\u8fc7\u4f60\u4eec\u80fd\u77e5\u9053\u8fd9\u4e9b\uff0c\u6211\u8fd8\u633a\u5f00\u5fc3\u7684\u3002\u5728\u8fd9\u91cc\u5fd9\u4e86\u8fd9\u4e48\u4e45\uff0c\u8001\u6709\u4eba\u8fc7\u6765\u95ee\uff0c\u4e00\u904d\u4e00\u904d\u8bf4\uff0c\u70e6\u90fd\u70e6\u6b7b\u4e86\u3002",
    "\u8fd9\u745e\u517d\u53ef\u662f\u6709\u540d\u5b57\u7684\uff0c\u53eb\u201c\u4f0a\u4f2f\u201d\u3002\u6211\u8ddf\u4f60\u4eec\u8bf4\uff0c\u8fd9\u4e24\u5929\u8fd8\u53ea\u662f\u5e86\u5178\u7684\u9884\u70ed\u9636\u6bb5\uff0c\u540e\u5929\uff0c\u5c31\u662f\u5e86\u5178\u65e5\u5f53\u5929\uff0c\u4f1a\u6709\u66f4\u591a\u6d3b\u52a8\uff0c\u8fd9\u5927\u5bb6\u4f19\u4e5f\u8981\u5927\u51fa\u98ce\u5934\uff0c\u4f60\u4eec\u4e00\u5b9a\u8981\u6765\u770b\u554a\u3002",
    "\u5b83\uff1f\u5927\u51fa\u98ce\u5934\uff1f",
    "\u90a3\u662f\uff0c\u5728\u5e86\u5178\u65e5\u5f53\u5929\uff0c\u8981\u7531\u57ce\u91cc\u6700\u6d3b\u8dc3\u7684\u5c0f\u4f19\u5b50\u722c\u4e0a\u76f4\u901a\u821e\u53f0\u9876\u90e8\u7684\u68af\u5b50\uff0c\u4e3a\u8fd9\u5bb6\u4f19\u201c\u70b9\u775b\u201d\uff0c\u8fd9\u624d\u7b97\u8fdb\u5165\u4e86\u5e86\u5178\u6700\u9ad8\u6f6e\u5462\uff01\u7f3a\u4e86\u8fd9\u4e2a\u73af\u8282\uff0c\u5927\u5bb6\u4f19\u53ef\u90fd\u4e0d\u4f1a\u7b54\u5e94\u7684\uff01",
    "\u54c8\uff0c\u90fd7\u70b9\u4e86\u554a\uff0c\u4f60\u4eec\u6709\u5174\u8da3\u5c31\u5728\u8fd9\u91cc\u591a\u770b\u4f1a\u5427\u3002|\u7b49\u641e\u5b9a\u8fd9\u4e2a\u5927\u5bb6\u4f19\u4e4b\u540e\uff0c\u6211\u4eec\u8fd8\u8981\u53bb\u5927\u5403\u4e00\u987f\u3002",
    "\uff08\u5de5\u4eba\u62cd\u4e86\u62cd\u201c\u4f0a\u4f2f\u201d\u7c97\u58ee\u7684\u524d\u817f\uff0c\u56de\u5230\u5c97\u4f4d\u4e0a\u7ee7\u7eed\u6572\u6572\u6253\u6253\u3002\uff09",
    "\uff08\u9edb\u70df\u5fae\u7b11\u7740\u5411\u7740\u5de5\u4eba\u6b20\u4e86\u6b20\u8eab\uff0c\u8eab\u65c1\u7684\u7edb\u96e8\u542c\u5230\u201c\u5e86\u5178\u201d\uff0c\u8822\u8822\u6b32\u52a8\u8d77\u6765\u3002\uff09",
    "\u540e\u5929\u5c31\u662f\u5e86\u5178\u65e5\uff01\u66f4\u591a\u6d3b\u52a8\uff01",
    "\u8001\u59d0\uff0c\u6211\u4eec\u6765CHE-02\u6709\u4e00\u5468\u4e86\u6b38\uff0c\u9664\u4e86\u6392\u7ec3\u5c31\u662f\u6f14\u51fa\uff0c\u6211\u90fd\u5feb\u65e0\u804a\u5230\u53d1\u9709\u4e86\uff01\u8001\u59d0\uff0c\u5c31\u628a\u540e\u5929\u884c\u7a0b\u7a7a\u4e0b\u6765\u6211\u4eec\u4e00\u8d77\u51fa\u6765\u901b\u901b\u597d\u4e0d\u597d\uff1f\u597d\u4e0d\u597d\u561b~",
    "\u603b\u662f\u5728\u73a9\uff0c\u5e2e\u671d\u6656\u7ef4\u4fee\u7d20\u4f53\u7684\u94b1\u8981\u6512\u5230\u4ec0\u4e48\u65f6\u5019\uff1f",
    "\u53ef\u8001\u59d0\u4f60\u4e0d\u662f\u8fd8\u8981\u5199\u8fd9\u5ea7\u57ce\u5e02\u7684\u5370\u8c61\u66f2\u5417\uff1f\u53ea\u662f\u901a\u8fc7\u8d44\u6599\u548c\u9053\u542c\u9014\u8bf4\u600e\u4e48\u80fd\u884c\u5566\uff0c\u8981\u628a\u81ea\u5df1\u653e\u5230\u8fd9\u5ea7\u57ce\u5e02\u7684\u6c1b\u56f4\u91cc\uff0c\u624d\u80fd\u4f53\u4f1a\u5230\u5b83\u72ec\u6709\u7684\u98ce\u683c\u561b\u3002",
    "\u8001\u59d0\uff0c\u8001\u59d0\uff0c\u8001\u59d0\uff0c\u4e00\u5929\uff0c\u5c31\u4e00\u5929\u2014\u2014\u4e0d\u4e0d\u4e0d\u534a\u5929\u597d\u4e0d\u597d\uff01\u534a\u5929\u554a\u534a\u5929\u5566\u5c31\u53ea\u534a\u5929\u2014\u2014",
    "\u597d\u4e86\u597d\u4e86\uff0c\u771f\u62ff\u4f60\u6ca1\u529e\u6cd5\u3002",
    "\u6700\u8fd1\u5927\u5bb6\u786e\u5b9e\u5f88\u8f9b\u82e6\uff0c\u540e\u5929\u662f\u5e86\u5178\u65e5\uff0c\u9910\u9986\u591a\u5c11\u4e5f\u4f1a\u63d0\u524d\u6253\u70ca\u3002\u6211\u4eec\u5c31\u4e00\u8d77\u53c2\u52a0\u5e86\u5178\u2014\u2014\u987a\u4fbf\u4e5f\u89c1\u8bc6\u4e00\u4e0b\u201c\u4f0a\u4f2f\u201d\u7684\u98ce\u91c7\uff0c\u653e\u677e\u4e00\u4e0b\u5427\u3002",
    "\u597d\u8036\uff01\u6211\u8001\u59d0\u4e16\u754c\u65e0\u654c\u5b87\u5b99\u7b2c\u4e00\uff01|\u4e3a\u4e86\u611f\u8c22\u4eb2\u7231\u7684\u8001\u59d0\uff0c\u4eca\u665a\u6211\u51b3\u5b9a\u5411\u5927\u5bb6\u5c55\u793a\u6211\u65b0\u5b66\u4f1a\u7684\u9f13\u70b9\uff01\u8001\u59d0\uff0c\u4f60\u5c31\u7b49\u7740\u770b\u6211\u60ca\u8273\u5168\u573a\u5427\uff01",
    "\uff08\u7edb\u96e8\u6b22\u547c\u7740\u8f6c\u4e86\u51e0\u5708\uff0c\u8fd8\u4e00\u628a\u62b1\u4f4f\u9edb\u70df\uff0c\u6765\u8868\u8fbe\u81ea\u5df1\u7684\u559c\u60a6\u3002\uff09",
    "\u4f60\u5440\uff0c\u53c8\u5728\u54ea\u91cc\u5b66\u7684\u8fd9\u4e9b\u5947\u602a\u7684\u8bf4\u6cd5\uff0c\u600e\u4e48\u8ddf\u514b\u62c9\u4e3d\u838e\u4e00\u6837\uff0c\u603b\u80fd\u8e66\u51fa\u4e9b\u5947\u602a\u7684\u8bcd\u2026\u2026|\u4e0d\u8fc7\uff0c\u4f60\u7b2c\u4e00\u6b21\u7528\u6253\u9f13\u7684\u65b9\u5f0f\u548c\u6211\u914d\u5408\uff0c\u6211\u8fd8\u662f\u6709\u70b9\u671f\u5f85\u7684\u54e6\u3002",
    "\uff08\u4e24\u4eba\u4ece\u7a7a\u5730\u79bb\u5f00\uff0c\u6162\u6162\u5730\u8d70\u8fdb\u9644\u8fd1\u7684\u5c0f\u5df7\u5b50\u91cc\u3002\uff09",
    "\uff08\u8857\u8fb9\u7684\u5c0f\u5e97\u9646\u7eed\u6253\u5f00\u5e97\u95e8\uff0c\u5f00\u59cb\u8425\u4e1a\uff0c\u7edb\u96e8\u5174\u594b\u5730\u4e1c\u77a7\u897f\u770b\u3002\uff09",
    "\u8001\u59d0\uff0c\u4f60\u770b\u8fd9\u4e2a\uff01",
    "\uff08\u7edb\u96e8\u732e\u5b9d\u4f3c\u5730\u4e3e\u8d77\u4e00\u4e2a\u5f88\u5c0f\u7684\u73bb\u7483\u74f6\uff0c\u4f38\u5230\u9edb\u70df\u773c\u524d\u3002\uff09",
    "\u8001\u59d0\u4e0d\u662f\u8bf4\u8981\u6536\u96c6\u7075\u611f\u5417\uff0c\u4f60\u770b\u8fd9\u4e2a\u74f6\u5b50\u5408\u4e0d\u5408\u9002\uff1f|\u4e0a\u9762\u8fd8\u6302\u7740\u7ef3\u5b50\uff0c\u662f\u4e00\u6761\u540a\u5760\u5427\uff1f",
    "\u201c\u7075\u611f\u201d\u53c8\u4e0d\u662f\u7269\u54c1\uff0c\u600e\u4e48\u80fd\u2014\u2014",
    "\uff08\u5c0f\u73bb\u7483\u74f6\u540a\u5760\u5728\u7edb\u96e8\u7684\u624b\u4e2d\u6643\u52a8\uff0c\u5728\u706f\u5149\u7684\u7167\u5c04\u4e0b\u71a0\u71a0\u751f\u8f89\u3002\uff09",
    "\uff08\u9edb\u70df\u7684\u76ee\u5149\u4e0d\u7531\u5730\u88ab\u5c0f\u73bb\u7483\u74f6\u5438\u5f15\uff0c\u5979\u4ece\u7edb\u96e8\u624b\u91cc\u63a5\u8fc7\u540a\u5760\u3002\uff09",
    "\u559c\u6b22\u7684\u8bdd\uff0c\u5c31\u4e70\u4e0b\uff1f",
    "\uff08\u51b7\u6de1\u7684\u58f0\u97f3\u4ece\u4e0d\u8fdc\u5904\u7684\u4e00\u5806\u8d27\u7269\u540e\u4f20\u6765\u3002\uff09",
    "\uff08\u2014\u2014\u90a3\u5806\u8d27\u7269\uff0c\u6b63\u5300\u901f\u79fb\u52a8\u7740\u5411\u4e24\u4eba\u9760\u8fd1\u3002\uff09",
    "\u671d\u6656\uff01\u4e0d\u597d\u610f\u601d\uff0c\u8fd8\u8981\u8ba9\u4f60\u8fc7\u6765\u3002|\u8001\u677f\u4e34\u65f6\u52a0\u8d2d\u4e86\u8fd9\u4e48\u591a\u4e1c\u897f\uff0c\u4f60\u5168\u90e8\u4e70\u597d\u4e86\uff1f",
    "\uff08\u505c\u4e0b\u76db\u653e\u7740\u8d27\u7269\u7684\u63a8\u8f66\uff0c\u671d\u6656\u770b\u5411\u9edb\u70df\uff0c\u70b9\u4e86\u70b9\u5934\u3002\uff09",
    "\u8001\u677f\u8ffd\u52a0\u4e8610\u888b\u9a6c\u94c3\u85af\u7684\u5e72\u82b1\uff0c\u6211\u8fd8\u987a\u4fbf\u628a\u6e05\u5355\u4e0a\u4f60\u4eec\u6ca1\u6709\u6253\u52fe\u7684\u7269\u54c1\u4e5f\u4e00\u8d77\u4e70\u4e86\uff0c\u6ca1\u60f3\u5230\uff0c\u5c31\u7b97\u6211\u5411\u8001\u677f\u501f\u4e86\u8f86\u63a8\u8f66\uff0c\u8fd8\u662f\u653e\u4e0d\u4e0b\u3002",
    "\uff08\u9762\u524d\u7684\u671d\u6656\u4e00\u624b\u63a8\u7740\u98df\u6750\u5806\u5f97\u5c0f\u5c71\u4e00\u6837\u7684\u63a8\u8f66\uff0c\u4e00\u624b\u62b1\u7740\u6ee1\u6ee1\u5f53\u5f53\u7684\u76d2\u5b50\u7bb1\u5b50\u888b\u5b50\u3002\uff09",
    "\uff08\u9edb\u70df\u7684\u76ee\u5149\u5b9a\u5728\u5979\u7684\u53f3\u624b\u81c2\u4e0e\u8eab\u4f53\u7684\u8fde\u63a5\u5904\uff0c\u90a3\u91cc\u6709\u51e0\u4e2a\u8f74\u627f\u53d1\u51fa\u7ec6\u5fae\u7684\u201c\u5431\u5440\u201d\u58f0\u3002\uff09",
    "\u660e\u660e\u662f\u8be5\u6211\u4eec\u4e24\u4e2a\u91c7\u8d2d\u7684\u2026\u2026\u8fd9\u4e0b\u5168\u90fd\u8ba9\u4f60\u505a\u4e86\u3002\u4f60\u7684\u53f3\u624b\u6ca1\u4e8b\u5427\uff1f",
    "\u6ca1\u4e8b\u3002",
    "\uff08\u5306\u5306\u4e70\u4e0b\u540a\u5760\uff0c\u9edb\u70df\u4f38\u624b\u60f3\u8981\u63a5\u8fc7\u671d\u6656\u62b1\u7740\u7684\u4e00\u534a\u8d27\u7269\uff0c\u671d\u6656\u4e5f\u6ca1\u6709\u62d2\u7edd\uff0c\u4e24\u4eba\u5f88\u81ea\u7136\u5730\u4ea4\u63a5\u4e86\u624b\u91cc\u7684\u7269\u54c1\u3002\uff09",
    "\uff08\u7b49\u9edb\u70df\u518d\u56de\u5934\u5bfb\u627e\u7edb\u96e8\u7684\u65f6\u5019\uff0c\u5df2\u7ecf\u4e0d\u89c1\u5979\u4eba\u5f71\u4e86\u3002\uff09",
    "\u7edb\u96e8\u662f\u4e0d\u662f\u6709\u4e9b\u8fc7\u4e8e\u6d3b\u8dc3\u4e86\uff1f\u4e00\u5206\u949f\u524d\u8fd8\u5728\u8fd9\u91cc\u7684\u3002|\u867d\u7136\u6211\u5df2\u7ecf\u5728\u5c3d\u529b\u8ba9\u81ea\u5df1\u4e0d\u8981\u592a\u5520\u53e8\uff0c\u4f46\u770b\u5979\u8fd9\u83bd\u649e\u7684\u6837\u5b50\uff0c\u8fd8\u662f\u8ba9\u4eba\u653e\u5fc3\u4e0d\u4e0b\u3002",
    "\u5979\u6700\u8fd1\u5e76\u6ca1\u6709\u505a\u4ec0\u4e48\u51fa\u683c\u7684\u4e8b\uff0c\u4f60\u4e5f\u53ef\u4ee5\u591a\u76f8\u4fe1\u5979\u4e00\u70b9\u3002",
    "\u4f46\u613f\u5979\u662f\u771f\u7684\u6210\u719f\u4e86\u3002",
    "\u4eba\u9010\u6e10\u591a\u8d77\u6765\u4e86\u5462\uff0c\u4e5f\u4e0d\u77e5\u9053\u5979\u8dd1\u5230\u54ea\u91cc\u53bb\u4e86\uff1f\u73b0\u5728\u662f\u2026\u20267\u70b920\uff0c\u8981\u5c3d\u5feb\u8d76\u56de\u9910\u9986\u3002|\u7b49\u7b49\uff0c\u6211\u597d\u50cf\u542c\u5230\u97f3\u4e50\u58f0\uff1f",
    "\u5728\u9694\u58c1\u8857\u9053\u91c7\u8d2d\u7684\u65f6\u5019\u6211\u6709\u770b\u5230\uff0c\u662f\u5c45\u6c11\u4eec\u81ea\u53d1\u4e3e\u884c\u7684\u82b1\u8f66\u5de1\u6e38\u6d3b\u52a8\u3002",
    "\u90a3\uff0c\u7edb\u96e8\uff0c\u53bb\u2026\u2026\u770b\u82b1\u8f66\u4e86\uff1f",
    "\u5e94\u8be5\u662f\uff0c\u6211\u521a\u770b\u5230\u4f60\u7684\u65f6\u5019\uff0c\u5979\u5927\u53eb\u7740\u5411\u8fd9\u6761\u5c0f\u5df7\u8dd1\u53bb\u4e86\u3002|\u8d70\u8fd9\u91cc\u3002",
    "\uff08\u671d\u6656\u9762\u65e0\u8868\u60c5\u5730\u8f6c\u4e86\u4e2a\u65b9\u5411\uff0c\u5e26\u7740\u9edb\u70df\u5411\u82b1\u8f66\u5de1\u6e38\u6240\u5728\u7684\u8857\u9053\u8d70\u53bb\u3002\uff09",
    "\u4e0d\u597d\u610f\u601d\uff0c\u53c8\u662f\u5728\u9ebb\u70e6\u4f60\u2026\u2026",
    "\u554a\uff0c\u7a0d\u7b49\u2026\u2026",
    "\uff08\u671d\u6656\u4e3b\u52a8\u63a5\u8fc7\u4e86\u9edb\u70df\u62b1\u7740\u7684\u4e00\u90e8\u5206\u8d27\u7269\uff0c\u9edb\u70df\u8d76\u5feb\u62ff\u51fa\u4e86\u54cd\u4e2a\u4e0d\u505c\u7684\u7ec8\u7aef\u3002\uff09",
    "\u7edb\u96e8\uff1f\u4f60\u73b0\u5728\u5728\u54ea\u2014\u2014",
    "\uff08\u8fd8\u6ca1\u7b49\u9edb\u70df\u95ee\u5b8c\uff0c\u7edb\u96e8\u7684\u58f0\u97f3\u5c31\u8feb\u4e0d\u53ca\u5f85\u5730\u4ece\u5608\u6742\u7684\u80cc\u666f\u97f3\u4e2d\u5192\u4e86\u51fa\u6765\u3002\uff09",
    "\u8001\u59d0\uff01\u662f\u82b1\u8f66\u6b38\uff01|\u6211\u8fd9\u91cc\u8d85\u7ea7\u70ed\u95f9\uff0c\u4e0a\u9762\u8fd8\u6709\u4f1a\u8f6c\u52a8\uff0c\u5e26\u52a8\u753b\u7684\u706f\uff01",
    "\u54c7\u54e6\uff0c\u7a81\u7136\u65cb\u8f6c\u7740\u5192\u51fa\u6765\u4e00\u4e2a\u5f88\u5947\u602a\u7684\u4e1c\u897f\uff0c\u597d\u50cf\u4e2a\u5927\u706f\u7b3c\uff01\u54c8\u54c8\u54c8\u3002|\u7b49\u7b49\u2026\u2026\u90a3\u662f\u2026\u2026\u94c1\u8840\u72d7\uff1f\uff01"
  ]
]